{
  ".workspace-leaf-content[data-type=\"markdown\"]": {
    ".theme-light .markdown-preview-view, .theme-light .view-content { background": {
      "light": "none",
      "dark": null
    },
    "background-image": {
      "light": "radial-gradient(circle at 30% 30%, #ffdab9, transparent 60%), radial-gradient(circle at 70% 70%, #fffacd, transparent 60%), radial-gradient(circle at 50% 50%, #f08080, transparent 60%), radial-gradient(circle at 50% 50%, #b0e0e6, transparent 60%), radial-gradient(circle at 30% 70%, #98ff98, transparent 60%), radial-gradient(circle at 70% 30%, #add8e6, transparent 60%), radial-gradient(circle at 60% 40%, #e6e6fa, transparent 60%), radial-gradient(circle at 40% 60%, #ffb6c1, transparent 60%)",
      "dark": "radial-gradient(circle at 30% 30%, #19123fff, transparent 60%), radial-gradient(circle at 70% 70%, #483d8b, transparent 60%), radial-gradient(circle at 50% 50%, #191970, transparent 60%), radial-gradient(circle at 50% 50%, #8b008b, transparent 60%), radial-gradient(circle at 30% 70%, #008b8b, transparent 60%), radial-gradient(circle at 70% 30%, #2f4f4f, transparent 60%), radial-gradient(circle at 60% 40%, #556b2f, transparent 60%), radial-gradient(circle at 40% 60%, #696969, transparent 60%)"
    },
    ".theme-dark .markdown-preview-view, .theme-dark .view-content { background": {
      "light": null,
      "dark": "none"
    }
  },
  ".workspace-split.mod-vertical": {
    ".theme-light .workspace-split.mod-horizontal, .theme-light .workspace-leaf-content, .theme-dark .workspace-split.mod-vertical, .theme-dark .workspace-split.mod-horizontal, .theme-dark .workspace-leaf-content { background": {
      "light": "none !important",
      "dark": null
    }
  },
  ".workspace-tab-header-container": {
    ".theme-light .workspace-tab-header.is-active { background-color": {
      "light": "#ffffff",
      "dark": null
    },
    ".theme-dark .workspace-tab-header.is-active { background-color": {
      "light": null,
      "dark": "#000000"
    }
  },
  ".markdown-preview-view": {
    ".theme-light .cm-s-obsidian { color": {
      "light": "var(--text-normal)",
      "dark": null
    },
    ".theme-dark .cm-s-obsidian { color": {
      "light": null,
      "dark": "var(--text-normal)"
    }
  },
  "h1": { "font-size": "2.5em", "margin-bottom": "0.5em" },
  "h2": { "font-size": "1.75em", "margin-bottom": "0.5em" },
  "h3": { "font-size": "1.5em", "margin-bottom": "0.5em" },
  "h4": { "font-size": "1.25em", "margin-bottom": "0.5em" },
  "h5": { "font-size": "1em", "margin-bottom": "0.5em" },
  "h6": { "font-size": "0.75em", "margin-bottom": "0.5em" },
  ".cm-s-obsidian HyperMD-codeblock": {
    ".theme-light .markdown-preview-view, .theme-light .markdown-preview-view .math-block { background-color": {
      "light": "var(--code-background-light)",
      "dark": null
    },
    ".theme-dark .markdown-preview-view, .theme-dark .markdown-preview-view .math-block { background-color": {
      "light": null,
      "dark": "var(--code-background-dark)"
    }
  },
  ".mermaid > svg": {
    "filter": {
      "light": null,
      "dark": "invert(100%) hue-rotate(180deg) saturate(1.25)"
    },
    "} .markdown-source-view.mod-cm6 .cm-hmd-codeblock { --indentation-guide-editing-indent": {
      "light": null,
      "dark": "0"
    },
    "} .markdown-source-view.mod-cm6 .cm-preview-code-block pre { margin": {
      "light": null,
      "dark": "0"
    },
    "} .markdown-source-view.mod-cm6 .code-block-flair { position": {
      "light": null,
      "dark": "absolute"
    },
    "right": { "light": null, "dark": "6px" },
    "top": { "light": null, "dark": "6px" },
    "z-index": { "light": null, "dark": "1" },
    "display": { "light": null, "dark": "inline-block" },
    "padding": { "light": null, "dark": "var(--size-4-1) var(--size-4-2)" },
    "border-radius": { "light": null, "dark": "var(--code-radius)" },
    "font-family": { "light": null, "dark": "var(--font-interface)" },
    "font-size": { "light": null, "dark": "var(--font-ui-smaller)" },
    "color": { "light": null, "dark": "var(--text-muted)" },
    "cursor": { "light": null, "dark": "var(--cursor)" }
  }
}

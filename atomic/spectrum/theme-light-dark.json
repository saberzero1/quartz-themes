    "display": { "light": "inline-block", "dark": null },
    "height": { "light": "1rem", "dark": null },
    "left": { "light": "-1.3rem", "dark": null },
    "position": { "light": "absolute", "dark": null },
    "top": { "light": "var(--size-1)", "dark": null },
    "width": { "light": "1rem", "dark": null }

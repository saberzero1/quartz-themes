    "background-color": { "light": "#262626", "dark": null },
    "box-shadow": { "light": "inset 0px 0px 0px 1px #363636", "dark": null }
  },
  "a.external-link[href*=\"github\"]:before": {
    "background-image": "url(\"data",
    "background-repeat": "no-repeat",
    "content": "\"\"",
    "display": "inline-block",
    "height": "1rem",
    "left": "-1.3rem",
    "position": "absolute",
    "top": "var(--size-1)",
    "width": "1rem"
  },
  "a.external-link[href*=\"wikipedia\"]:before": {
    "background-image": "url(\"data",
    "background-repeat": "no-repeat",
    "content": "\"\"",
    "display": "inline-block",
    "height": "1rem",
    "left": "-1.3rem",
    "position": "absolute",
    "top": "var(--size-1)",
    "width": "1rem"
  },
  "a.external-link[href*=\"medium\"]:before": {
    "background-image": "url(\"data",
    "background-repeat": "no-repeat",
    "content": "\"\"",
    "display": "inline-block",
    "height": "1rem",
    "left": "-1.3rem",
    "position": "absolute",
    "top": "var(--size-1)",
    "width": "1rem"
  },
  "a.external-link[href*=\"dev.to\"]:before": {

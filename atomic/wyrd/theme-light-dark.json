{
  "body": {
    "color-scheme": { "light": "light", "dark": null },
    "--highlight-mix-blend-mode": { "light": "darken", "dark": null },
    "--mono-rgb-0": { "light": "255, 255, 255", "dark": null },
    "--mono-rgb-100": { "light": "0, 0, 0", "dark": null },
    "--color-orange-rgb": { "light": "236, 117, 0", "dark": null },
    "--color-yellow-rgb": { "light": "224, 172, 0", "dark": null },
    "--color-cyan-rgb": { "light": "0, 191, 188", "dark": null },
    "--color-blue-rgb": { "light": "8, 109, 221", "dark": null },
    "--color-purple-rgb": { "light": "120, 82, 238", "dark": null },
    "--color-pink-rgb": { "light": "213, 57, 132", "dark": null },
    "--color-accent-hsl": {
      "light": "var(--accent-h), var(--accent-s), var(--accent-l)",
      "dark": null
    },
    "--color-accent": {
      "light": "hsl(var(--accent-h), var(--accent-s), var(--accent-l))",
      "dark": null
    },
    "--color-accent-1": {
      "light": "hsl(\n    calc(var(--accent-h) - 1),\n    calc(var(--accent-s) * 1.01),\n    calc(var(--accent-l) * 1.075)\n  )",
      "dark": null
    },
    "--color-accent-2": {
      "light": "hsl(\n    calc(var(--accent-h) - 3),\n    calc(var(--accent-s) * 1.02),\n    calc(var(--accent-l) * 1.15)\n  )",
      "dark": null
    },
    "--background-secondary-alt": {
      "light": "var(--color-base-05)",
      "dark": null
    },
    "--background-modifier-box-shadow": { "light": "#000000", "dark": null },
    "--background-modifier-cover": { "light": "#dcdcdc", "dark": null },
    "--input-shadow": {
      "light": "inset 0 0 0 1px #000000, 0 2px 3px 0 #000000, 0 1px 1.5px 0 #000000,\n    0 1px 2px 0 #000000, 0 0 0 0 transparent",
      "dark": null
    },
    "--input-shadow-hover": {
      "light": "inset 0 0 0 1px #000000, 0 2px 3px 0 #000000, 0 1px 1.5px 0 #000000,\n    0 1px 2px 0 #000000, 0 0 0 0 transparent",
      "dark": null
    },
    "--shadow-s": {
      "light": "0px 1px 2px #000000, 0px 3.4px 6.7px #000000, 0px 15px 30px #000000",
      "dark": null
    },
    "--shadow-l": {
      "light": "0px 1.8px 7.3px #000000, 0px 6.3px 24.7px #000000, 0px 30px 90px #000000",
      "dark": null
    },
    "--color-blue": { "light": "var(--wyrd-blue)", "dark": null },
    "--color-cyan": { "light": "var(--wyrd-cyan)", "dark": null },
    "--color-green": { "light": "var(--wyrd-green)", "dark": null },
    "--color-green-rgb": { "light": "var(--wyrd-green-rgb)", "dark": null },
    "--color-orange": { "light": "var(--wyrd-orange)", "dark": null },
    "--color-pink": { "light": "var(--wyrd-pink)", "dark": null },
    "--color-purple": { "light": "var(--wyrd-purple)", "dark": null },
    "--color-red": { "light": "var(--wyrd-red)", "dark": null },
    "--color-red-rgb": { "light": "var(--wyrd-red-rgb)", "dark": null },
    "--color-yellow": { "light": "var(--wyrd-yellow)", "dark": null },
    "--color-base-00": { "light": "var(--wyrd-base-100)", "dark": null },
    "--color-base-05": { "light": "var(--wyrd-base-95)", "dark": null },
    "--color-base-10": { "light": "var(--wyrd-base-90)", "dark": null },
    "--color-base-20": { "light": "var(--wyrd-base-80)", "dark": null },
    "--color-base-25": { "light": "var(--wyrd-base-70)", "dark": null },
    "--color-base-30": { "light": "var(--wyrd-base-60)", "dark": null },
    "--color-base-35": { "light": "var(--wyrd-base-50)", "dark": null },
    "--color-base-40": { "light": "var(--wyrd-base-40)", "dark": null },
    "--color-base-50": { "light": "var(--wyrd-base-30)", "dark": null },
    "--color-base-60": { "light": "var(--wyrd-base-20)", "dark": null },
    "--color-base-70": { "light": "var(--wyrd-base-10)", "dark": null },
    "--color-base-100": { "light": "var(--wyrd-base-00)", "dark": null },
    "--text-normal": { "light": "var(--color-base-40)", "dark": null },
    "--text-muted": { "light": "var(--color-base-50)", "dark": null },
    "--text-faint": { "light": "var(--color-base-60)", "dark": null },
    "--text-highlight-bg": {
      "light": "rgba(var(--wyrd-yellow-rgb), 0.7)",
      "dark": null
    },
    "--text-highlight-bg-active": {
      "light": "rgba(var(--wyrd-yellow-light-rgb), 0.8)",
      "dark": null
    },
    "--canvas-color": { "light": null, "dark": "126, 126, 126" }
  },
  ".notice progress[value]::-webkit-progress-bar": {
    "background-color": { "light": "#262626", "dark": null },
    "box-shadow": { "light": "inset 0px 0px 0px 1px #363636", "dark": null }
  },
  ".dropdown": {
    "background-image": {
      "light": null,
      "dark": "url(\"data:image/svg+xml,%3Csvg xmlns=%27http://www.w3.org/2000/svg%27 width=%2724%27 height=%2724%27 viewBox=%270 0 24 24%27 fill=%27none%27 stroke=%27%23FFF%27 opacity=%270.6%27 stroke-width=%272%27 stroke-linecap=%27round%27 stroke-linejoin=%27round%27 %3E%3Cpath d=%27m7 15 5 5 5-5%27/%3E%3Cpath d=%27m7 9 5-5 5 5%27/%3E%3C/svg%3E\")"
    }
  },
  ".pdf-container.mod-themed\n  .annotationLayer\n  .textWidgetAnnotation\n  input": {
    "filter": { "light": null, "dark": "invert(100%) hue-rotate(180deg)" }
  },
  ".pdf-container.mod-themed\n  .annotationLayer\n  .textWidgetAnnotation\n  textarea": {
    "filter": { "light": null, "dark": "invert(100%) hue-rotate(180deg)" }
  },
  ".pdf-container.mod-themed .popupWrapper > div": {
    "background-color": {
      "light": null,
      "dark": "var(--background-secondary-alt)"
    },
    "filter": {
      "light": null,
      "dark": "drop-shadow(0px 0px 1px var(--color-base-60))\n    drop-shadow(0px 1px 2px #000000) drop-shadow(0px 4px 6px #000000)"
    }
  },
  ".pdf-container.mod-themed .popupWrapper > div::after": {
    "background-color": {
      "light": null,
      "dark": "var(--background-secondary-alt)"
    }
  },
  ".pdf-container.mod-themed .popupContent:empty + .popupMeta": {
    "background-color": {
      "light": null,
      "dark": "var(--background-secondary-alt)"
    }
  },
  ".mermaid > svg": {
    "filter": {
      "light": null,
      "dark": "invert(100%) hue-rotate(180deg) saturate(1.25)"
    }
  },
  ".canvas-card-menu": {
    "background-color": { "light": null, "dark": "var(--background-secondary)" }
  },
  ".canvas-card-menu .canvas-card-menu-button svg": {
    "fill": { "light": null, "dark": "var(--background-secondary)" }
  },
  ".is-mobile": {
    "--color-base-00": { "light": null, "dark": "#000" },
    "--color-base-10": { "light": null, "dark": "#111" },
    "--color-base-20": { "light": null, "dark": "#1e1e1e" },
    "--tag-background": {
      "light": null,
      "dark": "hsla(var(--interactive-accent-hsl), 0.2)"
    },
    "--search-result-background": {
      "light": null,
      "dark": "var(--background-secondary)"
    },
    "--background-modifier-form-field": {
      "light": null,
      "dark": "var(--background-modifier-border)"
    },
    "--background-modifier-cover": { "light": null, "dark": "#000000" },
    "--background-modifier-hover": {
      "light": null,
      "dark": "rgba(var(--mono-rgb-100), 0.15)"
    },
    "--settings-home-background": {
      "light": null,
      "dark": "var(--background-primary)"
    },
    "--mobile-sidebar-background": {
      "light": null,
      "dark": "var(--background-secondary)"
    },
    "--background-modifier-message": {
      "light": null,
      "dark": "var(--background-secondary)"
    }
  },
  ".is-tablet": {
    "--titlebar-background": {
      "light": null,
      "dark": "var(--background-primary)"
    },
    "--titlebar-background-focused": {
      "light": null,
      "dark": "var(--background-primary)"
    },
    "--interactive-normal": {
      "light": null,
      "dark": "var(--background-modifier-border)"
    },
    "--interactive-hover": {
      "light": null,
      "dark": "var(--background-modifier-border-hover)"
    },
    "--modal-background": {
      "light": null,
      "dark": "var(--background-secondary)"
    }
  },
  ".is-tablet .community-item": {
    "background-color": { "light": null, "dark": "var(--background-secondary)" }
  },
  ".is-tablet .vertical-tab-content": {
    "background-color": { "light": null, "dark": "var(--background-secondary)" }
  },
  ".workspace-drawer-inner": {
    "background-color": { "light": null, "dark": "var(--background-secondary)" }
  },
  ".is-mobile .prompt": {
    "background-color": { "light": null, "dark": "var(--background-secondary)" }
  },
  ".is-mobile .prompt input.prompt-input": {
    "background-color": { "light": null, "dark": "var(--background-secondary)" }
  },
  ".is-tablet .suggestion-container": {
    "background-color": { "light": null, "dark": "var(--background-secondary)" }
  }
}

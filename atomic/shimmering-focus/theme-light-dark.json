    "background-color": { "light": null, "dark": "var(--background-secondary)" }
  },
  ".is-mobile .prompt": {
    "background-color": { "light": null, "dark": "var(--background-secondary)" }
  },
  ".is-mobile .prompt input.prompt-input": {
    "background-color": { "light": null, "dark": "var(--background-secondary)" }
  },
  ".is-tablet .suggestion-container": {
    "background-color": { "light": null, "dark": "var(--background-secondary)" }
  },
  ".colorscheme-grubox :is(.nav-file-title": {
    "color": { "light": null, "dark": "var(--color-base-85)" }
  },
  ".vertical-tab-header-group:nth-last-child(3) .vertical-tab-header-group-items .vertical-tab-nav-item:is(:nth-child(4)": {
    "text-shadow": {
      "light": null,
      "dark": "var(--readability-text-shadow-dark)"
    }
  }

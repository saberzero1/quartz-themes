    "color": { "light": "#97a800", "dark": null }
  },
  ":is(.vertical-tab-header": {
    "background-color": { "light": "var(--bg2)", "dark": "var(--bg1)" }
  },
  ".modal-bg": {
    "background-color": {
      "light": "hsl(var(--bg-hue-light) 5% 25%)",
      "dark": "hsl(var(--bg-hue-dark) 10% 5%)"
    }
  },

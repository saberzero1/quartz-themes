    "background-color": { "light": "var(--bg2)", "dark": null }
  },
  ".active-line-background .cm-active.cm-line:not(.HyperMD-quote": {
    "background-color": { "light": "var(--bg3)", "dark": "var(--bg4)" }
  },
  ".link-favicon:is([data-host^=\"github\\.com\"]": {
    "filter": { "light": "invert(0.2)", "dark": null }
  },
  ".kanban-plugin__lane-header-wrapper": {
    "background-color": { "light": "var(--bg6)", "dark": null },
    "border-bottom-color": {
      "light": null,
      "dark": "var(--muted-border-color)"
    }
  },
  ".kanban-plugin__lane": {
    "background-color": { "light": "var(--bg3)", "dark": "var(--bg5)" },
    "border": { "light": "var(--thin-border)", "dark": "1px solid var(--bg1)" }
  },
  ".dropdown": { "background-image": { "light": null, "dark": "url(\"data" } },

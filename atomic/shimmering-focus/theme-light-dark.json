    "--link-unresolved-color": { "light": "#b183c3", "dark": "#bc94cc" },
    "--secondary-accent": { "light": "#309167", "dark": "#0a7046" }
  },
  ".colorscheme-everforest": {
    "--alt-heading-color": { "light": "#f85654", "dark": "#e77e80" },
    "--bg-hue": { "light": "43", "dark": "204" },
    "--bg-sat": { "light": "57%", "dark": "14%" },
    "--bg-val": { "light": "89%", "dark": "21%" },
    "--color-accent": { "light": "#8da201", "dark": "#a8c181" },

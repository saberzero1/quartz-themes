{
  ".markdown-source-view": {
    "background-color": {
      "light": "var(--edit-view-background-color)",
      "dark": "var(--edit-view-background-color-dark)"
    }
  },
  ".markdown-reading-view": {
    "background-color": {
      "light": "var(--nav-background-color)",
      "dark": "var(--nav-background-color-dark)"
    }
  },
  ".nav-folder:not(:last-child)": {
    ".theme-light .nav-file": {
      "light": "not(:last-child) { border-bottom:1px solid var(--nav-separator-color)",
      "dark": null
    }
  },
  ".markdown-rendered pre": {
    "background-color": {
      "light": "var(--code-block-background-color)",
      "dark": null
    }
  },
  ".mermaid > svg": {
    "filter": {
      "light": null,
      "dark": "invert(100%) hue-rotate(180deg) saturate(1.25)"
    },
    "} .markdown-source-view.mod-cm6 .cm-hmd-codeblock { --indentation-guide-editing-indent": {
      "light": null,
      "dark": "0"
    },
    "} .markdown-source-view.mod-cm6 .cm-preview-code-block pre { margin": {
      "light": null,
      "dark": "0"
    },
    "} .markdown-source-view.mod-cm6 .code-block-flair { position": {
      "light": null,
      "dark": "absolute"
    },
    "right": { "light": null, "dark": "6px" },
    "top": { "light": null, "dark": "6px" },
    "z-index": { "light": null, "dark": "1" },
    "display": { "light": null, "dark": "inline-block" },
    "padding": { "light": null, "dark": "var(--size-4-1) var(--size-4-2)" },
    "border-radius": { "light": null, "dark": "var(--code-radius)" },
    "font-family": { "light": null, "dark": "var(--font-interface)" },
    "font-size": { "light": null, "dark": "var(--font-ui-smaller)" },
    "color": { "light": null, "dark": "var(--text-muted)" },
    "cursor": { "light": null, "dark": "var(--cursor)" }
  },
  ".workspace-tab-container .workspace-tab-header-inner": {
    "border-right": {
      "light": null,
      "dark": "1px dashed var(--my-accent-color-dark)"
    }
  },
  ".nav-folder": {
    ".theme-dark .tree-item .nav-file": {
      "light": null,
      "dark": "not(:last-child) { border-bottom:1px solid var(--nav-separator-color-dark)"
    }
  },
  ".HyperMD-codeblock": {
    ".theme-dark .markdown-rendered pre { background-color": {
      "light": null,
      "dark": "var(--code-block-background-color-dark) !important"
    }
  },
  ".cm-hashtag": {
    ".theme-dark .cm-underline { color": {
      "light": null,
      "dark": "var(--text-color-dark)"
    },
    "text-decoration-color": {
      "light": null,
      "dark": "var(--hashtag-underline-color-dark) !important"
    }
  },
  "[data-task=\"x\"] .cm-underline": {
    ".theme-dark [data-task=\"x\"] .cm-hashtag, .theme-dark li[data-task=\"x\"] a.internal-link, .theme-dark li[data-task=\"x\"] a.tag { color": {
      "light": null,
      "dark": "var(--list-done-color-dark)"
    },
    "text-decoration-color": {
      "light": null,
      "dark": "var(--list-done-color-dark) !important"
    }
  },
  "a.internal-link": {
    ".theme-dark a.tag, .theme-dark a.external-link { color": {
      "light": null,
      "dark": "var(--text-color-dark)"
    },
    "text-decoration-color": {
      "light": null,
      "dark": "var(--text-color-dark) !important"
    }
  },
  ".cm-active .cm-link:not(.cm-formatting-link)": {
    "color": { "light": null, "dark": "var(--text-color-dark) !important" }
  },
  ".cm-url": { "color": { "light": null, "dark": "#7b7a78 !important" } },
  "input[type=\"checkbox\"]:checked:after": {
    "background-color": { "light": null, "dark": "var(--list-done-color-dark)" }
  },
  ".task-list-item-checkbox[data-task=\"x\"]": {
    "border-color": {
      "light": null,
      "dark": "var(--list-done-color-dark) !important"
    }
  },
  ".task-list-item-checkbox[checked]": {
    "border-color": {
      "light": null,
      "dark": "var(--list-done-color-dark) !important"
    }
  }
}

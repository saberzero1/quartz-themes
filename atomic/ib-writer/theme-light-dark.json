{
  "body": {
    "--accent-h": { "light": "201", "dark": null },
    "--accent-l": { "light": "66%", "dark": null },
    "--accent-s": { "light": "93%", "dark": null },
    "--background-modifier-box-shadow": {
      "light": "rgba(0, 0, 0, 0.1)",
      "dark": null
    },
    "--background-modifier-cover": {
      "light": "rgba(220, 220, 220, 0.4)",
      "dark": null
    },
    "--background-primary": {
      "light": "var(--edit-view-background-color)",
      "dark": null
    },
    "--background-secondary-alt": {
      "light": "var(--color-base-05)",
      "dark": null
    },
    "--color-accent": { "light": "var(--text-color)", "dark": null },
    "--color-accent-1": {
      "light": "hsl(calc(var(--accent-h) - 1), calc(var(--accent-s) * 1.01), calc(var(--accent-l) * 1.075))",
      "dark": null
    },
    "--color-accent-2": {
      "light": "hsl(calc(var(--accent-h) - 3), calc(var(--accent-s) * 1.02), calc(var(--accent-l) * 1.15))",
      "dark": null
    },
    "--color-accent-hsl": {
      "light": "var(--accent-h), var(--accent-s), var(--accent-l)",
      "dark": null
    },
    "--color-base-00": {
      "light": "var(--edit-view-background-color)",
      "dark": null
    },
    "--color-base-05": { "light": "#fcfcfc", "dark": null },
    "--color-base-10": {
      "light": "var(--code-block-background-color)",
      "dark": null
    },
    "--color-base-100": { "light": "var(--text-color)", "dark": null },
    "--color-base-20": { "light": "var(--nav-background-color)", "dark": null },
    "--color-base-25": { "light": "#e3e3e3", "dark": null },
    "--color-base-30": { "light": "#e0e0e0", "dark": null },
    "--color-base-35": { "light": "#d4d4d4", "dark": null },
    "--color-base-40": { "light": "#bdbdbd", "dark": null },
    "--color-base-50": { "light": "#ababab", "dark": null },
    "--color-base-60": { "light": "#707070", "dark": null },
    "--color-base-70": { "light": "#5c5c5c", "dark": null },
    "--color-blue": { "light": "#086ddd", "dark": null },
    "--color-blue-rgb": { "light": "8, 109, 221", "dark": null },
    "--color-cyan": { "light": "#00bfbc", "dark": null },
    "--color-cyan-rgb": { "light": "0, 191, 188", "dark": null },
    "--color-green": { "light": "#08b94e", "dark": null },
    "--color-green-rgb": { "light": "8, 185, 78", "dark": null },
    "--color-orange": { "light": "#ec7500", "dark": null },
    "--color-orange-rgb": { "light": "236, 117, 0", "dark": null },
    "--color-pink": { "light": "#d53984", "dark": null },
    "--color-pink-rgb": { "light": "213, 57, 132", "dark": null },
    "--color-purple": { "light": "#7852ee", "dark": null },
    "--color-purple-rgb": { "light": "120, 82, 238", "dark": null },
    "--color-red": { "light": "#e93147", "dark": null },
    "--color-red-rgb": { "light": "233, 49, 71", "dark": null },
    "--color-yellow": { "light": "#e0ac00", "dark": null },
    "--color-yellow-rgb": { "light": "224, 172, 0", "dark": null },
    "--highlight-mix-blend-mode": { "light": "darken", "dark": null },
    "--input-shadow": {
      "light": "inset 0 0 0 1px rgba(0, 0, 0, 0.12), 0 2px 3px 0 rgba(0, 0, 0, 0.05), 0 1px 1.5px 0 rgba(0, 0, 0, 0.03), 0 1px 2px 0 rgba(0, 0, 0, 0.04), 0 0 0 0 transparent",
      "dark": null
    },
    "--input-shadow-hover": {
      "light": "inset 0 0 0 1px rgba(0, 0, 0, 0.17), 0 2px 3px 0 rgba(0, 0, 0, 0.1), 0 1px 1.5px 0 rgba(0, 0, 0, 0.03), 0 1px 2px 0 rgba(0, 0, 0, 0.04), 0 0 0 0 transparent",
      "dark": null
    },
    "--mono-rgb-0": { "light": "255, 255, 255", "dark": null },
    "--mono-rgb-100": { "light": "0, 0, 0", "dark": null },
    "--shadow-l": {
      "light": "0px 1.8px 7.3px rgba(0, 0, 0, 0.071), 0px 6.3px 24.7px rgba(0, 0, 0, 0.112), 0px 30px 90px rgba(0, 0, 0, 0.2)",
      "dark": null
    },
    "--shadow-s": {
      "light": "0px 1px 2px rgba(0, 0, 0, 0.028), 0px 3.4px 6.7px rgba(0, 0, 0, 0.042), 0px 15px 30px rgba(0, 0, 0, 0.07)",
      "dark": null
    },
    "--tab-background-active": {
      "light": "var(--edit-view-background-color)",
      "dark": null
    },
    "--text-selection": { "light": "var(--my-accent-color)", "dark": null },
    "--canvas-color": { "light": null, "dark": "126, 126, 126" }
  },
  ".notice progress[value]::-webkit-progress-bar": {
    "background-color": { "light": "#262626", "dark": null },
    "box-shadow": { "light": "inset 0px 0px 0px 1px #363636", "dark": null }
  },
  ".markdown-source-view": {
    "background-color": {
      "light": "var(--edit-view-background-color)",
      "dark": "var(--edit-view-background-color-dark)"
    }
  },
  ".markdown-reading-view": {
    "background-color": {
      "light": "var(--nav-background-color)",
      "dark": "var(--nav-background-color-dark)"
    }
  },
  ".nav-folder:not(:last-child)": {
    "border-bottom": {
      "light": "1px solid var(--nav-separator-color)",
      "dark": null
    }
  },
  ".nav-file:not(:last-child)": {
    "border-bottom": {
      "light": "1px solid var(--nav-separator-color)",
      "dark": null
    }
  },
  ".markdown-rendered pre": {
    "background-color": {
      "light": "var(--code-block-background-color)",
      "dark": "var(--code-block-background-color-dark)"
    }
  },
  ".dropdown": {
    "background-image": {
      "light": null,
      "dark": "url(\"data:image/svg+xml, %3Csvg xmlns=%27http://www.w3.org/2000/svg%27 width=%2724%27 height=%2724%27 viewBox=%270 0 24 24%27 fill=%27none%27 stroke=%27%23FFF%27 opacity=%270.6%27 stroke-width=%272%27 stroke-linecap=%27round%27 stroke-linejoin=%27round%27 %3E%3Cpath d=%27m7 15 5 5 5-5%27/%3E%3Cpath d=%27m7 9 5-5 5 5%27/%3E%3C/svg%3E\")"
    }
  },
  ".pdf-container.mod-themed .popupWrapper > div": {
    "background-color": {
      "light": null,
      "dark": "var(--background-secondary-alt)"
    },
    "filter": {
      "light": null,
      "dark": "drop-shadow(0px 0px 1px var(--color-base-60))drop-shadow(0px 1px 2px rgba(0, 0, 0, 0.4))drop-shadow(0px 4px 6px rgba(0, 0, 0, 0.4))"
    }
  },
  ".pdf-container.mod-themed .popupWrapper > div::after": {
    "background-color": {
      "light": null,
      "dark": "var(--background-secondary-alt)"
    }
  },
  ".pdf-container.mod-themed .popupContent:empty + .popupMeta": {
    "background-color": {
      "light": null,
      "dark": "var(--background-secondary-alt)"
    }
  },
  ".mermaid > svg": {
    "filter": {
      "light": null,
      "dark": "invert(100%) hue-rotate(180deg) saturate(1.25)"
    }
  },
  ".canvas-card-menu": {
    "background-color": { "light": null, "dark": "var(--background-secondary)" }
  },
  ".canvas-card-menu .canvas-card-menu-button svg": {
    "fill": { "light": null, "dark": "var(--background-secondary)" }
  },
  ".is-mobile": {
    "--background-modifier-cover": {
      "light": null,
      "dark": "rgba(0, 0, 0, 0.5)"
    },
    "--background-modifier-form-field": {
      "light": null,
      "dark": "var(--background-modifier-border)"
    },
    "--background-modifier-hover": {
      "light": null,
      "dark": "rgba(var(--mono-rgb-100), 0.15)"
    },
    "--background-modifier-message": {
      "light": null,
      "dark": "var(--background-secondary)"
    },
    "--color-base-00": { "light": null, "dark": "#000" },
    "--color-base-10": { "light": null, "dark": "#111" },
    "--color-base-20": { "light": null, "dark": "#1e1e1e" },
    "--mobile-sidebar-background": {
      "light": null,
      "dark": "var(--background-secondary)"
    },
    "--search-result-background": {
      "light": null,
      "dark": "var(--background-secondary)"
    },
    "--settings-home-background": {
      "light": null,
      "dark": "var(--background-primary)"
    },
    "--tag-background": {
      "light": null,
      "dark": "hsla(var(--interactive-accent-hsl), 0.2)"
    }
  },
  ".is-tablet": {
    "--interactive-hover": {
      "light": null,
      "dark": "var(--background-modifier-border-hover)"
    },
    "--interactive-normal": {
      "light": null,
      "dark": "var(--background-modifier-border)"
    },
    "--modal-background": {
      "light": null,
      "dark": "var(--background-secondary)"
    },
    "--titlebar-background": {
      "light": null,
      "dark": "var(--background-primary)"
    },
    "--titlebar-background-focused": {
      "light": null,
      "dark": "var(--background-primary)"
    }
  },
  ".is-tablet .community-item": {
    "background-color": { "light": null, "dark": "var(--background-secondary)" }
  },
  ".is-tablet .vertical-tab-content": {
    "background-color": { "light": null, "dark": "var(--background-secondary)" }
  },
  ".workspace-drawer-inner": {
    "background-color": { "light": null, "dark": "var(--background-secondary)" }
  },
  ".is-mobile .prompt": {
    "background-color": { "light": null, "dark": "var(--background-secondary)" }
  },
  ".is-mobile .prompt input.prompt-input": {
    "background-color": { "light": null, "dark": "var(--background-secondary)" }
  },
  ".is-tablet .suggestion-container": {
    "background-color": { "light": null, "dark": "var(--background-secondary)" }
  },
  ".workspace-tab-container .workspace-tab-header-inner": {
    "border-right": {
      "light": null,
      "dark": "1px dashed var(--my-accent-color-dark)"
    }
  },
  ".nav-folder": {
    "border-bottom": {
      "light": null,
      "dark": "1px solid var(--nav-separator-color-dark)"
    }
  },
  ".tree-item .nav-file:not(:last-child)": {
    "border-bottom": {
      "light": null,
      "dark": "1px solid var(--nav-separator-color-dark)"
    }
  },
  ".HyperMD-codeblock": {
    "background-color": {
      "light": null,
      "dark": "var(--code-block-background-color-dark)"
    }
  },
  ".cm-hashtag": {
    "color": { "light": null, "dark": "var(--text-color-dark)" },
    "text-decoration-color": {
      "light": null,
      "dark": "var(--hashtag-underline-color-dark)"
    }
  },
  ".cm-underline": {
    "color": { "light": null, "dark": "var(--text-color-dark)" },
    "text-decoration-color": {
      "light": null,
      "dark": "var(--hashtag-underline-color-dark)"
    }
  },
  "[data-task=\"x\"] .cm-underline": {
    "color": { "light": null, "dark": "var(--list-done-color-dark)" },
    "text-decoration-color": {
      "light": null,
      "dark": "var(--list-done-color-dark)"
    }
  },
  "[data-task=\"x\"] .cm-hashtag": {
    "color": { "light": null, "dark": "var(--list-done-color-dark)" },
    "text-decoration-color": {
      "light": null,
      "dark": "var(--list-done-color-dark)"
    }
  },
  "li[data-task=\"x\"] a.internal-link": {
    "color": { "light": null, "dark": "var(--list-done-color-dark)" },
    "text-decoration-color": {
      "light": null,
      "dark": "var(--list-done-color-dark)"
    }
  },
  "li[data-task=\"x\"] a.tag": {
    "color": { "light": null, "dark": "var(--list-done-color-dark)" },
    "text-decoration-color": {
      "light": null,
      "dark": "var(--list-done-color-dark)"
    }
  },
  "a.internal-link": {
    "color": { "light": null, "dark": "var(--text-color-dark)" },
    "text-decoration-color": { "light": null, "dark": "var(--text-color-dark)" }
  },
  "a.tag": {
    "color": { "light": null, "dark": "var(--text-color-dark)" },
    "text-decoration-color": { "light": null, "dark": "var(--text-color-dark)" }
  },
  "a.external-link": {
    "color": { "light": null, "dark": "var(--text-color-dark)" },
    "text-decoration-color": { "light": null, "dark": "var(--text-color-dark)" }
  },
  ".cm-active .cm-link:not(.cm-formatting-link)": {
    "color": { "light": null, "dark": "var(--text-color-dark)" }
  },
  ".cm-url": { "color": { "light": null, "dark": "#7b7a78" } },
  "input[type=\"checkbox\"]:checked:after": {
    "background-color": { "light": null, "dark": "var(--list-done-color-dark)" }
  },
  ".task-list-item-checkbox[data-task=\"x\"]": {
    "border-color": { "light": null, "dark": "var(--list-done-color-dark)" }
  },
  ".task-list-item-checkbox[checked]": {
    "border-color": { "light": null, "dark": "var(--list-done-color-dark)" }
  }
}

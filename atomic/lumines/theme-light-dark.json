{
  "body": {
    "--active-bg": {
      "light": "var(--nav-item-background-active)",
      "dark": null
    },
    "--background-primary": { "light": "rgb(249, 249, 249)", "dark": null },
    "--background-primary-alt": { "light": "rgb(239, 239, 239)", "dark": null },
    "--background-secondary": { "light": "rgb(239, 239, 239)", "dark": null },
    "--background-secondary-alt": {
      "light": "rgb(233, 233, 233)",
      "dark": null
    },
    "--color-base-00": { "light": "rgb(249, 249, 249)", "dark": null },
    "--color-base-10": { "light": "rgb(245, 245, 245)", "dark": null },
    "--color-base-100": { "light": "rgb(70, 70, 70)", "dark": null },
    "--color-base-20": { "light": "rgb(241, 241, 241)", "dark": null },
    "--color-base-25": { "light": "rgb(237, 237, 237)", "dark": null },
    "--color-base-30": { "light": "rgb(233, 233, 233)", "dark": null },
    "--color-base-35": { "light": "rgb(229, 229, 229)", "dark": null },
    "--color-base-40": { "light": "rgb(225, 225, 225)", "dark": null },
    "--color-base-50": { "light": "rgb(140, 140, 140)", "dark": null },
    "--color-base-60": { "light": "rgb(80, 80, 80)", "dark": null },
    "--color-base-70": { "light": "rgb(73, 73, 73)", "dark": null },
    "--h1-color": {
      "light": "color-mix( in srgb, var(--color-accent) 100%, var(--color-100) 0% )",
      "dark": null
    },
    "--h2-color": {
      "light": "color-mix( in srgb, var(--color-accent) 80%, var(--color-110) 20% )",
      "dark": null
    },
    "--h3-color": {
      "light": "color-mix( in srgb, var(--color-accent) 60%, var(--color-120) 40% )",
      "dark": null
    },
    "--h4-color": {
      "light": "color-mix( in srgb, var(--color-accent) 40%, var(--color-130) 60% )",
      "dark": null
    },
    "--h5-color": {
      "light": "color-mix( in srgb, var(--color-accent) 20%, var(--color-140) 80% )",
      "dark": null
    },
    "--h6-color": {
      "light": "color-mix( in srgb, var(--color-accent) 0%, var(--color-150) 100% )",
      "dark": null
    },
    "--canvas-color": { "light": null, "dark": "126, 126, 126" }
  },
  ".notice progress[value]::-webkit-progress-bar": {
    "background-color": { "light": "#262626", "dark": null },
    "box-shadow": { "light": "inset 0px 0px 0px 1px #363636", "dark": null }
  },
  ".dropdown": { "background-image": { "light": null, "dark": "url(\"data" } },
  ".pdf-container.mod-themed .annotationLayer .textWidgetAnnotation input": {
    "filter": { "light": null, "dark": "invert(100%) hue-rotate(180deg)" }
  },
  ".pdf-container.mod-themed .annotationLayer .textWidgetAnnotation textarea": {
    "filter": { "light": null, "dark": "invert(100%) hue-rotate(180deg)" }
  },
  ".pdf-container.mod-themed .popupWrapper > div": {
    "background-color": {
      "light": null,
      "dark": "var(--background-secondary-alt)"
    },
    "filter": {
      "light": null,
      "dark": "drop-shadow(0px 0px 1px var(--color-base-60)) drop-shadow(0px 1px 2px rgba(0, 0, 0, 0.4)) drop-shadow(0px 4px 6px rgba(0, 0, 0, 0.4))"
    }
  },
  ".pdf-container.mod-themed .popupWrapper > div::after": {
    "background-color": {
      "light": null,
      "dark": "var(--background-secondary-alt)"
    }
  },
  ".pdf-container.mod-themed .popupContent:empty + .popupMeta": {
    "background-color": {
      "light": null,
      "dark": "var(--background-secondary-alt)"
    }
  },
  ".mermaid > svg": {
    "filter": {
      "light": null,
      "dark": "invert(100%) hue-rotate(180deg) saturate(1.25)"
    }
  },
  ".canvas-card-menu": {
    "background-color": { "light": null, "dark": "var(--background-secondary)" }
  },
  ".canvas-card-menu .canvas-card-menu-button svg": {
    "fill": { "light": null, "dark": "var(--background-secondary)" }
  },
  ".is-mobile": {
    "--background-modifier-cover": {
      "light": null,
      "dark": "rgba(0, 0, 0, 0.5)"
    },
    "--background-modifier-form-field": {
      "light": null,
      "dark": "var(--background-modifier-border)"
    },
    "--background-modifier-hover": {
      "light": null,
      "dark": "rgba(var(--mono-rgb-100), 0.15)"
    },
    "--background-modifier-message": {
      "light": null,
      "dark": "var(--background-secondary)"
    },
    "--color-base-00": { "light": null, "dark": "#000" },
    "--color-base-10": { "light": null, "dark": "#111" },
    "--color-base-20": { "light": null, "dark": "#1e1e1e" },
    "--mobile-sidebar-background": {
      "light": null,
      "dark": "var(--background-secondary)"
    },
    "--search-result-background": {
      "light": null,
      "dark": "var(--background-secondary)"
    },
    "--settings-home-background": {
      "light": null,
      "dark": "var(--background-primary)"
    },
    "--tag-background": {
      "light": null,
      "dark": "hsla(var(--interactive-accent-hsl), 0.2)"
    }
  },
  ".is-tablet": {
    "--interactive-hover": {
      "light": null,
      "dark": "var(--background-modifier-border-hover)"
    },
    "--interactive-normal": {
      "light": null,
      "dark": "var(--background-modifier-border)"
    },
    "--modal-background": {
      "light": null,
      "dark": "var(--background-secondary)"
    },
    "--titlebar-background": {
      "light": null,
      "dark": "var(--background-primary)"
    },
    "--titlebar-background-focused": {
      "light": null,
      "dark": "var(--background-primary)"
    }
  },
  ".is-tablet .community-item": {
    "background-color": { "light": null, "dark": "var(--background-secondary)" }
  },
  ".is-tablet .vertical-tab-content": {
    "background-color": { "light": null, "dark": "var(--background-secondary)" }
  },
  ".workspace-drawer-inner": {
    "background-color": { "light": null, "dark": "var(--background-secondary)" }
  },
  ".is-mobile .prompt": {
    "background-color": { "light": null, "dark": "var(--background-secondary)" }
  },
  ".is-mobile .prompt input.prompt-input": {
    "background-color": { "light": null, "dark": "var(--background-secondary)" }
  },
  ".is-tablet .suggestion-container": {
    "background-color": { "light": null, "dark": "var(--background-secondary)" }
  }
}

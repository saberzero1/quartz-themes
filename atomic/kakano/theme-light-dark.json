{
  "body": {
    ".theme-light .theme-dark { --background-modifier-active-hover-alt": {
      "light": "var(--theme-color-baseLightened25)",
      "dark": null
    },
    "--background-modifier-active-hover": {
      "light": "var(--theme-color-baseLightened60)",
      "dark": "var(--theme-color-baseDarkened70)"
    },
    "--background-modifier-border-focus": {
      "light": "var(--theme-color-base)",
      "dark": "var(--theme-color-baseDarkened25)"
    },
    "--background-modifier-border": {
      "light": "var(--theme-color-baseLightened25)",
      "dark": "var(--theme-color-baseDarkened25)"
    },
    "--background-modifier-hover": "var(--theme-color-base)",
    "--blockquote-border-color": {
      "light": "var(--theme-color-baseLightened80)",
      "dark": "var(--theme-color-baseDarkened80)"
    },
    "--caret-color": {
      "light": "hsl(var(--accent-h), 150%, 50%)",
      "dark": "hsl(var(--accent-h), 150%, 80%)"
    },
    "--collapse-icon-color": "var(--theme-color-base)",
    "--collapse-icon-color-collapsed": "var(--theme-color-base)",
    "--empty-pane-color": "var(--theme-color-baseDarkened70)",
    "--icon-color-focused": {
      "light": "white",
      "dark": "var(--theme-color-baseLightened80)"
    },
    "--icon-color-hover": {
      "light": "white",
      "dark": "var(--theme-color-baseLightened80)"
    },
    "--icon-color": "var(--theme-color-base)",
    "--modal-background": {
      "light": "var(--theme-color-baseLightened80)",
      "dark": "var(--theme-color-baseDarkened80)"
    },
    "--nav-collapse-icon-color-collapsed": "var(--theme-color-baseTransparent75)",
    "--nav-collapse-icon-color": "var(--theme-color-baseTransparent75)",
    "--nav-indentation-guide-color": "var(--theme-color-baseTransparent25)",
    "--nav-item-background-active": "var(--theme-color-base)",
    "--nav-item-background-selected": "var(--theme-color-baseTransparent25)",
    "--nav-item-color-active": {
      "light": "var(--theme-color-baseLightened80)",
      "dark": "var(--theme-color-baseDarkened80)"
    },
    "--nav-item-color": {
      "light": "var(--theme-color-baseDarkened80)",
      "dark": "var(--theme-color-baseLightened60)"
    },
    "--nav-item-color-hover": { "light": "white", "dark": "black" },
    "--search-clear-button-color": "var(--theme-color-base)",
    "--search-icon-color": "var(--theme-color-base)",
    "--status-bar-text-color": { "light": "black", "dark": null },
    "--tab-background-active": "var(--background-primary)",
    "--tab-background-inactive": {
      "light": "var(--theme-color-baseLightened80)",
      "dark": "var(--theme-color-baseDarkened85)"
    },
    "--tab-text-color": "var(--theme-color-baseTransparent50)",
    "--tab-text-color-active": "var(--theme-color-baseTransparent50)",
    "--tab-text-color-focused": {
      "light": "var(--theme-color-baseDarkened25)",
      "dark": "var(--theme-color-baseLightened25)"
    },
    "--tab-text-color-focused-active": {
      "light": "var(--theme-color-baseDarkened25)",
      "dark": "var(--theme-color-baseLightened25)"
    },
    "--tab-text-color-focused-highlighted": "var(--text-accent)",
    "--tab-text-color-focused-active-current": "var(--text-normal)",
    "--tag-color": {
      "light": "hsl( var(--accent-h), calc(var(--accent-s)), calc(var(--accent-l) * 0.9) )",
      "dark": "hsl( var(--accent-h), calc(var(--accent-s) * 1.2), calc(var(--accent-l)) )"
    },
    "--tag-background": {
      "light": "hsl( var(--accent-h), var(--accent-s), var(--accent-l), 0.15 )",
      "dark": "hsl(var(--accent-h), var(--accent-s), var(--accent-l), 0.2)"
    },
    "--text-normal": {
      "light": "var(--theme-color-baseDarkened90)",
      "dark": "var(--theme-color-baseLightened60)"
    },
    "--text-faint": {
      "light": "var(--theme-color-baseDarkened70)",
      "dark": "var(--theme-color-baseLightened25)"
    },
    "--background-prompt": {
      "light": "var(--theme-color-baseLightened95)",
      "dark": "var(--theme-color-baseDarkened80)"
    },
    "--interactive-accent": "var(--theme-color-base)",
    "--interactive-accent-hover": {
      "light": "var(--theme-color-base)",
      "dark": null
    },
    "--table-border-color": {
      "light": "var(--theme-color-baseLightened60)",
      "dark": "var(--theme-color-baseDarkened70)"
    },
    ".theme-dark .theme-light { --background-modifier-active-hover-alt": {
      "light": null,
      "dark": "var(--theme-color-baseDarkened25)"
    }
  },
  ".mermaid > svg": {
    "filter": {
      "light": null,
      "dark": "invert(100%) hue-rotate(180deg) saturate(1.25)"
    },
    "} .markdown-source-view.mod-cm6 .cm-hmd-codeblock { --indentation-guide-editing-indent": {
      "light": null,
      "dark": "0"
    },
    "} .markdown-source-view.mod-cm6 .cm-preview-code-block pre { margin": {
      "light": null,
      "dark": "0"
    },
    "} .markdown-source-view.mod-cm6 .code-block-flair { position": {
      "light": null,
      "dark": "absolute"
    },
    "right": { "light": null, "dark": "6px" },
    "top": { "light": null, "dark": "6px" },
    "z-index": { "light": null, "dark": "1" },
    "display": { "light": null, "dark": "inline-block" },
    "padding": { "light": null, "dark": "var(--size-4-1) var(--size-4-2)" },
    "border-radius": { "light": null, "dark": "var(--code-radius)" },
    "font-family": { "light": null, "dark": "var(--font-interface)" },
    "font-size": { "light": null, "dark": "var(--font-ui-smaller)" },
    "color": { "light": null, "dark": "var(--text-muted)" },
    "cursor": { "light": null, "dark": "var(--cursor)" },
    "width": { "light": null, "dark": "auto" },
    "min-width": { "light": null, "dark": "2em" },
    "text-wrap": { "light": null, "dark": "nowrap" }
  }
}

  ".notice progress[value]::-webkit-progress-bar": {
    "background-color": { "light": "#262626", "dark": null },
    "box-shadow": { "light": "inset 0px 0px 0px 1px #363636", "dark": null }
  },
  "body.theme-setting-customGradient": {
    "--theme-color-gradientTop": {
      "light": "color-mix( in srgb, var(--theme-color-customGradientBaseTop), white calc(100% * (0.5 + var(--theme-value-gradientContrast) * 0.5)) )",
      "dark": "color-mix( in srgb, var(--theme-color-customGradientBaseTop), black calc(100% * (0.7 - var(--theme-value-gradientContrast) * 0.5)) )"
    },
    "--theme-color-gradientBottom": {
      "light": "color-mix( in srgb, var(--theme-color-customGradientBaseBottom), white calc(100% * (0.5 - var(--theme-value-gradientContrast) * 0.5)) )",
      "dark": "color-mix( in srgb, var(--theme-color-customGradientBaseBottom), black calc(100% * (0.7 + var(--theme-value-gradientContrast) * 0.5)) )"
    }
  },
  ".metadata-property:not([data-property-key=\"tags\"])": {
    "--pill-background": "var(--theme-color-baseTransparent75)",
    "--pill-background-hover": "var(--theme-color-base)",
    "--pill-background-remove-hover": {
      "light": "var(--theme-color-baseLightened25)",
      "dark": "var(--theme-color-baseDarkened25)"
    },
    "--pill-color": { "light": "white", "dark": "black" },
    "--pill-color-hover": { "light": "white", "dark": "black" },
    "--pill-color-remove": {
      "light": "var(--theme-color-baseLightened80)",
      "dark": "var(--theme-color-baseDarkened80)"
    },
    "--pill-color-remove-hover": {
      "light": "var(--theme-color-baseLightened80)",
      "dark": "var(--theme-color-baseDarkened80)"
    }
  },
  ".theme-setting-lightModeNoteBgNearWhite": {
    "--background-primary": {
      "light": "color-mix(in srgb, var(--theme-color-base), white 97%)",
      "dark": null
    }
  },
  ".dropdown": {
    "background-image": {
      "light": null,
      "dark": "url(\"data:image/svg+xml, %3Csvg xmlns=%27http://www.w3.org/2000/svg%27 width=%2724%27 height=%2724%27 viewBox=%270 0 24 24%27 fill=%27none%27 stroke=%27%23FFF%27 opacity=%270.6%27 stroke-width=%272%27 stroke-linecap=%27round%27 stroke-linejoin=%27round%27 %3E%3Cpath d=%27m7 15 5 5 5-5%27/%3E%3Cpath d=%27m7 9 5-5 5 5%27/%3E%3C/svg%3E\")"
    }
  },

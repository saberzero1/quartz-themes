{
  ".search-result:not(.is-collapsed) .search-result-file-title": {
    "color": {
      "light": "var(--color-base-40)",
      "dark": "var(--color-base-160)"
    }
  },
  ".backlink-pane > .tree-item-self.is-collapsed": {
    ".theme-light .outgoing-link-pane > .tree-item-self.is-collapsed { color": {
      "light": "var(--color-base-40)",
      "dark": null
    },
    ".theme-dark .outgoing-link-pane > .tree-item-self.is-collapsed { color": {
      "light": null,
      "dark": "var(--color-base-160)"
    }
  },
  ".backlink-pane > .tree-item-self": {
    ".theme-light .outgoing-link-pane > .tree-item-self { color": {
      "light": "var(--color-base-40)",
      "dark": null
    },
    ".theme-dark .outgoing-link-pane > .tree-item-self { color": {
      "light": null,
      "dark": "var(--color-base-160)"
    }
  },
  ".backlink-pane > .tree-item-self.is-clickable:hover": {
    ".theme-light .outgoing-link-pane > .tree-item-self.is-clickable": {
      "light": "hover { color:var(--color-base-00)",
      "dark": null
    },
    ".theme-dark .outgoing-link-pane > .tree-item-self.is-clickable": {
      "light": null,
      "dark": "hover { color:var(--color-base-100)"
    }
  },
  ".nav-file-title:hover svg": {
    ".theme-light .nav-folder-title": {
      "light": "hover svg { color:var(--color-base-00)",
      "dark": null
    },
    ".theme-dark .nav-folder-title": {
      "light": null,
      "dark": "hover svg { color:var(--color-base-00)"
    }
  },
  ".nav-file-title": {
    "padding": { "light": "var(--nav-item-padding)", "dark": null }
  },
  ".menu": {
    "border": {
      "light": "1px solid var(--color-base-90)",
      "dark": "1px solid var(--color-base-110)"
    }
  },
  ".menu-item": {
    "color": {
      "light": "var(--color-base-40)",
      "dark": "var(--color-base-160)"
    }
  },
  ".menu-item-icon": {
    "color": {
      "light": "var(--color-base-70)",
      "dark": "var(--color-base-130)"
    }
  },
  ".menu-separator": {
    "border-bottom": {
      "light": "1px solid var(--color-base-90)",
      "dark": "1px solid var(--color-base-110)"
    }
  },
  ".menu-item:hover:not(.is-disabled):not(.is-label)": {
    "color": "var(--color-base-20)",
    "background-color": "var(--color-accent)"
  },
  ".horizontal-tab-nav-item": {
    ".theme-light .vertical-tab-nav-item { color": {
      "light": "var(--color-base-40)",
      "dark": null
    },
    ".theme-dark .vertical-tab-nav-item { color": {
      "light": null,
      "dark": "var(--color-base-160)"
    }
  },
  ".community-modal-search-results-wrapper": {
    "border-top": {
      "light": "var(--border-width) solid var(--color-base-30)",
      "dark": "var(--border-width) solid var(--color-base-100)"
    }
  },
  ".mod-right-split .metadata-container .metadata-content input": {
    "color": { "light": "var(--color-base-40)", "dark": null }
  },
  ".search-suggest-item.mod-group:not(:first-child)": {
    "border-top": { "light": "1px solid var(--color-base-90)", "dark": null }
  },
  ".suggestion-container": {
    ".theme-light .suggestion-container.mod-search-suggestion { background-color": {
      "light": "var(--background-secondary)",
      "dark": null
    },
    "border": { "light": "1px solid var(--color-base-90)", "dark": null },
    "color": { "light": "var(--color-base-40)", "dark": null }
  },
  ".suggestion-container .suggestion": {
    ".theme-light .suggestion-container.mod-search-suggestion .suggestion { background-color": {
      "light": "var(--background-secondary)",
      "dark": null
    }
  },
  ".suggestion-container .suggestion .suggestion-item.is-selected": {
    ".theme-light .suggestion-container.mod-search-suggestion .suggestion .suggestion-item.is-selected { background-color": {
      "light": "var(--color-accent)",
      "dark": null
    },
    "color": { "light": "var(--color-base-20)", "dark": null }
  },
  "#calendar-container": {
    "--color-background-heading": "transparent",
    "--color-background-day": "transparent",
    "--color-background-day-empty": "transparent",
    "--color-background-day-active": "var(--background-nav-selected)",
    "--color-background-day-hover": "var(--background-nav-alt)",
    "--color-dot": "var(--text-nav)",
    "--color-arrow": "var(--text-nav)",
    "--color-text-title": "var(--text-faint)",
    "--color-text-heading": "var(--text-faint)",
    "--color-text-day": "var(--nav-item-color)",
    "--color-text-today": "var(--color-accent)"
  },
  ".checklist-plugin-main": {
    "color": {
      "light": "var(--color-base-20)",
      "dark": "var(--color-base-160)"
    },
    "--background-modifier-border": "var(--color-accent)",
    "--background-modifier-cover": {
      "light": "var(--color-base-110)",
      "dark": "var(--color-base-90)"
    },
    "--checklist-checkboxBorder": {
      "light": "2px solid var(--color-base-20)",
      "dark": "2px solid var(--color-base-120)"
    },
    "--checklist-listItemBackground": {
      "light": "var(--color-base-90)",
      "dark": "var(--color-base-95)"
    },
    "--checklist-listItemBackground--hover": "var(--color-accent)",
    "--checklist-iconFill": {
      "light": "var(--color-base-20)",
      "dark": "var(--color-base-160)"
    },
    "--checklist-textColor": {
      "light": "var(--color-base-20)",
      "dark": "var(--color-base-160)"
    },
    "--checklist-countBackground": "var(--color-base-90)",
    "--checklist-searchBackground": {
      "light": "var(--color-base-110)",
      "dark": "var(--color-base-95)"
    }
  },
  ".checklist-plugin-main li:hover": {
    "color": {
      "light": "var(--color-base-20)",
      "dark": "var(--color-base-100)"
    }
  },
  ".checklist-plugin-main li:hover .checkbox": {
    "border-color": {
      "light": "var(--color-base-00)",
      "dark": "var(--color-base-120)"
    }
  },
  ".chat-container .chat-messages .chat-message-container .message": {
    "color": { "light": "var(--color-text-title)", "dark": null }
  },
  ".chat-container .chat-messages .chat-message-container pre": {
    "background-color": "var(--color-base-110)",
    "border": "none"
  },
  ".chat-container .chat-messages .chat-message-container pre code": {
    "color": { "light": "var(--color-base-10)", "dark": null }
  },
  ".chat-container .chat-messages .chat-message-container code": {
    "background-color": "var(--color-base-110)",
    "border": "none",
    "color": { "light": "var(--color-base-10)", "dark": null }
  },
  "#dangling-links": {
    "color": {
      "light": "var(--color-base-20)",
      "dark": "var(--color-base-160)"
    }
  },
  ".kanban-plugin": { "--kanban-border": "var(--border-width)" },
  ".kanban-plugin .kanban-plugin__lane-form-wrapper": {
    "background-color": { "light": "var(--color-base-00)", "dark": null },
    "border": "0px"
  },
  ".kanban-plugin .kanban-plugin__lane-action-add": {
    "background-color": "var(--interactive-accent-hover)"
  },
  ".kanban-plugin .kanban-plugin__lane": {
    "background-color": { "light": "var(--color-base-00)", "dark": null }
  },
  ".mk-sidebar": { "color": { "light": "var(--color-base-40)", "dark": null } },
  ".mk-menu": { "color": { "light": "var(--color-base-40)", "dark": null } },
  ".mk-menu .mk-active": {
    "background": { "light": "var(--color-accent)", "dark": null },
    "color": { "light": "var(--color-base-00)", "dark": null }
  },
  ".mk-menu .mk-menu-option:not(.mk-disabled):hover": {
    "background": { "light": "var(--color-accent)", "dark": null },
    "color": { "light": "var(--color-base-00)", "dark": null }
  },
  ".style-settings-container[data-level=\"2\"]": {
    "background": {
      "light": "var(--color-base-25)",
      "dark": "var(--color-base-110)"
    },
    "border": {
      "light": "1px solid var(--color-base-30)",
      "dark": "1px solid var(--color-base-100)"
    },
    "border-radius": "5px",
    "padding": "10px 20px",
    "margin": "2px 0 2px -20px"
  },
  ".mermaid > svg": {
    "filter": {
      "light": null,
      "dark": "invert(100%) hue-rotate(180deg) saturate(1.25)"
    },
    "} .markdown-source-view.mod-cm6 .cm-hmd-codeblock { --indentation-guide-editing-indent": {
      "light": null,
      "dark": "0"
    },
    "} .markdown-source-view.mod-cm6 .cm-preview-code-block pre { margin": {
      "light": null,
      "dark": "0"
    },
    "} .markdown-source-view.mod-cm6 .code-block-flair { position": {
      "light": null,
      "dark": "absolute"
    },
    "right": { "light": null, "dark": "6px" },
    "top": { "light": null, "dark": "6px" },
    "z-index": { "light": null, "dark": "1" },
    "display": { "light": null, "dark": "inline-block" },
    "padding": { "light": null, "dark": "var(--size-4-1) var(--size-4-2)" },
    "border-radius": { "light": null, "dark": "var(--code-radius)" },
    "font-family": { "light": null, "dark": "var(--font-interface)" },
    "font-size": { "light": null, "dark": "var(--font-ui-smaller)" },
    "color": { "light": null, "dark": "var(--text-muted)" },
    "cursor": { "light": null, "dark": "var(--cursor)" }
  }
}

{
  ".ct-reporter": {
    "--background-primary": { "light": "#fffdf8", "dark": "#252423" },
    "--background-primary-alt": { "light": "#ebe4d6", "dark": "#302e2c" }
  },
  ".ct-explorer": {
    "--background-primary": { "light": "#e7e5d0", "dark": "#161616" },
    "--background-primary-alt": { "light": "#dad7c3", "dark": "#242323" }
  },
  ".ct-keeper": {
    "--background-primary": { "light": "#d8dee9", "dark": "#2e3440" },
    "--background-primary-alt": { "light": "#c5cdda", "dark": "#3b4252" }
  },
  ".ct-undercover": {
    "--background-primary": { "light": "#fcfcfc", "dark": "#08090d" },
    "--background-primary-alt": { "light": "#dbe1eb", "dark": "#171c23" }
  },
  ".ct-obsidian": {
    ".theme-dark.ct-obsidian { --background-primary": {
      "light": "var(--color-base-00)",
      "dark": null
    },
    "--background-primary-alt": {
      "light": "var(--color-base-10)",
      "dark": null
    }
  },
  ".mermaid > svg": {
    "filter": {
      "light": null,
      "dark": "invert(100%) hue-rotate(180deg) saturate(1.25)"
    },
    "} .markdown-source-view.mod-cm6 .cm-hmd-codeblock { --indentation-guide-editing-indent": {
      "light": null,
      "dark": "0"
    },
    "} .markdown-source-view.mod-cm6 .cm-preview-code-block pre { margin": {
      "light": null,
      "dark": "0"
    },
    "} .markdown-source-view.mod-cm6 .code-block-flair { position": {
      "light": null,
      "dark": "absolute"
    },
    "right": { "light": null, "dark": "6px" },
    "top": { "light": null, "dark": "6px" },
    "z-index": { "light": null, "dark": "1" },
    "display": { "light": null, "dark": "inline-block" },
    "padding": { "light": null, "dark": "var(--size-4-1) var(--size-4-2)" },
    "border-radius": { "light": null, "dark": "var(--code-radius)" },
    "font-family": { "light": null, "dark": "var(--font-interface)" },
    "font-size": { "light": null, "dark": "var(--font-ui-smaller)" },
    "color": { "light": null, "dark": "var(--text-muted)" },
    "cursor": { "light": null, "dark": "var(--cursor)" }
  },
  "body": {
    ".theme-light { --background-secondary-alt": {
      "light": null,
      "dark": "var(--background-secondary)"
    },
    "--indentation-guide-color": {
      "light": null,
      "dark": "var(--background-modifier-border)"
    },
    "--background-modifier-hover": {
      "light": null,
      "dark": "var(--background-primary-alt)"
    },
    "--background-modifier-form-field": {
      "light": null,
      "dark": "var(--background-primary)"
    }
  }
}

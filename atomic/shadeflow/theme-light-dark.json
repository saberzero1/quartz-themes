{
  "body": {
    "--accent-h": { "light": "240", "dark": null },
    "--accent-l": { "light": "12%", "dark": null },
    "--accent-s": { "light": "2%", "dark": null },
    "--background-modifier-border": {
      "light": "rgb(245, 245, 245)",
      "dark": null
    },
    "--background-modifier-hover": {
      "light": "rgb(245, 245, 245)",
      "dark": null
    },
    "--background-primary": { "light": "rgb(245, 245, 245)", "dark": null },
    "--background-primary-alt": { "light": "rgb(245, 245, 245)", "dark": null },
    "--background-secondary": { "light": "rgb(245, 245, 245)", "dark": null },
    "--background-secondary-alt": {
      "light": "rgb(245, 245, 245)",
      "dark": null
    },
    "--code-background": { "light": "#ffffff", "dark": null },
    "--code-normal": { "light": "rgb(0, 0, 0)", "dark": null },
    "--font-interface-theme": { "light": "Inter", "dark": null },
    "--font-monospace-theme": { "light": "Geist Mono", "dark": null },
    "--font-text-theme": { "light": "Geist", "dark": null },
    "--h1-line-height": { "light": "2.25rem", "dark": null },
    "--h1-size": { "light": "1.875rem", "dark": null },
    "--h1-weight": { "light": "500", "dark": null },
    "--h2-line-height": { "light": "1.3333333", "dark": null },
    "--h2-size": { "light": "1.5em", "dark": null },
    "--h2-weight": { "light": "500", "dark": null },
    "--h3-line-height": { "light": "1.6", "dark": null },
    "--h3-size": { "light": "1.25em", "dark": null },
    "--he-weight": { "light": "500", "dark": null },
    "--hr-color": { "light": "#353533", "dark": null },
    "--line-height-normal": { "light": "1.75", "dark": null },
    "--link-color": { "light": "#000000", "dark": null },
    "--link-external-color": { "light": "#000000", "dark": null },
    "--list-marker-color": { "light": "#d4d4d4", "dark": null },
    "--p-spacing": { "light": "2.25rem", "dark": null },
    "--slider-thumb-border-color": { "light": "black", "dark": null },
    "--slider-track-background": { "light": "black", "dark": null },
    "--text-muted": { "light": "rgb(163, 163, 163)", "dark": null },
    "--text-normal": { "light": "rgb(0, 0, 0)", "dark": null },
    "--text-selection": { "light": "#007bcf", "dark": null },
    "--canvas-color": { "light": null, "dark": "126, 126, 126" }
  },
  ".notice progress[value]::-webkit-progress-bar": {
    "background-color": { "light": "#262626", "dark": null },
    "box-shadow": { "light": "inset 0px 0px 0px 1px #363636", "dark": null }
  },
  ".dropdown": { "background-image": { "light": null, "dark": "url(\"data" } },
  ".pdf-container.mod-themed .annotationLayer .textWidgetAnnotation input": {
    "filter": { "light": null, "dark": "invert(100%) hue-rotate(180deg)" }
  },
  ".pdf-container.mod-themed .annotationLayer .textWidgetAnnotation textarea": {
    "filter": { "light": null, "dark": "invert(100%) hue-rotate(180deg)" }
  },
  ".pdf-container.mod-themed .popupWrapper > div": {
    "background-color": {
      "light": null,
      "dark": "var(--background-secondary-alt)"
    },
    "filter": {
      "light": null,
      "dark": "drop-shadow(0px 0px 1px var(--color-base-60)) drop-shadow(0px 1px 2px rgba(0, 0, 0, 0.4)) drop-shadow(0px 4px 6px rgba(0, 0, 0, 0.4))"
    }
  },
  ".pdf-container.mod-themed .popupWrapper > div::after": {
    "background-color": {
      "light": null,
      "dark": "var(--background-secondary-alt)"
    }
  },
  ".pdf-container.mod-themed .popupContent:empty + .popupMeta": {
    "background-color": {
      "light": null,
      "dark": "var(--background-secondary-alt)"
    }
  },
  ".mermaid > svg": {
    "filter": {
      "light": null,
      "dark": "invert(100%) hue-rotate(180deg) saturate(1.25)"
    }
  },
  ".canvas-card-menu": {
    "background-color": { "light": null, "dark": "var(--background-secondary)" }
  },
  ".canvas-card-menu .canvas-card-menu-button svg": {
    "fill": { "light": null, "dark": "var(--background-secondary)" }
  },
  ".is-mobile": {
    "--background-modifier-cover": {
      "light": null,
      "dark": "rgba(0, 0, 0, 0.5)"
    },
    "--background-modifier-form-field": {
      "light": null,
      "dark": "var(--background-modifier-border)"
    },
    "--background-modifier-hover": {
      "light": null,
      "dark": "rgba(var(--mono-rgb-100), 0.15)"
    },
    "--background-modifier-message": {
      "light": null,
      "dark": "var(--background-secondary)"
    },
    "--color-base-00": { "light": null, "dark": "#000" },
    "--color-base-10": { "light": null, "dark": "#111" },
    "--color-base-20": { "light": null, "dark": "#1e1e1e" },
    "--mobile-sidebar-background": {
      "light": null,
      "dark": "var(--background-secondary)"
    },
    "--search-result-background": {
      "light": null,
      "dark": "var(--background-secondary)"
    },
    "--settings-home-background": {
      "light": null,
      "dark": "var(--background-primary)"
    },
    "--tag-background": {
      "light": null,
      "dark": "hsla(var(--interactive-accent-hsl), 0.2)"
    }
  },
  ".is-tablet": {
    "--interactive-hover": {
      "light": null,
      "dark": "var(--background-modifier-border-hover)"
    },
    "--interactive-normal": {
      "light": null,
      "dark": "var(--background-modifier-border)"
    },
    "--modal-background": {
      "light": null,
      "dark": "var(--background-secondary)"
    },
    "--titlebar-background": {
      "light": null,
      "dark": "var(--background-primary)"
    },
    "--titlebar-background-focused": {
      "light": null,
      "dark": "var(--background-primary)"
    }
  },
  ".is-tablet .community-item": {
    "background-color": { "light": null, "dark": "var(--background-secondary)" }
  },
  ".is-tablet .vertical-tab-content": {
    "background-color": { "light": null, "dark": "var(--background-secondary)" }
  },
  ".workspace-drawer-inner": {
    "background-color": { "light": null, "dark": "var(--background-secondary)" }
  },
  ".is-mobile .prompt": {
    "background-color": { "light": null, "dark": "var(--background-secondary)" }
  },
  ".is-mobile .prompt input.prompt-input": {
    "background-color": { "light": null, "dark": "var(--background-secondary)" }
  },
  ".is-tablet .suggestion-container": {
    "background-color": { "light": null, "dark": "var(--background-secondary)" }
  }
}

{
  ".graph-view.color-line-highlight": {
    "color": { "light": "#c72e6e !important", "dark": null },
    "} .theme-light .graph-view.color-fill-highlight { color": {
      "light": "#c72e6e !important",
      "dark": null
    },
    "} .theme-light .graph-view.color-fill { color": {
      "light": "var(--graph-dots)",
      "dark": null
    }
  },
  ".modal": {
    "border": { "light": "2px solid var(--interactive-accent)", "dark": null }
  },
  ".vertical-tab-header": {
    "background-color": {
      "light": "var(--background-primary-alt)",
      "dark": null
    }
  },
  ".horizontal-tab-nav-item": {
    ".vertical-tab-nav-item { background-color": {
      "light": "var(--background-primary-alt)",
      "dark": null
    },
    "padding": { "light": "10px 6px 10px 24px", "dark": null },
    "cursor": { "light": "default", "dark": null },
    "font-size": { "light": "16px", "dark": null },
    "color": { "light": "var(--text-faint)", "dark": null }
  },
  ".mermaid > svg": {
    "filter": {
      "light": null,
      "dark": "invert(100%) hue-rotate(180deg) saturate(1.25)"
    },
    "} .markdown-source-view.mod-cm6 .cm-hmd-codeblock { --indentation-guide-editing-indent": {
      "light": null,
      "dark": "0"
    },
    "} .markdown-source-view.mod-cm6 .cm-preview-code-block pre { margin": {
      "light": null,
      "dark": "0"
    },
    "} .markdown-source-view.mod-cm6 .code-block-flair { position": {
      "light": null,
      "dark": "absolute"
    },
    "right": { "light": null, "dark": "6px" },
    "top": { "light": null, "dark": "6px" },
    "z-index": { "light": null, "dark": "1" },
    "display": { "light": null, "dark": "inline-block" },
    "padding": { "light": null, "dark": "var(--size-4-1) var(--size-4-2)" },
    "border-radius": { "light": null, "dark": "var(--code-radius)" },
    "font-family": { "light": null, "dark": "var(--font-interface)" },
    "font-size": { "light": null, "dark": "var(--font-ui-smaller)" },
    "color": { "light": null, "dark": "var(--text-muted)" },
    "cursor": { "light": null, "dark": "var(--cursor)" }
  },
  ".cm-s-obsidian pre.HyperMD-codeblock-begin": {
    "border-top": {
      "light": null,
      "dark": "1px solid var(--background-primary)"
    },
    "border-top-left-radius": { "light": null, "dark": "5px" },
    "border-top-right-radius": { "light": null, "dark": "5px" }
  },
  ".cm-s-obsidian pre.HyperMD-codeblock-end": {
    "border-bottom": {
      "light": null,
      "dark": "1px solid var(--background-primary)"
    },
    "border-bottom-left-radius": { "light": null, "dark": "5px" },
    "border-bottom-right-radius": { "light": null, "dark": "5px" }
  },
  ".graph-view.color-arrow": { "color": { "light": null, "dark": "#50fa7b" } },
  ".graph-view.color-fill-tag": {
    "color": { "light": null, "dark": "#ffb86c" }
  },
  ".graph-view.color-fill-attachment": {
    "color": { "light": null, "dark": "#3b4354" }
  },
  ".graph-view.color-fill-unresolved": {
    "color": { "light": null, "dark": "#a34d61" }
  }
}

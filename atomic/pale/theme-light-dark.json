{
  ".workspace": {
    ".theme-dark .workspace { background-color": {
      "light": "var(--background-primary)",
      "dark": null
    }
  },
  ".workspace-tab-header-container:not(.workspace-split)": {
    "align-items": "center",
    "position": "relative",
    "height": "60px",
    "background-color": { "light": "var(--accent-color-bg)", "dark": null },
    "border": { "light": null, "dark": "none" }
  },
  ".menu": {
    "background-color": { "light": "#ffffff", "dark": "#454545" },
    "backdrop-filter": "blur(50px)",
    "color": { "light": "#212121", "dark": "#b4b4b4" }
  },
  ".mermaid > svg": {
    "filter": {
      "light": null,
      "dark": "invert(100%) hue-rotate(180deg) saturate(1.25)"
    },
    "} .markdown-source-view.mod-cm6 .cm-hmd-codeblock { --indentation-guide-editing-indent": {
      "light": null,
      "dark": "0"
    },
    "} .markdown-source-view.mod-cm6 .cm-preview-code-block pre { margin": {
      "light": null,
      "dark": "0"
    },
    "} .markdown-source-view.mod-cm6 .code-block-flair { position": {
      "light": null,
      "dark": "absolute"
    },
    "right": { "light": null, "dark": "6px" },
    "top": { "light": null, "dark": "6px" },
    "z-index": { "light": null, "dark": "1" },
    "display": { "light": null, "dark": "inline-block" },
    "padding": { "light": null, "dark": "var(--size-4-1) var(--size-4-2)" },
    "border-radius": { "light": null, "dark": "var(--code-radius)" },
    "font-family": { "light": null, "dark": "var(--font-interface)" },
    "font-size": { "light": null, "dark": "var(--font-ui-smaller)" },
    "color": { "light": null, "dark": "var(--text-muted)" },
    "cursor": { "light": null, "dark": "var(--cursor)" }
  },
  "body": {
    ".theme-light { --code-keyword": { "light": null, "dark": "#ff6347" },
    "--code-normal": { "light": null, "dark": "#c4c4c4" },
    "--code-operator": { "light": null, "dark": "#b89cdd" },
    "--code-value": { "light": null, "dark": "#e3b750" },
    "--code-function": { "light": null, "dark": "#dcbb59" },
    "--code-property": { "light": null, "dark": "#d29a8c" },
    "--code-string": { "light": null, "dark": "#05a3a3" }
  },
  ".workspace-ribbon .sidebar-toggle-button .clickable-icon": {
    "color": { "light": null, "dark": "var(--text-color)" }
  }
}

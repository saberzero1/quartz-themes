    }
  },
  ".callout[data-callout=\"cite\"]": {
    ".theme-dark .callout[data-callout=\"links\"], .theme-dark .callout[data-callout=\"meta\"] { --callout-color": {
      "light": null,
      "dark": "var(--w0-rgb)"

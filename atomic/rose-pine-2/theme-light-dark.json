{
  "body": {
    ".theme-dark { --background-primary": {
      "light": "var(--rp-surface)",
      "dark": null
    },
    "--background-primary-alt": { "light": "var(--rp-base)", "dark": null },
    "--background-secondary": { "light": "var(--rp-base)", "dark": null },
    "--background-secondary-alt": { "light": "var(--rp-base)", "dark": null },
    "--background-accent": { "light": "var(--rp-base)", "dark": null },
    "--background-modifier-border": {
      "light": "var(--rp-highlightMed)",
      "dark": null
    },
    "--background-modifier-form-field": { "light": "#000000", "dark": null },
    "--background-modifier-form-field-highlighted": {
      "light": "#000000",
      "dark": null
    },
    "--background-modifier-box-shadow": { "light": "#000000", "dark": null },
    ".theme-dark.rose-norm { --rp-base": { "light": null, "dark": "#191724" },
    "--rp-surface": { "light": null, "dark": "#1f1d2e" },
    "--rp-overlay": { "light": null, "dark": "#26233a" },
    "--rp-muted": { "light": null, "dark": "#6e6a86" },
    "--rp-subtle": { "light": null, "dark": "#908caa" },
    "--rp-text": { "light": null, "dark": "#e0def4" },
    "--rp-love": { "light": null, "dark": "#eb6f92" },
    "--rp-gold": { "light": null, "dark": "#f6c177" },
    "--rp-rose": { "light": null, "dark": "#ebbcba" },
    "--rp-iris": { "light": null, "dark": "#c4a7e7" },
    "--rp-pine": { "light": null, "dark": "#31748f" },
    "--rp-foam": { "light": null, "dark": "#9ccfd8" },
    "--rp-highlightLow": { "light": null, "dark": "#21202e" },
    "--rp-highlightMed": { "light": null, "dark": "#403d52" },
    "--rp-highlightHigh": { "light": null, "dark": "#524f67" }
  },
  ".mermaid > svg": {
    "filter": {
      "light": null,
      "dark": "invert(100%) hue-rotate(180deg) saturate(1.25)"
    },
    "} .markdown-source-view.mod-cm6 .cm-hmd-codeblock { --indentation-guide-editing-indent": {
      "light": null,
      "dark": "0"
    },
    "} .markdown-source-view.mod-cm6 .cm-preview-code-block pre { margin": {
      "light": null,
      "dark": "0"
    },
    "} .markdown-source-view.mod-cm6 .code-block-flair { position": {
      "light": null,
      "dark": "absolute"
    },
    "right": { "light": null, "dark": "6px" },
    "top": { "light": null, "dark": "6px" },
    "z-index": { "light": null, "dark": "1" },
    "display": { "light": null, "dark": "inline-block" },
    "padding": { "light": null, "dark": "var(--size-4-1) var(--size-4-2)" },
    "border-radius": { "light": null, "dark": "var(--code-radius)" },
    "font-family": { "light": null, "dark": "var(--font-interface)" },
    "font-size": { "light": null, "dark": "var(--font-ui-smaller)" },
    "color": { "light": null, "dark": "var(--text-muted)" },
    "cursor": { "light": null, "dark": "var(--cursor)" }
  },
  ".rose-moon": {
    "--rp-base": { "light": null, "dark": "#232136" },
    "--rp-surface": { "light": null, "dark": "#2a273f" },
    "--rp-overlay": { "light": null, "dark": "#393552" },
    "--rp-muted": { "light": null, "dark": "#6e6a86" },
    "--rp-subtle": { "light": null, "dark": "#908caa" },
    "--rp-text": { "light": null, "dark": "#e0def4" },
    "--rp-love": { "light": null, "dark": "#eb6f92" },
    "--rp-gold": { "light": null, "dark": "#f6c177" },
    "--rp-rose": { "light": null, "dark": "#ea9a97" },
    "--rp-iris": { "light": null, "dark": "#c4a7e7" },
    "--rp-pine": { "light": null, "dark": "#3e8fb0" },
    "--rp-foam": { "light": null, "dark": "#9ccfd8" },
    "--rp-highlightLow": { "light": null, "dark": "#2a283e" },
    "--rp-highlightMed": { "light": null, "dark": "#44415a" },
    "--rp-highlightHigh": { "light": null, "dark": "#56526e" }
  },
  "code[class*=\"language-\"]": {
    ".theme-dark pre[class*=\"language-\"], .theme-light code[class*=\"language-\"], .theme-light pre[class*=\"language-\"] { text-shadow": {
      "light": null,
      "dark": "none !important"
    },
    "color": { "light": null, "dark": "var(--text-faint) !important" },
    "background-color": { "light": null, "dark": "var(--pre-code)" }
  }
}

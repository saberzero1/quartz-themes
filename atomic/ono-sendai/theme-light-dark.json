{
  "code[class*=\"language-\"]": {
    ".theme-light pre[class*=\"language-\"] { background": {
      "light": "var(--background-primary-alt)",
      "dark": null
    },
    "text-shadow": { "light": "0px 0px white", "dark": null },
    "font-family": { "light": "var(--font-monospace)", "dark": null },
    "text-align": { "light": "left", "dark": null },
    "white-space": { "light": "pre", "dark": null },
    "word-spacing": { "light": "normal", "dark": null },
    "word-break": { "light": "normal", "dark": null },
    "word-wrap": { "light": "normal", "dark": null },
    "line-height": { "light": "1.5", "dark": null }
  },
  ".token.operator": {
    "background": { "light": "hsla(0, 0%, 100%, 0)", "dark": null }
  },
  ".is-translucent .titlebar": {
    "opacity": { "light": "1", "dark": null },
    "} .is-translucent.theme-dark .titlebar { opacity": {
      "light": "1",
      "dark": null
    },
    "} .is-translucent img { background-color": {
      "light": "transparent",
      "dark": null
    },
    "filter": { "light": "contrast(1.12) brightness(0.74)", "dark": null },
    "} .is-translucent .workspace-split.mod-root { opacity": {
      "light": "var(--opacity-translucency)",
      "dark": null
    },
    "} .is-translucent.theme-light .workspace-leaf-resize-handle { opacity": {
      "light": "0.1",
      "dark": null
    },
    "background-color": {
      "light": "var(--background-secondary-alt)",
      "dark": null
    },
    "} .is-translucent.theme-dark .workspace-leaf-resize-handle { opacity": {
      "light": "1",
      "dark": null
    },
    "} .is-translucent.theme-light .workspace-ribbon { opacity": {
      "light": "0.55",
      "dark": null
    },
    "} .is-translucent.theme-dark .workspace-ribbon { opacity": {
      "light": "1",
      "dark": null
    },
    "} .is-translucent .workspace-tabs { opacity": {
      "light": "0.6",
      "dark": null
    },
    "} .is-translucent.theme-dark .status-bar { opacity": {
      "light": "1",
      "dark": null
    },
    "border-top": {
      "light": "0px solid var(--background-secondary-alt)",
      "dark": null
    },
    "margin": { "light": "0px 0px", "dark": null },
    "} .is-translucent.theme-light .status-bar { opacity": {
      "light": "0.8",
      "dark": null
    },
    "} .is-translucent.theme-dark .view-content > iframe { background-color": {
      "light": "#010c0a !important",
      "dark": null
    }
  },
  ".mermaid > svg": {
    "filter": {
      "light": null,
      "dark": "invert(100%) hue-rotate(180deg) saturate(1.25)"
    },
    "} .markdown-source-view.mod-cm6 .cm-hmd-codeblock { --indentation-guide-editing-indent": {
      "light": null,
      "dark": "0"
    },
    "} .markdown-source-view.mod-cm6 .cm-preview-code-block pre { margin": {
      "light": null,
      "dark": "0"
    },
    "} .markdown-source-view.mod-cm6 .code-block-flair { position": {
      "light": null,
      "dark": "absolute"
    },
    "right": { "light": null, "dark": "6px" },
    "top": { "light": null, "dark": "6px" },
    "z-index": { "light": null, "dark": "1" },
    "display": { "light": null, "dark": "inline-block" },
    "padding": { "light": null, "dark": "var(--size-4-1) var(--size-4-2)" },
    "border-radius": { "light": null, "dark": "var(--code-radius)" },
    "font-family": { "light": null, "dark": "var(--font-interface)" },
    "font-size": { "light": null, "dark": "var(--font-ui-smaller)" },
    "color": { "light": null, "dark": "var(--text-muted)" },
    "cursor": { "light": null, "dark": "var(--cursor)" }
  }
}

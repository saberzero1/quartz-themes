{
  "body": {
    "--background-modifier-border": { "light": "#a2a9b1", "dark": null },
    "--callout-quote": { "light": "110, 110, 110", "dark": null },
    "--color-accent-1": {
      "light": "hsl( var(--accent-h), var(--accent-s), calc(var(--accent-l) - 2.5%) )",
      "dark": null
    },
    "--color-accent-2": {
      "light": "hsl( calc(var(--accent-h) - 2), var(--accent-s), calc(var(--accent-l) + 10%) )",
      "dark": null
    },
    "--color-accent-3": {
      "light": "hsl( calc(var(--accent-h) - 10), var(--accent-s), var(--accent-l), 20% )",
      "dark": null
    },
    "--color-base-00": { "light": "#ffffff", "dark": null },
    "--color-base-05": { "light": "#f8f9fa", "dark": null },
    "--color-base-10": { "light": "#fafafa", "dark": null },
    "--color-base-100": { "light": "#222222", "dark": null },
    "--color-base-20": { "light": "#f6f6f6", "dark": null },
    "--color-base-25": { "light": "#e3e3e3", "dark": null },
    "--color-base-30": { "light": "#e0e0e0", "dark": null },
    "--color-base-35": { "light": "#d4d4d4", "dark": null },
    "--color-base-40": { "light": "#bdbdbd", "dark": null },
    "--color-base-50": { "light": "#989898", "dark": null },
    "--color-base-60": { "light": "#707070", "dark": null },
    "--color-base-70": { "light": "#5a5a5a", "dark": null },
    "--color-blue": { "light": "rgb(0, 105, 224)", "dark": null },
    "--color-blue-rgb": { "light": "0, 105, 224", "dark": null },
    "--color-cyan": { "light": "rgb(0, 163, 81)", "dark": null },
    "--color-cyan-rgb": { "light": "0, 194, 97", "dark": null },
    "--color-green": { "light": "rgb(82, 163, 0)", "dark": null },
    "--color-green-rgb": { "light": "97, 194, 0", "dark": null },
    "--color-orange": { "light": "rgb(230, 111, 0)", "dark": null },
    "--color-orange-rgb": { "light": "230, 111, 0", "dark": null },
    "--color-pink": { "light": "rgb(194, 0, 81)", "dark": null },
    "--color-pink-rgb": { "light": "194, 0, 81", "dark": null },
    "--color-purple": { "light": "rgb(140, 63, 247)", "dark": null },
    "--color-purple-rgb": { "light": "81, 0, 194", "dark": null },
    "--color-red": { "light": "rgb(232, 41, 41)", "dark": null },
    "--color-red-rgb": { "light": "232, 51, 51", "dark": null },
    "--color-yellow": { "light": "rgb(163, 163, 0)", "dark": null },
    "--color-yellow-rgb": { "light": "204, 204, 0", "dark": null },
    "--h6-background": { "light": "rgb(235, 235, 210)", "dark": null },
    "--link-color": {
      "light": "hsl( calc(var(--accent-h) + 5), var(--accent-s), calc(var(--accent-l) - 5%) )",
      "dark": null
    },
    "--link-external-color": {
      "light": "hsl( calc(var(--accent-h) + 20), var(--accent-s), calc(var(--accent-l) - 5%) )",
      "dark": null
    },
    "--link-external-color-hover": {
      "light": "hsl( calc(var(--accent-h) + 10), var(--accent-s), calc(var(--accent-l) + 3%) )",
      "dark": null
    },
    "--link-unresolved-color": {
      "light": "var(--color-accent-1)",
      "dark": null
    },
    "--tab-gradient": {
      "light": "linear-gradient( to top, var(--color-accent-2) 0px, var(--color-accent-3) 0px, var(--background-primary) 100% )",
      "dark": null
    },
    "--tab-outline-color": {
      "light": "hsl( calc(var(--accent-h) - 10), var(--accent-s), var(--accent-l), 40% )",
      "dark": null
    },
    "--canvas-color": { "light": null, "dark": "126, 126, 126" }
  },
  ".notice progress[value]::-webkit-progress-bar": {
    "background-color": { "light": "#262626", "dark": null },
    "box-shadow": { "light": "inset 0px 0px 0px 1px #363636", "dark": null }
  },
  ":not(.custom-accent)": {
    "--accent-h": "215",
    "--accent-l": "50%",
    "--accent-s": "79%"
  },
  ".dropdown": { "background-image": { "light": null, "dark": "url(\"data" } },
  ".pdf-container.mod-themed .annotationLayer .textWidgetAnnotation input": {
    "filter": { "light": null, "dark": "invert(100%) hue-rotate(180deg)" }
  },
  ".pdf-container.mod-themed .annotationLayer .textWidgetAnnotation textarea": {
    "filter": { "light": null, "dark": "invert(100%) hue-rotate(180deg)" }
  },
  ".pdf-container.mod-themed .popupWrapper > div": {
    "background-color": {
      "light": null,
      "dark": "var(--background-secondary-alt)"
    },
    "filter": {
      "light": null,
      "dark": "drop-shadow(0px 0px 1px var(--color-base-60)) drop-shadow(0px 1px 2px rgba(0, 0, 0, 0.4)) drop-shadow(0px 4px 6px rgba(0, 0, 0, 0.4))"
    }
  },
  ".pdf-container.mod-themed .popupWrapper > div::after": {
    "background-color": {
      "light": null,
      "dark": "var(--background-secondary-alt)"
    }
  },
  ".pdf-container.mod-themed .popupContent:empty + .popupMeta": {
    "background-color": {
      "light": null,
      "dark": "var(--background-secondary-alt)"
    }
  },
  ".mermaid > svg": {
    "filter": {
      "light": null,
      "dark": "invert(100%) hue-rotate(180deg) saturate(1.25)"
    }
  },
  ".canvas-card-menu": {
    "background-color": { "light": null, "dark": "var(--background-secondary)" }
  },
  ".canvas-card-menu .canvas-card-menu-button svg": {
    "fill": { "light": null, "dark": "var(--background-secondary)" }
  },
  ".is-mobile": {
    "--background-modifier-cover": {
      "light": null,
      "dark": "rgba(0, 0, 0, 0.5)"
    },
    "--background-modifier-form-field": {
      "light": null,
      "dark": "var(--background-modifier-border)"
    },
    "--background-modifier-hover": {
      "light": null,
      "dark": "rgba(var(--mono-rgb-100), 0.15)"
    },
    "--background-modifier-message": {
      "light": null,
      "dark": "var(--background-secondary)"
    },
    "--color-base-00": { "light": null, "dark": "#000" },
    "--color-base-10": { "light": null, "dark": "#111" },
    "--color-base-20": { "light": null, "dark": "#1e1e1e" },
    "--mobile-sidebar-background": {
      "light": null,
      "dark": "var(--background-secondary)"
    },
    "--search-result-background": {
      "light": null,
      "dark": "var(--background-secondary)"
    },
    "--settings-home-background": {
      "light": null,
      "dark": "var(--background-primary)"
    },
    "--tag-background": {
      "light": null,
      "dark": "hsla(var(--interactive-accent-hsl), 0.2)"
    }
  },
  ".is-tablet": {
    "--interactive-hover": {
      "light": null,
      "dark": "var(--background-modifier-border-hover)"
    },
    "--interactive-normal": {
      "light": null,
      "dark": "var(--background-modifier-border)"
    },
    "--modal-background": {
      "light": null,
      "dark": "var(--background-secondary)"
    },
    "--titlebar-background": {
      "light": null,
      "dark": "var(--background-primary)"
    },
    "--titlebar-background-focused": {
      "light": null,
      "dark": "var(--background-primary)"
    }
  },
  ".is-tablet .community-item": {
    "background-color": { "light": null, "dark": "var(--background-secondary)" }
  },
  ".is-tablet .vertical-tab-content": {
    "background-color": { "light": null, "dark": "var(--background-secondary)" }
  },
  ".workspace-drawer-inner": {
    "background-color": { "light": null, "dark": "var(--background-secondary)" }
  },
  ".is-mobile .prompt": {
    "background-color": { "light": null, "dark": "var(--background-secondary)" }
  },
  ".is-mobile .prompt input.prompt-input": {
    "background-color": { "light": null, "dark": "var(--background-secondary)" }
  },
  ".is-tablet .suggestion-container": {
    "background-color": { "light": null, "dark": "var(--background-secondary)" }
  }
}

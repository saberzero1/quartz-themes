{
  ":not(.custom-accent)": {
    ".theme-dark": {
      "light": "not(.custom-accent) { --accent-h:215 !important",
      "dark": null
    },
    "--accent-s": { "light": "79% !important", "dark": null },
    "--accent-l": { "light": "50% !important", "dark": null }
  },
  "body": {
    ".theme-dark { --color-red-rgb": { "light": "232, 51, 51", "dark": null },
    "--color-red": { "light": "#e82929", "dark": null },
    "--color-cyan-rgb": { "light": "0, 194, 97", "dark": null },
    "--color-cyan": { "light": "#00a351", "dark": null },
    "--color-orange-rgb": { "light": "230, 111, 0", "dark": null },
    "--color-orange": { "light": "#e66f00", "dark": null },
    "--color-yellow-rgb": { "light": "204, 204, 0", "dark": null },
    "--color-yellow": { "light": "#a3a300", "dark": null },
    "--color-green-rgb": { "light": "97, 194, 0", "dark": null },
    "--color-green": { "light": "#52a300", "dark": null },
    "--color-blue-rgb": { "light": "0, 105, 224", "dark": null },
    "--color-blue": { "light": "#0069e0", "dark": null },
    "--color-purple-rgb": { "light": "81, 0, 194", "dark": null },
    "--color-purple": { "light": "#8c3ff7", "dark": null },
    "--color-pink-rgb": { "light": "194, 0, 81", "dark": null },
    "--color-pink": { "light": "#c20051", "dark": null },
    "--callout-quote": { "light": "110, 110, 110", "dark": null }
  },
  ".mermaid > svg": {
    "filter": {
      "light": null,
      "dark": "invert(100%) hue-rotate(180deg) saturate(1.25)"
    },
    "} .markdown-source-view.mod-cm6 .cm-hmd-codeblock { --indentation-guide-editing-indent": {
      "light": null,
      "dark": "0"
    },
    "} .markdown-source-view.mod-cm6 .cm-preview-code-block pre { margin": {
      "light": null,
      "dark": "0"
    },
    "} .markdown-source-view.mod-cm6 .code-block-flair { position": {
      "light": null,
      "dark": "absolute"
    },
    "right": { "light": null, "dark": "6px" },
    "top": { "light": null, "dark": "6px" },
    "z-index": { "light": null, "dark": "1" },
    "display": { "light": null, "dark": "inline-block" },
    "padding": { "light": null, "dark": "var(--size-4-1) var(--size-4-2)" },
    "border-radius": { "light": null, "dark": "var(--code-radius)" },
    "font-family": { "light": null, "dark": "var(--font-interface)" },
    "font-size": { "light": null, "dark": "var(--font-ui-smaller)" },
    "color": { "light": null, "dark": "var(--text-muted)" },
    "cursor": { "light": null, "dark": "var(--cursor)" }
  }
}

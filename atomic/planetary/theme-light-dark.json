{
  "body": {
    "--background-code": { "light": "var(--canvas2)", "dark": null },
    "--background-code-preview": { "light": "var(--canvas2)", "dark": null },
    "--background-modifier-cover": {
      "light": "rgba(0, 0, 0, 0.8)",
      "dark": null
    },
    "--background-preview": { "light": "var(--canvas1)", "dark": null },
    "--background-primary": { "light": "var(--canvas1)", "dark": null },
    "--background-primary-alt": { "light": "var(--canvas2)", "dark": null },
    "--background-quick": { "light": "var(--canvas2)", "dark": null },
    "--background-secondary": { "light": "var(--canvas3)", "dark": null },
    "--background-secondary-alt": { "light": "var(--canvas3)", "dark": null },
    "--background-tag": { "light": "#000000", "dark": null },
    "--canvas-contrast-high": { "light": "#4a3827", "dark": null },
    "--canvas-contrast-low": { "light": "#b29688", "dark": null },
    "--canvas-contrast-medium": { "light": "#765c4f", "dark": null },
    "--canvas1": { "light": "#fffefd", "dark": null },
    "--canvas2": { "light": "#f6e2db", "dark": null },
    "--canvas3": { "light": "#fdf9f5", "dark": null },
    "--canvas4": { "light": "#d7c3ba", "dark": null },
    "--color-accent": {
      "light": "hsl(var(--accent-h), var(--accent-s), var(--accent-l))",
      "dark": null
    },
    "--color-accent-1": { "light": "#f85c51", "dark": null },
    "--color-accent-2": { "light": "#a83e37", "dark": null },
    "--color-accent-hsl": {
      "light": "var(--accent-h), var(--accent-s), var(--accent-l)",
      "dark": null
    },
    "--color-blue": { "light": "#d6e9ff", "dark": null },
    "--color-blue-rgb": { "light": "2, 122, 255", "dark": null },
    "--color-cyan": { "light": "rgb(14, 205, 201)", "dark": null },
    "--color-cyan-rgb": { "light": "83, 223, 221", "dark": null },
    "--color-green": { "light": "#0cc77c", "dark": null },
    "--color-green-rgb": { "light": "68, 207, 110", "dark": null },
    "--color-head": { "light": "var(--color-accent)", "dark": null },
    "--color-orange": { "light": "#e9973f", "dark": null },
    "--color-orange-rgb": { "light": "233, 151, 63", "dark": null },
    "--color-pink": { "light": "#f22c96", "dark": null },
    "--color-pink-rgb": { "light": "250, 153, 205", "dark": null },
    "--color-purple": { "light": "#a29bfe", "dark": null },
    "--color-purple-rgb": { "light": "168, 130, 255", "dark": null },
    "--color-red": { "light": "#f85c51", "dark": null },
    "--color-red-rgb": { "light": "251, 70, 76", "dark": null },
    "--color-yellow": { "light": "#eacf00", "dark": null },
    "--color-yellow-rgb": { "light": "224, 222, 113", "dark": null },
    "--file-hr": { "light": "var(--canvas2)", "dark": null },
    "--interactive-accent": { "light": "var(--color-accent)", "dark": null },
    "--interactive-accent-hover": {
      "light": "var(--color-accent-2)",
      "dark": null
    },
    "--interactive-hover": { "light": "var(--canvas4)", "dark": null },
    "--interactive-normal": { "light": "var(--canvas2)", "dark": null },
    "--leaf-handle": { "light": "var(--canvas2)", "dark": null },
    "--menu-selection": { "light": "var(--canvas2)", "dark": null },
    "--menu-selection-text": {
      "light": "var(--canvas-contrast-high)",
      "dark": null
    },
    "--scrollbar-bg": { "light": "var(--canvas2)", "dark": null },
    "--table-line": { "light": "var(--canvas-contrast-low)", "dark": null },
    "--text-accent": { "light": "var(--color-accent)", "dark": null },
    "--text-faint": { "light": "var(--color-accent)", "dark": null },
    "--text-highlight-bg": { "light": "var(--color-blue)", "dark": null },
    "--text-muted": { "light": "var(--canvas-contrast-low)", "dark": null },
    "--text-normal": { "light": "var(--canvas-contrast-high)", "dark": null },
    "--text-on-accent": { "light": "var(--canvas1)", "dark": null },
    "--text-selection": { "light": "var(--canvas4)", "dark": null },
    "--text-tag": { "light": "var(--color-green)", "dark": null },
    "--canvas-color": { "light": null, "dark": "126, 126, 126" }
  },
  ".notice progress[value]::-webkit-progress-bar": {
    "background-color": { "light": "#262626", "dark": null },
    "box-shadow": { "light": "inset 0px 0px 0px 1px #363636", "dark": null }
  },
  ".dropdown": { "background-image": { "light": null, "dark": "url(\"data" } },
  ".pdf-container.mod-themed .annotationLayer .textWidgetAnnotation input": {
    "filter": { "light": null, "dark": "invert(100%) hue-rotate(180deg)" }
  },
  ".pdf-container.mod-themed .annotationLayer .textWidgetAnnotation textarea": {
    "filter": { "light": null, "dark": "invert(100%) hue-rotate(180deg)" }
  },
  ".pdf-container.mod-themed .popupWrapper > div": {
    "background-color": {
      "light": null,
      "dark": "var(--background-secondary-alt)"
    },
    "filter": {
      "light": null,
      "dark": "drop-shadow(0px 0px 1px var(--color-base-60)) drop-shadow(0px 1px 2px rgba(0, 0, 0, 0.4)) drop-shadow(0px 4px 6px rgba(0, 0, 0, 0.4))"
    }
  },
  ".pdf-container.mod-themed .popupWrapper > div::after": {
    "background-color": {
      "light": null,
      "dark": "var(--background-secondary-alt)"
    }
  },
  ".pdf-container.mod-themed .popupContent:empty + .popupMeta": {
    "background-color": {
      "light": null,
      "dark": "var(--background-secondary-alt)"
    }
  },
  ".mermaid > svg": {
    "filter": {
      "light": null,
      "dark": "invert(100%) hue-rotate(180deg) saturate(1.25)"
    }
  },
  ".canvas-card-menu": {
    "background-color": { "light": null, "dark": "var(--background-secondary)" }
  },
  ".canvas-card-menu .canvas-card-menu-button svg": {
    "fill": { "light": null, "dark": "var(--background-secondary)" }
  },
  ".is-mobile": {
    "--background-modifier-cover": {
      "light": null,
      "dark": "rgba(0, 0, 0, 0.5)"
    },
    "--background-modifier-form-field": {
      "light": null,
      "dark": "var(--background-modifier-border)"
    },
    "--background-modifier-hover": {
      "light": null,
      "dark": "rgba(var(--mono-rgb-100), 0.15)"
    },
    "--background-modifier-message": {
      "light": null,
      "dark": "var(--background-secondary)"
    },
    "--color-base-00": { "light": null, "dark": "#000" },
    "--color-base-10": { "light": null, "dark": "#111" },
    "--color-base-20": { "light": null, "dark": "#1e1e1e" },
    "--mobile-sidebar-background": {
      "light": null,
      "dark": "var(--background-secondary)"
    },
    "--search-result-background": {
      "light": null,
      "dark": "var(--background-secondary)"
    },
    "--settings-home-background": {
      "light": null,
      "dark": "var(--background-primary)"
    },
    "--tag-background": {
      "light": null,
      "dark": "hsla(var(--interactive-accent-hsl), 0.2)"
    }
  },
  ".is-tablet": {
    "--interactive-hover": {
      "light": null,
      "dark": "var(--background-modifier-border-hover)"
    },
    "--interactive-normal": {
      "light": null,
      "dark": "var(--background-modifier-border)"
    },
    "--modal-background": {
      "light": null,
      "dark": "var(--background-secondary)"
    },
    "--titlebar-background": {
      "light": null,
      "dark": "var(--background-primary)"
    },
    "--titlebar-background-focused": {
      "light": null,
      "dark": "var(--background-primary)"
    }
  },
  ".is-tablet .community-item": {
    "background-color": { "light": null, "dark": "var(--background-secondary)" }
  },
  ".is-tablet .vertical-tab-content": {
    "background-color": { "light": null, "dark": "var(--background-secondary)" }
  },
  ".workspace-drawer-inner": {
    "background-color": { "light": null, "dark": "var(--background-secondary)" }
  },
  ".is-mobile .prompt": {
    "background-color": { "light": null, "dark": "var(--background-secondary)" }
  },
  ".is-mobile .prompt input.prompt-input": {
    "background-color": { "light": null, "dark": "var(--background-secondary)" }
  },
  ".is-tablet .suggestion-container": {
    "background-color": { "light": null, "dark": "var(--background-secondary)" }
  }
}

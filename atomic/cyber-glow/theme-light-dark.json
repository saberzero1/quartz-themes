    "--color-base-10": { "light": "#fafafa", "dark": null },
    "--color-base-25": { "light": "#e3e3e3", "dark": null },
    "--color-base-30": { "light": "#e0e0e0", "dark": null },
    "--color-base-35": { "light": "#d4d4d4", "dark": null },
    "--color-base-40": { "light": "#bdbdbd", "dark": null },
    "--color-base-50": { "light": "#ababab", "dark": null },
    "--color-base-60": { "light": "#707070", "dark": null },
    "--color-base-70": { "light": "#5c5c5c", "dark": null },
    "--color-base-100": { "light": "#222222", "dark": null },
    "--color-accent-2": {
      "light": "hsl(\n    calc(var(--accent-h) - 3),\n    calc(var(--accent-s) * 1.02),\n    calc(var(--accent-l) * 1.15)\n  )",
      "dark": null
    },
    "--background-secondary-alt": {
      "light": "var(--color-base-05)",
      "dark": null
    },
    "--background-modifier-box-shadow": { "light": "#000000", "dark": null },
    "--background-modifier-cover": { "light": "#dcdcdc", "dark": null },
    "--input-shadow": {
      "light": "inset 0 0 0 1px #000000, 0 2px 3px 0 #000000, 0 1px 1.5px 0 #000000,\n    0 1px 2px 0 #000000, 0 0 0 0 transparent",
      "dark": null
    },
    "--input-shadow-hover": {
      "light": "inset 0 0 0 1px #000000, 0 2px 3px 0 #000000, 0 1px 1.5px 0 #000000,\n    0 1px 2px 0 #000000, 0 0 0 0 transparent",
      "dark": null
    },
    "--shadow-s": {
      "light": "0px 1px 2px #000000, 0px 3.4px 6.7px #000000, 0px 15px 30px #000000",
      "dark": null
    },
    "--shadow-l": {
      "light": "0px 1.8px 7.3px #000000, 0px 6.3px 24.7px #000000, 0px 30px 90px #000000",
      "dark": null
    },
    "--color-base-00": { "light": "#e6e5e5", "dark": null },
    "--color-base-20": { "light": "#c2c2c2", "dark": null },
    "--color-base-05": { "light": "#f6f4f4b7", "dark": null },
    "--bg-primary": { "light": "230, 229, 229", "dark": null },
    "--bg-secondary": { "light": "194, 194, 194", "dark": null },
    "--background-settings": { "light": "#ffffff", "dark": null },
    "--background-settings-alt": { "light": "#cbcaca", "dark": null },
    "--background-settings-2": { "light": "#b3b3b3", "dark": null },
    "--background-settings-3": { "light": "#e2e2e2", "dark": null },
    "--text-selection": {
      "light": "hsla(var(--interactive-accent-hsl), 0.35)",
      "dark": null
    },
    "--accent-h": { "light": "198", "dark": null },
    "--accent-s": { "light": "78%", "dark": null },
    "--accent-l": { "light": "64%", "dark": null },
    "--color-accent-faint": {
      "light": "hsl(\n    var(--accent-h),\n    calc(var(--accent-s) + 10%),\n    calc(var(--accent-l) - 4%)\n  )",
      "dark": null
    },
    "--color-accent-active": {
      "light": "hsla(\n    var(--accent-h),\n    calc(var(--accent-s) - 13%),\n    calc(var(--accent-l) + 10%),\n    0.65\n  )",
      "dark": null
    },
    "--color-accent-active2": {
      "light": "hsla(\n    var(--accent-h),\n    calc(var(--accent-s) - 13%),\n    calc(var(--accent-l) + 10%),\n    0.45\n  )",
      "dark": null
    },
    "--color-glow": {
      "light": "hsl(\n    calc(var(--accent-h) + 1),\n    calc(var(--accent-s) - 36%),\n    calc(var(--accent-l) - 25%)\n  )",
      "dark": null
    },
    "--color-accent-hsl": {
      "light": "var(--accent-h), var(--accent-s), var(--accent-l)",
      "dark": null
    },
    "--color-accent": {
      "light": "hsl(\n    var(--accent-h),\n    calc(var(--accent-s) - 20%),\n    calc(var(--accent-l) - 10%)\n  )",
      "dark": null
    },
    "--color-accent-1": {
      "light": "hsl(\n    var(--accent-h),\n    var(--accent-s),\n    calc(var(--accent-l) - 3.8%)\n  )",
      "dark": null
    },
    "--code-background": { "light": "#e1e1e1", "dark": null },
    "--text-close-color": { "light": "#791d26", "dark": null },
    "--background-close-hover": { "light": "#99212d70", "dark": null },
    "--background-close-active": { "light": "#88111c8a", "dark": null },
    "--background-close": { "light": "#b4000066", "dark": null },
    "--folder-rose": { "light": "222, 149, 132", "dark": null },
    "--folder-lightpink": { "light": "221, 120, 120", "dark": null },
    "--folder-pink": { "light": "236, 131, 208", "dark": null },
    "--folder-mauve": { "light": "136, 57, 239", "dark": null },
    "--folder-red": { "light": "210, 15, 57", "dark": null },

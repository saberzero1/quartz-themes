{
  "body": {
    "--accent-h": { "light": "198", "dark": null },
    "--accent-l": { "light": "64%", "dark": null },
    "--accent-s": { "light": "78%", "dark": null },
    "--background-close": { "light": "#b4000066", "dark": null },
    "--background-close-active": { "light": "#88111c8a", "dark": null },
    "--background-close-hover": { "light": "#99212d70", "dark": null },
    "--background-settings": {
      "light": "rgba(255, 255, 255, 0.9)",
      "dark": null
    },
    "--background-settings-2": {
      "light": "rgba(179, 179, 179, 0.25)",
      "dark": null
    },
    "--background-settings-3": {
      "light": "rgba(226, 226, 226, 0.35)",
      "dark": null
    },
    "--background-settings-alt": {
      "light": "rgba(203, 202, 202, 0.65)",
      "dark": null
    },
    "--bg-primary": { "light": "230, 229, 229", "dark": null },
    "--bg-secondary": { "light": "194, 194, 194", "dark": null },
    "--code-background": { "light": "rgba(225, 225, 225, 0.75)", "dark": null },
    "--color-accent": {
      "light": "hsl( var(--accent-h), calc(var(--accent-s) - 20%), calc(var(--accent-l) - 10%) )",
      "dark": null
    },
    "--color-accent-1": {
      "light": "hsl( var(--accent-h), var(--accent-s), calc(var(--accent-l) - 3.8%) )",
      "dark": null
    },
    "--color-accent-active": {
      "light": "hsla( var(--accent-h), calc(var(--accent-s) - 13%), calc(var(--accent-l) + 10%), 0.65 )",
      "dark": null
    },
    "--color-accent-active2": {
      "light": "hsla( var(--accent-h), calc(var(--accent-s) - 13%), calc(var(--accent-l) + 10%), 0.45 )",
      "dark": null
    },
    "--color-accent-faint": {
      "light": "hsl( var(--accent-h), calc(var(--accent-s) + 10%), calc(var(--accent-l) - 4%) )",
      "dark": null
    },
    "--color-accent-hsl": {
      "light": "var(--accent-h), var(--accent-s), var(--accent-l)",
      "dark": null
    },
    "--color-base-00": { "light": "rgb(230, 229, 229)", "dark": null },
    "--color-base-05": { "light": "#f6f4f4b7", "dark": null },
    "--color-base-20": { "light": "rgb(194, 194, 194)", "dark": null },
    "--color-glow": {
      "light": "hsl( calc(var(--accent-h) + 1), calc(var(--accent-s) - 36%), calc(var(--accent-l) - 25%) )",
      "dark": null
    },
    "--folder-blue": { "light": "42, 110, 245", "dark": null },
    "--folder-green": { "light": "64, 160, 43", "dark": null },
    "--folder-lavender": { "light": "114, 135, 253", "dark": null },
    "--folder-lightpink": { "light": "221, 120, 120", "dark": null },
    "--folder-maroon": { "light": "230, 69, 83", "dark": null },
    "--folder-mauve": { "light": "136, 57, 239", "dark": null },
    "--folder-peach": { "light": "254, 100, 11", "dark": null },
    "--folder-pink": { "light": "236, 131, 208", "dark": null },
    "--folder-red": { "light": "210, 15, 57", "dark": null },
    "--folder-rose": { "light": "222, 149, 132", "dark": null },
    "--folder-sapphire": { "light": "32, 159, 181", "dark": null },
    "--folder-sky": { "light": "4, 165, 229", "dark": null },
    "--folder-teal": { "light": "23, 146, 153", "dark": null },
    "--folder-yellow": { "light": "228, 147, 32", "dark": null },
    "--text-close-color": { "light": "#791d26", "dark": null },
    "--text-selection": {
      "light": "hsla(var(--interactive-accent-hsl), 0.35)",
      "dark": null
    },
    "--canvas-color": { "light": null, "dark": "126, 126, 126" }
  },
  ".notice progress[value]::-webkit-progress-bar": {
    "background-color": { "light": "#262626", "dark": null },
    "box-shadow": { "light": "inset 0px 0px 0px 1px #363636", "dark": null }
  },
  ".callout.callout[data-callout=\"grid\"] .callout-content": {
    "--callout-color": { "light": "0, 31, 63", "dark": "0, 116, 217" },
    "border-bottom": "2px solid",
    "border-image-slice": "1",
    "border-image-source": "linear-gradient( to right, transparent, rgb(var(--callout-color)), transparent )",
    "border-top": "2px solid"
  },
  ".callout.callout[data-callout~=\"caption\"] > .callout-content": {
    "--callout-color": { "light": "102, 187, 106", "dark": "178, 223, 181" }
  },
  ".dropdown": { "background-image": { "light": null, "dark": "url(\"data" } },
  ".pdf-container.mod-themed .annotationLayer .textWidgetAnnotation input": {
    "filter": { "light": null, "dark": "invert(100%) hue-rotate(180deg)" }
  },
  ".pdf-container.mod-themed .annotationLayer .textWidgetAnnotation textarea": {
    "filter": { "light": null, "dark": "invert(100%) hue-rotate(180deg)" }
  },
  ".pdf-container.mod-themed .popupWrapper > div": {
    "background-color": {
      "light": null,
      "dark": "var(--background-secondary-alt)"
    },
    "filter": {
      "light": null,
      "dark": "drop-shadow(0px 0px 1px var(--color-base-60)) drop-shadow(0px 1px 2px rgba(0, 0, 0, 0.4)) drop-shadow(0px 4px 6px rgba(0, 0, 0, 0.4))"
    }
  },
  ".pdf-container.mod-themed .popupWrapper > div::after": {
    "background-color": {
      "light": null,
      "dark": "var(--background-secondary-alt)"
    }
  },
  ".pdf-container.mod-themed .popupContent:empty + .popupMeta": {
    "background-color": {
      "light": null,
      "dark": "var(--background-secondary-alt)"
    }
  },
  ".mermaid > svg": {
    "filter": {
      "light": null,
      "dark": "invert(100%) hue-rotate(180deg) saturate(1.25)"
    }
  },
  ".canvas-card-menu": {
    "backdrop-filter": { "light": null, "dark": "blur(5px) saturate(120%)" },
    "background-color": { "light": null, "dark": "var(--background-settings)" },
    "border-bottom": { "light": null, "dark": "1px solid" },
    "border-image-slice": { "light": null, "dark": "1" },
    "border-image-source": {
      "light": null,
      "dark": "linear-gradient( to right, transparent, var(--text-accent), transparent )"
    },
    "border-top": { "light": null, "dark": "1px solid" },
    "box-shadow": { "light": null, "dark": "none" }
  },
  ".canvas-card-menu .canvas-card-menu-button svg": {
    "fill": { "light": null, "dark": "transparent" }
  },
  ".is-mobile": {
    "--background-modifier-cover": {
      "light": null,
      "dark": "rgba(0, 0, 0, 0.5)"
    },
    "--background-modifier-form-field": {
      "light": null,
      "dark": "var(--background-modifier-border)"
    },
    "--background-modifier-hover": {
      "light": null,
      "dark": "rgba(var(--mono-rgb-100), 0.15)"
    },
    "--background-modifier-message": {
      "light": null,
      "dark": "var(--background-secondary)"
    },
    "--color-base-00": { "light": null, "dark": "#000" },
    "--color-base-10": { "light": null, "dark": "#111" },
    "--color-base-20": { "light": null, "dark": "#1e1e1e" },
    "--mobile-sidebar-background": {
      "light": null,
      "dark": "var(--background-secondary)"
    },
    "--search-result-background": {
      "light": null,
      "dark": "var(--background-secondary)"
    },
    "--settings-home-background": {
      "light": null,
      "dark": "var(--background-primary)"
    },
    "--tag-background": {
      "light": null,
      "dark": "hsla(var(--interactive-accent-hsl), 0.2)"
    }
  },
  ".is-tablet": {
    "--interactive-hover": {
      "light": null,
      "dark": "var(--background-modifier-border-hover)"
    },
    "--interactive-normal": {
      "light": null,
      "dark": "var(--background-modifier-border)"
    },
    "--modal-background": {
      "light": null,
      "dark": "var(--background-secondary)"
    },
    "--titlebar-background": {
      "light": null,
      "dark": "var(--background-primary)"
    },
    "--titlebar-background-focused": {
      "light": null,
      "dark": "var(--background-primary)"
    }
  },
  ".is-tablet .community-item": {
    "background-color": { "light": null, "dark": "var(--background-secondary)" }
  },
  ".is-tablet .vertical-tab-content": {
    "background-color": { "light": null, "dark": "var(--background-secondary)" }
  },
  ".workspace-drawer-inner": {
    "background-color": { "light": null, "dark": "var(--background-secondary)" }
  },
  ".is-mobile .prompt": {
    "background": { "light": null, "dark": "var(--background-settings)" },
    "border-bottom": { "light": null, "dark": "2px solid" },
    "border-image-slice": { "light": null, "dark": "1" },
    "border-image-source": {
      "light": null,
      "dark": "linear-gradient( to right, transparent, var(--text-accent), transparent )"
    },
    "border-radius": { "light": null, "dark": "3px" },
    "border-top": { "light": null, "dark": "2px solid" }
  },
  ".is-mobile .prompt input.prompt-input": {
    "background": { "light": null, "dark": "var(--background-settings-2)" },
    "background-color": { "light": null, "dark": "var(--color-accent-active)" },
    "border-image-slice": { "light": null, "dark": "1" },
    "border-image-source": {
      "light": null,
      "dark": "linear-gradient( to right, transparent, var(--text-accent), transparent )"
    },
    "border-radius": { "light": null, "dark": "5px" },
    "box-shadow": { "light": null, "dark": "none" },
    "margin": { "light": null, "dark": "5px" },
    "padding": { "light": null, "dark": "10px" }
  },
  ".is-tablet .suggestion-container": {
    "background-color": { "light": null, "dark": "var(--background-secondary)" }
  },
  ".is-phone .modal.mod-settings .vertical-tab-nav-item:active": {
    "background-color": { "light": null, "dark": "var(--hover-bg2)" },
    "box-shadow": {
      "light": null,
      "dark": "inset 0em 0em 1em var(--hover-color)"
    },
    "color": { "light": null, "dark": "var(--text-on-accent)" },
    "transition": {
      "light": null,
      "dark": "background-color 0.2s, box-shadow 0.2s, animation 0.2s, border 1s"
    }
  },
  ".is-phone .modal.mod-settings .vertical-tab-nav-item": {
    "background-color": {
      "light": null,
      "dark": "var(--background-settings-alt)"
    },
    "border-bottom": { "light": null, "dark": "1px solid transparent" },
    "border-radius": { "light": null, "dark": "var(--radius-s)" },
    "color": { "light": null, "dark": "var(--text-muted)" },
    "margin": { "light": null, "dark": "5px" }
  },
  ".is-phone .modal.mod-settings .vertical-tab-nav-item:is(:active) .vertical-tab-nav-item-chevron": {
    "color": { "light": null, "dark": "var(--text-on-accent)" }
  }
}

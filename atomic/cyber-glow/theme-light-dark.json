{
  "body": {
    "color-scheme": { "light": "light", "dark": null },
    "--highlight-mix-blend-mode": { "light": "darken", "dark": null },
    "--mono-rgb-0": { "light": "255, 255, 255", "dark": null },
    "--mono-rgb-100": { "light": "0, 0, 0", "dark": null },
    "--color-red-rgb": { "light": "233, 49, 71", "dark": null },
    "--color-red": { "light": "#e93147", "dark": null },
    "--color-orange-rgb": { "light": "236, 117, 0", "dark": null },
    "--color-orange": { "light": "#ec7500", "dark": null },
    "--color-yellow-rgb": { "light": "224, 172, 0", "dark": null },
    "--color-yellow": { "light": "#e0ac00", "dark": null },
    "--color-green-rgb": { "light": "8, 185, 78", "dark": null },
    "--color-green": { "light": "#08b94e", "dark": null },
    "--color-cyan-rgb": { "light": "0, 191, 188", "dark": null },
    "--color-cyan": { "light": "#00bfbc", "dark": null },
    "--color-blue-rgb": { "light": "8, 109, 221", "dark": null },
    "--color-blue": { "light": "#086ddd", "dark": null },
    "--color-purple-rgb": { "light": "120, 82, 238", "dark": null },
    "--color-purple": { "light": "#7852ee", "dark": null },
    "--color-pink-rgb": { "light": "213, 57, 132", "dark": null },
    "--color-pink": { "light": "#d53984", "dark": null },
    "--color-base-10": { "light": "#fafafa", "dark": null },
    "--color-base-25": { "light": "#e3e3e3", "dark": null },
    "--color-base-30": { "light": "#e0e0e0", "dark": null },
    "--color-base-35": { "light": "#d4d4d4", "dark": null },
    "--color-base-40": { "light": "#bdbdbd", "dark": null },
    "--color-base-50": { "light": "#ababab", "dark": null },
    "--color-base-60": { "light": "#707070", "dark": null },
    "--color-base-70": { "light": "#5c5c5c", "dark": null },
    "--color-base-100": { "light": "#222222", "dark": null },
    "--color-accent-2": {
      "light": "hsl( calc(var(--accent-h) - 3), calc(var(--accent-s) * 1.02), calc(var(--accent-l) * 1.15) )",
      "dark": null
    },
    "--background-secondary-alt": {
      "light": "var(--color-base-05)",
      "dark": null
    },
    "--background-modifier-box-shadow": { "light": "#000000", "dark": null },
    "--background-modifier-cover": { "light": "#dcdcdc", "dark": null },
    "--input-shadow": {
      "light": "inset 0 0 0 1px #000000, 0 2px 3px 0 #000000, 0 1px 1.5px 0 #000000, 0 1px 2px 0 #000000, 0 0 0 0 transparent",
      "dark": null
    },
    "--input-shadow-hover": {
      "light": "inset 0 0 0 1px #000000, 0 2px 3px 0 #000000, 0 1px 1.5px 0 #000000, 0 1px 2px 0 #000000, 0 0 0 0 transparent",
      "dark": null
    },
    "--shadow-s": {
      "light": "0px 1px 2px #000000, 0px 3.4px 6.7px #000000, 0px 15px 30px #000000",
      "dark": null
    },
    "--shadow-l": {
      "light": "0px 1.8px 7.3px #000000, 0px 6.3px 24.7px #000000, 0px 30px 90px #000000",
      "dark": null
    },
    "--color-base-00": { "light": "#e6e5e5", "dark": null },
    "--color-base-20": { "light": "#c2c2c2", "dark": null },
    "--color-base-05": { "light": "#f6f4f4b7", "dark": null },
    "--bg-primary": { "light": "230, 229, 229", "dark": null },
    "--bg-secondary": { "light": "194, 194, 194", "dark": null },
    "--background-settings": { "light": "#ffffff", "dark": null },
    "--background-settings-alt": { "light": "#cbcaca", "dark": null },
    "--background-settings-2": { "light": "#b3b3b3", "dark": null },
    "--background-settings-3": { "light": "#e2e2e2", "dark": null },
    "--text-selection": {
      "light": "hsla(var(--interactive-accent-hsl), 0.35)",
      "dark": null
    },
    "--accent-h": { "light": "198", "dark": null },
    "--accent-s": { "light": "78%", "dark": null },
    "--accent-l": { "light": "64%", "dark": null },
    "--color-accent-faint": {
      "light": "hsl( var(--accent-h), calc(var(--accent-s) + 10%), calc(var(--accent-l) - 4%) )",
      "dark": null
    },
    "--color-accent-active": {
      "light": "hsla( var(--accent-h), calc(var(--accent-s) - 13%), calc(var(--accent-l) + 10%), 0.65 )",
      "dark": null
    },
    "--color-accent-active2": {
      "light": "hsla( var(--accent-h), calc(var(--accent-s) - 13%), calc(var(--accent-l) + 10%), 0.45 )",
      "dark": null
    },
    "--color-glow": {
      "light": "hsl( calc(var(--accent-h) + 1), calc(var(--accent-s) - 36%), calc(var(--accent-l) - 25%) )",
      "dark": null
    },
    "--color-accent-hsl": {
      "light": "var(--accent-h), var(--accent-s), var(--accent-l)",
      "dark": null
    },
    "--color-accent": {
      "light": "hsl( var(--accent-h), calc(var(--accent-s) - 20%), calc(var(--accent-l) - 10%) )",
      "dark": null
    },
    "--color-accent-1": {
      "light": "hsl( var(--accent-h), var(--accent-s), calc(var(--accent-l) - 3.8%)",
      "dark": null
    },
    "--canvas-color": { "light": null, "dark": "126, 126, 126" }
  },
  ".notice progress[value]::-webkit-progress-bar": {
    "background-color": { "light": "#262626", "dark": null },
    "box-shadow": { "light": "inset 0px 0px 0px 1px #363636", "dark": null }
  },
  ".callout.callout[data-callout=\"grid\"] .callout-content": {
    "border-bottom": "2px solid",
    "border-image-slice": "1",
    "border-image-source": "linear-gradient( to right, transparent, rgb(var(--callout-color)), transparent )",
    "border-top": "2px solid",
    "--callout-color": { "light": "0, 31, 63", "dark": "0, 116, 217" }
  },
  ".callout.callout[data-callout~=\"caption\"] > .callout-content": {
    "--callout-color": { "light": "102, 187, 106", "dark": "178, 223, 181" }
  },
  ".dropdown": {
    "background-image": {
      "light": null,
      "dark": "url(\"data:image/svg+xml, %3Csvg xmlns=%27http://www.w3.org/2000/svg%27 width=%2724%27 height=%2724%27 viewBox=%270 0 24 24%27 fill=%27none%27 stroke=%27%23FFF%27 opacity=%270.6%27 stroke-width=%272%27 stroke-linecap=%27round%27 stroke-linejoin=%27round%27 %3E%3Cpath d=%27m7 15 5 5 5-5%27/%3E%3Cpath d=%27m7 9 5-5 5 5%27/%3E%3C/svg%3E\")"
    }
  },
  ".pdf-container.mod-themed .popupWrapper > div": {
    "background-color": {
      "light": null,
      "dark": "var(--background-secondary-alt)"
    },
    "filter": {
      "light": null,
      "dark": "drop-shadow(0px 0px 1px var(--color-base-60)) drop-shadow(0px 1px 2px #000000) drop-shadow(0px 4px 6px #000000)"
    }
  },
  ".pdf-container.mod-themed .popupWrapper > div::after": {
    "background-color": {
      "light": null,
      "dark": "var(--background-secondary-alt)"
    }
  },
  ".pdf-container.mod-themed .popupContent:empty + .popupMeta": {
    "background-color": {
      "light": null,
      "dark": "var(--background-secondary-alt)"
    }
  },
  ".mermaid > svg": {
    "filter": {
      "light": null,
      "dark": "invert(100%) hue-rotate(180deg) saturate(1.25)"
    }
  },
  ".canvas-card-menu": {
    "backdrop-filter": {
      "light": null,
      "dark": "blur(5px) saturate(120%) !important"
    },
    "background-color": { "light": null, "dark": "var(--background-settings)" },
    "border-bottom": { "light": null, "dark": "1px solid" },
    "border-image-slice": { "light": null, "dark": "1" },
    "border-image-source": {
      "light": null,
      "dark": "linear-gradient( to right, transparent, var(--text-accent), transparent )"
    },
    "border-top": { "light": null, "dark": "1px solid" },
    "box-shadow": { "light": null, "dark": "none" }
  },
  ".canvas-card-menu .canvas-card-menu-button svg": {
    "fill": { "light": null, "dark": "transparent" }
  },
  ".is-mobile": {
    "--color-base-00": { "light": null, "dark": "#000" },
    "--color-base-10": { "light": null, "dark": "#111" },
    "--color-base-20": { "light": null, "dark": "#1e1e1e" },
    "--tag-background": {
      "light": null,
      "dark": "hsla(var(--interactive-accent-hsl), 0.2)"
    },
    "--search-result-background": {
      "light": null,
      "dark": "var(--background-secondary)"
    },
    "--background-modifier-form-field": {
      "light": null,
      "dark": "var(--background-modifier-border)"
    },
    "--background-modifier-cover": { "light": null, "dark": "#000000" },
    "--background-modifier-hover": {
      "light": null,
      "dark": "rgba(var(--mono-rgb-100), 0.15)"
    },
    "--settings-home-background": {
      "light": null,
      "dark": "var(--background-primary)"
    },
    "--mobile-sidebar-background": {
      "light": null,
      "dark": "var(--background-secondary)"
    },
    "--background-modifier-message": {
      "light": null,
      "dark": "var(--background-secondary)"
    }
  },
  ".is-tablet": {
    "--titlebar-background": {
      "light": null,
      "dark": "var(--background-primary)"
    },
    "--titlebar-background-focused": {
      "light": null,
      "dark": "var(--background-primary)"
    },
    "--interactive-normal": {
      "light": null,
      "dark": "var(--background-modifier-border)"
    },
    "--interactive-hover": {
      "light": null,
      "dark": "var(--background-modifier-border-hover)"
    },
    "--modal-background": {
      "light": null,
      "dark": "var(--background-secondary)"
    }
  },
  ".is-tablet .community-item": {
    "background-color": { "light": null, "dark": "var(--background-secondary)" }
  },
  ".is-tablet .vertical-tab-content": {
    "background-color": { "light": null, "dark": "var(--background-secondary)" }
  },
  ".workspace-drawer-inner": {
    "background-color": { "light": null, "dark": "var(--background-secondary)" }
  },
  ".is-mobile .prompt": {
    "background-color": {
      "light": null,
      "dark": "var(--background-secondary)"
    },
    "background": { "light": null, "dark": "var(--background-settings)" },
    "border-bottom": { "light": null, "dark": "2px solid" },
    "border-image-slice": { "light": null, "dark": "1" },
    "border-image-source": {
      "light": null,
      "dark": "linear-gradient( to right, transparent, var(--text-accent), transparent )"
    },
    "border-radius": { "light": null, "dark": "3px" },
    "border-top": { "light": null, "dark": "2px solid" }
  },
  ".is-mobile .prompt input.prompt-input": {
    "background-color": { "light": null, "dark": "var(--color-accent-active)" },
    "border-image-slice": { "light": null, "dark": "1" },
    "border-image-source": {
      "light": null,
      "dark": "linear-gradient( to right, transparent, var(--text-accent), transparent )"
    },
    "box-shadow": { "light": null, "dark": "none" },
    "background": { "light": null, "dark": "var(--background-settings-2)" },
    "border-radius": { "light": null, "dark": "5px" },
    "margin": { "light": null, "dark": "5px" },
    "padding": { "light": null, "dark": "10px" }
  },
  ".is-tablet .suggestion-container": {
    "background-color": { "light": null, "dark": "var(--background-secondary)" }
  },
  ".is-phone .modal.mod-settings .vertical-tab-nav-item:active": {
    "background-color": { "light": null, "dark": "var(--hover-bg2)" },
    "box-shadow": {
      "light": null,
      "dark": "inset 0em 0em 1em var(--hover-color)"
    },
    "color": { "light": null, "dark": "var(--text-on-accent)" },
    "transition": {
      "light": null,
      "dark": "background-color 0.2s, box-shadow 0.2s, animation 0.2s, border 1s !important"
    }
  },
  ".is-phone .modal.mod-settings .vertical-tab-nav-item": {
    "background-color": {
      "light": null,
      "dark": "var(--background-settings-alt)"
    },
    "border-bottom": { "light": null, "dark": "1px solid transparent" },
    "border-radius": { "light": null, "dark": "var(--radius-s)" },
    "color": { "light": null, "dark": "var(--text-muted)" },
    "margin": { "light": null, "dark": "5px" }
  }
}

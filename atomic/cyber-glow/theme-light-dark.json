      "light": "1",
      "dark": null
    },
    "border-image-source": {
      "light": "linear-gradient( to right, transparent, rgb(var(--callout-color)), transparent )",
      "dark": null
    },
    "border-bottom": { "light": "2px solid", "dark": null },
    "border-top": { "light": "2px solid", "dark": null },
    "} .theme-light .callout.callout[data-callout=\"grid\"] .callout-content { --callout-color": {
      "light": "0, 31, 63",
      "dark": null
    },
    "} .theme-dark .callout.callout[data-callout=\"grid\"] .callout-content { --callout-color": {
      "light": "0, 116, 217",
      "dark": null
    }
  },

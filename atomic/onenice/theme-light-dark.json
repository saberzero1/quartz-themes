{
  "body": {
    "--accent-h": { "light": "75", "dark": null },
    "--accent-l": { "light": "48%", "dark": null },
    "--accent-s": { "light": "26%", "dark": null },
    "--aqua": { "light": "#0184bc", "dark": null },
    "--background-accent": { "light": "#fff", "dark": null },
    "--background-modifier-border": { "light": "#a0a1a7", "dark": null },
    "--background-modifier-border-focus": {
      "light": "var(--interactive-hover)",
      "dark": null
    },
    "--background-modifier-border-hover": {
      "light": "var(--interactive-hover)",
      "dark": null
    },
    "--background-modifier-box-shadow": { "light": "#0000001a", "dark": null },
    "--background-modifier-cover": { "light": "#000000cc", "dark": null },
    "--background-modifier-error": { "light": "#e45649", "dark": null },
    "--background-modifier-error-hover": { "light": "#e45649", "dark": null },
    "--background-modifier-error-rgb": {
      "light": "230, 135, 135",
      "dark": null
    },
    "--background-modifier-form-field": { "light": "#a0a1a7", "dark": null },
    "--background-modifier-form-field-highlighted": {
      "light": "#fff",
      "dark": null
    },
    "--background-modifier-message": { "light": "#a0a1a7", "dark": null },
    "--background-modifier-success": { "light": "#50a14f", "dark": null },
    "--background-primary": { "light": "#e6e6e6", "dark": null },
    "--background-primary-alt": { "light": "#e1e1e1", "dark": null },
    "--background-secondary": { "light": "#dcdcdc", "dark": null },
    "--background-secondary-alt": { "light": "#dcdcdc", "dark": null },
    "--blue": { "light": "#4078f2", "dark": null },
    "--checkbox-color": { "light": "var(--green)", "dark": null },
    "--checkbox-color-hover": { "light": "var(--purple)", "dark": null },
    "--divider-color": { "light": "transparent", "dark": null },
    "--divider-color-hover": { "light": "#4fa6ed", "dark": null },
    "--drag-ghost-background": {
      "light": "var(--background-secondary-alt)",
      "dark": null
    },
    "--embed-padding": { "light": "0 0 0 var(--size-4-4)", "dark": null },
    "--gray-1": { "light": "#a0a1a7", "dark": null },
    "--gray-2": { "light": "#a0a1a7", "dark": null },
    "--green": { "light": "#50a14f", "dark": null },
    "--inline-title-weight": { "light": "var(--bold-weight)", "dark": null },
    "--interactive-accent-hover": {
      "light": "hsl( var(--accent-h), calc(var(--accent-s) - 10%), calc(var(--accent-l) - 4%) )",
      "dark": null
    },
    "--interactive-accent-rgb": { "light": "21, 146, 255", "dark": null },
    "--interactive-hover": { "light": "#e5556197", "dark": null },
    "--interactive-normal": { "light": "#fafafa", "dark": null },
    "--link-decoration": { "light": "none", "dark": null },
    "--link-external-decoration": { "light": "none", "dark": null },
    "--modal-border-color": { "light": "var(--green)", "dark": null },
    "--nav-item-background-active": { "light": "#4fa6ed", "dark": null },
    "--nav-item-color-active": { "light": "#ffffff", "dark": null },
    "--orange": { "light": "#c18401", "dark": null },
    "--panel-border-color": { "light": "#383a42", "dark": null },
    "--prompt-border-color": { "light": "var(--green)", "dark": null },
    "--purple": { "light": "#a626a4", "dark": null },
    "--red": { "light": "#e45649", "dark": null },
    "--ribbon-background": {
      "light": "var(--background-secondary-alt)",
      "dark": null
    },
    "--scrollbar-active-thumb-bg": { "light": "#4fa6ed", "dark": null },
    "--scrollbar-bg": { "light": "rgba(0, 0, 0, 0.05)", "dark": null },
    "--scrollbar-thumb-bg": { "light": "#686868", "dark": null },
    "--tab-outline-color": { "light": "transparent", "dark": null },
    "--tag-background": {
      "light": "var(--background-primary-alt)",
      "dark": null
    },
    "--tag-background-hover": { "light": "#8ebd6b", "dark": null },
    "--tag-color": { "light": "var(--purple)", "dark": null },
    "--tag-color-hover": { "light": "var(--purple)", "dark": null },
    "--tag-padding-x": { "light": "5px", "dark": null },
    "--tag-padding-y": { "light": "5px", "dark": null },
    "--tag-radius": { "light": "40px", "dark": null },
    "--text-accent": { "light": "#4078f2", "dark": null },
    "--text-accent-hover": { "light": "#e45649", "dark": null },
    "--text-error": { "light": "#e75545", "dark": null },
    "--text-error-hover": { "light": "#f86959", "dark": null },
    "--text-faint": { "light": "#101012", "dark": null },
    "--text-highlight-bg": { "light": "#8a9a5b", "dark": null },
    "--text-muted": { "light": "#2d2f30", "dark": null },
    "--text-normal": { "light": "#383a42", "dark": null },
    "--text-on-accent": { "light": "#f2f2f2", "dark": null },
    "--text-selection": { "light": "#00000050", "dark": null },
    "--text-title-h1": { "light": "#e45649", "dark": null },
    "--text-title-h2": { "light": "#50a14f", "dark": null },
    "--text-title-h3": { "light": "#c18401", "dark": null },
    "--text-title-h4": { "light": "#0184bc", "dark": null },
    "--text-title-h5": { "light": "#a626a4", "dark": null },
    "--text-title-h6": { "light": "#818387", "dark": null },
    "--yellow": { "light": "#986801", "dark": null },
    "--canvas-color": { "light": null, "dark": "126, 126, 126" }
  },
  ".notice progress[value]::-webkit-progress-bar": {
    "background-color": { "light": "#262626", "dark": null },
    "box-shadow": { "light": "inset 0px 0px 0px 1px #363636", "dark": null }
  },
  ":not(pre) > code": {
    "background": { "light": "var(--background-primary)", "dark": null },
    "border-radius": { "light": "10px", "dark": null },
    "box-shadow": {
      "light": "inset 0 0 0 1px var(--background-primary-alt)",
      "dark": null
    }
  },
  "pre": {
    "background": { "light": "var(--background-primary)", "dark": null },
    "border-radius": { "light": "10px", "dark": null },
    "box-shadow": {
      "light": "inset 0 0 0 1px var(--background-primary-alt)",
      "dark": null
    }
  },
  "button.mod-warning": {
    "--background-modifier-error": { "light": "#f23f3f", "dark": "#d42020" },
    "--background-modifier-error-hover": {
      "light": "#d72020",
      "dark": "#b01515"
    }
  },
  ".dropdown": { "background-image": { "light": null, "dark": "url(\"data" } },
  ".pdf-container.mod-themed .annotationLayer .textWidgetAnnotation input": {
    "filter": { "light": null, "dark": "invert(100%) hue-rotate(180deg)" }
  },
  ".pdf-container.mod-themed .annotationLayer .textWidgetAnnotation textarea": {
    "filter": { "light": null, "dark": "invert(100%) hue-rotate(180deg)" }
  },
  ".pdf-container.mod-themed .popupWrapper > div": {
    "background-color": {
      "light": null,
      "dark": "var(--background-secondary-alt)"
    },
    "filter": {
      "light": null,
      "dark": "drop-shadow(0px 0px 1px var(--color-base-60)) drop-shadow(0px 1px 2px rgba(0, 0, 0, 0.4)) drop-shadow(0px 4px 6px rgba(0, 0, 0, 0.4))"
    }
  },
  ".pdf-container.mod-themed .popupWrapper > div::after": {
    "background-color": {
      "light": null,
      "dark": "var(--background-secondary-alt)"
    }
  },
  ".pdf-container.mod-themed .popupContent:empty + .popupMeta": {
    "background-color": {
      "light": null,
      "dark": "var(--background-secondary-alt)"
    }
  },
  ".mermaid > svg": {
    "filter": {
      "light": null,
      "dark": "invert(100%) hue-rotate(180deg) saturate(1.25)"
    }
  },
  ".canvas-card-menu": {
    "background-color": { "light": null, "dark": "var(--background-secondary)" }
  },
  ".canvas-card-menu .canvas-card-menu-button svg": {
    "fill": { "light": null, "dark": "var(--background-secondary)" }
  },
  ".is-mobile": {
    "--background-modifier-cover": {
      "light": null,
      "dark": "rgba(0, 0, 0, 0.5)"
    },
    "--background-modifier-form-field": {
      "light": null,
      "dark": "var(--background-modifier-border)"
    },
    "--background-modifier-hover": {
      "light": null,
      "dark": "rgba(var(--mono-rgb-100), 0.15)"
    },
    "--background-modifier-message": {
      "light": null,
      "dark": "var(--background-secondary)"
    },
    "--color-base-00": { "light": null, "dark": "#000" },
    "--color-base-10": { "light": null, "dark": "#111" },
    "--color-base-20": { "light": null, "dark": "#1e1e1e" },
    "--mobile-sidebar-background": {
      "light": null,
      "dark": "var(--background-secondary)"
    },
    "--search-result-background": {
      "light": null,
      "dark": "var(--background-secondary)"
    },
    "--settings-home-background": {
      "light": null,
      "dark": "var(--background-primary)"
    },
    "--tag-background": {
      "light": null,
      "dark": "hsla(var(--interactive-accent-hsl), 0.2)"
    }
  },
  ".is-tablet": {
    "--interactive-hover": {
      "light": null,
      "dark": "var(--background-modifier-border-hover)"
    },
    "--interactive-normal": {
      "light": null,
      "dark": "var(--background-modifier-border)"
    },
    "--modal-background": {
      "light": null,
      "dark": "var(--background-secondary)"
    },
    "--titlebar-background": {
      "light": null,
      "dark": "var(--background-primary)"
    },
    "--titlebar-background-focused": {
      "light": null,
      "dark": "var(--background-primary)"
    }
  },
  ".is-tablet .community-item": {
    "background-color": { "light": null, "dark": "var(--background-secondary)" }
  },
  ".is-tablet .vertical-tab-content": {
    "background-color": { "light": null, "dark": "var(--background-secondary)" }
  },
  ".workspace-drawer-inner": {
    "background-color": { "light": null, "dark": "var(--background-secondary)" }
  },
  ".is-mobile .prompt": {
    "background-color": { "light": null, "dark": "var(--background-secondary)" }
  },
  ".is-mobile .prompt input.prompt-input": {
    "background-color": { "light": null, "dark": "var(--background-secondary)" }
  },
  ".is-tablet .suggestion-container": {
    "background-color": { "light": null, "dark": "var(--background-secondary)" }
  },
  ".dropdown:hover": {
    "background-color": {
      "light": null,
      "dark": "var(--background-modifier-form-field)"
    }
  }
}

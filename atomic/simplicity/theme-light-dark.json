  ") .workspace-tab-header-container": {
    "background-color": { "light": "var(--background-secondary)", "dark": null }
  },
  ".cm-s-obsidian span.cm-hmd-internal-link:hover": {
    "background-color": {
      "light": "hsla(var(--accent-h), var(--accent-s), var(--accent-l), 0.08)",
      "dark": "hsla(var(--accent-h), var(--accent-s), var(--accent-l), 0.3)"
    },
    "transition": "0.2s"
  },
  "a.internal-link:hover": {
    "background-color": {
      "light": "hsla(var(--accent-h), var(--accent-s), var(--accent-l), 0.08)",
      "dark": "hsla(var(--accent-h), var(--accent-s), var(--accent-l), 0.3)"
    },
    "transition": "0.2s"
  },

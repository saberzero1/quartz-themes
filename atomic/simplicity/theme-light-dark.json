      "light": null,
      "dark": "hsla(var(--accent-h), var(--accent-s), var(--accent-l), 0.06)"
    }
  },
  ".theme-rounded:not(.theme-black) .workspace-tab-header-container": {
    "background-color": {
      "light": null,
      "dark": "hsla(var(--accent-h), var(--accent-s), var(--accent-l), 0.06)"
    }
  },
  ".theme-rounded:not(.theme-black) .workspace-ribbon.mod-left:before": {
    "background-color": {
      "light": null,
      "dark": "hsla(var(--accent-h), var(--accent-s), var(--accent-l), 0.05)"
    }
  },
  ".theme-rounded.theme-black .workspace-ribbon.mod-left:before": {
    "background-color": {
      "light": null,
      "dark": "hsla(var(--accent-h), var(--accent-s), var(--accent-l), 0.05)"
    }
  },
  ".theme-rounded:not(.theme-black) .workspace-ribbon.mod-left": {
    "background-color": {
      "light": null,
      "dark": "hsla(var(--accent-h), var(--accent-s), var(--accent-l), 0.06)"
    }
  },
  ".theme-rounded.theme-black .workspace-tab-header.is-active": {
    "background-color": { "light": null, "dark": "transparent" },
    "box-shadow": { "light": null, "dark": "none" }
  },
  ".theme-rounded.theme-black .workspace-tab-header": {
    "background-color": { "light": null, "dark": "transparent" },
    "box-shadow": { "light": null, "dark": "none" }
  },
  ".theme-rounded .workspace-leaf": {
    "border": {
      "light": null,
      "dark": "1px solid hsla(var(--accent-h), var(--accent-s), var(--accent-l), 0.1)"
    },
    "border-radius": { "light": null, "dark": "25px" }
  },
  ".theme-rounded.theme-black .workspace-split": {
    "background-color": {
      "light": null,
      "dark": "hsla(var(--accent-h), var(--accent-s), var(--accent-l), 0.05)"
    }
  },
  ".theme-rounded.theme-blac": {
    ".workspace-tabs": {
      "light": null,
      "dark": "not(.mod-stacked).workspace-tab-header-container {background-color:transparent"
    }
  },
  ".theme-black:not(.theme-rounded) .workspace-ribbon.mod-left": {
    "background-color": { "light": null, "dark": "#111" }
  },
  ".theme-black": {
    "--nav-item-background-hover": {
      "light": null,
      "dark": "hsla(var(--accent-h), var(--accent-s), var(--accent-l), 0.03)"
    }
  }

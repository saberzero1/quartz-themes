    "background-color": {
      "light": "hsla(var(--accent-h), var(--accent-s), var(--accent-l), 0.08)",
      "dark": "hsla(var(--accent-h), var(--accent-s), var(--accent-l), 0.3)"
    },
    "transition": "0.2s"
  },
  ".external-link:hover": {
    "background-color": {
      "light": "hsla(var(--accent-h), var(--accent-s), var(--accent-l), 0.08)",
      "dark": "hsla(var(--accent-h), var(--accent-s), var(--accent-l), 0.3)"
    },
    "transition": "0.2s"
  },

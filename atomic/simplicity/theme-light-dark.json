    "border-radius": { "light": "25px", "dark": null }
  },
  ".theme-rounded .workspace-split": {
    "background-color": {
      "light": "hsla(var(--accent-h), var(--accent-s), var(--accent-l), 0.06)",
      "dark": null
    }
  },
  "body.nav-1 .tree-item-self.is-active": {
    "background-color": {
      "light": "hsla(var(--accent-h), var(--accent-s), var(--accent-s), 0.1)",
      "dark": "hsla(var(--accent-h), var(--accent-s), var(--accent-s), 0.04)"
    },
    "border-radius": "0px",
    "border-right": "5px solid var(--color-accent)",
    "color": "var(--color-accent)",
    "font-weight": "400"
  },
  "body.nav-1 .tree-item-self:hover": {
    "background-color": {
      "light": "hsla(var(--accent-h), var(--accent-s), var(--accent-s), 0.1)",
      "dark": "hsla(var(--accent-h), var(--accent-s), var(--accent-s), 0.04)"
    },
    "border-radius": "0px",
    "color": "var(--color-accent)",
    "font-weight": "400"

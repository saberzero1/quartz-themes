    "--background-primary": { "light": null, "dark": "#111" },
    "--background-secondary": { "light": null, "dark": "#111" }
  },
  ":not(.theme-black": {
    "background-color": {
      "light": null,
      "dark": "hsl(var(--accent-h), var(--accent-s), calc(var(--accent-l) * 0.25))"
    }
  },
  ".is-hidden-frameless:not": {
    ".is-fullscreen, .theme-black, .theme-rounded).workspace-tabs.mod-top-left-space.workspace-tab-header-container": {
      "light": null,
      "dark": "before {background-color:hsl(var(--accent-h), var(--accent-s), calc(var(--accent-l) * 0.25))"
    }
  },
  ".theme-rounded:not(.theme-black)": {
    "--background-primary": {
      "light": null,
      "dark": "hsla(var(--accent-h), var(--accent-s), var(--accent-l), 0.01)"
    }
  },

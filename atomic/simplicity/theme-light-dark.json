{
  "body": {
    "--text-highlight-bg": {
      "light": "hsla( var(--accent-h), var(--accent-s), var(--accent-l), 0.1 )",
      "dark": null
    }
  },
  "body:not(.theme-rounded)": {
    "--background-secondary": {
      "light": "hsla( var(--accent-h), var(--accent-s), var(--accent-l), 0.06 ) !important",
      "dark": null
    }
  },
  ":not(.theme-rounded)": {
    "--tab-container-background": {
      "light": "var(--color-base-00)",
      "dark": null
    }
  },
  ":not(.theme-rounded) .mod-right .workspace-tab-header-container": {
    "background-color": {
      "light": "var(--color-base-00) !important",
      "dark": null
    }
  },
  ".theme-rounded": {
    ".theme-rounded.theme-dark { --background-primary": {
      "light": "var(--color-base-00) !important",
      "dark": null
    },
    "--background-secondary": {
      "light": "var(--color-base-00) !important",
      "dark": null
    }
  },
  ".theme-rounded .workspace-ribbon.mod-left": {
    ".theme-dark.theme-rounded .workspace-ribbon.mod-left { border-right": {
      "light": "none !important",
      "dark": null
    },
    "background-color": {
      "light": "hsla( var(--accent-h), var(--accent-s), var(--accent-l), 0.05 ) !important",
      "dark": null
    }
  },
  ".theme-rounded .workspace-ribbon.mod-left:before": {
    ".theme-dark.theme-rounded": {
      "light": "not(.theme-black) .workspace-ribbon.mod-left:before, .theme-dark.theme-rounded.theme-black .workspace-ribbon.mod-left:before { background-color:hsla( var(--accent-h), var(--accent-s), var(--accent-l), 0.05 ) !important",
      "dark": null
    }
  },
  ".theme-rounded .workspace-tabs:not(.mod-stacked) .workspace-leaf": {
    "border": {
      "light": "1px solid hsla(var(--accent-h), var(--accent-s), var(--accent-l), 0.22) !important",
      "dark": null
    },
    "border-radius": { "light": "25px", "dark": null }
  },
  ".theme-rounded .workspace-split": {
    "background-color": {
      "light": "hsla( var(--accent-h), var(--accent-s), var(--accent-l), 0.06 )",
      "dark": null
    }
  },
  "body.nav-1 .tree-item-self.is-active": {
    "body.theme-dark.nav-1 .tree-item-self.is-active { border-right": {
      "light": "5px solid var(--color-accent) !important",
      "dark": null
    },
    "body.theme-dark.nav-1 .tree-item-self": {
      "light": null,
      "dark": "hover { background-color:hsla( var(--accent-h), var(--accent-s), var(--accent-s), 0.04 ) !important"
    },
    "border-radius": { "light": null, "dark": "0px !important" },
    "color": { "light": null, "dark": "var(--color-accent) !important" },
    "font-weight": { "light": null, "dark": "400 !important" }
  },
  ".cm-s-obsidian span.cm-hmd-internal-link:hover": {
    ".theme-light a.internal-link": {
      "light": "hover, .theme-light .cm-link:hover, .theme-light .external-link:hover { transition:0.2s",
      "dark": null
    },
    "background-color": {
      "light": "hsla( var(--accent-h), var(--accent-s), var(--accent-l), 0.08 )",
      "dark": "hsla( var(--accent-h), var(--accent-s), var(--accent-l), 0.3 )"
    },
    ".theme-dark a.internal-link": {
      "light": null,
      "dark": "hover, .theme-dark .cm-link:hover, .theme-dark .external-link:hover { transition:0.2s"
    }
  },
  ".search-result-file-matched-text": {
    "background-color": {
      "light": "hsla( var(--accent-h), var(--accent-s), var(--accent-l), 0.08 ) !important",
      "dark": "hsla( var(--accent-h), var(--accent-s), var(--accent-l), 0.3 ) !important"
    }
  },
  ".mermaid > svg": {
    "filter": {
      "light": null,
      "dark": "invert(100%) hue-rotate(180deg) saturate(1.25)"
    },
    "} .markdown-source-view.mod-cm6 .cm-hmd-codeblock { --indentation-guide-editing-indent": {
      "light": null,
      "dark": "0"
    },
    "} .markdown-source-view.mod-cm6 .cm-preview-code-block pre { margin": {
      "light": null,
      "dark": "0"
    },
    "} .markdown-source-view.mod-cm6 .code-block-flair { position": {
      "light": null,
      "dark": "absolute"
    },
    "right": { "light": null, "dark": "6px" },
    "top": { "light": null, "dark": "6px" },
    "z-index": { "light": null, "dark": "1" },
    "display": { "light": null, "dark": "inline-block" },
    "padding": { "light": null, "dark": "var(--size-4-1) var(--size-4-2)" },
    "border-radius": { "light": null, "dark": "var(--code-radius)" },
    "font-family": { "light": null, "dark": "var(--font-interface)" },
    "font-size": { "light": null, "dark": "var(--font-ui-smaller)" },
    "color": { "light": null, "dark": "var(--text-muted)" },
    "cursor": { "light": null, "dark": "var(--cursor)" }
  },
  "body:not(.theme-black)": {
    "--background-primary": {
      "light": null,
      "dark": "hsl( var(--accent-h), var(--accent-s), calc(var(--accent-l) * 0.25) )"
    },
    "--background-secondary": {
      "light": null,
      "dark": "hsla( var(--accent-h), var(--accent-s), var(--accent-l), 0.06 ) !important"
    },
    "--nav-item-color": { "light": null, "dark": "#fff" }
  },
  "body.theme-black": {
    "--background-primary": { "light": null, "dark": "#111 !important" },
    "--background-secondary": { "light": null, "dark": "#111 !important" }
  },
  ":not(.theme-black):not(.theme-rounded) .workspace-ribbon.mod-left": {
    ".theme-dark": {
      "light": null,
      "dark": "not(.theme-black):not(.theme-rounded) .workspace-tabs:not(.mod-stacked) .workspace-tab-header.is-active, .theme-dark:not(.theme-black):not(.theme-rounded) .workspace-tab-header-container, .theme-dark.is-hidden-frameless:not(.is-fullscreen):not(.theme-black):not( .theme-rounded ) .workspace-tabs.mod-top-left-space .workspace-tab-header-container:before, .theme-dark:not(.theme-black):not(.theme-rounded) .workspace-tab-header-inner { background-color:hsl( var(--accent-h), var(--accent-s), calc(var(--accent-l) * 0.25) ) !important"
    }
  },
  ".theme-rounded:not(.theme-black)": {
    "--background-primary": {
      "light": null,
      "dark": "hsla( var(--accent-h), var(--accent-s), var(--accent-l), 0.01 ) !important"
    }
  },
  ".theme-rounded:not(.theme-black) .workspace-tab-header-container": {
    ".theme-rounded": {
      "light": null,
      "dark": "not(.theme-black) .workspace-ribbon.mod-left, .theme-dark.theme-rounded:not(.theme-black) .mod-root .workspace-tabs .workspace-leaf { background-color:hsla( var(--accent-h), var(--accent-s), var(--accent-l), 0.06 ) !important"
    }
  },
  ".theme-rounded:not(.theme-black) .workspace-ribbon.mod-left": {
    "background-color": {
      "light": null,
      "dark": "hsla( var(--accent-h), var(--accent-s), var(--accent-l), 0.06 ) !important"
    }
  },
  ".theme-rounded.theme-black .workspace-tab-header.is-active": {
    ".theme-rounded.theme-dark.theme-black .workspace-tab-header { background-color": {
      "light": null,
      "dark": "transparent !important"
    },
    "box-shadow": { "light": null, "dark": "none !important" }
  },
  ".theme-rounded .workspace-leaf": {
    "border": {
      "light": null,
      "dark": "1px solid hsla(var(--accent-h), var(--accent-s), var(--accent-l), 0.1) !important"
    },
    "border-radius": { "light": null, "dark": "25px" }
  },
  ".theme-rounded.theme-black .workspace-split": {
    "background-color": {
      "light": null,
      "dark": "hsla( var(--accent-h), var(--accent-s), var(--accent-l), 0.05 ) !important"
    }
  },
  ".theme-rounded.theme-blac": {
    ".workspace-tabs": {
      "light": null,
      "dark": "not(.mod-stacked) .workspace-tab-header-container { background-color:transparent !important"
    }
  },
  ".theme-black:not(.theme-rounded) .workspace-ribbon.mod-left": {
    "background-color": { "light": null, "dark": "#111 !important" }
  },
  ".theme-black": {
    "--nav-item-background-hover": {
      "light": null,
      "dark": "hsla( var(--accent-h), var(--accent-s), var(--accent-l), 0.03 ) !important"
    }
  }
}

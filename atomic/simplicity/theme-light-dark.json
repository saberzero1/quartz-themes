{
  "body": {
    "--text-highlight-bg": {
      "light": "hsla(var(--accent-h), var(--accent-s), var(--accent-l), 0.1)",
      "dark": "hsla(var(--accent-h), var(--accent-s), var(--accent-l), 0.4)"
    },
    "--canvas-color": { "light": null, "dark": "126, 126, 126" }
  },
  ".notice progress[value]::-webkit-progress-bar": {
    "background-color": { "light": "#262626", "dark": null },
    "box-shadow": { "light": "inset 0px 0px 0px 1px #363636", "dark": null }
  },
  ":not(.theme-rounded)": {
    "--tab-container-background": {
      "light": "var(--color-base-00)",
      "dark": null
    }
  },
  ":not(.theme-rounded) .mod-right .workspace-tab-header-container": {
    "background-color": { "light": "var(--color-base-00)", "dark": null }
  },
  ".theme-rounded": {
    "--background-primary": "var(--color-base-00)",
    "--background-secondary": "var(--color-base-00)"
  },
  ".theme-rounded .workspace-tab-header-container": {
    "background-color": { "light": "transparent", "dark": null },
    "box-shadow": { "light": "none", "dark": null }
  },
  ".theme-rounded .workspace-ribbon.mod-left": {
    "background-color": "hsla(var(--accent-h), var(--accent-s), var(--accent-l), 0.05)",
    "border-right": "none",
    "box-shadow": { "light": "none", "dark": null }
  },
  ".theme-rounded .workspace-ribbon.mod-left:before": {
    "background-color": {
      "light": "hsla(var(--accent-h), var(--accent-s), var(--accent-l), 0.05)",
      "dark": null

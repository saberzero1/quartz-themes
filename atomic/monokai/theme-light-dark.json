{
  ":not(pre) > code[class*=\"language-\"]": {
    ".theme-light pre[class*=\"language-\"] { background": {
      "light": "var(--background-primary)",
      "dark": null
    },
    "box-shadow": {
      "light": "inset 0 0 0 1px var(--background-primary-alt)",
      "dark": null
    },
    "border-radius": { "light": "4px", "dark": null },
    ".theme-dark pre[class*=\"language-\"], .theme-light": {
      "light": null,
      "dark": "not(pre) > code[class*=\"language-\"], .theme-light pre[class*=\"language-\"] { background:var(--code-background)"
    }
  },
  ".token.operator": {
    ".theme-light .token.entity, .theme-light .token.url, .theme-light .language-css .token.string, .theme-light .style .token.string { background": {
      "light": "transparent",
      "dark": null
    }
  },
  ".horizontal-tab-nav-item.is-active": {
    ".vertical-tab-nav-item.is-active { background-color": {
      "light": "var(--interactive-accent)",
      "dark": null
    },
    "color": { "light": "var(--base-1)", "dark": null }
  },
  ".mermaid > svg": {
    "filter": {
      "light": null,
      "dark": "invert(100%) hue-rotate(180deg) saturate(1.25)"
    },
    "} .markdown-source-view.mod-cm6 .cm-hmd-codeblock { --indentation-guide-editing-indent": {
      "light": null,
      "dark": "0"
    },
    "} .markdown-source-view.mod-cm6 .cm-preview-code-block pre { margin": {
      "light": null,
      "dark": "0"
    },
    "} .markdown-source-view.mod-cm6 .code-block-flair { position": {
      "light": null,
      "dark": "absolute"
    },
    "right": { "light": null, "dark": "6px" },
    "top": { "light": null, "dark": "6px" },
    "z-index": { "light": null, "dark": "1" },
    "display": { "light": null, "dark": "inline-block" },
    "padding": { "light": null, "dark": "var(--size-4-1) var(--size-4-2)" },
    "border-radius": { "light": null, "dark": "var(--code-radius)" },
    "font-family": { "light": null, "dark": "var(--font-interface)" },
    "font-size": { "light": null, "dark": "var(--font-ui-smaller)" },
    "color": { "light": null, "dark": "var(--text-muted)" },
    "cursor": { "light": null, "dark": "var(--cursor)" }
  }
}

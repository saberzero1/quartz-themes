{
  "body": {
    "--bold-color": "hsl(var(--accent-h), var(--accent-s), var(--accent-l))",
    "--color-background-1": { "light": "#f3f4f6", "dark": "#525568" },
    "--color-background-2": { "light": "#e8e8ee", "dark": "#44475a" },
    "--color-background-3": { "light": "#dcdde5", "dark": "#353747" },
    "--color-background-4": { "light": "#d0d2dc", "dark": "#282a36" },
    "--color-background-5": { "light": "#c4c7d4", "dark": "#21222c" },
    "--color-base-00": {
      "light": "var(--color-background-3)",
      "dark": "var(--color-background-5)"
    },
    "--color-base-10": {
      "light": "var(--color-background-2)",
      "dark": "var(--color-background-3)"
    },
    "--color-base-100": {
      "light": "var(--color-foreground-3)",
      "dark": "var(--color-foreground-2)"
    },
    "--color-base-20": {
      "light": "var(--color-background-3)",
      "dark": "var(--color-background-5)"
    },
    "--color-base-25": { "light": "blue", "dark": "var(--color-background-5)" },
    "--color-base-30": {
      "light": "var(--color-background-3)",
      "dark": "var(--color-background-5)"
    },
    "--color-base-35": {
      "light": "var(--color-background-5)",
      "dark": "var(--color-background-2)"
    },
    "--color-base-40": {
      "light": "var(--color-background-5)",
      "dark": "var(--color-background-2)"
    },
    "--color-base-50": {
      "light": "var(--color-foreground-1)",
      "dark": "var(--color-background-1)"
    },
    "--color-base-60": { "light": "green", "dark": "blue" },
    "--color-base-70": {
      "light": "var(--color-foreground-1)",
      "dark": "var(--color-foreground-3)"
    },
    "--color-base-80": { "light": "blue", "dark": null },
    "--color-base-90": { "light": "blue", "dark": null },
    "--color-foreground-1": { "light": "#44475a", "dark": "#f8f8f2" },
    "--color-foreground-2": { "light": "#353747", "dark": "#d9dadf" },
    "--color-foreground-3": { "light": "#282a36", "dark": "#b9bbcb" },
    "--color-shadow-1": { "light": "#dcdde5", "dark": "#353747" },
    "--color-shadow-2": { "light": "#c4c7d4", "dark": "#21222c" },
    "--color-shadow-3": { "light": "#b2b5c2", "dark": "#1a1c24" },
    "--hr-color": {
      "light": "var(--color-background-3)",
      "dark": "var(--color-background-1)"
    },
    "--nav-item-background-active": "var(--color-accent)",
    "--ribbon-background": "blue",
    "--status-bar-background": "var(--color-background-3)",
    "--table-header-background": "var(--color-background-3)",
    "--text-selection": "hsla(var(--accent-h), var(--accent-s), 70%, 0.25)",
    "--titlebar-background": {
      "light": "var(--color-background-3)",
      "dark": "var(--color-background-5)"
    },
    "background-color": {
      "light": "var(--color-background-3)",
      "dark": "var(--color-background-5)"
    },
    "color": "var(--color-foreground-1)"
  },
  ".notice progress[value]::-webkit-progress-bar": {
    "background-color": { "light": "#262626", "dark": null },
    "box-shadow": { "light": "inset 0px 0px 0px 1px #363636", "dark": null }
  },
  "body.is-mobile": {
    "--color-base-00": "var(--color-background-5)",
    "--color-base-10": "var(--color-background-4)",
    "--color-base-20": "var(--color-background-3)"
  },
  ".workspace-split.mod-root .view-content": {
    "background-color": {
      "light": "var(--color-background-1)",
      "dark": "var(--color-background-4)"
    },
    "box-shadow": { "light": null, "dark": "0 0 10px var(--color-shadow-3)" }
  },
  ".nav-header": {
    "background-color": {
      "light": "var(--color-background-1)",
      "dark": "var(--color-background-4)"
    },
    "box-shadow": { "light": null, "dark": "0 0 10px var(--color-shadow-3)" }
  },
  ".nav-files-container": {
    "background-color": {
      "light": "var(--color-background-1)",
      "dark": "var(--color-background-4)"
    },
    "box-shadow": "0 0 10px var(--color-shadow-3)"
  },
  "#calendar-container": {
    "background-color": {
      "light": "var(--color-background-1)",
      "dark": "var(--color-background-4)"
    },
    "box-shadow": "0 0 10px var(--color-shadow-3)"
  },
  ".markdown-rendered thead tr": { "background": "var(--color-background-3)" },
  "textarea": {
    "background-color": {
      "light": "var(--color-background-1)",
      "dark": "var(--color-background-4)"
    }
  },
  "input.metadata-input-text": {
    "background-color": {
      "light": "var(--color-background-1)",
      "dark": "var(--color-background-4)"
    }
  },
  "input[type=\"date\"]": {
    "background-color": {
      "light": "var(--color-background-1)",
      "dark": "var(--color-background-4)"
    }
  },
  "input[type=\"datetime-local\"]": {
    "background-color": {
      "light": "var(--color-background-1)",
      "dark": "var(--color-background-4)"
    }
  },
  "input[type=\"text\"]": {
    "background-color": {
      "light": "var(--color-background-1)",
      "dark": "var(--color-background-4)"
    }
  },
  "input[type=\"search\"]": {
    "background-color": {
      "light": "var(--color-background-1)",
      "dark": "var(--color-background-4)"
    }
  },
  "input[type=\"email\"]": {
    "background-color": {
      "light": "var(--color-background-1)",
      "dark": "var(--color-background-4)"
    }
  },
  "input[type=\"password\"]": {
    "background-color": {
      "light": "var(--color-background-1)",
      "dark": "var(--color-background-4)"
    }
  },
  "input[type=\"number\"]": {
    "background-color": {
      "light": "var(--color-background-1)",
      "dark": "var(--color-background-4)"
    }
  },
  ".markdown-source-view .cm-string.cm-url": {
    "background-color": {
      "light": "var(--color-background-1)",
      "dark": "var(--color-background-4)"
    },
    "color": { "light": "var(--color-blue-2)", "dark": "var(--color-cyan-2)" }
  },
  ".dropdown": { "background-image": { "light": null, "dark": "url(\"data" } },
  ".pdf-container.mod-themed .annotationLayer .textWidgetAnnotation input": {
    "filter": { "light": null, "dark": "invert(100%) hue-rotate(180deg)" }
  },
  ".pdf-container.mod-themed .annotationLayer .textWidgetAnnotation textarea": {
    "filter": { "light": null, "dark": "invert(100%) hue-rotate(180deg)" }
  },
  ".pdf-container.mod-themed .popupWrapper > div": {
    "background-color": {
      "light": null,
      "dark": "var(--background-secondary-alt)"
    },
    "filter": {
      "light": null,
      "dark": "drop-shadow(0px 0px 1px var(--color-base-60)) drop-shadow(0px 1px 2px rgba(0, 0, 0, 0.4)) drop-shadow(0px 4px 6px rgba(0, 0, 0, 0.4))"
    }
  },
  ".pdf-container.mod-themed .popupWrapper > div::after": {
    "background-color": {
      "light": null,
      "dark": "var(--background-secondary-alt)"
    }
  },
  ".pdf-container.mod-themed .popupContent:empty + .popupMeta": {
    "background-color": {
      "light": null,
      "dark": "var(--background-secondary-alt)"
    }
  },
  ".mermaid > svg": {
    "filter": {
      "light": null,
      "dark": "invert(100%) hue-rotate(180deg) saturate(1.25)"
    }
  },
  ".canvas-card-menu": {
    "background-color": { "light": null, "dark": "var(--background-secondary)" }
  },
  ".canvas-card-menu .canvas-card-menu-button svg": {
    "fill": { "light": null, "dark": "var(--background-secondary)" }
  },
  ".is-mobile": {
    "--background-modifier-cover": {
      "light": null,
      "dark": "rgba(0, 0, 0, 0.5)"
    },
    "--background-modifier-form-field": {
      "light": null,
      "dark": "var(--background-modifier-border)"
    },
    "--background-modifier-hover": {
      "light": null,
      "dark": "rgba(var(--mono-rgb-100), 0.15)"
    },
    "--background-modifier-message": {
      "light": null,
      "dark": "var(--background-secondary)"
    },
    "--color-base-00": { "light": null, "dark": "#000" },
    "--color-base-10": { "light": null, "dark": "#111" },
    "--color-base-20": { "light": null, "dark": "#1e1e1e" },
    "--mobile-sidebar-background": {
      "light": null,
      "dark": "var(--background-secondary)"
    },
    "--search-result-background": {
      "light": null,
      "dark": "var(--background-secondary)"
    },
    "--settings-home-background": {
      "light": null,
      "dark": "var(--background-primary)"
    },
    "--tag-background": {
      "light": null,
      "dark": "hsla(var(--interactive-accent-hsl), 0.2)"
    }
  },
  ".is-tablet": {
    "--interactive-hover": {
      "light": null,
      "dark": "var(--background-modifier-border-hover)"
    },
    "--interactive-normal": {
      "light": null,
      "dark": "var(--background-modifier-border)"
    },
    "--modal-background": {
      "light": null,
      "dark": "var(--background-secondary)"
    },
    "--titlebar-background": {
      "light": null,
      "dark": "var(--background-primary)"
    },
    "--titlebar-background-focused": {
      "light": null,
      "dark": "var(--background-primary)"
    }
  },
  ".is-tablet .community-item": {
    "background-color": { "light": null, "dark": "var(--background-secondary)" }
  },
  ".is-tablet .vertical-tab-content": {
    "background-color": { "light": null, "dark": "var(--background-secondary)" }
  },
  ".workspace-drawer-inner": {
    "background-color": { "light": null, "dark": "var(--background-secondary)" }
  },
  ".is-mobile .prompt": {
    "background-color": { "light": null, "dark": "var(--background-secondary)" }
  },
  ".is-mobile .prompt input.prompt-input": {
    "background-color": { "light": null, "dark": "var(--background-secondary)" }
  },
  ".is-tablet .suggestion-container": {
    "background-color": { "light": null, "dark": "var(--background-secondary)" }
  }
}

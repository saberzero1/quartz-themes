{
  "body": {
    "--color-background-1": { "light": "#f3f4f6", "dark": null },
    "--color-background-2": { "light": "#e8e8ee", "dark": null },
    "--color-background-3": { "light": "#dcdde5", "dark": null },
    "--color-background-4": { "light": "#d0d2dc", "dark": null }
  },
  "body.is-mobile": {
    "--color-base-00": "var(--color-background-5)",
    "--color-base-10": "var(--color-background-4)",
    "--color-base-20": "var(--color-background-3)"
  },
  ".workspace-split.mod-root .view-content": {
    "background-color": {
      "light": "var(--color-background-1)",
      "dark": "var(--color-background-4)"
    },
    "box-shadow": { "light": null, "dark": "0 0 10px var(--color-shadow-3)" }
  },
  ".nav-header": {
    "background-color": {
      "light": "var(--color-background-1)",
      "dark": "var(--color-background-4)"
    },
    "box-shadow": { "light": null, "dark": "0 0 10px var(--color-shadow-3)" }
  },
  ".nav-files-container": {
    "background-color": {
      "light": "var(--color-background-1)",
      "dark": "var(--color-background-4)"
    },
    "box-shadow": "0 0 10px var(--color-shadow-3)"
  },
  "#calendar-container": {
    "box-shadow": "0 0 10px var(--color-shadow-3)",
    "background-color": {
      "light": "var(--color-background-1)",
      "dark": "var(--color-background-4)"
    }
  },
  ".markdown-rendered thead tr": { "background": "var(--color-background-3)" },
  "textarea": {
    ".theme-light input.metadata-input-text, .theme-light input[type=\"date\"], .theme-light input[type=\"datetime-local\"], .theme-light input[type=\"text\"], .theme-light input[type=\"search\"], .theme-light input[type=\"email\"], .theme-light input[type=\"password\"], .theme-light input[type=\"number\"] { background-color": {
      "light": "var(--color-background-1)",
      "dark": null
    },
    ".theme-dark input.metadata-input-text, .theme-dark input[type=\"date\"], .theme-dark input[type=\"datetime-local\"], .theme-dark input[type=\"text\"], .theme-dark input[type=\"search\"], .theme-dark input[type=\"email\"], .theme-dark input[type=\"password\"], .theme-dark input[type=\"number\"] { background-color": {
      "light": null,
      "dark": "var(--color-background-4)"
    }
  },
  ".markdown-source-view .cm-string.cm-url": {
    "color": { "light": "#54b3ff", "dark": "#8be9fd" },
    "background-color": {
      "light": "var(--color-background-1)",
      "dark": "var(--color-background-4)"
    }
  },
  ".mermaid > svg": {
    "filter": {
      "light": null,
      "dark": "invert(100%) hue-rotate(180deg) saturate(1.25)"
    },
    "} .markdown-source-view.mod-cm6 .cm-hmd-codeblock { --indentation-guide-editing-indent": {
      "light": null,
      "dark": "0"
    },
    "} .markdown-source-view.mod-cm6 .cm-preview-code-block pre { margin": {
      "light": null,
      "dark": "0"
    },
    "} .markdown-source-view.mod-cm6 .code-block-flair { position": {
      "light": null,
      "dark": "absolute"
    },
    "right": { "light": null, "dark": "6px" },
    "top": { "light": null, "dark": "6px" },
    "z-index": { "light": null, "dark": "1" },
    "display": { "light": null, "dark": "inline-block" },
    "padding": { "light": null, "dark": "var(--size-4-1) var(--size-4-2)" },
    "border-radius": { "light": null, "dark": "var(--code-radius)" },
    "font-family": { "light": null, "dark": "var(--font-interface)" },
    "font-size": { "light": null, "dark": "var(--font-ui-smaller)" },
    "color": { "light": null, "dark": "var(--text-muted)" },
    "cursor": { "light": null, "dark": "var(--cursor)" }
  }
}

{
  "body": {
    "--accent-h": { "light": "145", "dark": null },
    "--accent-l": { "light": "17%", "dark": null },
    "--accent-s": { "light": "100%", "dark": null },
    "--background-modifier-border": {
      "light": "var(--color-base-30)",
      "dark": null
    },
    "--background-modifier-border-focus": {
      "light": "var(--color-base-40)",
      "dark": null
    },
    "--background-modifier-border-hover": {
      "light": "var(--color-base-30)",
      "dark": null
    },
    "--background-modifier-form-field": {
      "light": "var(--color-base-30)",
      "dark": null
    },
    "--background-primary": { "light": "var(--color-base-20)", "dark": null },
    "--background-primary-alt": {
      "light": "var(--color-base-30)",
      "dark": null
    },
    "--background-secondary": { "light": "var(--color-base-20)", "dark": null },
    "--background-secondary-alt": {
      "light": "var(--color-base-30)",
      "dark": null
    },
    "--color-accent-1": {
      "light": "hsl( calc(var(--accent-h) - 1), calc(var(--accent-s) * 1.01), calc(var(--accent-l) * 1.15) )",
      "dark": null
    },
    "--color-accent-2": {
      "light": "hsl( calc(var(--accent-h) - 3), calc(var(--accent-s) * 1.02), calc(var(--accent-l) * 1.5) )",
      "dark": null
    },
    "--color-base-10": { "light": "#eef1f8", "dark": null },
    "--color-base-20": { "light": "#e0e2ea", "dark": null },
    "--color-base-30": { "light": "#c4c6cd", "dark": null },
    "--color-base-40": { "light": "#9b9ea4", "dark": null },
    "--color-base-50": { "light": "#4f5258", "dark": null },
    "--color-base-60": { "light": "#2c2e33", "dark": null },
    "--color-base-70": { "light": "#14161b", "dark": null },
    "--color-base-80": { "light": "#07080d", "dark": null },
    "--color-blue": { "light": "#004c73", "dark": null },
    "--color-blue-rgb": { "light": "0, 76, 115", "dark": null },
    "--color-cyan": { "light": "#007373", "dark": null },
    "--color-cyan-rgb": { "light": "0, 115, 115", "dark": null },
    "--color-green": { "light": "#005523", "dark": null },
    "--color-green-rgb": { "light": "0, 85, 35", "dark": null },
    "--color-purple": { "light": "#470045", "dark": null },
    "--color-purple-rgb": { "light": "71, 0, 69", "dark": null },
    "--color-red": { "light": "#590008", "dark": null },
    "--color-red-rgb": { "light": "89, 0, 8", "dark": null },
    "--color-yellow": { "light": "#6b5300", "dark": null },
    "--color-yellow-rgb": { "light": "107, 83, 0", "dark": null },
    "--interactive-hover": { "light": "var(--color-base-20)", "dark": null },
    "--interactive-normal": { "light": "var(--color-base-10)", "dark": null },
    "--text-accent": { "light": "var(--color-accent)", "dark": null },
    "--text-accent-hover": { "light": "var(--color-accent-2)", "dark": null },
    "--canvas-color": { "light": null, "dark": "126, 126, 126" }
  },
  ".notice progress[value]::-webkit-progress-bar": {
    "background-color": { "light": "#262626", "dark": null },
    "box-shadow": { "light": "inset 0px 0px 0px 1px #363636", "dark": null }
  },
  ".dropdown": { "background-image": { "light": null, "dark": "url(\"data" } },
  ".pdf-container.mod-themed .annotationLayer .textWidgetAnnotation input": {
    "filter": { "light": null, "dark": "invert(100%) hue-rotate(180deg)" }
  },
  ".pdf-container.mod-themed .annotationLayer .textWidgetAnnotation textarea": {
    "filter": { "light": null, "dark": "invert(100%) hue-rotate(180deg)" }
  },
  ".pdf-container.mod-themed .popupWrapper > div": {
    "background-color": {
      "light": null,
      "dark": "var(--background-secondary-alt)"
    },
    "filter": {
      "light": null,
      "dark": "drop-shadow(0px 0px 1px var(--color-base-60)) drop-shadow(0px 1px 2px rgba(0, 0, 0, 0.4)) drop-shadow(0px 4px 6px rgba(0, 0, 0, 0.4))"
    }
  },
  ".pdf-container.mod-themed .popupWrapper > div::after": {
    "background-color": {
      "light": null,
      "dark": "var(--background-secondary-alt)"
    }
  },
  ".pdf-container.mod-themed .popupContent:empty + .popupMeta": {
    "background-color": {
      "light": null,
      "dark": "var(--background-secondary-alt)"
    }
  },
  ".mermaid > svg": {
    "filter": {
      "light": null,
      "dark": "invert(100%) hue-rotate(180deg) saturate(1.25)"
    }
  },
  ".canvas-card-menu": {
    "background-color": { "light": null, "dark": "var(--background-secondary)" }
  },
  ".canvas-card-menu .canvas-card-menu-button svg": {
    "fill": { "light": null, "dark": "var(--background-secondary)" }
  },
  ".is-mobile": {
    "--background-modifier-cover": {
      "light": null,
      "dark": "rgba(0, 0, 0, 0.5)"
    },
    "--background-modifier-form-field": {
      "light": null,
      "dark": "var(--background-modifier-border)"
    },
    "--background-modifier-hover": {
      "light": null,
      "dark": "rgba(var(--mono-rgb-100), 0.15)"
    },
    "--background-modifier-message": {
      "light": null,
      "dark": "var(--background-secondary)"
    },
    "--color-base-00": { "light": null, "dark": "#000" },
    "--color-base-10": { "light": null, "dark": "#111" },
    "--color-base-20": { "light": null, "dark": "#1e1e1e" },
    "--mobile-sidebar-background": {
      "light": null,
      "dark": "var(--background-secondary)"
    },
    "--search-result-background": {
      "light": null,
      "dark": "var(--background-secondary)"
    },
    "--settings-home-background": {
      "light": null,
      "dark": "var(--background-primary)"
    },
    "--tag-background": {
      "light": null,
      "dark": "hsla(var(--interactive-accent-hsl), 0.2)"
    }
  },
  ".is-tablet": {
    "--interactive-hover": {
      "light": null,
      "dark": "var(--background-modifier-border-hover)"
    },
    "--interactive-normal": {
      "light": null,
      "dark": "var(--background-modifier-border)"
    },
    "--modal-background": {
      "light": null,
      "dark": "var(--background-secondary)"
    },
    "--titlebar-background": {
      "light": null,
      "dark": "var(--background-primary)"
    },
    "--titlebar-background-focused": {
      "light": null,
      "dark": "var(--background-primary)"
    }
  },
  ".is-tablet .community-item": {
    "background-color": { "light": null, "dark": "var(--background-secondary)" }
  },
  ".is-tablet .vertical-tab-content": {
    "background-color": { "light": null, "dark": "var(--background-secondary)" }
  },
  ".workspace-drawer-inner": {
    "background-color": { "light": null, "dark": "var(--background-secondary)" }
  },
  ".is-mobile .prompt": {
    "background-color": { "light": null, "dark": "var(--background-secondary)" }
  },
  ".is-mobile .prompt input.prompt-input": {
    "background-color": { "light": null, "dark": "var(--background-secondary)" }
  },
  ".is-tablet .suggestion-container": {
    "background-color": { "light": null, "dark": "var(--background-secondary)" }
  }
}

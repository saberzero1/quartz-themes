{
  "body": {
    "--background-secondary-alt": {
      "light": "var(--color-base-20)",
      "dark": null
    },
    "--checkbox-border-color-hover": {
      "light": "var(--checkbox-color)",
      "dark": null
    },
    "--checkbox-border-color-unchecked": {
      "light": "var(--color-base-50)",
      "dark": null
    },
    "--color-base-00": { "light": "#d8d6d8", "dark": null },
    "--color-base-05": { "light": "#cecdcf", "dark": null },
    "--color-base-10": { "light": "#c4c3c5", "dark": null },
    "--color-base-100": { "light": "#1d1f21", "dark": null },
    "--color-base-20": { "light": "#b1b0b2", "dark": null },
    "--color-base-25": { "light": "#a7a6a8", "dark": null },
    "--color-base-30": { "light": "#9d9c9e", "dark": null },
    "--color-base-35": { "light": "#939395", "dark": null },
    "--color-base-40": { "light": "#89898b", "dark": null },
    "--color-base-50": { "light": "#767678", "dark": null },
    "--color-base-60": { "light": "#626365", "dark": null },
    "--color-base-70": { "light": "#4e4f51", "dark": null },
    "--color-blue": { "light": "#687995", "dark": null },
    "--color-blue_rgb": { "light": "104, 121, 149", "dark": null },
    "--color-cyan": { "light": "var(--color-teal)", "dark": null },
    "--color-cyan-rgb": { "light": "var(--color-teal-rgb)", "dark": null },
    "--color-green": { "light": "#3f7f31", "dark": null },
    "--color-green-rgb": { "light": "63, 127, 49", "dark": null },
    "--color-orange": { "light": "#98623b", "dark": null },
    "--color-orange_rgb": { "light": "152, 98, 59", "dark": null },
    "--color-pink": { "light": "#752e5c", "dark": null },
    "--color-pink-rgb": { "light": "117, 46, 92", "dark": null },
    "--color-purple": { "light": "#3b3169", "dark": null },
    "--color-purple-rgb": { "light": "59, 49, 105", "dark": null },
    "--color-red": { "light": "#933942", "dark": null },
    "--color-red_rgb": { "light": "147, 57, 66", "dark": null },
    "--color-teal": { "light": "#5e8d87", "dark": null },
    "--color-teal-rgb": { "light": "94, 141, 135", "dark": null },
    "--color-yellow": { "light": "#98863b", "dark": null },
    "--color-yellow-rgb": { "light": "152, 119, 59", "dark": null },
    "--h1-weight": { "light": "500", "dark": null },
    "--h2-weight": { "light": "400", "dark": null },
    "--h3-weight": { "light": "400", "dark": null },
    "--h4-weight": { "light": "400", "dark": null },
    "--h5-weight": { "light": "400", "dark": null },
    "--h6-weight": { "light": "400", "dark": null },
    "--mono-rgb-0": { "light": "226, 224, 226", "dark": null },
    "--mono-rgb-100": { "light": "29, 31, 33", "dark": null },
    "--canvas-color": { "light": null, "dark": "126, 126, 126" }
  },
  ".notice progress[value]::-webkit-progress-bar": {
    "background-color": { "light": "#262626", "dark": null },
    "box-shadow": { "light": "inset 0px 0px 0px 1px #363636", "dark": null }
  },
  ".dropdown": { "background-image": { "light": null, "dark": "url(\"data" } },
  ".pdf-container.mod-themed .annotationLayer .textWidgetAnnotation input": {
    "filter": { "light": null, "dark": "invert(100%) hue-rotate(180deg)" }
  },
  ".pdf-container.mod-themed .annotationLayer .textWidgetAnnotation textarea": {
    "filter": { "light": null, "dark": "invert(100%) hue-rotate(180deg)" }
  },
  ".pdf-container.mod-themed .popupWrapper > div": {
    "background-color": {
      "light": null,
      "dark": "var(--background-secondary-alt)"
    },
    "filter": {
      "light": null,
      "dark": "drop-shadow(0px 0px 1px var(--color-base-60)) drop-shadow(0px 1px 2px rgba(0, 0, 0, 0.4)) drop-shadow(0px 4px 6px rgba(0, 0, 0, 0.4))"
    }
  },
  ".pdf-container.mod-themed .popupWrapper > div::after": {
    "background-color": {
      "light": null,
      "dark": "var(--background-secondary-alt)"
    }
  },
  ".pdf-container.mod-themed .popupContent:empty + .popupMeta": {
    "background-color": {
      "light": null,
      "dark": "var(--background-secondary-alt)"
    }
  },
  ".mermaid > svg": {
    "filter": {
      "light": null,
      "dark": "invert(100%) hue-rotate(180deg) saturate(1.25)"
    }
  },
  ".canvas-card-menu": {
    "background-color": { "light": null, "dark": "var(--background-secondary)" }
  },
  ".canvas-card-menu .canvas-card-menu-button svg": {
    "fill": { "light": null, "dark": "var(--background-secondary)" }
  },
  ".is-mobile": {
    "--background-modifier-cover": {
      "light": null,
      "dark": "rgba(0, 0, 0, 0.5)"
    },
    "--background-modifier-form-field": {
      "light": null,
      "dark": "var(--background-modifier-border)"
    },
    "--background-modifier-hover": {
      "light": null,
      "dark": "rgba(var(--mono-rgb-100), 0.15)"
    },
    "--background-modifier-message": {
      "light": null,
      "dark": "var(--background-secondary)"
    },
    "--color-base-00": { "light": null, "dark": "#000" },
    "--color-base-10": { "light": null, "dark": "#111" },
    "--color-base-20": { "light": null, "dark": "#1e1e1e" },
    "--mobile-sidebar-background": {
      "light": null,
      "dark": "var(--background-secondary)"
    },
    "--search-result-background": {
      "light": null,
      "dark": "var(--background-secondary)"
    },
    "--settings-home-background": {
      "light": null,
      "dark": "var(--background-primary)"
    },
    "--tag-background": {
      "light": null,
      "dark": "hsla(var(--interactive-accent-hsl), 0.2)"
    }
  },
  ".is-tablet": {
    "--interactive-hover": {
      "light": null,
      "dark": "var(--background-modifier-border-hover)"
    },
    "--interactive-normal": {
      "light": null,
      "dark": "var(--background-modifier-border)"
    },
    "--modal-background": {
      "light": null,
      "dark": "var(--background-secondary)"
    },
    "--titlebar-background": {
      "light": null,
      "dark": "var(--background-primary)"
    },
    "--titlebar-background-focused": {
      "light": null,
      "dark": "var(--background-primary)"
    }
  },
  ".is-tablet .community-item": {
    "background-color": { "light": null, "dark": "var(--background-secondary)" }
  },
  ".is-tablet .vertical-tab-content": {
    "background-color": { "light": null, "dark": "var(--background-secondary)" }
  },
  ".workspace-drawer-inner": {
    "background-color": { "light": null, "dark": "var(--background-secondary)" }
  },
  ".is-mobile .prompt": {
    "background-color": { "light": null, "dark": "var(--background-secondary)" }
  },
  ".is-mobile .prompt input.prompt-input": {
    "background-color": { "light": null, "dark": "var(--background-secondary)" }
  },
  ".is-tablet .suggestion-container": {
    "background-color": { "light": null, "dark": "var(--background-secondary)" }
  }
}

{
  "body": {
    "--background-modifier-accent": {
      "light": "hsl( var(--theme-accent-h), var(--theme-accent-s), calc(var(--theme-accent-l) + 10%) )",
      "dark": null
    },
    "--background-modifier-border": {
      "light": "hsl( var(--base-h), var(--base-s), calc(var(--base-l) - 6%) )",
      "dark": null
    },
    "--background-modifier-border-focus": {
      "light": "hsl( var(--base-h), var(--base-s), calc(var(--base-l) - 20%) )",
      "dark": null
    },
    "--background-modifier-border-hover": {
      "light": "hsl( var(--base-h), var(--base-s), calc(var(--base-l) - 12%) )",
      "dark": null
    },
    "--background-modifier-form-field": {
      "light": "hsl( var(--base-h), var(--base-s), calc(var(--base-l) + 6%) )",
      "dark": null
    },
    "--background-modifier-form-field-highlighted": {
      "light": "hsl( var(--base-h), var(--base-s), calc(var(--base-l) + 8%) )",
      "dark": null
    },
    "--background-primary": {
      "light": "hsl( calc(var(--base-h) + 44), calc(var(--base-s) + 79%), calc(var(--base-l) - 2%) )",
      "dark": null
    },
    "--background-primary-alt": { "light": "#fcf5e4", "dark": null },
    "--background-secondary": {
      "light": "hsl( calc(var(--base-h) + 42), calc(var(--base-s) + 34%), calc(var(--base-l) - 12%) )",
      "dark": null
    },
    "--background-secondary-alt": { "light": "#fcf5e4", "dark": null },
    "--background-translucent": {
      "light": "rgba(255, 255, 255, 0.85)",
      "dark": null
    },
    "--background-transparent": {
      "light": "hsla( var(--base-h), var(--base-s), var(--base-l), 0 )",
      "dark": null
    },
    "--color-accent": {
      "light": "hsl( var(--theme-accent-h), var(--theme-accent-s), var(--theme-accent-l) )",
      "dark": null
    },
    "--graph-line-color": { "light": "#c0c9ce", "dark": null },
    "--highlight-mix-blend-mode": { "light": "darken", "dark": null },
    "--icon-color": { "light": "var(--text-muted)", "dark": null },
    "--icon-hex": { "light": "000", "dark": null },
    "--interactive-accent": {
      "light": "hsl( var(--theme-accent-h), var(--theme-accent-s), calc(var(--theme-accent-l) + 10%) )",
      "dark": null
    },
    "--interactive-accent-hover": {
      "light": "hsl( var(--theme-accent-h), var(--theme-accent-s), calc(var(--theme-accent-l) - 0%) )",
      "dark": null
    },
    "--interactive-accent-rgb": { "light": "123, 108, 217", "dark": null },
    "--interactive-hover": { "light": "#e9e9e9", "dark": null },
    "--interactive-normal": { "light": "#f2f3f5", "dark": null },
    "--interactive-success": { "light": "#197300", "dark": null },
    "--scrollbar-active-thumb-bg": {
      "light": "rgba(0, 0, 0, 0.2)",
      "dark": null
    },
    "--scrollbar-bg": { "light": "rgba(0, 0, 0, 0.05)", "dark": null },
    "--scrollbar-thumb-bg": { "light": "rgba(0, 0, 0, 0.1)", "dark": null },
    "--text-accent": {
      "light": "hsl( var(--theme-accent-h), var(--theme-accent-s), var(--theme-accent-l) )",
      "dark": null
    },
    "--text-accent-hover": {
      "light": "hsl( var(--theme-accent-h), var(--theme-accent-s), calc(var(--theme-accent-l) - 10%) )",
      "dark": null
    },
    "--text-faint": {
      "light": "hsl(var(--base-h), var(--base-s), calc(var(--base-d) + 47%))",
      "dark": null
    },
    "--text-highlight-bg": {
      "light": "hsla(53, 100%, 50%, 0.5)",
      "dark": null
    },
    "--text-highlight-bg-active": {
      "light": "rgba(255, 128, 0, 0.4)",
      "dark": null
    },
    "--text-muted": {
      "light": "hsl(var(--base-h), var(--base-s), calc(var(--base-d) + 20%))",
      "dark": null
    },
    "--text-normal": {
      "light": "hsl(var(--base-h), var(--base-s), var(--base-d))",
      "dark": null
    },
    "--text-on-accent": { "light": "#f2f2f2", "dark": null },
    "--text-selection": { "light": "rgba(204, 230, 255, 0.99)", "dark": null },
    "--vim-cursor": {
      "light": "hsla( var(--theme-accent-h), var(--theme-accent-s), var(--theme-accent-l), 0.8 )",
      "dark": null
    },
    "--canvas-color": { "light": null, "dark": "126, 126, 126" }
  },
  ".notice progress[value]::-webkit-progress-bar": {
    "background-color": { "light": "#262626", "dark": null },
    "box-shadow": { "light": "inset 0px 0px 0px 1px #363636", "dark": null }
  },
  ".dropdown": { "background-image": { "light": null, "dark": "url(\"data" } },
  ".pdf-container.mod-themed .annotationLayer .textWidgetAnnotation input": {
    "filter": { "light": null, "dark": "invert(100%) hue-rotate(180deg)" }
  },
  ".pdf-container.mod-themed .annotationLayer .textWidgetAnnotation textarea": {
    "filter": { "light": null, "dark": "invert(100%) hue-rotate(180deg)" }
  },
  ".pdf-container.mod-themed .popupWrapper > div": {
    "background-color": {
      "light": null,
      "dark": "var(--background-secondary-alt)"
    },
    "filter": {
      "light": null,
      "dark": "drop-shadow(0px 0px 1px var(--color-base-60)) drop-shadow(0px 1px 2px rgba(0, 0, 0, 0.4)) drop-shadow(0px 4px 6px rgba(0, 0, 0, 0.4))"
    }
  },
  ".pdf-container.mod-themed .popupWrapper > div::after": {
    "background-color": {
      "light": null,
      "dark": "var(--background-secondary-alt)"
    }
  },
  ".pdf-container.mod-themed .popupContent:empty + .popupMeta": {
    "background-color": {
      "light": null,
      "dark": "var(--background-secondary-alt)"
    }
  },
  ".mermaid > svg": {
    "filter": {
      "light": null,
      "dark": "invert(100%) hue-rotate(180deg) saturate(1.25)"
    }
  },
  ".canvas-card-menu": {
    "background-color": { "light": null, "dark": "var(--background-secondary)" }
  },
  ".canvas-card-menu .canvas-card-menu-button svg": {
    "fill": { "light": null, "dark": "var(--background-secondary)" }
  },
  ".is-mobile": {
    "--background-modifier-cover": {
      "light": null,
      "dark": "rgba(0, 0, 0, 0.5)"
    },
    "--background-modifier-form-field": {
      "light": null,
      "dark": "var(--background-modifier-border)"
    },
    "--background-modifier-hover": {
      "light": null,
      "dark": "rgba(var(--mono-rgb-100), 0.15)"
    },
    "--background-modifier-message": {
      "light": null,
      "dark": "var(--background-secondary)"
    },
    "--color-base-00": { "light": null, "dark": "#000" },
    "--color-base-10": { "light": null, "dark": "#111" },
    "--color-base-20": { "light": null, "dark": "#1e1e1e" },
    "--mobile-sidebar-background": {
      "light": null,
      "dark": "var(--background-secondary)"
    },
    "--search-result-background": {
      "light": null,
      "dark": "var(--background-secondary)"
    },
    "--settings-home-background": {
      "light": null,
      "dark": "var(--background-primary)"
    },
    "--tag-background": {
      "light": null,
      "dark": "hsla(var(--interactive-accent-hsl), 0.2)"
    }
  },
  ".is-tablet": {
    "--interactive-hover": {
      "light": null,
      "dark": "var(--background-modifier-border-hover)"
    },
    "--interactive-normal": {
      "light": null,
      "dark": "var(--background-modifier-border)"
    },
    "--modal-background": {
      "light": null,
      "dark": "var(--background-secondary)"
    },
    "--titlebar-background": {
      "light": null,
      "dark": "var(--background-primary)"
    },
    "--titlebar-background-focused": {
      "light": null,
      "dark": "var(--background-primary)"
    }
  },
  ".is-tablet .community-item": {
    "background-color": { "light": null, "dark": "var(--background-secondary)" }
  },
  ".is-tablet .vertical-tab-content": {
    "background-color": { "light": null, "dark": "var(--background-secondary)" }
  },
  ".workspace-drawer-inner": {
    "background-color": { "light": null, "dark": "var(--background-secondary)" }
  },
  ".is-mobile .prompt": {
    "background-color": { "light": null, "dark": "var(--background-secondary)" }
  },
  ".is-mobile .prompt input.prompt-input": {
    "background-color": { "light": null, "dark": "var(--background-secondary)" }
  },
  ".is-tablet .suggestion-container": {
    "background-color": { "light": null, "dark": "var(--background-secondary)" }
  }
}

{
  "body": {
    "--accent-mild": { "light": "#e6b64f", "dark": null },
    "--accent-strong": { "light": "#ec0d0d", "dark": null },
    "--background-modifier-border": { "light": "#f7f7f7", "dark": null },
    "--background-modifier-box-shadow": {
      "light": "rgba(0, 0, 0, 0.85)",
      "dark": null
    },
    "--background-primary": { "light": "#fff", "dark": null },
    "--background-primary-alt": { "light": "#dedede", "dark": null },
    "--background-secondary": { "light": "#dedede", "dark": null },
    "--background-secondary-alt": { "light": "#dedede", "dark": null },
    "--bw": { "light": "#03050b", "dark": null },
    "--interactive-accent": { "light": "#df4a16", "dark": null },
    "--interactive-accent-hover": { "light": "#df7a16", "dark": null },
    "--interactive-accent-rgb": { "light": "#df7a16", "dark": null },
    "--lines-identation": { "light": "#dad8d8", "dark": null },
    "--text-accent": { "light": "#df4a16", "dark": null },
    "--text-accent-hover": { "light": "#df7a16", "dark": null },
    "--text-error": { "light": "#ff3333", "dark": null },
    "--text-error-hover": { "light": "#990000", "dark": null },
    "--text-highlight-bg": { "light": "#df4a16", "dark": null },
    "--text-matched": { "light": "#dcddde", "dark": null },
    "--text-muted": { "light": "#555", "dark": null },
    "--text-normal": { "light": "#03050b", "dark": null },
    "--text-on-accent": { "light": "#dcddde", "dark": null },
    "--text-selection": { "light": "rgba(223, 74, 22, 0.8)", "dark": null },
    "--tooltip-bg": { "light": "rgba(223, 74, 22, 0.9)", "dark": null },
    "--workspace-leaf-resize": { "light": "#c1c1c1", "dark": null },
    "--canvas-color": { "light": null, "dark": "126, 126, 126" }
  },
  ".notice progress[value]::-webkit-progress-bar": {
    "background-color": { "light": "#262626", "dark": null },
    "box-shadow": { "light": "inset 0px 0px 0px 1px #363636", "dark": null }
  },
  "code[class*=\"language-\"]": { "background": "var(--background-secondary)" },
  "pre[class*=\"language-\"]": { "background": "var(--background-secondary)" },
  ".markdown-preview-view code::selection": {
    "background-color": "var(--text-selection)",
    "color": "#fff"
  },
  ".markdown-preview-view code > *::selection": {
    "background-color": "var(--text-selection)",
    "color": "#fff"
  },
  ".markdown-preview-view pre::selection": {
    "background-color": "var(--text-selection)",
    "color": "#fff"
  },
  ".markdown-preview-view pre > *::selection": {
    "background-color": "var(--text-selection)",
    "color": "#fff"
  },
  ".token.operator": {
    "background": { "light": "hsla(0, 0%, 100%, 0)", "dark": null }
  },
  ".suggestion-item.is-selected": {
    "color": { "light": "#fff", "dark": null }
  },
  ".markdown-preview-view mark": { "color": { "light": "#fff", "dark": null } },
  ".graph-view.color-fill-tag": { "color": "var(--text-accent)" },
  ".graph-view.color-fill-attachment": { "color": "var(--text-muted)" },
  ".markdown-preview-view tr:nth-child(even)": {
    "background-color": {
      "light": "rgba(0, 0, 0, 0.05)",
      "dark": "rgba(255, 255, 255, 0.03)"
    }
  },
  ".dropdown": { "background-image": { "light": null, "dark": "url(\"data" } },
  ".pdf-container.mod-themed .annotationLayer .textWidgetAnnotation input": {
    "filter": { "light": null, "dark": "invert(100%) hue-rotate(180deg)" }
  },
  ".pdf-container.mod-themed .annotationLayer .textWidgetAnnotation textarea": {
    "filter": { "light": null, "dark": "invert(100%) hue-rotate(180deg)" }
  },
  ".pdf-container.mod-themed .popupWrapper > div": {
    "background-color": {
      "light": null,
      "dark": "var(--background-secondary-alt)"
    },
    "filter": {
      "light": null,
      "dark": "drop-shadow(0px 0px 1px var(--color-base-60)) drop-shadow(0px 1px 2px rgba(0, 0, 0, 0.4)) drop-shadow(0px 4px 6px rgba(0, 0, 0, 0.4))"
    }
  },
  ".pdf-container.mod-themed .popupWrapper > div::after": {
    "background-color": {
      "light": null,
      "dark": "var(--background-secondary-alt)"
    }
  },
  ".pdf-container.mod-themed .popupContent:empty + .popupMeta": {
    "background-color": {
      "light": null,
      "dark": "var(--background-secondary-alt)"
    }
  },
  ".mermaid > svg": {
    "filter": {
      "light": null,
      "dark": "invert(100%) hue-rotate(180deg) saturate(1.25)"
    }
  },
  ".canvas-card-menu": {
    "background-color": { "light": null, "dark": "var(--background-secondary)" }
  },
  ".canvas-card-menu .canvas-card-menu-button svg": {
    "fill": { "light": null, "dark": "var(--background-secondary)" }
  },
  ".is-mobile": {
    "--background-modifier-cover": {
      "light": null,
      "dark": "rgba(0, 0, 0, 0.5)"
    },
    "--background-modifier-form-field": {
      "light": null,
      "dark": "var(--background-modifier-border)"
    },
    "--background-modifier-hover": {
      "light": null,
      "dark": "rgba(var(--mono-rgb-100), 0.15)"
    },
    "--background-modifier-message": {
      "light": null,
      "dark": "var(--background-secondary)"
    },
    "--color-base-00": { "light": null, "dark": "#000" },
    "--color-base-10": { "light": null, "dark": "#111" },
    "--color-base-20": { "light": null, "dark": "#1e1e1e" },
    "--mobile-sidebar-background": {
      "light": null,
      "dark": "var(--background-secondary)"
    },
    "--search-result-background": {
      "light": null,
      "dark": "var(--background-secondary)"
    },
    "--settings-home-background": {
      "light": null,
      "dark": "var(--background-primary)"
    },
    "--tag-background": {
      "light": null,
      "dark": "hsla(var(--interactive-accent-hsl), 0.2)"
    }
  },
  ".is-tablet": {
    "--interactive-hover": {
      "light": null,
      "dark": "var(--background-modifier-border-hover)"
    },
    "--interactive-normal": {
      "light": null,
      "dark": "var(--background-modifier-border)"
    },
    "--modal-background": {
      "light": null,
      "dark": "var(--background-secondary)"
    },
    "--titlebar-background": {
      "light": null,
      "dark": "var(--background-primary)"
    },
    "--titlebar-background-focused": {
      "light": null,
      "dark": "var(--background-primary)"
    }
  },
  ".is-tablet .community-item": {
    "background-color": { "light": null, "dark": "var(--background-secondary)" }
  },
  ".is-tablet .vertical-tab-content": {
    "background-color": { "light": null, "dark": "var(--background-secondary)" }
  },
  ".workspace-drawer-inner": {
    "background-color": { "light": null, "dark": "var(--background-secondary)" }
  },
  ".is-mobile .prompt": {
    "background-color": { "light": null, "dark": "var(--background-secondary)" }
  },
  ".is-mobile .prompt input.prompt-input": {
    "background-color": { "light": null, "dark": "var(--background-secondary)" }
  },
  ".is-tablet .suggestion-container": {
    "background-color": { "light": null, "dark": "var(--background-secondary)" }
  }
}

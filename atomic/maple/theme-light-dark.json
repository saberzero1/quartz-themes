    },
    "--frame-muted-color": {
      "light": "hsl( var(--accent-h), calc(var(--accent-s) - 20%), calc(var(--accent-l) - 10%) )",
      "dark": "hsl( var(--accent-h), calc(var(--accent-s) - 20%), calc(var(--accent-l) + 10%) )"

      "light": "hsl( var(--accent-h), var(--accent-s), calc(var(--accent-l) - 30%) )",
      "dark": "hsl(var(--accent-h), var(--accent-s), 85%)"
    },
    "--code-mac-style-header-bg": { "light": "#333", "dark": "#000" },
    "--text-normal": {
      "light": null,
      "dark": "hsla(var(--accent-h), 50%, 94%, 75%)"
    }
  },

    "--background-modifier-border": { "light": "#ddd", "dark": null },
    "--background-modifier-box-shadow": {
      "light": "rgba(0, 0, 0, 0.1)",
      "dark": null
    },
    "--background-modifier-cover": {
      "light": "rgba(0, 0, 0, 0.8)",
      "dark": null
    },
    "--background-modifier-error": { "light": "#e68787", "dark": null },
    "--background-modifier-error-hover": { "light": "#ff9494", "dark": null },
    "--background-modifier-error-rgb": {
      "light": "rgb(230, 135, 135)",
      "dark": null
    },
    "--background-modifier-form-field": { "light": "#fff", "dark": null },
    "--background-modifier-form-field-highlighted": {
      "light": "#fff",
      "dark": null
    },
    "--background-modifier-success": { "light": "#a4e7c3", "dark": null },
    "--background-nav": { "light": "rgb(41, 44, 46)", "dark": null },
    "--background-nav-alt": { "light": "rgb(58, 62, 63)", "dark": null },
    "--background-nav-file-tag": { "light": "rgb(203, 77, 73)", "dark": null },
    "--background-nav-selected": { "light": "rgb(203, 77, 73)", "dark": null },
    "--background-primary": { "light": "#ffffff", "dark": null },
    "--background-primary-alt": { "light": "#f0f2f5", "dark": null },
    "--background-secondary": { "light": "rgb(57, 63, 63)", "dark": null },
    "--background-secondary-alt": { "light": "rgb(203, 77, 73)", "dark": null },
    "--bw": { "light": "#000000", "dark": null },
    "--checklist-done-decoration": { "light": "none", "dark": null },
    "--graph-attach": { "light": "#83d4f4", "dark": null },
    "--graph-bg": { "light": "rgb(255, 255, 255)", "dark": null },
    "--graph-circle": { "light": "#ec0c0c", "dark": null },
    "--graph-line": { "light": "#cccccc", "dark": null },
    "--graph-text": { "light": "#969696", "dark": null },
    "--graph-unresolved": { "light": "rgb(143, 147, 143)", "dark": null },
    "--graphtag": { "light": "#b9ec41", "dark": null },
    "--interactive-accent": { "light": "rgb(34, 182, 226)", "dark": null },
    "--interactive-accent-hover": { "light": "#1da3d6", "dark": null },
    "--interactive-accent-rgb": { "light": "34, 182, 226", "dark": null },
    "--interactive-hover": { "light": "#e9e9e9", "dark": null },
    "--interactive-normal": { "light": "#f2f3f5", "dark": null },
    "--scrollbar-active-thumb-bg": {
      "light": "rgb(97, 170, 221)",
      "dark": null
    },
    "--scrollbar-bg": { "light": "rgba(0, 0, 0, 0)", "dark": null },
    "--scrollbar-thumb-bg": { "light": "rgb(0, 0, 0)", "dark": null },
    "--text-accent": { "light": "rgb(203, 77, 73)", "dark": null },
    "--text-accent-hover": { "light": "rgb(203, 77, 73)", "dark": null },
    "--text-error": { "light": "#ce2222", "dark": null },
    "--text-error-hover": { "light": "#cb2c22", "dark": null },
    "--text-faint": { "light": "#999999", "dark": null },
    "--text-highlight-bg": {
      "light": "rgba(240, 255, 82, 0.76)",
      "dark": null
    },
    "--text-matched": { "light": "#000000", "dark": null },
    "--text-muted": { "light": "rgb(143, 147, 143)", "dark": null },
    "--text-nav": { "light": "rgb(190, 190, 190)", "dark": null },
    "--text-nav-selected": { "light": "rgb(255, 255, 255)", "dark": null },
    "--text-normal": { "light": "rgb(203, 77, 73)", "dark": null },
    "--text-normal-original": { "light": "#2e3338", "dark": null },
    "--text-on-accent": { "light": "#f2f2f2", "dark": null },
    "--text-selection": { "light": "rgba(134, 202, 255, 0.99)", "dark": null },
    "--canvas-color": { "light": null, "dark": "126, 126, 126" }
  },
  ".notice progress[value]::-webkit-progress-bar": {
    "background-color": { "light": "#262626", "dark": null },
    "box-shadow": { "light": "inset 0px 0px 0px 1px #363636", "dark": null }
  },
  ".markdown-preview-view .task-list-item-checkbox": {
    "content": { "light": "\"\"", "dark": null },
    "filter": {
      "light": "hue-rotate(130deg) saturate(80%) brightness(90%) contrast(110%)",
      "dark": "hue-rotate(360deg) brightness(120%) contrast(120%)"
    }
  },
  ".dropdown": { "background-image": { "light": null, "dark": "url(\"data" } },
  ".pdf-container.mod-themed .annotationLayer .textWidgetAnnotation input": {
    "filter": { "light": null, "dark": "invert(100%) hue-rotate(180deg)" }
  },
  ".pdf-container.mod-themed .annotationLayer .textWidgetAnnotation textarea": {
    "filter": { "light": null, "dark": "invert(100%) hue-rotate(180deg)" }
  },
  ".pdf-container.mod-themed .popupWrapper > div": {
    "background-color": {
      "light": null,
      "dark": "var(--background-secondary-alt)"
    },
    "filter": {
      "light": null,
      "dark": "drop-shadow(0px 0px 1px var(--color-base-60)) drop-shadow(0px 1px 2px rgba(0, 0, 0, 0.4)) drop-shadow(0px 4px 6px rgba(0, 0, 0, 0.4))"
    }
  },
  ".pdf-container.mod-themed .popupWrapper > div::after": {
    "background-color": {
      "light": null,
      "dark": "var(--background-secondary-alt)"
    }
  },
  ".pdf-container.mod-themed .popupContent:empty + .popupMeta": {
    "background-color": {
      "light": null,
      "dark": "var(--background-secondary-alt)"
    }
  },
  ".mermaid > svg": {
    "filter": {
      "light": null,
      "dark": "invert(100%) hue-rotate(180deg) saturate(1.25)"
    }
  },
  ".canvas-card-menu": {
    "background-color": { "light": null, "dark": "var(--background-secondary)" }
  },
  ".canvas-card-menu .canvas-card-menu-button svg": {
    "fill": { "light": null, "dark": "var(--background-secondary)" }
  },
  ".is-mobile": {
    "--background-modifier-cover": {
      "light": null,
      "dark": "rgba(0, 0, 0, 0.5)"
    },
    "--background-modifier-form-field": {
      "light": null,
      "dark": "var(--background-modifier-border)"
    },
    "--background-modifier-hover": {
      "light": null,
      "dark": "rgba(var(--mono-rgb-100), 0.15)"
    },
    "--background-modifier-message": {
      "light": null,
      "dark": "var(--background-secondary)"
    },
    "--color-base-00": { "light": null, "dark": "#000" },
    "--color-base-10": { "light": null, "dark": "#111" },
    "--color-base-20": { "light": null, "dark": "#1e1e1e" },
    "--mobile-sidebar-background": {
      "light": null,
      "dark": "var(--background-secondary)"
    },
    "--search-result-background": {
      "light": null,
      "dark": "var(--background-secondary)"
    },
    "--settings-home-background": {
      "light": null,
      "dark": "var(--background-primary)"
    },
    "--tag-background": {
      "light": null,
      "dark": "hsla(var(--interactive-accent-hsl), 0.2)"
    }
  },
  ".is-tablet": {
    "--interactive-hover": {
      "light": null,
      "dark": "var(--background-modifier-border-hover)"
    },
    "--interactive-normal": {
      "light": null,
      "dark": "var(--background-modifier-border)"
    },
    "--modal-background": {
      "light": null,
      "dark": "var(--background-secondary)"
    },
    "--titlebar-background": {
      "light": null,
      "dark": "var(--background-primary)"
    },
    "--titlebar-background-focused": {
      "light": null,
      "dark": "var(--background-primary)"
    }
  },
  ".is-tablet .community-item": {
    "background-color": { "light": null, "dark": "var(--background-secondary)" }
  },
  ".is-tablet .vertical-tab-content": {
    "background-color": { "light": null, "dark": "var(--background-secondary)" }
  },
  ".workspace-drawer-inner": {
    "background-color": { "light": null, "dark": "var(--background-secondary)" }
  },
  ".is-mobile .prompt": {
    "background-color": { "light": null, "dark": "var(--background-secondary)" }
  },
  ".is-mobile .prompt input.prompt-input": {
    "background-color": { "light": null, "dark": "var(--background-secondary)" }
  },
  ".is-tablet .suggestion-container": {
    "background-color": { "light": null, "dark": "var(--background-secondary)" }
  }
}

{
  "body": { "--code-background": { "light": "#f9f9f9", "dark": null } },
  "a.tag": {
    "color": { "light": "#1e7d58 !important", "dark": "#f1f1f1 !important" },
    "font-weight": "500",
    "background-color": {
      "light": "#c3e1d3 !important",
      "dark": "#2f7155 !important"
    },
    "border": "none !important",
    "text-decoration": "none !important",
    "font-family": "\"Formular\", var(--font-text)",
    "padding": "5px 0.6em 3px 0 !important"
  },
  "span.cm-hashtag": {
    "color": { "light": "#1e7d58 !important", "dark": "#f1f1f1 !important" },
    "font-weight": { "light": "500 !important", "dark": "500" },
    "background-color": {
      "light": "#c3e1d3 !important",
      "dark": "#2f7155 !important"
    },
    "border": "none !important",
    "text-decoration": "none !important"
  },
  ".metadata-property[data-property-key=\"tags\"] .multi-select-pill": {
    "color": { "light": "#1e7d58 !important", "dark": "#f1f1f1 !important" },
    "font-weight": "500",
    "background-color": {
      "light": "#c3e1d3 !important",
      "dark": "#2f7155 !important"
    },
    "border": { "light": "none !important", "dark": null },
    "text-decoration": { "light": "none !important", "dark": null }
  },
  ".prompt": {
    "box-shadow": {
      "light": "0 0px 12px #000000",
      "dark": "0 0px 18px #000000"
    },
    "background-color": {
      "light": "#f5f6f8",
      "dark": "var(--modal-background) !important"
    }
  },
  "input.prompt-input:hover": {
    ".theme-light input.prompt-input": {
      "light": "focus, .theme-light input.prompt-input:focus-visible, .theme-light input.prompt-input { background-color:#dbdde4",
      "dark": null
    },
    "font-weight": { "light": "400", "dark": "500" },
    "color": { "light": "#000000", "dark": "#ffffff" },
    ".theme-dark input.prompt-input": {
      "light": null,
      "dark": "focus, .theme-dark input.prompt-input:focus-visible, .theme-dark input.prompt-input { background-color:#515151"
    }
  },
  ".prompt-input::placeholder": {
    "color": { "light": "#8b9099 !important", "dark": "#a8a8a8 !important" }
  },
  ".suggestion-item.is-selected": {
    "background-color": { "light": "#c3d8ff !important", "dark": null }
  },
  "::selection": {
    "background": {
      "light": "#c3d7ff !important",
      "dark": "#2260b0 !important"
    },
    "color": "var(--normal-text) !important"
  },
  ".iconize-icon": {
    "color": {
      "light": "var(--color-red) !important",
      "dark": "var(--color-grey) !important"
    }
  },
  ".mermaid > svg": {
    "filter": {
      "light": null,
      "dark": "invert(100%) hue-rotate(180deg) saturate(1.25)"
    },
    "} .markdown-source-view.mod-cm6 .cm-hmd-codeblock { --indentation-guide-editing-indent": {
      "light": null,
      "dark": "0"
    },
    "} .markdown-source-view.mod-cm6 .cm-preview-code-block pre { margin": {
      "light": null,
      "dark": "0"
    },
    "} .markdown-source-view.mod-cm6 .code-block-flair { position": {
      "light": null,
      "dark": "absolute"
    },
    "right": { "light": null, "dark": "6px" },
    "top": { "light": null, "dark": "6px" },
    "z-index": { "light": null, "dark": "1" },
    "display": { "light": null, "dark": "inline-block" },
    "padding": { "light": null, "dark": "var(--size-4-1) var(--size-4-2)" },
    "border-radius": { "light": null, "dark": "var(--code-radius)" },
    "font-family": { "light": null, "dark": "var(--font-interface)" },
    "font-size": { "light": null, "dark": "var(--font-ui-smaller)" },
    "color": { "light": null, "dark": "var(--text-muted)" },
    "cursor": { "light": null, "dark": "var(--cursor)" }
  },
  ".workspace-tab-header-container": {
    ".theme-dark .workspace-split, .theme-dark .workspace-leaf { background-color": {
      "light": null,
      "dark": "var(--titlebar-background-focused)"
    },
    "transition": { "light": null, "dark": "background-color 0.2s ease" },
    "border": { "light": null, "dark": "0px !important" },
    "border-radius": { "light": null, "dark": "0px !important" }
  },
  "strong": {
    ".theme-dark b, .theme-dark .cm-strong, .theme-dark .markdown-preview-view strong { color": {
      "light": null,
      "dark": "#ffffff !important"
    }
  },
  "input.prompt-input": {
    "background-color": { "light": null, "dark": "#434a54 !important" },
    "color": { "light": null, "dark": "#9ba1ab !important" },
    "} .theme-dark .suggestion-item.is-selected { background-color": {
      "light": null,
      "dark": "#286bbd !important"
    }
  },
  ".markdown-preview-view mark": {
    ".theme-dark .cm-s-obsidian span.cm-highlight { background-color": {
      "light": null,
      "dark": "#726630 !important"
    },
    "color": { "light": null, "dark": "#ffffff !important" }
  }
}

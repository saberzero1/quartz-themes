{
  "body": { "--code-background": { "light": "#f9f9f9", "dark": "#3b3b3b" } },
  ".notice progress[value]::-webkit-progress-bar": {
    "background-color": { "light": "#262626", "dark": null },
    "box-shadow": { "light": "inset 0px 0px 0px 1px #363636", "dark": null }
  },
  "a.tag": {
    "background-color": { "light": "#c3e1d3", "dark": "#2f7155" },
    "border": "none",
    "color": { "light": "#1e7d58", "dark": "#f1f1f1" },
    "font-family": "\"Formular\", var(--font-text)",
    "font-weight": "500",
    "padding": "5px 0.6em 3px 0",
    "text-decoration": "none"
  },
  "span.cm-hashtag": {
    "background-color": { "light": "#c3e1d3", "dark": "#2f7155" },
    "border": "none",
    "color": { "light": "#1e7d58", "dark": "#f1f1f1" },
    "font-weight": "500",
    "text-decoration": "none"
  },
  ".metadata-property:is([data-property-key=\"tags\"]) .multi-select-pill": {
    "background-color": { "light": "#c3e1d3", "dark": "#2f7155" },
    "border": { "light": "none", "dark": null },
    "color": { "light": "#1e7d58", "dark": "#f1f1f1" },
    "font-weight": "500",
    "text-decoration": { "light": "none", "dark": null }
  },
  ".prompt": {
    "background-color": {
      "light": "#f5f6f8",
      "dark": "var(--modal-background)"
    },
    "box-shadow": {
      "light": "0 0px 12px rgba(0, 0, 0, 0.4)",
      "dark": "0 0px 18px rgba(0, 0, 0, 0.9)"
    }
  },
  "input.prompt-input:hover": {
    "background-color": { "light": "#dbdde4", "dark": "#515151" },
    "color": { "light": "#000000", "dark": "#ffffff" },
    "font-weight": { "light": "400", "dark": "500" }
  },
  "input.prompt-input:focus": {
    "background-color": { "light": "#dbdde4", "dark": "#515151" },
    "color": { "light": "#000000", "dark": "#ffffff" },
    "font-weight": { "light": "400", "dark": "500" }
  },
  "input.prompt-input:focus-visible": {
    "background-color": { "light": "#dbdde4", "dark": "#515151" },
    "color": { "light": "#000000", "dark": "#ffffff" },
    "font-weight": { "light": "400", "dark": "500" }
  },
  "input.prompt-input": {
    "background-color": { "light": "#dbdde4", "dark": "#434a54" },
    "color": { "light": "#000000", "dark": "#9ba1ab" },
    "font-weight": { "light": "400", "dark": "500" }
  },
  ".prompt-input::placeholder": {
    "color": { "light": "#8b9099", "dark": "#a8a8a8" }
  },
  ".suggestion-item.is-selected": {
    "background-color": { "light": "#c3d8ff", "dark": "#286bbd" }
  },
  "::selection": {
    "background": { "light": "#c3d7ff", "dark": "#2260b0" },
    "color": "var(--normal-text)"
  },
  ".iconize-icon": {
    "color": { "light": "var(--color-red)", "dark": "var(--color-grey)" }
  },
  ".dropdown": { "background-image": { "light": null, "dark": "url(\"data" } },

{
  "body": {
    "--background-primary": { "light": "#ffffff", "dark": null },
    "--background-secondary": { "light": "#ececec", "dark": null },
    "--background-secondary-alt": { "light": "#d4d0c8", "dark": null },
    "--background-tertiary": { "light": "#c0c0c0", "dark": null },
    "--button-background": { "light": "#d4d0c8", "dark": null },
    "--button-text": { "light": "#000000", "dark": null },
    "--folder-color-1": { "light": "#965d5d", "dark": null },
    "--folder-color-10": { "light": "#8b7e6d", "dark": null },
    "--folder-color-11": { "light": "#8d6b83", "dark": null },
    "--folder-color-12": { "light": "#6c8299", "dark": null },
    "--folder-color-2": { "light": "#8470a6", "dark": null },
    "--folder-color-3": { "light": "#507a9e", "dark": null },
    "--folder-color-4": { "light": "#5a8d76", "dark": null },
    "--folder-color-5": { "light": "#a19c6c", "dark": null },
    "--folder-color-6": { "light": "#a67a5b", "dark": null },
    "--folder-color-7": { "light": "#96707e", "dark": null },
    "--folder-color-8": { "light": "#5b8b9e", "dark": null },
    "--folder-color-9": { "light": "#6b8c6b", "dark": null },
    "--interactive-accent": { "light": "#000080", "dark": null },
    "--interactive-accent-hover": { "light": "#0000aa", "dark": null },
    "--interactive-hover": { "light": "#e6e6e6", "dark": null },
    "--interactive-normal": { "light": "#d4d0c8", "dark": null },
    "--text-accent": { "light": "#008015", "dark": null },
    "--text-faint": { "light": "#666666", "dark": null },
    "--text-muted": { "light": "#333333", "dark": null },
    "--text-normal": { "light": "#000000", "dark": null },
    "--titlebar-background": { "light": "#3c8000", "dark": null },
    "--titlebar-text": { "light": "#ffffff", "dark": null },
    "--window-shadow": {
      "light": "inset -1px -1px #0a0a0a, inset 1px 1px #dfdfdf, inset -2px -2px grey, inset 2px 2px #fff",
      "dark": null
    },
    "--canvas-color": { "light": null, "dark": "126, 126, 126" }
  },
  ".notice progress[value]::-webkit-progress-bar": {
    "background-color": { "light": "#262626", "dark": null },
    "box-shadow": { "light": "inset 0px 0px 0px 1px #363636", "dark": null }
  },
  "button": {
    "box-shadow": {
      "light": "inset -1px -1px #0a0a0a, inset 1px 1px #fff, inset -2px -2px grey, inset 2px 2px #dfdfdf",
      "dark": "inset -1px -1px #0a0a0a, inset 1px 1px #666, inset -2px -2px #333, inset 2px 2px #555"
    }
  },
  ".clickable-icon": {
    "box-shadow": {
      "light": "inset -1px -1px #0a0a0a, inset 1px 1px #fff, inset -2px -2px grey, inset 2px 2px #dfdfdf",
      "dark": "inset -1px -1px #0a0a0a, inset 1px 1px #666, inset -2px -2px #333, inset 2px 2px #555"
    }
  },
  "button:active": {
    "box-shadow": {
      "light": "inset 1px 1px #0a0a0a, inset -1px -1px #fff, inset 2px 2px grey, inset -2px -2px #dfdfdf",
      "dark": "inset 1px 1px #0a0a0a, inset -1px -1px #666, inset 2px 2px #333, inset -2px -2px #555"
    }
  },
  ".clickable-icon:active": {
    "box-shadow": {
      "light": "inset 1px 1px #0a0a0a, inset -1px -1px #fff, inset 2px 2px grey, inset -2px -2px #dfdfdf",
      "dark": "inset 1px 1px #0a0a0a, inset -1px -1px #666, inset 2px 2px #333, inset -2px -2px #555"
    }
  },
  "button:hover": {
    "box-shadow": {
      "light": "inset -1px -1px #0a0a0a, inset 1px 1px #dfdfdf",
      "dark": "inset -1px -1px #3a3a3a, inset 1px 1px #888888"
    }
  },
  ".cm-link": {
    "color": { "light": "#0000ff", "dark": "#8cb4ff" },
    "text-decoration": "underline"
  },
  ".cm-hmd-internal-link": {
    "color": { "light": "#0000ff", "dark": "#8cb4ff" },
    "text-decoration": "underline"
  },
  "::-webkit-scrollbar-thumb": {
    "box-shadow": {
      "light": "inset -1px -1px #0a0a0a, inset 1px 1px #fff, inset -2px -2px grey, inset 2px 2px #dfdfdf",
      "dark": "inset -1px -1px #0a0a0a, inset 1px 1px #666, inset -2px -2px #333, inset 2px 2px #555"
    }
  },
  "::-webkit-scrollbar-button": {
    "box-shadow": {
      "light": "inset -1px -1px #0a0a0a, inset 1px 1px #fff, inset -2px -2px grey, inset 2px 2px #dfdfdf",
      "dark": "inset -1px -1px #0a0a0a, inset 1px 1px #666, inset -2px -2px #333, inset 2px 2px #555"
    }
  },
  "::-webkit-scrollbar-button:active": {
    "box-shadow": {
      "light": "inset 1px 1px #0a0a0a, inset -1px -1px #fff, inset 2px 2px grey, inset -2px -2px #dfdfdf",
      "dark": "inset 1px 1px #0a0a0a, inset -1px -1px #666, inset 2px 2px #333, inset -2px -2px #555"
    }
  },
  ".workspace-tab-header": {
    "background-color": "var(--background-tertiary)",
    "border": "1px solid #000",
    "border-radius": "5px",
    "box-shadow": {
      "light": "inset -1px -1px #0a0a0a, inset 1px 1px #fff, inset -2px -2px grey, inset 2px 2px #dfdfdf",
      "dark": "inset -1px -1px #0a0a0a, inset 1px 1px #666, inset -2px -2px #333, inset 2px 2px #555"
    },
    "color": "var(--text-normal)"
  },
  ".workspace-tab-header.is-active": {
    "background-color": "var(--background-secondary)",
    "border-radius": "5px",
    "box-shadow": {
      "light": "inset 1px 1px #0a0a0a, inset -1px -1px #fff, inset 2px 2px grey, inset -2px -2px #dfdfdf",
      "dark": "inset 1px 1px #0a0a0a, inset -1px -1px #666, inset 2px 2px #333, inset -2px -2px #555"
    }
  },
  "pre": {
    "background-color": { "light": "#f0f0f0", "dark": "#2a2a2a" },
    "border": { "light": "1px solid #c0c0c0", "dark": "1px solid #555555" },
    "border-radius": "5px",
    "color": { "light": "#0b6e4f", "dark": "#6fcf9f" },
    "font-family": "\"Courier New\", monospace",
    "padding": "8px"
  },
  "code": {
    "background-color": { "light": "#f0f0f0", "dark": "#2a2a2a" },
    "border": { "light": "1px solid #c0c0c0", "dark": "1px solid #555555" },
    "border-radius": "5px",
    "color": { "light": "#0b6e4f", "dark": "#6fcf9f" },
    "font-family": "\"Courier New\", monospace",
    "padding": "8px"
  },
  ".cm-inline-code": {
    "background-color": { "light": "#f0f0f0", "dark": "#2a2a2a" },
    "border-radius": "3px",
    "color": { "light": "#0b6e4f", "dark": "#6fcf9f" },
    "padding": "2px 6px"
  },
  ".metadata-container": {
    "background": { "light": "#d4d0c8", "dark": "#3a3a3a" },
    "border": { "light": "1px solid #000000", "dark": "1px solid #666666" },
    "border-radius": "5px",
    "padding": "1em 1em 0.5em 2em"
  },
  ".table-view-table > tbody > tr > td": {
    "background-color": { "light": "#ccccff1f", "dark": "#33335f" },
    "padding": "10px",
    "white-space": "pre-wrap"
  },
  ".nav-files-container > div > .nav-folder": {
    "border": { "light": "1px solid #c0c0c0", "dark": "1px solid #555555" },
    "border-left": "4px solid var(--folder-color)",
    "border-radius": "var(--radius-s)"
  },
  ".dropdown": { "background-image": { "light": null, "dark": "url(\"data" } },
  ".pdf-container.mod-themed .annotationLayer .textWidgetAnnotation input": {
    "filter": { "light": null, "dark": "invert(100%) hue-rotate(180deg)" }
  },
  ".pdf-container.mod-themed .annotationLayer .textWidgetAnnotation textarea": {
    "filter": { "light": null, "dark": "invert(100%) hue-rotate(180deg)" }
  },
  ".pdf-container.mod-themed .popupWrapper > div": {
    "background-color": {
      "light": null,
      "dark": "var(--background-secondary-alt)"
    },
    "filter": {
      "light": null,
      "dark": "drop-shadow(0px 0px 1px var(--color-base-60)) drop-shadow(0px 1px 2px rgba(0, 0, 0, 0.4)) drop-shadow(0px 4px 6px rgba(0, 0, 0, 0.4))"
    }
  },
  ".pdf-container.mod-themed .popupWrapper > div::after": {
    "background-color": {
      "light": null,
      "dark": "var(--background-secondary-alt)"
    }
  },
  ".pdf-container.mod-themed .popupContent:empty + .popupMeta": {
    "background-color": {
      "light": null,
      "dark": "var(--background-secondary-alt)"
    }
  },
  ".mermaid > svg": {
    "filter": {
      "light": null,
      "dark": "invert(100%) hue-rotate(180deg) saturate(1.25)"
    }
  },
  ".canvas-card-menu": {
    "background-color": { "light": null, "dark": "var(--background-secondary)" }
  },
  ".canvas-card-menu .canvas-card-menu-button svg": {
    "fill": { "light": null, "dark": "var(--background-secondary)" }
  },
  ".is-mobile": {
    "--background-modifier-cover": {
      "light": null,
      "dark": "rgba(0, 0, 0, 0.5)"
    },
    "--background-modifier-form-field": {
      "light": null,
      "dark": "var(--background-modifier-border)"
    },
    "--background-modifier-hover": {
      "light": null,
      "dark": "rgba(var(--mono-rgb-100), 0.15)"
    },
    "--background-modifier-message": {
      "light": null,
      "dark": "var(--background-secondary)"
    },
    "--color-base-00": { "light": null, "dark": "#000" },
    "--color-base-10": { "light": null, "dark": "#111" },
    "--color-base-20": { "light": null, "dark": "#1e1e1e" },
    "--mobile-sidebar-background": {
      "light": null,
      "dark": "var(--background-secondary)"
    },
    "--search-result-background": {
      "light": null,
      "dark": "var(--background-secondary)"
    },
    "--settings-home-background": {
      "light": null,
      "dark": "var(--background-primary)"
    },
    "--tag-background": {
      "light": null,
      "dark": "hsla(var(--interactive-accent-hsl), 0.2)"
    }
  },
  ".is-tablet": {
    "--interactive-hover": {
      "light": null,
      "dark": "var(--background-modifier-border-hover)"
    },
    "--interactive-normal": {
      "light": null,
      "dark": "var(--background-modifier-border)"
    },
    "--modal-background": {
      "light": null,
      "dark": "var(--background-secondary)"
    },
    "--titlebar-background": {
      "light": null,
      "dark": "var(--background-primary)"
    },
    "--titlebar-background-focused": {
      "light": null,
      "dark": "var(--background-primary)"
    }
  },
  ".is-tablet .community-item": {
    "background-color": { "light": null, "dark": "var(--background-secondary)" }
  },
  ".is-tablet .vertical-tab-content": {
    "background-color": { "light": null, "dark": "var(--background-secondary)" }
  },
  ".workspace-drawer-inner": {
    "background-color": { "light": null, "dark": "var(--background-secondary)" }
  },
  ".is-mobile .prompt": {
    "background-color": { "light": null, "dark": "var(--background-secondary)" }
  },
  ".is-mobile .prompt input.prompt-input": {
    "background-color": { "light": null, "dark": "var(--background-secondary)" }
  },
  ".is-tablet .suggestion-container": {
    "background-color": { "light": null, "dark": "var(--background-secondary)" }
  }
}

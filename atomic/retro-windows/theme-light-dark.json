{
  "button": {
    ".theme-light .clickable-icon { box-shadow": {
      "light": "inset -1px -1px #0a0a0a, inset 1px 1px #fff, inset -2px -2px grey, inset 2px 2px #dfdfdf",
      "dark": null
    },
    ".theme-dark .clickable-icon { box-shadow": {
      "light": null,
      "dark": "inset -1px -1px #0a0a0a, inset 1px 1px #666, inset -2px -2px #333, inset 2px 2px #555"
    }
  },
  "button:active": {
    ".theme-light .clickable-icon": {
      "light": "active { box-shadow:inset 1px 1px #0a0a0a, inset -1px -1px #fff, inset 2px 2px grey, inset -2px -2px #dfdfdf",
      "dark": null
    },
    ".theme-dark .clickable-icon": {
      "light": null,
      "dark": "active { box-shadow:inset 1px 1px #0a0a0a, inset -1px -1px #666, inset 2px 2px #333, inset -2px -2px #555"
    }
  },
  "button:hover": {
    "box-shadow": {
      "light": "inset -1px -1px #0a0a0a, inset 1px 1px #dfdfdf",
      "dark": "inset -1px -1px #3a3a3a, inset 1px 1px #888888"
    }
  },
  ".cm-link": {
    ".theme-light .cm-hmd-internal-link { color": {
      "light": "#0000ff",
      "dark": null
    },
    "text-decoration": "underline",
    ".theme-dark .cm-hmd-internal-link { color": {
      "light": null,
      "dark": "#8cb4ff"
    }
  },
  "::-webkit-scrollbar-thumb": {
    ".theme-light": {
      "light": ":-webkit-scrollbar-button { box-shadow:inset -1px -1px #0a0a0a, inset 1px 1px #fff, inset -2px -2px grey, inset 2px 2px #dfdfdf",
      "dark": null
    },
    ".theme-dark": {
      "light": null,
      "dark": ":-webkit-scrollbar-button { box-shadow:inset -1px -1px #0a0a0a, inset 1px 1px #666, inset -2px -2px #333, inset 2px 2px #555"
    }
  },
  "::-webkit-scrollbar-button:active": {
    "box-shadow": {
      "light": "inset 1px 1px #0a0a0a, inset -1px -1px #fff, inset 2px 2px grey, inset -2px -2px #dfdfdf",
      "dark": "inset 1px 1px #0a0a0a, inset -1px -1px #666, inset 2px 2px #333, inset -2px -2px #555"
    }
  },
  ".workspace-tab-header": {
    "background-color": "var(--background-tertiary)",
    "color": "var(--text-normal)",
    "border": "1px solid #000",
    "box-shadow": {
      "light": "inset -1px -1px #0a0a0a, inset 1px 1px #fff, inset -2px -2px grey, inset 2px 2px #dfdfdf",
      "dark": "inset -1px -1px #0a0a0a, inset 1px 1px #666, inset -2px -2px #333, inset 2px 2px #555"
    },
    "border-radius": "5px"
  },
  ".workspace-tab-header.is-active": {
    "background-color": "var(--background-secondary)",
    "box-shadow": {
      "light": "inset 1px 1px #0a0a0a, inset -1px -1px #fff, inset 2px 2px grey, inset -2px -2px #dfdfdf",
      "dark": "inset 1px 1px #0a0a0a, inset -1px -1px #666, inset 2px 2px #333, inset -2px -2px #555"
    },
    "border-radius": "5px"
  },
  "pre": {
    ".theme-light code { background-color": {
      "light": "#f0f0f0",
      "dark": null
    },
    "color": { "light": "#0b6e4f", "dark": "#6fcf9f" },
    "border": { "light": "1px solid #c0c0c0", "dark": "1px solid #555555" },
    "font-family": "\"Courier New\", monospace",
    "padding": "8px",
    "border-radius": "5px",
    ".theme-dark code { background-color": { "light": null, "dark": "#2a2a2a" }
  },
  ".cm-inline-code": {
    "background-color": { "light": "#f0f0f0", "dark": "#2a2a2a" },
    "color": { "light": "#0b6e4f", "dark": "#6fcf9f" },
    "padding": "2px 6px",
    "border-radius": "3px"
  },
  ".metadata-container": {
    "border": { "light": "1px solid #000000", "dark": "1px solid #666666" },
    "padding": "1em 1em 0.5em 2em",
    "border-radius": "5px",
    "background": { "light": "#d4d0c8", "dark": "#3a3a3a" }
  },
  ".table-view-table > tbody > tr > td": {
    "white-space": "pre-wrap !important",
    "background-color": {
      "light": "#ccccff1f !important",
      "dark": "#33335f !important"
    },
    "padding": "10px !important"
  },
  ".nav-files-container > div > .nav-folder": {
    "border": { "light": "1px solid #c0c0c0", "dark": "1px solid #555555" },
    "border-left": "4px solid var(--folder-color)",
    "border-radius": "var(--radius-s)"
  },
  ".mermaid > svg": {
    "filter": {
      "light": null,
      "dark": "invert(100%) hue-rotate(180deg) saturate(1.25)"
    },
    "} .markdown-source-view.mod-cm6 .cm-hmd-codeblock { --indentation-guide-editing-indent": {
      "light": null,
      "dark": "0"
    },
    "} .markdown-source-view.mod-cm6 .cm-preview-code-block pre { margin": {
      "light": null,
      "dark": "0"
    },
    "} .markdown-source-view.mod-cm6 .code-block-flair { position": {
      "light": null,
      "dark": "absolute"
    },
    "right": { "light": null, "dark": "6px" },
    "top": { "light": null, "dark": "6px" },
    "z-index": { "light": null, "dark": "1" },
    "display": { "light": null, "dark": "inline-block" },
    "padding": { "light": null, "dark": "var(--size-4-1) var(--size-4-2)" },
    "border-radius": { "light": null, "dark": "var(--code-radius)" },
    "font-family": { "light": null, "dark": "var(--font-interface)" },
    "font-size": { "light": null, "dark": "var(--font-ui-smaller)" },
    "color": { "light": null, "dark": "var(--text-muted)" },
    "cursor": { "light": null, "dark": "var(--cursor)" }
  }
}

{
  "body": {
    "--background-primary": { "light": "#e7e7e7", "dark": "#1a2023" },
    "--background-primary-alt": { "light": "#e7e7e7", "dark": "#1a2023" },
    "--background-secondary": { "light": "#e3e3e3", "dark": "#222b2f" },
    "--background-secondary-alt": { "light": "#bebebe", "dark": "#073334" },
    "--text-normal": { "light": "#1a2023", "dark": "#faf2d6" },
    "--text-faint": { "light": "#665c54", "dark": "#bdae93" },
    "--text-title-h1": { "light": "#1bb0b6", "dark": "#c1dde1" },
    "--text-title-h2": { "light": "#3fa7ab", "dark": "#8ab8bd" },
    "--text-title-h3": { "light": "#458588", "dark": "#56a7b0" },
    "--text-title-h4": { "light": "#4b6667", "dark": "#309093" },
    "--text-title-h5": { "light": "#3b4646", "dark": "#0b797d" },
    "--text-link": { "light": "#458588", "dark": "#83a598" },
    "--text-a-hover": { "light": "#458588", "dark": "#83a598" },
    "--text-title-h6": { "light": null, "dark": "#0b797d" },
    "--text-highlight-bg": { "light": null, "dark": "#22a578" },
    "--inline-code": { "light": null, "dark": "#83a598" },
    "--code-block": { "light": null, "dark": "#83a598" },
    "--text-a": { "light": null, "dark": "#2ccab7" },
    "--interactive-accent": { "light": null, "dark": "#0b797d" },
    "--text-accent": { "light": null, "dark": "#2ccab7" },
    "--text-on-accent": { "light": null, "dark": "#fff" },
    "--interactive-accent-rgb": { "light": null, "dark": "#2ccab7" }
  },
  ".mermaid > svg": {
    "filter": {
      "light": null,
      "dark": "invert(100%) hue-rotate(180deg) saturate(1.25)"
    },
    "} .markdown-source-view.mod-cm6 .cm-hmd-codeblock { --indentation-guide-editing-indent": {
      "light": null,
      "dark": "0"
    },
    "} .markdown-source-view.mod-cm6 .cm-preview-code-block pre { margin": {
      "light": null,
      "dark": "0"
    },
    "} .markdown-source-view.mod-cm6 .code-block-flair { position": {
      "light": null,
      "dark": "absolute"
    },
    "right": { "light": null, "dark": "6px" },
    "top": { "light": null, "dark": "6px" },
    "z-index": { "light": null, "dark": "1" },
    "display": { "light": null, "dark": "inline-block" },
    "padding": { "light": null, "dark": "var(--size-4-1) var(--size-4-2)" },
    "border-radius": { "light": null, "dark": "var(--code-radius)" },
    "font-family": { "light": null, "dark": "var(--font-interface)" },
    "font-size": { "light": null, "dark": "var(--font-ui-smaller)" },
    "color": { "light": null, "dark": "var(--text-muted)" },
    "cursor": { "light": null, "dark": "var(--cursor)" }
  },
  ".cm-s-obsidian span.obsidian-search-match-highlight": {
    "color": { "light": null, "dark": "var(--text-normal)" },
    "background-color": { "light": null, "dark": "#483699" }
  },
  "code[class*=\"language-\"]": {
    ".theme-dark pre[class*=\"language-\"], .theme-light code[class*=\"language-\"], .theme-light pre[class*=\"language-\"] { text-shadow": {
      "light": null,
      "dark": "none !important"
    },
    "background-color": { "light": null, "dark": "var(--pre-code) !important" }
  }
}

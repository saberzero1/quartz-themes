{
  ".vertical-tab-header-group-title": {
    "color": { "light": "var(--primary-light)", "dark": "var(--primary-dark)" }
  },
  "body": {
    "background-color": { "light": "var(--surface-variant)", "dark": null },
    "--tooltip-color": { "light": "white", "dark": null },
    "--primary": { "light": "var(--color-accent-2)", "dark": null },
    "--primary-container": { "light": "var(--color-accent-1)", "dark": null },
    "--on-primary-container": { "light": "var(--color-accent)", "dark": null },
    "--secondary": { "light": "var(--color-accent-2)", "dark": null },
    "--background": { "light": "#fff", "dark": null },
    "--on-background": { "light": "#111", "dark": null },
    "--surface": { "light": "#fff", "dark": null },
    "--surface-variant": {
      "light": "hsl(var(--accent-h), calc(var(--accent-s) * 0.75), 93%)",
      "dark": null
    },
    "--surface-mid": {
      "light": "hsl(var(--accent-h), calc(var(--accent-s) * 0.35), 93%)",
      "dark": null
    },
    "--on-surface-variant": {
      "light": "hsl(var(--accent-h), calc(var(--accent-s) * 0.3), 20%)",
      "dark": null
    },
    "--outline": {
      "light": "hsl(var(--accent-h), calc(var(--accent-s) * 0.5), 85%)",
      "dark": null
    },
    "--inverse-on-surface": { "light": "#eee", "dark": null },
    "--inverse-surface": { "light": "#111", "dark": null },
    "--on-secondary": { "light": "var(--text-on-accent)", "dark": null },
    "--secondary-container": { "light": "var(--text-on-accent)", "dark": null },
    "--on-secondary-container": {
      "light": "var(--color-accent-1)",
      "dark": null
    },
    "--tertiary-container": {
      "light": "hsl(var(--accent-h), calc(var(--accent-s) * 0.6), 66%)",
      "dark": null
    },
    "--on-tertiary-container": {
      "light": "hsl( var(--accent-h), calc(var(--accent-s) * 0.2), 12% )",
      "dark": null
    },
    "--interactive-hover": {
      "light": "hsl(var(--accent-h), 15%, 85%)",
      "dark": null
    },
    "--text-selection": {
      "light": "hsl(var(--accent-h), calc(var(--accent-s) * 0.5), 80%)",
      "dark": null
    }
  },
  "body.mod-windows.enable-mica.is-popout-window": {
    "background": {
      "light": "var(--surface-variant) !important",
      "dark": "#202020 !important"
    }
  },
  "body .canvas-wrapper": {
    "--shadow-stationary": { "light": "none", "dark": null },
    "--canvas-dot-pattern": { "light": "#bbb", "dark": null },
    "} body.theme-light .canvas-wrapper .canvas-node { box-shadow": {
      "light": "#000000 0px 3px 10px",
      "dark": null
    },
    "border": { "light": "none", "dark": null },
    "border-radius": { "light": "var(--radius-l)", "dark": null },
    "} body.theme-light .canvas-wrapper .canvas-node-container { border": {
      "light": "none",
      "dark": null
    }
  },
  "body:not(.vibrant)": {
    "--color-accent": {
      "light": "hsl( var(--accent-h), calc(var(--accent-s) * 0.8), 75% ) !important",
      "dark": "hsl( var(--accent-h), calc(var(--accent-s) * 0.6), 70% ) !important"
    },
    "--color-accent-1": "hsl( var(--accent-h), calc(var(--accent-s) * 0.7), 80% ) !important",
    "--color-accent-2": {
      "light": "hsl( var(--accent-h), calc(var(--accent-s) * 0.6), 85% ) !important",
      "dark": "hsl( var(--accent-h), calc(var(--accent-s) * 0.8), 90% ) !important"
    },
    "--primary": {
      "light": "hsl(var(--accent-h), calc(var(--accent-s) * 0.86), 73%) !important",
      "dark": "hsl(var(--accent-h), calc(var(--accent-s) * 0.75), 80%) !important"
    },
    "--secondary": {
      "light": "hsl( var(--accent-h), calc(var(--accent-s) * 0.6), 70% ) !important",
      "dark": "hsl( var(--accent-h), calc(var(--accent-s) * 0.4), 70% ) !important"
    },
    "--text-accent": {
      "light": "hsl( var(--accent-h), calc(var(--accent-s) * 0.6), 50% ) !important",
      "dark": "hsl( var(--accent-h), calc(var(--accent-s) * 0.4), 90% ) !important"
    }
  },
  "body.dim-light-mode": {
    "--background": {
      "light": "hsl(var(--accent-h), calc(var(--accent-s) * 0.15), 95%)",
      "dark": null
    },
    "--surface": {
      "light": "hsl(var(--accent-h), calc(var(--accent-s) * 0.1), 98%)",
      "dark": null
    },
    "}": { "light": "", "dark": null }
  },
  ".mermaid > svg": {
    "filter": {
      "light": null,
      "dark": "invert(100%) hue-rotate(180deg) saturate(1.25)"
    },
    "} .markdown-source-view.mod-cm6 .cm-hmd-codeblock { --indentation-guide-editing-indent": {
      "light": null,
      "dark": "0"
    },
    "} .markdown-source-view.mod-cm6 .cm-preview-code-block pre { margin": {
      "light": null,
      "dark": "0"
    },
    "} .markdown-source-view.mod-cm6 .code-block-flair { position": {
      "light": null,
      "dark": "absolute"
    },
    "right": { "light": null, "dark": "6px" },
    "top": { "light": null, "dark": "6px" },
    "z-index": { "light": null, "dark": "1" },
    "display": { "light": null, "dark": "inline-block" },
    "padding": { "light": null, "dark": "var(--size-4-1) var(--size-4-2)" },
    "border-radius": { "light": null, "dark": "var(--code-radius)" },
    "font-family": { "light": null, "dark": "var(--font-interface)" },
    "font-size": { "light": null, "dark": "var(--font-ui-smaller)" },
    "color": { "light": null, "dark": "var(--text-muted)" },
    "cursor": { "light": null, "dark": "var(--cursor)" }
  },
  "body .canvas-wrapper .canvas-node": {
    "box-shadow": { "light": null, "dark": "#000000 0px 3px 5px" },
    "border-radius": { "light": null, "dark": "var(--radius-l)" },
    "} body.theme-dark .canvas-wrapper .canvas-node.is-themed .markdown-embed-content { background-color": {
      "light": null,
      "dark": "#ffffff"
    }
  },
  "body.mod-windows.win-menu div.menu": {
    "background-color": { "light": null, "dark": "#201f21 !important" },
    "border-color": { "light": null, "dark": "#ffffff" },
    "} body.mod-windows.win-menu.theme-dark div.menu .menu-item.selected": {
      "light": null,
      "dark": "not(.is-disabled):not(.is-label) { background-color:#ffffff"
    },
    "} body.mod-windows.win-menu.theme-dark div.menu .menu-separator { border-color": {
      "light": null,
      "dark": "#ffffff"
    },
    "} body.mod-windows.win-menu.theme-dark div.menu .menu-item { --menu-text-color": {
      "light": null,
      "dark": "white"
    },
    "} body.mod-windows.win-menu.theme-dark div.menu .menu-item .menu-item-icon svg { fill": {
      "light": null,
      "dark": "none"
    }
  },
  "body.mod-windows.win-menu.enable-mica div.menu": {
    "background-color": { "light": null, "dark": "#272627 !important" }
  }
}

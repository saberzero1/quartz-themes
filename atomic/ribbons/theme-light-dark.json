{
  "body": {
    "--background-mythic": { "light": "#082283", "dark": null },
    "--background-primary": { "light": "#fbf8f4", "dark": null },
    "--background-secondary": { "light": "#f1edf6", "dark": null },
    "--chbx-a": { "light": "248, 114, 181", "dark": null },
    "--chbx-b": { "light": "251, 146, 76", "dark": null },
    "--chbx-B": { "light": "182, 118, 255", "dark": null },
    "--chbx-c": { "light": "149, 217, 238", "dark": null },
    "--chbx-C": { "light": "214, 108, 108", "dark": null },
    "--chbx-d": { "light": "203, 212, 235", "dark": null },
    "--chbx-h": { "light": "168, 177, 189", "dark": null },
    "--chbx-i": { "light": "219, 1, 1", "dark": null },
    "--chbx-I": { "light": "82, 139, 212", "dark": null },
    "--chbx-id": { "light": "202, 199, 0", "dark": null },
    "--chbx-N": { "light": "161, 106, 73", "dark": null },
    "--chbx-P": { "light": "79, 165, 79", "dark": null },
    "--chbx-q": { "light": "253, 175, 0", "dark": null },
    "--chbx-r": { "light": "249, 190, 123", "dark": null },
    "--chbx-R": { "light": "159, 186, 223", "dark": null },
    "--field-img": { "light": "var(--img-field-light)", "dark": null },
    "--frame-a-img": { "light": "var(--img-frame-a-light)", "dark": null },
    "--frame-b-img": { "light": "var(--img-frame-b-light)", "dark": null },
    "--med-small": { "light": "250px", "dark": null },
    "--med-tall": { "light": "450px", "dark": null },
    "--medium": { "light": "300px", "dark": null },
    "--micro": { "light": "70px", "dark": null },
    "--radius": { "light": "0px", "dark": null },
    "--ribbon-img": { "light": "var(--img-ribbon-light)", "dark": null },
    "--small": { "light": "150px", "dark": null },
    "--small-med": { "light": "200px", "dark": null },
    "--tall": { "light": "500px", "dark": null },
    "--text-faint": { "light": "rgba(80, 80, 80, 0.65)", "dark": null },
    "--text-normal": { "light": "black", "dark": null },
    "--text-subtitle": { "light": "#77220d", "dark": null },
    "--text-title": { "light": "#4b254d", "dark": null },
    "--tiny": { "light": "100px", "dark": null },
    "--canvas-color": { "light": null, "dark": "126, 126, 126" }
  },
  ".notice progress[value]::-webkit-progress-bar": {
    "background-color": { "light": "#262626", "dark": null },
    "box-shadow": { "light": "inset 0px 0px 0px 1px #363636", "dark": null }
  },
  ".dropdown": { "background-image": { "light": null, "dark": "url(\"data" } },
  ".pdf-container.mod-themed .annotationLayer .textWidgetAnnotation input": {
    "filter": { "light": null, "dark": "invert(100%) hue-rotate(180deg)" }
  },
  ".pdf-container.mod-themed .annotationLayer .textWidgetAnnotation textarea": {
    "filter": { "light": null, "dark": "invert(100%) hue-rotate(180deg)" }
  },
  ".pdf-container.mod-themed .popupWrapper > div": {
    "background-color": {
      "light": null,
      "dark": "var(--background-secondary-alt)"
    },
    "filter": {
      "light": null,
      "dark": "drop-shadow(0px 0px 1px var(--color-base-60)) drop-shadow(0px 1px 2px rgba(0, 0, 0, 0.4)) drop-shadow(0px 4px 6px rgba(0, 0, 0, 0.4))"
    }
  },
  ".pdf-container.mod-themed .popupWrapper > div::after": {
    "background-color": {
      "light": null,
      "dark": "var(--background-secondary-alt)"
    }
  },
  ".pdf-container.mod-themed .popupContent:empty + .popupMeta": {
    "background-color": {
      "light": null,
      "dark": "var(--background-secondary-alt)"
    }
  },
  ".mermaid > svg": {
    "filter": {
      "light": null,
      "dark": "invert(100%) hue-rotate(180deg) saturate(1.25)"
    }
  },
  ".canvas-card-menu": {
    "background-color": { "light": null, "dark": "var(--background-secondary)" }
  },
  ".canvas-card-menu .canvas-card-menu-button svg": {
    "fill": { "light": null, "dark": "var(--background-secondary)" }
  },
  ".is-mobile": {
    "--background-modifier-cover": {
      "light": null,
      "dark": "rgba(0, 0, 0, 0.5)"
    },
    "--background-modifier-form-field": {
      "light": null,
      "dark": "var(--background-modifier-border)"
    },
    "--background-modifier-hover": {
      "light": null,
      "dark": "rgba(var(--mono-rgb-100), 0.15)"
    },
    "--background-modifier-message": {
      "light": null,
      "dark": "var(--background-secondary)"
    },
    "--color-base-00": { "light": null, "dark": "#000" },
    "--color-base-10": { "light": null, "dark": "#111" },
    "--color-base-20": { "light": null, "dark": "#1e1e1e" },
    "--mobile-sidebar-background": {
      "light": null,
      "dark": "var(--background-secondary)"
    },
    "--search-result-background": {
      "light": null,
      "dark": "var(--background-secondary)"
    },
    "--settings-home-background": {
      "light": null,
      "dark": "var(--background-primary)"
    },
    "--tag-background": {
      "light": null,
      "dark": "hsla(var(--interactive-accent-hsl), 0.2)"
    }
  },
  ".is-tablet": {
    "--interactive-hover": {
      "light": null,
      "dark": "var(--background-modifier-border-hover)"
    },
    "--interactive-normal": {
      "light": null,
      "dark": "var(--background-modifier-border)"
    },
    "--modal-background": {
      "light": null,
      "dark": "var(--background-secondary)"
    },
    "--titlebar-background": {
      "light": null,
      "dark": "var(--background-primary)"
    },
    "--titlebar-background-focused": {
      "light": null,
      "dark": "var(--background-primary)"
    }
  },
  ".is-tablet .community-item": {
    "background-color": { "light": null, "dark": "var(--background-secondary)" }
  },
  ".is-tablet .vertical-tab-content": {
    "background-color": { "light": null, "dark": "var(--background-secondary)" }
  },
  ".workspace-drawer-inner": {
    "background-color": { "light": null, "dark": "var(--background-secondary)" }
  },
  ".is-mobile .prompt": {
    "background-color": { "light": null, "dark": "var(--background-secondary)" }
  },
  ".is-mobile .prompt input.prompt-input": {
    "background-color": { "light": null, "dark": "var(--background-secondary)" }
  },
  ".is-tablet .suggestion-container": {
    "background-color": { "light": null, "dark": "var(--background-secondary)" }
  }
}

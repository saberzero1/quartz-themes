{
  ".is-mobile": {
    "--mobile-sidebar-background": {
      "light": "var(--background-secondary)",
      "dark": null
    },
    "--nav-item-color": { "light": "var(--text-muted)", "dark": null },
    "--mobile-sidebar-left-shadow": {
      "light": "5px 0px 14px -0.6px #000000, 10px 0px 20px -4.9px #000000",
      "dark": null
    },
    "--mobile-sidebar-right-shadow": {
      "light": "-5px 0px 14px -0.6px #000000, -10px 0px 20px -4.9px #000000",
      "dark": null
    },
    "--mobile-sidebar-tablet-shadow": {
      "light": "0px 2px 6px -0.3px #000000, 0px 5px 14px -0.6px #000000, 0px 10px 20px -4.9px #000000",
      "dark": null
    },
    "--mobile-toolbar-bg": {
      "light": "color-mix( in srgb, var(--background-primary) 50%, var(--background-primary-alt) )",
      "dark": null
    },
    "--navbar-shadow": {
      "light": "0px 4px 20px 0px #000000, 0px 1px 4px 0px #000000",
      "dark": null
    },
    "--pull-down-action-shadow": {
      "light": "inset 0px 0px 0px 1px #000000, 0px 2px 9px -2.1px #000000, 0px 3px 17px -4.3px #000000, 0px 7px 30px -5.5px #000000",
      "dark": null
    }
  },
  "#calendar-container": {
    "--color-dot": {
      "light": "var(--color-l-blue-20)",
      "dark": "var(--color-d-blue-20)"
    },
    "--color-arrow": {
      "light": "var(--color-l-gray-70)",
      "dark": "var(--color-d-gray-60)"
    },
    "--color-arrow-hover": {
      "light": "var(--color-l-gray-90)",
      "dark": "var(--color-d-gray-40)"
    },
    "--color-arrow-active": {
      "light": "var(--color-l-gray-100)",
      "dark": "var(--color-d-gray-30)"
    },
    "--color-button": {
      "light": "var(--color-l-gray-100)",
      "dark": "var(--color-l-gray-30)"
    },
    "--pmr-cal-month-color": {
      "light": "var(--color-l-gray-120)",
      "dark": "var(--color-d-gray-30)"
    },
    "--pmr-cal-year-color": {
      "light": "var(--color-l-gray-90)",
      "dark": "var(--color-d-gray-50)"
    },
    "--color-text-heading": {
      "light": "var(--color-l-gray-90)",
      "dark": "var(--color-d-gray-50)"
    },
    "--color-text-day": {
      "light": "var(--color-l-gray-130)",
      "dark": "var(--color-d-gray-40)"
    },
    "--color-text-today": {
      "light": "var(--color-l-gray-140)",
      "dark": "var(--color-d-gray-20)"
    },
    "--color-text-weeknum": {
      "light": "var(--color-l-gray-80)",
      "dark": "var(--color-d-gray-60)"
    },
    "--pmr-cal-day-shadow": "inset 0px 0px 0px 1px #000000",
    "--pmr-cal-day-shadow-hover": "inset 0px 0px 0px 1px #000000, 0px 0px 4px 0px #000000",
    "--pmr-cal-day-color-active": {
      "light": "var(--color-l-gray-80)",
      "dark": "var(--color-d-gray-50)"
    },
    "--pmr-cal-day-dot-opacity-active": "0.5",
    "--pmr-cal-day-background-active": {
      "light": "var(--color-l-gray-60)",
      "dark": "var(--color-d-gray-140)"
    },
    "--pmr-cal-active-day-color": {
      "light": "var(--color-l-gray-140)",
      "dark": "var(--color-d-gray-10)"
    },
    "--pmr-cal-active-day-background": {
      "light": "#ffffff",
      "dark": "#000000"
    },
    "--pmr-cal-active-day-shadow": "var(--nav-file-shadow-active)"
  },
  ".mermaid > svg": {
    "filter": {
      "light": null,
      "dark": "invert(100%) hue-rotate(180deg) saturate(1.25)"
    },
    "} .markdown-source-view.mod-cm6 .cm-hmd-codeblock { --indentation-guide-editing-indent": {
      "light": null,
      "dark": "0"
    },
    "} .markdown-source-view.mod-cm6 .cm-preview-code-block pre { margin": {
      "light": null,
      "dark": "0"
    },
    "} .markdown-source-view.mod-cm6 .code-block-flair { position": {
      "light": null,
      "dark": "absolute"
    },
    "right": { "light": null, "dark": "6px" },
    "top": { "light": null, "dark": "6px" },
    "z-index": { "light": null, "dark": "1" },
    "display": { "light": null, "dark": "inline-block" },
    "padding": { "light": null, "dark": "var(--size-4-1) var(--size-4-2)" },
    "border-radius": { "light": null, "dark": "var(--code-radius)" },
    "font-family": { "light": null, "dark": "var(--font-interface)" },
    "font-size": { "light": null, "dark": "var(--font-ui-smaller)" },
    "color": { "light": null, "dark": "var(--text-muted)" },
    "cursor": { "light": null, "dark": "var(--cursor)" }
  },
  "body": {
    "--canvas-color": { "light": null, "dark": "var(--color-gray-rgb)" }
  },
  "input[type=\"checkbox\"]:not(:checked)": {
    ".theme-light input[type=\"checkbox\"]": {
      "light": null,
      "dark": "not(:checked) { background:var(--checklist-bg)"
    }
  },
  "input[type=\"checkbox\"]:hover": {
    ".theme-light input[type=\"checkbox\"]": {
      "light": null,
      "dark": "hover { box-shadow:var(--checklist-shadow-hover)"
    }
  },
  ".is-mobile .vertical-tab-content": {
    "background": { "light": null, "dark": "var(--background-primary)" }
  }
}

{
  "body": {
    ".theme-dark, .is-mobile.theme-dark { --color-base-00": {
      "light": "var(--background)",
      "dark": null
    },
    "--color-base-05": { "light": "var(--surface)", "dark": null },
    "--color-base-10": { "light": "var(--surface-variant)", "dark": null },
    "--color-base-20": { "light": "var(--surface)", "dark": null },
    "--color-base-25": { "light": "var(--surface-variant)", "dark": null },
    "--color-base-30": { "light": "var(--surface-variant)", "dark": null },
    "--color-base-35": { "light": "var(--surface-variant)", "dark": null },
    "--color-base-40": { "light": "var(--surface-variant)", "dark": null },
    "--color-base-50": { "light": "var(--outline)", "dark": null },
    "--color-base-60": { "light": "var(--secondary)", "dark": null },
    "--color-base-70": { "light": "var(--on-surface-variant)", "dark": null },
    "--color-base-100": { "light": "var(--on-background)", "dark": null },
    "--color-accent-hsl": {
      "light": "var(--accent-h), var(--accent-s), var(--accent-l)",
      "dark": null
    },
    "--shadow-bottom": {
      "light": "0 10px 10px -10px #000000, 0 6px 6px -6px #000000",
      "dark": null
    },
    "--shadow-s": {
      "light": "0 1px 3px #000000, 0 1px 2px #000000",
      "dark": null
    },
    "--shadow-m": {
      "light": "0 14px 28px #000000, 0 10px 10px #000000",
      "dark": null
    },
    "--color-accent-2": { "light": "var(--secondary)", "dark": null }
  },
  ".theme-green": {
    "--accent-h": { "light": "94 !important", "dark": null },
    "--accent-s": { "light": "100% !important", "dark": null }
  },
  ".theme-classic": {
    "--accent-h": { "light": "256 !important", "dark": null },
    "--accent-s": { "light": "34% !important", "dark": null }
  },
  ".theme-lumen": {
    "--accent-h": { "light": "231 !important", "dark": null },
    "--accent-s": { "light": "49% !important", "dark": null }
  },
  ".is-phone": {
    ".is-phone.theme-dark { --lower-accent": {
      "light": "var(--upper-accent)",
      "dark": null
    }
  },
  ".mermaid > svg": {
    "filter": {
      "light": null,
      "dark": "invert(100%) hue-rotate(180deg) saturate(1.25)"
    },
    "} .markdown-source-view.mod-cm6 .cm-hmd-codeblock { --indentation-guide-editing-indent": {
      "light": null,
      "dark": "0"
    },
    "} .markdown-source-view.mod-cm6 .cm-preview-code-block pre { margin": {
      "light": null,
      "dark": "0"
    },
    "} .markdown-source-view.mod-cm6 .code-block-flair { position": {
      "light": null,
      "dark": "absolute"
    },
    "right": { "light": null, "dark": "6px" },
    "top": { "light": null, "dark": "6px" },
    "z-index": { "light": null, "dark": "1" },
    "display": { "light": null, "dark": "inline-block" },
    "padding": { "light": null, "dark": "var(--size-4-1) var(--size-4-2)" },
    "border-radius": { "light": null, "dark": "var(--code-radius)" },
    "font-family": { "light": null, "dark": "var(--font-interface)" },
    "font-size": { "light": null, "dark": "var(--font-ui-smaller)" },
    "color": { "light": null, "dark": "var(--text-muted)" },
    "cursor": { "light": null, "dark": "var(--cursor)" }
  },
  ".pdf-viewer-container": {
    "filter": { "light": null, "dark": "invert(100%)" }
  }
}

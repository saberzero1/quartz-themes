    "background-repeat": "repeat",
    "background-size": "182px",
    "opacity": { "light": "0.3", "dark": "0.1" },
    "top": "0",
    "left": "0",
    "position": "absolute",
    "width": "100%",
    "height": "100%",
    "pointer-events": "none"

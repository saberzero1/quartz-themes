{
  ".workspace-leaf-content": {
    "background": {
      "light": "var(--accent-color-bg2)",
      "dark": "var(--accent-color-bg_d2)"
    }
  },
  "body": {
    ".theme-dark { --accent-color-hsl": {
      "light": "hsla(var(--accent-h), var(--accent-s), var(--accent-l))",
      "dark": null
    },
    "--color-accent": {
      "light": "hsla(var(--accent-h), var(--accent-s), var(--accent-l))",
      "dark": null
    }
  },
  ".workspace-tab-header-inner-title:not(.workspace-split)": {
    "text-align": "center !important",
    "padding-top": "0.2em",
    "color": "var(--accent-color-light9)",
    "font-size": "15px"
  },
  ".nav-folder-title-content": {
    "color": { "light": "#3d3d3d", "dark": "#c1c1c1" },
    "font-weight": { "light": "800", "dark": "600" }
  },
  ".canvas-node": {
    "border-radius": "10px !important",
    "box-shadow": "0 0 10px #000000 !important"
  },
  ".callout-title": {
    "color": { "light": "black", "dark": "white" },
    "font-size": "1.1em"
  },
  ".kanban-plugin__item-title-wrapper": {
    "background": { "light": "white", "dark": "#161616" },
    "}": { "light": "", "dark": null }
  },
  ".mermaid > svg": {
    "filter": {
      "light": null,
      "dark": "invert(100%) hue-rotate(180deg) saturate(1.25)"
    },
    "} .markdown-source-view.mod-cm6 .cm-hmd-codeblock { --indentation-guide-editing-indent": {
      "light": null,
      "dark": "0"
    },
    "} .markdown-source-view.mod-cm6 .cm-preview-code-block pre { margin": {
      "light": null,
      "dark": "0"
    },
    "} .markdown-source-view.mod-cm6 .code-block-flair { position": {
      "light": null,
      "dark": "absolute"
    },
    "right": { "light": null, "dark": "6px" },
    "top": { "light": null, "dark": "6px" },
    "z-index": { "light": null, "dark": "1" },
    "display": { "light": null, "dark": "inline-block" },
    "padding": { "light": null, "dark": "var(--size-4-1) var(--size-4-2)" },
    "border-radius": { "light": null, "dark": "var(--code-radius)" },
    "font-family": { "light": null, "dark": "var(--font-interface)" },
    "font-size": { "light": null, "dark": "var(--font-ui-smaller)" },
    "color": { "light": null, "dark": "var(--text-muted)" },
    "cursor": { "light": null, "dark": "var(--cursor)" }
  }
}

{
  ".at-colorful-headings h1": {
    ".theme-light.at-colorful-headings .HyperMD-header-1 { color": {
      "light": "#4da8b2",
      "dark": null
    },
    ".theme-dark.at-colorful-headings .HyperMD-header-1 { color": {
      "light": null,
      "dark": "#70a7c2"
    }
  },
  ".at-colorful-headings h2": {
    ".theme-light.at-colorful-headings .HyperMD-header-2 { color": {
      "light": "#b24db2",
      "dark": null
    },
    ".theme-dark.at-colorful-headings .HyperMD-header-2 { color": {
      "light": null,
      "dark": "#c270c2"
    }
  },
  ".at-colorful-headings h3": {
    ".theme-light.at-colorful-headings .HyperMD-header-3 { color": {
      "light": "#c5aa26",
      "dark": null
    },
    ".theme-dark.at-colorful-headings .HyperMD-header-3 { color": {
      "light": null,
      "dark": "#d2b467"
    }
  },
  ".at-colorful-headings h4": {
    ".theme-light.at-colorful-headings .HyperMD-header-4 { color": {
      "light": "#fa3872",
      "dark": null
    },
    ".theme-dark.at-colorful-headings .HyperMD-header-4 { color": {
      "light": null,
      "dark": "#fb447b"
    }
  },
  ".at-colorful-headings h5": {
    ".theme-light.at-colorful-headings .HyperMD-header-5 { color": {
      "light": "#619726",
      "dark": null
    },
    ".theme-dark.at-colorful-headings .HyperMD-header-5 { color": {
      "light": null,
      "dark": "#a4d16a"
    }
  },
  ".at-colorful-headings h6": {
    ".theme-light.at-colorful-headings .HyperMD-header-6 { color": {
      "light": "#8f4bd2",
      "dark": null
    },
    ".theme-dark.at-colorful-headings .HyperMD-header-6 { color": {
      "light": null,
      "dark": "#d49cff"
    }
  },
  ".markdown-preview-view mark": {
    "span.search-result-file-matched-text { background": {
      "light": "linear-gradient(270deg, #fddc0a 0%, #f1eb1e 88%)",
      "dark": null
    },
    "border-radius": { "light": "var(--radius-s)", "dark": null },
    "padding": { "light": "0.05em 0.25em", "dark": null }
  },
  ".internal-embed.markdown-embed": {
    "border": { "light": "1px solid var(--color-base-20)", "dark": null }
  },
  ".token.class-name": {
    ".theme-light .token.constant, .theme-light .cm-atom, code .cm-tag, .theme-light .cm-variable-2, .theme-light .cm-type { color": {
      "light": "var(--color-blue)",
      "dark": null
    }
  },
  ".vertical-tab-header-group-title": {
    "border-bottom": {
      "light": "0.5px solid var(--color-base-30)",
      "dark": null
    }
  },
  ".setting-item": {
    "border-color": { "light": "var(--color-base-25)", "dark": null }
  },
  ".callout": {
    "background-color": {
      "light": "rgba(var(--callout-color), 0.15)",
      "dark": null
    },
    "}": { "light": "", "dark": null }
  },
  ".mermaid > svg": {
    "filter": {
      "light": null,
      "dark": "invert(100%) hue-rotate(180deg) saturate(1.25)"
    },
    "} .markdown-source-view.mod-cm6 .cm-hmd-codeblock { --indentation-guide-editing-indent": {
      "light": null,
      "dark": "0"
    },
    "} .markdown-source-view.mod-cm6 .cm-preview-code-block pre { margin": {
      "light": null,
      "dark": "0"
    },
    "} .markdown-source-view.mod-cm6 .code-block-flair { position": {
      "light": null,
      "dark": "absolute"
    },
    "right": { "light": null, "dark": "6px" },
    "top": { "light": null, "dark": "6px" },
    "z-index": { "light": null, "dark": "1" },
    "display": { "light": null, "dark": "inline-block" },
    "padding": { "light": null, "dark": "var(--size-4-1) var(--size-4-2)" },
    "border-radius": { "light": null, "dark": "var(--code-radius)" },
    "font-family": { "light": null, "dark": "var(--font-interface)" },
    "font-size": { "light": null, "dark": "var(--font-ui-smaller)" },
    "color": { "light": null, "dark": "var(--text-muted)" },
    "cursor": { "light": null, "dark": "var(--cursor)" }
  },
  "button.button-default": {
    "border-color": { "light": null, "dark": "var(--color-accent)" },
    "font-size": { "light": null, "dark": "var(--h6-size)" },
    "text-transform": { "light": null, "dark": "uppercase" }
  }
}

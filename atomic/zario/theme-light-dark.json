{
  "body": {
    "body.theme-light.is-mobile { color-scheme": {
      "light": "light",
      "dark": null
    },
    "--highlight-mix-blend-mode": { "light": "darken", "dark": "lighten" },
    "--mono-rgb-0": { "light": "255, 255, 255", "dark": "0, 0, 0" },
    "--mono-rgb-100": { "light": "0, 0, 0", "dark": "255, 255, 255" },
    "--color-red-rgb": { "light": "228, 55, 75", "dark": "251, 70, 76" },
    "--color-red": { "light": "#e4374b", "dark": "#fb464c" },
    "--color-green-rgb": { "light": "12, 181, 79", "dark": "68, 207, 110" },
    "--color-green": { "light": "#0cb54f", "dark": "#44cf6e" },
    "--color-orange": { "light": "#d96c00", "dark": "#e9973f" },
    "--color-yellow": { "light": "#bd8e37", "dark": "#e0de71" },
    "--color-cyan": { "light": "#2db7b5", "dark": "#53dfdd" },
    "--color-blue": { "light": "#086ddd", "dark": "#027aff" },
    "--color-purple": { "light": "#876be0", "dark": "#a882ff" },
    "--color-pink": { "light": "#c32b74", "dark": "#fa99cd" },
    "--color-base-00": { "light": "#ffffff", "dark": "#1c2127" },
    "--color-base-05": { "light": "#fcfcfc", "dark": null },
    "--color-base-10": { "light": "#f6f7f8", "dark": "#282c34" },
    "--color-base-20": { "light": "#f6f7f8", "dark": "#181c20" },
    "--color-base-25": { "light": "#f0f0f0", "dark": "#2c313c" },
    "--color-base-30": { "light": "#ebedf0", "dark": "#35393e" },
    "--color-base-35": { "light": "#d4d4d4", "dark": "#3f3f3f" },
    "--color-base-40": { "light": "#bdbdbd", "dark": "#555" },
    "--color-base-50": { "light": "#ababab", "dark": "#666" },
    "--color-base-60": { "light": "#707070", "dark": "#999" },
    "--color-base-70": { "light": "#5a5a5a", "dark": "#bababa" },
    "--color-base-100": { "light": "#222222", "dark": "#dadada" },
    "--color-accent-hsl": "var(--accent-h), var(--accent-s), var(--accent-l)",
    "--color-accent": "hsl(var(--accent-h), var(--accent-s), var(--accent-l))",
    "--color-accent-1": {
      "light": "hsl( var(--accent-h), var(--accent-s), calc(var(--accent-l) + 2.5%) )",
      "dark": "hsl( var(--accent-h), var(--accent-s), calc(var(--accent-l) - 3.8%) )"
    },
    "--color-accent-2": {
      "light": "hsl( var(--accent-h), var(--accent-s), calc(var(--accent-l) + 5%)",
      "dark": "hsl( var(--accent-h), var(--accent-s), calc(var(--accent-l) + 3.8%) )"
    },
    "body.theme-dark.is-mobile { color-scheme": {
      "light": null,
      "dark": "dark"
    },
    "--background-primary": { "light": null, "dark": "#12161c" },
    "--background-secondary": { "light": null, "dark": "#020409" },
    "--background-modifier-hover": {
      "light": null,
      "dark": "var(--background-primary)"
    },
    "--titlebar-background-focused": {
      "light": null,
      "dark": "var(--background-secondary)"
    },
    "--divider-color": { "light": null, "dark": "var(--background-secondary)" },
    "--background-modifier-form-field": {
      "light": null,
      "dark": "var(--color-base-25)"
    },
    "--background-secondary-alt": {
      "light": null,
      "dark": "var(--color-base-30)"
    },
    "--interactive-normal": { "light": null, "dark": "var(--color-base-30)" },
    "--interactive-hover": { "light": null, "dark": "var(--color-base-35)" },
    "--text-highlight-bg-d": { "light": null, "dark": "#ffd000" },
    "--text-highlight-bg": {
      "light": null,
      "dark": "var(--text-highlight-bg-d)"
    },
    "--text-highlight-bg-active": { "light": null, "dark": "#ff8000" },
    "--text-selection": {
      "light": null,
      "dark": "hsla(var(--interactive-accent-hsl), 0.25)"
    },
    "--input-shadow": {
      "light": null,
      "dark": "inset 0 0.5px 0.5px 0.5px #ffffff, 0 2px 4px 0 #000000, 0 1px 1.5px 0 #000000, 0 1px 2px 0 #000000, 0 0 0 0 transparent"
    },
    "--input-shadow-hover": {
      "light": null,
      "dark": "inset 0 0.5px 1px 0.5px #ffffff, 0 2px 3px 0 #000000, 0 1px 1.5px 0 #000000, 0 1px 2px 0 #000000, 0 0 0 0 transparent"
    },
    "--shadow-s": {
      "light": null,
      "dark": "0px 1px 2px #000000, 0px 3.4px 6.7px #000000, 0px 15px 30px #000000"
    },
    "--shadow-l": { "light": null, "dark": "" }
  },
  ".mermaid > svg": {
    "filter": {
      "light": null,
      "dark": "invert(100%) hue-rotate(180deg) saturate(1.25)"
    },
    "} .markdown-source-view.mod-cm6 .cm-hmd-codeblock { --indentation-guide-editing-indent": {
      "light": null,
      "dark": "0"
    },
    "} .markdown-source-view.mod-cm6 .cm-preview-code-block pre { margin": {
      "light": null,
      "dark": "0"
    },
    "} .markdown-source-view.mod-cm6 .code-block-flair { position": {
      "light": null,
      "dark": "absolute"
    },
    "right": { "light": null, "dark": "6px" },
    "top": { "light": null, "dark": "6px" },
    "z-index": { "light": null, "dark": "1" },
    "display": { "light": null, "dark": "inline-block" },
    "padding": { "light": null, "dark": "var(--size-4-1) var(--size-4-2)" },
    "border-radius": { "light": null, "dark": "var(--code-radius)" },
    "font-family": { "light": null, "dark": "var(--font-interface)" },
    "font-size": { "light": null, "dark": "var(--font-ui-smaller)" },
    "color": { "light": null, "dark": "var(--text-muted)" },
    "cursor": { "light": null, "dark": "var(--cursor)" }
  }
}

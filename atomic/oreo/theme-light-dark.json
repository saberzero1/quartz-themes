{
  ".oreo-safari-tab-toggle .workspace-split.mod-vertical.mod-root": {
    "--tab-background-inactive": { "light": "#f2f2f2", "dark": "#333333" },
    "--tab-background-active": { "light": "#ffffff", "dark": "#1a1a1a" },
    "--tab-background-inactive-hover": {
      "light": "#e0e0e0",
      "dark": "#4d4d4d"
    },
    "--tab-text-color": { "light": "#707070", "dark": "#cccccc" },
    "--tab-text-color-active": { "light": "#333333", "dark": "#ffffff" },
    "--tab-text-color-focused": { "light": "#333333", "dark": "#ffffff" },
    "--tab-text-color-focused-active": {
      "light": "#333333",
      "dark": "#ffffff"
    },
    "--tab-text-color-focused-highlighted": "#ffcc00",
    "--tab-text-color-focused-active-current": "#ffcc00",
    "--tab-divider-color": { "light": "#cccccc", "dark": "#666666" }
  },
  ".mermaid > svg": {
    "filter": {
      "light": null,
      "dark": "invert(100%) hue-rotate(180deg) saturate(1.25)"
    },
    "} .markdown-source-view.mod-cm6 .cm-hmd-codeblock { --indentation-guide-editing-indent": {
      "light": null,
      "dark": "0"
    },
    "} .markdown-source-view.mod-cm6 .cm-preview-code-block pre { margin": {
      "light": null,
      "dark": "0"
    },
    "} .markdown-source-view.mod-cm6 .code-block-flair { position": {
      "light": null,
      "dark": "absolute"
    },
    "right": { "light": null, "dark": "6px" },
    "top": { "light": null, "dark": "6px" },
    "z-index": { "light": null, "dark": "1" },
    "display": { "light": null, "dark": "inline-block" },
    "padding": { "light": null, "dark": "var(--size-4-1) var(--size-4-2)" },
    "border-radius": { "light": null, "dark": "var(--code-radius)" },
    "font-family": { "light": null, "dark": "var(--font-interface)" },
    "font-size": { "light": null, "dark": "12px" },
    "color": { "light": null, "dark": "var(--text-muted)" },
    "cursor": { "light": null, "dark": "var(--cursor)" }
  }
}

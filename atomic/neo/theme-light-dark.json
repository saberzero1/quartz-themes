{
  ".kanban-plugin": {
    "--kanban-item-background": {
      "light": "var(--base-60)",
      "dark": "var(--base-50)"
    },
    "--kanban-item-active-background": {
      "light": "var(--base-55)",
      "dark": "var(--base-60)"
    },
    "--kanban-item-new-background": "var(--base-60)"
  },
  ".cards table.dataview thead:before": {
    "background-image": {
      "light": "url('data:image/svg + xml, utf8, <svgxmlns=\"http://www.w3.org/2000/svg\"fill=\"none\"viewBox=\"0 0 100 100\"><pathfill=\"black\"d=\"M49.792 33.125l-5.892 5.892L33.333 28.45V83.333H25V28.45L14.438 39.017L8.542 33.125L29.167 12.5l20.625 20.625zm41.667 33.75L70.833 87.5l-20.625 -20.625l5.892 -5.892l10.571 10.567L66.667 16.667h8.333v54.883l10.567 -10.567l5.892 5.892z\"></path></svg>')",
      "dark": null
    }
  },
  ".mermaid > svg": {
    "filter": {
      "light": null,
      "dark": "invert(100%) hue-rotate(180deg) saturate(1.25)"
    },
    "} .markdown-source-view.mod-cm6 .cm-hmd-codeblock { --indentation-guide-editing-indent": {
      "light": null,
      "dark": "0"
    },
    "} .markdown-source-view.mod-cm6 .cm-preview-code-block pre { margin": {
      "light": null,
      "dark": "0"
    },
    "} .markdown-source-view.mod-cm6 .code-block-flair { position": {
      "light": null,
      "dark": "absolute"
    },
    "right": { "light": null, "dark": "6px" },
    "top": { "light": null, "dark": "6px" },
    "z-index": { "light": null, "dark": "1" },
    "display": { "light": null, "dark": "inline-block" },
    "padding": { "light": null, "dark": "var(--size-4-1) var(--size-4-2)" },
    "border-radius": { "light": null, "dark": "var(--code-radius)" },
    "font-family": { "light": null, "dark": "var(--font-interface)" },
    "font-size": { "light": null, "dark": "var(--font-ui-smaller)" },
    "color": { "light": null, "dark": "var(--text-muted)" },
    "cursor": { "light": null, "dark": "var(--cursor)" }
  },
  "body": { ".theme-light {": { "light": null, "dark": "" } },
  ".darker": {
    "--base-05": {
      "light": null,
      "dark": "hsl(var(--base-hue), var(--base-sat, 16%), 90%)"
    },
    "--base-10": {
      "light": null,
      "dark": "hsl(var(--base-hue), var(--base-sat, 14%), 79%)"
    },
    "--base-15": {
      "light": null,
      "dark": "hsl(var(--base-hue), var(--base-sat, 12%), 70%)"
    },
    "--base-20": {
      "light": null,
      "dark": "hsl(var(--base-hue), var(--base-sat, 12%), 60%)"
    },
    "--base-25": {
      "light": null,
      "dark": "hsl(var(--base-hue), var(--base-sat, 10%), 49%)"
    },
    "--base-30": {
      "light": null,
      "dark": "hsl(var(--base-hue), var(--base-sat, 10%), 37%)"
    },
    "--base-35": {
      "light": null,
      "dark": "hsl(var(--base-hue), var(--base-sat, 12%), 26%)"
    },
    "--base-40": {
      "light": null,
      "dark": "hsl(var(--base-hue), var(--base-sat, 13%), 19%)"
    },
    "--base-45": {
      "light": null,
      "dark": "hsl(var(--base-hue), var(--base-sat, 12%), 15%)"
    },
    "--base-50": {
      "light": null,
      "dark": "hsl(var(--base-hue), var(--base-sat, 10%), 12%)"
    },
    "--base-55": {
      "light": null,
      "dark": "hsl(var(--base-hue), var(--base-sat, 8%), 8%)"
    }
  }
}

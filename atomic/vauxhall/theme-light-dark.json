{
  "body": {
    "--background-modifier-form-field": {
      "light": "var(--color-base-20)",
      "dark": null
    },
    "--background-modifier-hover": {
      "light": "var(--color-base-35)",
      "dark": null
    },
    "--background-primary": { "light": "var(--color-base-05)", "dark": null },
    "--background-secondary": { "light": "var(--color-base-05)", "dark": null },
    "--black": { "light": "#000", "dark": null },
    "--blue": { "light": "#437cf3", "dark": null },
    "--blue-violet": { "light": "#6f51f4", "dark": null },
    "--code-background": { "light": "var(--color-base-30)", "dark": null },
    "--color-base-00": {
      "light": "color-mix(in srgb, var(--vauxhall-bg) 25%, var(--white))",
      "dark": null
    },
    "--color-base-05": {
      "light": "color-mix(in srgb, var(--vauxhall-bg) 40%, var(--white))",
      "dark": null
    },
    "--color-base-10": {
      "light": "color-mix(in srgb, var(--vauxhall-bg) 50%, var(--white))",
      "dark": null
    },
    "--color-base-100": {
      "light": "color-mix( in srgb, var(--vauxhall-bg) 0%, var(--vauxhall-fg) )",
      "dark": null
    },
    "--color-base-20": {
      "light": "color-mix(in srgb, var(--vauxhall-bg) 80%, var(--white))",
      "dark": null
    },
    "--color-base-25": {
      "light": "color-mix(in srgb, var(--vauxhall-bg) 80%, var(--white))",
      "dark": null
    },
    "--color-base-30": {
      "light": "color-mix( in srgb, var(--vauxhall-bg) 90%, var(--vauxhall-fg) )",
      "dark": null
    },
    "--color-base-35": {
      "light": "color-mix( in srgb, var(--vauxhall-bg) 95%, var(--vauxhall-fg) )",
      "dark": null
    },
    "--color-base-40": {
      "light": "color-mix( in srgb, var(--vauxhall-bg) 80%, var(--vauxhall-fg) )",
      "dark": null
    },
    "--color-base-50": {
      "light": "color-mix( in srgb, var(--vauxhall-bg) 60%, var(--vauxhall-fg) )",
      "dark": null
    },
    "--color-base-60": {
      "light": "color-mix( in srgb, var(--vauxhall-bg) 60%, var(--vauxhall-fg) )",
      "dark": null
    },
    "--color-base-70": {
      "light": "color-mix( in srgb, var(--vauxhall-bg) 30%, var(--vauxhall-fg) )",
      "dark": null
    },
    "--color-blue": { "light": "var(--blue)", "dark": null },
    "--color-cyan": { "light": "var(--cool-cyan)", "dark": null },
    "--color-green": { "light": "var(--mint)", "dark": null },
    "--color-orange": { "light": "#fa9f50", "dark": null },
    "--color-pink": { "light": "var(--magenta)", "dark": null },
    "--color-purple": { "light": "var(--violet)", "dark": null },
    "--color-red": { "light": "var(--hot-red)", "dark": null },
    "--color-yellow": { "light": "#ffd85e", "dark": null },
    "--cool-cyan": { "light": "#3bb2ce", "dark": null },
    "--cool-gray": { "light": "#515768", "dark": null },
    "--cyan": { "light": "#47c1cd", "dark": null },
    "--h1-color": { "light": "var(--cool-cyan)", "dark": null },
    "--h2-color": { "light": "var(--light-blue)", "dark": null },
    "--h3-color": { "light": "var(--blue)", "dark": null },
    "--h4-color": { "light": "var(--blue-violet)", "dark": null },
    "--h5-color": { "light": "var(--violet)", "dark": null },
    "--h6-color": { "light": "var(--purple)", "dark": null },
    "--hot-red": { "light": "#e3365e", "dark": null },
    "--hue-bg": { "light": "238", "dark": null },
    "--hue-fg": { "light": "255", "dark": null },
    "--indentation-guide-color": {
      "light": "var(--color-base-30)",
      "dark": null
    },
    "--indentation-guide-color-active": {
      "light": "var(--color-base-35)",
      "dark": null
    },
    "--light-blue": { "light": "#54b6f8", "dark": null },
    "--lum": { "light": "11%", "dark": null },
    "--lum-bg": { "light": "95%", "dark": null },
    "--lum-fg": { "light": "50%", "dark": null },
    "--magenta": { "light": "#e54f9b", "dark": null },
    "--mint": { "light": "#47cc91", "dark": null },
    "--nav-indentation-guide-color": {
      "light": "var(--color-base-30)",
      "dark": null
    },
    "--purple": { "light": "#c952ed", "dark": null },
    "--sat-bg": { "light": "60%", "dark": null },
    "--sat-fg": { "light": "100%", "dark": null },
    "--text-highlight-bg": {
      "light": "color-mix(in srgb, var(--h1-color) 25%, transparent)",
      "dark": null
    },
    "--text-highlight-bg-active": {
      "light": "color-mix( in srgb, var(--h1-color) 25%, transparent )",
      "dark": null
    },
    "--text-normal": {
      "light": "color-mix(in srgb, var(--vauxhall-fg) 50%, var(--black))",
      "dark": null
    },
    "--titlebar-background": { "light": "var(--color-base-00)", "dark": null },
    "--titlebar-background-focused": {
      "light": "var(--color-base-10)",
      "dark": null
    },
    "--vauxhall-bg": {
      "light": "hsl(var(--hue-bg), var(--sat-bg), var(--lum-bg))",
      "dark": null
    },
    "--vauxhall-fg": {
      "light": "hsl(var(--hue-fg), var(--sat-fg), var(--lum-fg))",
      "dark": null
    },
    "--violet": { "light": "#9446f8", "dark": null },
    "--white": { "light": "#fff", "dark": null },
    "--canvas-color": { "light": null, "dark": "126, 126, 126" }
  },
  ".notice progress[value]::-webkit-progress-bar": {
    "background-color": { "light": "#262626", "dark": null },
    "box-shadow": { "light": "inset 0px 0px 0px 1px #363636", "dark": null }
  },
  ".dropdown": { "background-image": { "light": null, "dark": "url(\"data" } },
  ".pdf-container.mod-themed .annotationLayer .textWidgetAnnotation input": {
    "filter": { "light": null, "dark": "invert(100%) hue-rotate(180deg)" }
  },
  ".pdf-container.mod-themed .annotationLayer .textWidgetAnnotation textarea": {
    "filter": { "light": null, "dark": "invert(100%) hue-rotate(180deg)" }
  },
  ".pdf-container.mod-themed .popupWrapper > div": {
    "background-color": {
      "light": null,
      "dark": "var(--background-secondary-alt)"
    },
    "filter": {
      "light": null,
      "dark": "drop-shadow(0px 0px 1px var(--color-base-60)) drop-shadow(0px 1px 2px rgba(0, 0, 0, 0.4)) drop-shadow(0px 4px 6px rgba(0, 0, 0, 0.4))"
    }
  },
  ".pdf-container.mod-themed .popupWrapper > div::after": {
    "background-color": {
      "light": null,
      "dark": "var(--background-secondary-alt)"
    }
  },
  ".pdf-container.mod-themed .popupContent:empty + .popupMeta": {
    "background-color": {
      "light": null,
      "dark": "var(--background-secondary-alt)"
    }
  },
  ".mermaid > svg": {
    "filter": {
      "light": null,
      "dark": "invert(100%) hue-rotate(180deg) saturate(1.25)"
    }
  },
  ".canvas-card-menu": {
    "background-color": { "light": null, "dark": "var(--background-secondary)" }
  },
  ".canvas-card-menu .canvas-card-menu-button svg": {
    "fill": { "light": null, "dark": "var(--background-secondary)" }
  },
  ".is-mobile": {
    "--background-modifier-cover": {
      "light": null,
      "dark": "rgba(0, 0, 0, 0.5)"
    },
    "--background-modifier-form-field": {
      "light": null,
      "dark": "var(--background-modifier-border)"
    },
    "--background-modifier-hover": {
      "light": null,
      "dark": "rgba(var(--mono-rgb-100), 0.15)"
    },
    "--background-modifier-message": {
      "light": null,
      "dark": "var(--background-secondary)"
    },
    "--color-base-00": { "light": null, "dark": "#000" },
    "--color-base-10": { "light": null, "dark": "#111" },
    "--color-base-20": { "light": null, "dark": "#1e1e1e" },
    "--mobile-sidebar-background": {
      "light": null,
      "dark": "var(--background-secondary)"
    },
    "--search-result-background": {
      "light": null,
      "dark": "var(--background-secondary)"
    },
    "--settings-home-background": {
      "light": null,
      "dark": "var(--background-primary)"
    },
    "--tag-background": {
      "light": null,
      "dark": "hsla(var(--interactive-accent-hsl), 0.2)"
    }
  },
  ".is-tablet": {
    "--interactive-hover": {
      "light": null,
      "dark": "var(--background-modifier-border-hover)"
    },
    "--interactive-normal": {
      "light": null,
      "dark": "var(--background-modifier-border)"
    },
    "--modal-background": {
      "light": null,
      "dark": "var(--background-secondary)"
    },
    "--titlebar-background": {
      "light": null,
      "dark": "var(--background-primary)"
    },
    "--titlebar-background-focused": {
      "light": null,
      "dark": "var(--background-primary)"
    }
  },
  ".is-tablet .community-item": {
    "background-color": { "light": null, "dark": "var(--background-secondary)" }
  },
  ".is-tablet .vertical-tab-content": {
    "background-color": { "light": null, "dark": "var(--background-secondary)" }
  },
  ".workspace-drawer-inner": {
    "background-color": { "light": null, "dark": "var(--background-secondary)" }
  },
  ".is-mobile .prompt": {
    "background-color": { "light": null, "dark": "var(--background-secondary)" }
  },
  ".is-mobile .prompt input.prompt-input": {
    "background-color": { "light": null, "dark": "var(--background-secondary)" }
  },
  ".is-tablet .suggestion-container": {
    "background-color": { "light": null, "dark": "var(--background-secondary)" }
  }
}

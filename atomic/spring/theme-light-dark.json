{
  "body": {
    "--background-primary": { "light": "#f0f0f0", "dark": null },
    "--background-secondary": { "light": "#e6e6fa", "dark": null },
    "--bold-color": { "light": "var(--dark-yellow)", "dark": null },
    "--checkbox-border-color": { "light": "black", "dark": null },
    "--code-comment": { "light": "#a9a9a9", "dark": null },
    "--code-function": { "light": "#228b22", "dark": null },
    "--code-important": { "light": "#8b008b", "dark": null },
    "--code-keyword": { "light": "#4682b4", "dark": null },
    "--code-normal": { "light": "#ff4500", "dark": null },
    "--code-operator": { "light": "#2f4f4f", "dark": null },
    "--code-property": { "light": "#696969", "dark": null },
    "--code-punctuation": { "light": "#696969", "dark": null },
    "--code-string": { "light": "#8b008b", "dark": null },
    "--code-tag": { "light": "#228b22", "dark": null },
    "--code-value": { "light": "#008000", "dark": null },
    "--graph-line": { "light": "black", "dark": null },
    "--graph-node-unresolved": {
      "light": "var(--background-primary)",
      "dark": null
    },
    "--h1-color": { "light": "var(--dark-green)", "dark": null },
    "--h2-color": { "light": "var(--purple)", "dark": null },
    "--h3-color": { "light": "var(--coral)", "dark": null },
    "--h4-color": { "light": "var(--pink)", "dark": null },
    "--h5-color": { "light": "var(--dark-yellow)", "dark": null },
    "--icon-color-active": { "light": "var(--dark-green)", "dark": null },
    "--icon-color-focused": { "light": "var(--dark-green)", "dark": null },
    "--italic-color": { "light": "var(--dark-yellow)", "dark": null },
    "--link-color": { "light": "var(--dark-yellow)", "dark": null },
    "--link-external-color": { "light": "var(--dark-yellow)", "dark": null },
    "--link-unresolved-color": { "light": "var(--code-keyword)", "dark": null },
    "--nav-collapse-icon-color": { "light": "var(--dark-green)", "dark": null },
    "--nav-item-color": { "light": "#171717", "dark": null },
    "--nav-item-color-active": { "light": "var(--dark-green)", "dark": null },
    "--nav-item-color-hover": { "light": "var(--dark-green)", "dark": null },
    "--nav-item-color-selected": { "light": "var(--dark-green)", "dark": null },
    "--ribbon-background": { "light": "#f0f0f0", "dark": null },
    "--tab-text-color": { "light": "var(--dark-green)", "dark": null },
    "--tab-text-color-active": { "light": "var(--dark-green)", "dark": null },
    "--tab-text-color-focused-active-current": {
      "light": "var(--dark-green)",
      "dark": null
    },
    "--text-selection": { "light": "var(--dark-yellow)", "dark": null },
    "--canvas-color": { "light": null, "dark": "126, 126, 126" }
  },
  ".notice progress[value]::-webkit-progress-bar": {
    "background-color": { "light": "#262626", "dark": null },
    "box-shadow": { "light": "inset 0px 0px 0px 1px #363636", "dark": null }
  },
  ".dropdown": { "background-image": { "light": null, "dark": "url(\"data" } },
  ".pdf-container.mod-themed .annotationLayer .textWidgetAnnotation input": {
    "filter": { "light": null, "dark": "invert(100%) hue-rotate(180deg)" }
  },
  ".pdf-container.mod-themed .annotationLayer .textWidgetAnnotation textarea": {
    "filter": { "light": null, "dark": "invert(100%) hue-rotate(180deg)" }
  },
  ".pdf-container.mod-themed .popupWrapper > div": {
    "background-color": {
      "light": null,
      "dark": "var(--background-secondary-alt)"
    },
    "filter": {
      "light": null,
      "dark": "drop-shadow(0px 0px 1px var(--color-base-60)) drop-shadow(0px 1px 2px rgba(0, 0, 0, 0.4)) drop-shadow(0px 4px 6px rgba(0, 0, 0, 0.4))"
    }
  },
  ".pdf-container.mod-themed .popupWrapper > div::after": {
    "background-color": {
      "light": null,
      "dark": "var(--background-secondary-alt)"
    }
  },
  ".pdf-container.mod-themed .popupContent:empty + .popupMeta": {
    "background-color": {
      "light": null,
      "dark": "var(--background-secondary-alt)"
    }
  },
  ".mermaid > svg": {
    "filter": {
      "light": null,
      "dark": "invert(100%) hue-rotate(180deg) saturate(1.25)"
    }
  },
  ".canvas-card-menu": {
    "background-color": { "light": null, "dark": "var(--background-secondary)" }
  },
  ".canvas-card-menu .canvas-card-menu-button svg": {
    "fill": { "light": null, "dark": "var(--background-secondary)" }
  },
  ".is-mobile": {
    "--background-modifier-cover": {
      "light": null,
      "dark": "rgba(0, 0, 0, 0.5)"
    },
    "--background-modifier-form-field": {
      "light": null,
      "dark": "var(--background-modifier-border)"
    },
    "--background-modifier-hover": {
      "light": null,
      "dark": "rgba(var(--mono-rgb-100), 0.15)"
    },
    "--background-modifier-message": {
      "light": null,
      "dark": "var(--background-secondary)"
    },
    "--color-base-00": { "light": null, "dark": "#000" },
    "--color-base-10": { "light": null, "dark": "#111" },
    "--color-base-20": { "light": null, "dark": "#1e1e1e" },
    "--mobile-sidebar-background": {
      "light": null,
      "dark": "var(--background-secondary)"
    },
    "--search-result-background": {
      "light": null,
      "dark": "var(--background-secondary)"
    },
    "--settings-home-background": {
      "light": null,
      "dark": "var(--background-primary)"
    },
    "--tag-background": {
      "light": null,
      "dark": "hsla(var(--interactive-accent-hsl), 0.2)"
    }
  },
  ".is-tablet": {
    "--interactive-hover": {
      "light": null,
      "dark": "var(--background-modifier-border-hover)"
    },
    "--interactive-normal": {
      "light": null,
      "dark": "var(--background-modifier-border)"
    },
    "--modal-background": {
      "light": null,
      "dark": "var(--background-secondary)"
    },
    "--titlebar-background": {
      "light": null,
      "dark": "var(--background-primary)"
    },
    "--titlebar-background-focused": {
      "light": null,
      "dark": "var(--background-primary)"
    }
  },
  ".is-tablet .community-item": {
    "background-color": { "light": null, "dark": "var(--background-secondary)" }
  },
  ".is-tablet .vertical-tab-content": {
    "background-color": { "light": null, "dark": "var(--background-secondary)" }
  },
  ".workspace-drawer-inner": {
    "background-color": { "light": null, "dark": "var(--background-secondary)" }
  },
  ".is-mobile .prompt": {
    "background-color": { "light": null, "dark": "var(--background-secondary)" }
  },
  ".is-mobile .prompt input.prompt-input": {
    "background-color": { "light": null, "dark": "var(--background-secondary)" }
  },
  ".is-tablet .suggestion-container": {
    "background-color": { "light": null, "dark": "var(--background-secondary)" }
  }
}

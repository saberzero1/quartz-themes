    "--interactive-accent-hover": {
      "light": "var(--col-accent-plus)",
      "dark": null
    },
    "--interactive-accent-hsl": { "light": "var(--col-accent)", "dark": null },
    "--interactive-hover": { "light": "var(--col-muted)", "dark": null },
    "--interactive-normal": { "light": "var(--col-base)", "dark": null },
    "--text-accent": { "light": "var(--col-accent-plus)", "dark": null },
    "--text-accent-hover": {
      "light": "var(--col-important-plus)",
      "dark": null
    },
    "--text-error": { "light": "var(--col-important)", "dark": null },
    "--text-faint": { "light": "var(--col-accent-plus)", "dark": null },
    "--text-muted": { "light": "var(--col-accent-plus)", "dark": null },

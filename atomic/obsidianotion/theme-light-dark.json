{
  "body": {
    "--color-base-23": { "light": "#eee", "dark": null },
    "--content-bgcolor": { "light": "#ffffff", "dark": null },
    "--default-border-color": { "light": "#000000", "dark": null },
    "--heading-text-color": { "light": "#37352f", "dark": null }
  },
  "body .table-view-table > thead > tr > th": {
    "background": { "light": "#000000", "dark": "#ffffff" },
    "border-bottom": {
      "light": null,
      "dark": "2px solid var(--default-border-color)"
    }
  },
  ".mermaid > svg": {
    "filter": {
      "light": null,
      "dark": "invert(100%) hue-rotate(180deg) saturate(1.25)"
    },
    "} .markdown-source-view.mod-cm6 .cm-hmd-codeblock { --indentation-guide-editing-indent": {
      "light": null,
      "dark": "0"
    },
    "} .markdown-source-view.mod-cm6 .cm-preview-code-block pre { margin": {
      "light": null,
      "dark": "0"
    },
    "} .markdown-source-view.mod-cm6 .code-block-flair { position": {
      "light": null,
      "dark": "absolute"
    },
    "right": { "light": null, "dark": "6px" },
    "top": { "light": null, "dark": "6px" },
    "z-index": { "light": null, "dark": "1" },
    "display": { "light": null, "dark": "inline-block" },
    "padding": { "light": null, "dark": "var(--size-4-1) var(--size-4-2)" },
    "border-radius": { "light": null, "dark": "var(--code-radius)" },
    "font-family": { "light": null, "dark": "var(--font-interface)" },
    "font-size": { "light": null, "dark": "var(--font-ui-smaller)" },
    "color": { "light": null, "dark": "var(--text-muted)" },
    "cursor": { "light": null, "dark": "var(--cursor)" }
  },
  ".callout[data-callout-metadata*=\"ghostwhite\"] .callout-title": {
    ".theme-dark .callout[data-callout-metadata*=\"white\"] .callout-title, .theme-dark .callout[data-callout-metadata*=\"floralwhite\"] .callout-title, .theme-dark .callout[data-callout-metadata*=\"antiquewhite\"] .callout-title, .theme-dark .callout[data-callout-metadata*=\"papayawhip\"] .callout-title, .theme-dark .callout[data-callout-metadata*=\"whitesmoke\"] .callout-title { color": {
      "light": null,
      "dark": "rgb(var(--color-black-rgb))"
    }
  },
  ".callout[data-callout-metadata*=\"papayawhip\"]": {
    ".theme-dark .callout[data-callout-metadata*=\"white\"] { color": {
      "light": null,
      "dark": "#000"
    }
  },
  ".markdown-rendered p": { "color": { "light": null, "dark": "#c8c8c8" } },
  ".multi-select-pill": {
    ".theme-dark .multi-select-pill-content { background": {
      "light": null,
      "dark": "transparent"
    },
    "color": { "light": null, "dark": "#fff" }
  },
  ".multi-select-pill-remove-button": {
    "color": { "light": null, "dark": "#ffffff" }
  },
  ".markdown-source-view.mod-cm6 .cm-contentContainer": {
    "color": { "light": null, "dark": "#c8c8c8" }
  },
  ".nav-files-container": {
    "background-color": { "light": null, "dark": "var(--color-base-25)" }
  },
  ".mk-tree-item.nav-folder-title.is-active.is-selected": {
    ".theme-dark .mk-tree-item.nav-file-title.is-active.is-selected { color": {
      "light": null,
      "dark": "#fff"
    }
  },
  ".callout[data-callout-metadata] .callout-content": {
    "color": { "light": null, "dark": "#ffffff" }
  }
}

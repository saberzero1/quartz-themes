{
  ".callout-style-1": {
    "--callout-border-width": "1px",
    "--callout-border-opacity": "0.25",
    "--callout-padding": "0",
    "--callout-radius": "var(--radius-s)",
    "--callout-title-color": "inherit",
    "--callout-title-padding": "8px 16px",
    "--callout-title-size": "inherit",
    "--callout-content-padding": "0px 16px",
    "--callout-content-background": {
      "light": "#ffffffbf",
      "dark": "#00000040"
    },
    "--callout-content-radius": "0px"
  },
  ".callout-style-2": {
    "--callout-border-width": "1px",
    "--callout-border-opacity": "0.25",
    "--callout-padding": "0 6px 6px",
    "--callout-radius": "var(--radius-s)",
    "--callout-title-color": "inherit",
    "--callout-title-padding": "8px 16px",
    "--callout-title-size": "inherit",
    "--callout-content-padding": "0px 16px",
    "--callout-content-background": {
      "light": "#ffffffbf",
      "dark": "#00000040"
    },
    "--callout-content-radius": "4px"
  },
  ".callout-style-3": {
    ".callout-style-3.theme-dark { --callout-border-width": {
      "light": "0 0 0 4px",
      "dark": null
    },
    "--callout-border-opacity": { "light": "1", "dark": null },
    "--callout-padding": {
      "light": "var(--size-4-3) var(--size-4-3) var(--size-4-3) var(--size-4-6)",
      "dark": null
    },
    "--callout-radius": { "light": "var(--radius-s)", "dark": null },
    "--callout-title-color": { "light": "inherit", "dark": null },
    "--callout-title-padding": { "light": "0", "dark": null },
    "--callout-title-size": { "light": "inherit", "dark": null },
    "--callout-content-padding": { "light": "0", "dark": null },
    "--callout-content-background": { "light": "transparent", "dark": null },
    "--callout-content-radius": { "light": "0px", "dark": null }
  },
  ".callout-style-4": {
    "--callout-border-width": "0 0 0 4px",
    "--callout-border-opacity": "1",
    "--callout-padding": "0",
    "--callout-radius": "var(--radius-s)",
    "--callout-title-color": "inherit",
    "--callout-title-padding": "8px 16px",
    "--callout-title-size": "inherit",
    "--callout-content-padding": "0px 16px",
    "--callout-content-background": {
      "light": "#ffffffbf",
      "dark": "#00000040"
    },
    "--callout-content-radius": "0px"
  },
  ".mermaid > svg": {
    "filter": {
      "light": null,
      "dark": "invert(100%) hue-rotate(180deg) saturate(1.25)"
    },
    "} .markdown-source-view.mod-cm6 .cm-hmd-codeblock { --indentation-guide-editing-indent": {
      "light": null,
      "dark": "0"
    },
    "} .markdown-source-view.mod-cm6 .cm-preview-code-block pre { margin": {
      "light": null,
      "dark": "0"
    },
    "} .markdown-source-view.mod-cm6 .code-block-flair { position": {
      "light": null,
      "dark": "absolute"
    },
    "right": { "light": null, "dark": "6px" },
    "top": { "light": null, "dark": "6px" },
    "z-index": { "light": null, "dark": "1" },
    "display": { "light": null, "dark": "inline-block" },
    "padding": { "light": null, "dark": "var(--size-4-1) var(--size-4-2)" },
    "border-radius": { "light": null, "dark": "var(--code-radius)" },
    "font-family": { "light": null, "dark": "var(--font-interface)" },
    "font-size": { "light": null, "dark": "var(--font-ui-smaller)" },
    "color": { "light": null, "dark": "var(--text-muted)" },
    "cursor": { "light": null, "dark": "var(--cursor)" }
  },
  "body": {
    ".theme-light .markdown-rendered thead tr { background": {
      "light": null,
      "dark": "var(--table-header-background)"
    }
  }
}

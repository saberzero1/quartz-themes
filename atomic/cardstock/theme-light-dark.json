{
  "body": {
    "--alternate-lights": { "light": "var(--light-beige)", "dark": null },
    "--background-modifier-active-hover": {
      "light": "var(--dark-beige)",
      "dark": null
    },
    "--background-modifier-border": { "light": "var(--beige)", "dark": null },
    "--background-modifier-border-focus": {
      "light": "var(--red-deep)",
      "dark": null
    },
    "--background-modifier-border-hover": {
      "light": "var(--red-deep)",
      "dark": null
    },
    "--background-modifier-error": { "light": "var(--red)", "dark": null },
    "--background-modifier-error-hover": {
      "light": "var(--red-deep)",
      "dark": null
    },
    "--background-modifier-error-rgb": {
      "light": "var(--red-rgb)",
      "dark": null
    },
    "--background-modifier-form-field": {
      "light": "var(--main-bg)",
      "dark": null
    },
    "--background-modifier-hover": { "light": "var(--beige)", "dark": null },
    "--background-modifier-message": { "light": "var(--red)", "dark": null },
    "--background-modifier-success": { "light": "var(--red)", "dark": null },
    "--background-modifier-success-rgb": {
      "light": "var(--red-rgb)",
      "dark": null
    },
    "--background-primary": { "light": "var(--main-bg)", "dark": null },
    "--background-primary-alt": { "light": "var(--beige)", "dark": null },
    "--background-secondary": { "light": "var(--main-bg)", "dark": null },
    "--background-secondary-alt": { "light": "var(--beige)", "dark": null },
    "--code-background": { "light": "var(--light-beige)", "dark": null },
    "--darky-dark": { "light": "var(--dark-beige)", "dark": null },
    "--handle-icon": { "light": "#000000", "dark": null },
    "--highlight-mix-blend-mode": { "light": "darken", "dark": null },
    "--interactive-accent": { "light": "var(--red)", "dark": null },
    "--interactive-accent-hover": { "light": "var(--red)", "dark": null },
    "--interactive-accent-hsl": { "light": "var(--red-hsl)", "dark": null },
    "--interactive-hover": { "light": "var(--light-beige)", "dark": null },
    "--interactive-normal": { "light": "var(--beige)", "dark": null },
    "--modal-buttons": { "light": "var(--text-faint)", "dark": null },
    "--nav-item-background-active": { "light": "var(--red)", "dark": null },
    "--nav-item-background-hover": {
      "light": "var(--dark-beige)",
      "dark": null
    },
    "--nav-item-background-selected": { "light": "var(--red)", "dark": null },
    "--nav-item-color": { "light": "var(--text-muted)", "dark": null },
    "--nav-item-color-active": { "light": "var(--main-bg)", "dark": null },
    "--nav-item-color-highlighted": { "light": "var(--main-bg)", "dark": null },
    "--nav-item-color-hover": { "light": "var(--text)", "dark": null },
    "--nav-item-color-selected": { "light": "var(--main-bg)", "dark": null },
    "--pre-bg": { "light": "var(--light-beige)", "dark": null },
    "--scrollbar-active-thumb-bg": {
      "light": "rgba(0, 0, 0, 0.2)",
      "dark": null
    },
    "--scrollbar-bg": { "light": "rgba(0, 0, 0, 0.05)", "dark": null },
    "--scrollbar-thumb-bg": { "light": "rgba(0, 0, 0, 0.1)", "dark": null },
    "--text-accent": { "light": "var(--red)", "dark": null },
    "--text-accent-hover": { "light": "var(--red-deep)", "dark": null },
    "--text-error": { "light": "var(--red)", "dark": null },
    "--text-faint": { "light": "var(--light-text)", "dark": null },
    "--text-highlight-bg": { "light": "var(--highlight)", "dark": null },
    "--text-muted": { "light": "var(--light-text)", "dark": null },
    "--text-normal": { "light": "var(--text)", "dark": null },
    "--text-selection": { "light": "var(--dark-beige)", "dark": null },
    "--text-success": { "light": "var(--green)", "dark": null },
    "--canvas-color": { "light": null, "dark": "126, 126, 126" }
  },
  ".notice progress[value]::-webkit-progress-bar": {
    "background-color": { "light": "#262626", "dark": null },
    "box-shadow": { "light": "inset 0px 0px 0px 1px #363636", "dark": null }
  },
  ".nav-buttons-container": {
    "background": {
      "light": "var(--alternate-lights)",
      "dark": "var(--background-primary)"
    }
  },
  ".dropdown": { "background-image": { "light": null, "dark": "url(\"data" } },
  ".pdf-container.mod-themed .annotationLayer .textWidgetAnnotation input": {
    "filter": { "light": null, "dark": "invert(100%) hue-rotate(180deg)" }
  },
  ".pdf-container.mod-themed .annotationLayer .textWidgetAnnotation textarea": {
    "filter": { "light": null, "dark": "invert(100%) hue-rotate(180deg)" }
  },
  ".pdf-container.mod-themed .popupWrapper > div": {
    "background-color": {
      "light": null,
      "dark": "var(--background-secondary-alt)"
    },
    "filter": {
      "light": null,
      "dark": "drop-shadow(0px 0px 1px var(--color-base-60)) drop-shadow(0px 1px 2px rgba(0, 0, 0, 0.4)) drop-shadow(0px 4px 6px rgba(0, 0, 0, 0.4))"
    }
  },
  ".pdf-container.mod-themed .popupWrapper > div::after": {
    "background-color": {
      "light": null,
      "dark": "var(--background-secondary-alt)"
    }
  },
  ".pdf-container.mod-themed .popupContent:empty + .popupMeta": {
    "background-color": {
      "light": null,
      "dark": "var(--background-secondary-alt)"
    }
  },
  ".mermaid > svg": {
    "filter": {
      "light": null,
      "dark": "invert(100%) hue-rotate(180deg) saturate(1.25)"
    }
  },
  ".canvas-card-menu": {
    "background-color": { "light": null, "dark": "var(--background-secondary)" }
  },
  ".canvas-card-menu .canvas-card-menu-button svg": {
    "fill": { "light": null, "dark": "var(--background-secondary)" }
  },
  ".is-mobile": {
    "--background-modifier-cover": {
      "light": null,
      "dark": "rgba(0, 0, 0, 0.5)"
    },
    "--background-modifier-form-field": {
      "light": null,
      "dark": "var(--background-modifier-border)"
    },
    "--background-modifier-hover": {
      "light": null,
      "dark": "rgba(var(--mono-rgb-100), 0.15)"
    },
    "--background-modifier-message": {
      "light": null,
      "dark": "var(--background-secondary)"
    },
    "--color-base-00": { "light": null, "dark": "#000" },
    "--color-base-10": { "light": null, "dark": "#111" },
    "--color-base-20": { "light": null, "dark": "#1e1e1e" },
    "--mobile-sidebar-background": {
      "light": null,
      "dark": "var(--background-secondary)"
    },
    "--search-result-background": {
      "light": null,
      "dark": "var(--background-secondary)"
    },
    "--settings-home-background": {
      "light": null,
      "dark": "var(--background-primary)"
    },
    "--tag-background": {
      "light": null,
      "dark": "hsla(var(--interactive-accent-hsl), 0.2)"
    }
  },
  ".is-tablet": {
    "--interactive-hover": {
      "light": null,
      "dark": "var(--background-modifier-border-hover)"
    },
    "--interactive-normal": {
      "light": null,
      "dark": "var(--background-modifier-border)"
    },
    "--modal-background": {
      "light": null,
      "dark": "var(--background-secondary)"
    },
    "--titlebar-background": {
      "light": null,
      "dark": "var(--background-primary)"
    },
    "--titlebar-background-focused": {
      "light": null,
      "dark": "var(--background-primary)"
    }
  },
  ".is-tablet .community-item": {
    "background-color": { "light": null, "dark": "var(--background-secondary)" }
  },
  ".is-tablet .vertical-tab-content": {
    "background-color": { "light": null, "dark": "var(--background-secondary)" }
  },
  ".workspace-drawer-inner": {
    "background-color": { "light": null, "dark": "var(--background-secondary)" }
  },
  ".is-mobile .prompt": {
    "background-color": { "light": null, "dark": "var(--background-secondary)" }
  },
  ".is-mobile .prompt input.prompt-input": {
    "background-color": { "light": null, "dark": "var(--background-secondary)" }
  },
  ".is-tablet .suggestion-container": {
    "background-color": { "light": null, "dark": "var(--background-secondary)" }
  }
}

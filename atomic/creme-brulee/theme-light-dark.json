{
  "body": {
    "color-scheme": { "light": "light", "dark": null },
    "--highlight-mix-blend-mode": { "light": "darken", "dark": null },
    "--mono-rgb-0": { "light": "255, 255, 255", "dark": null },
    "--mono-rgb-100": { "light": "0, 0, 0", "dark": null },
    "--color-base-00": { "light": "#ffffff", "dark": null },
    "--color-base-05": { "light": "#fcfcfc", "dark": null },
    "--color-base-10": { "light": "#fafafa", "dark": null },
    "--color-base-20": { "light": "#f6f6f6", "dark": null },
    "--color-base-25": { "light": "#e3e3e3", "dark": null },
    "--color-base-30": { "light": "#e0e0e0", "dark": null },
    "--color-base-35": { "light": "#d4d4d4", "dark": null },
    "--color-base-40": { "light": "#bdbdbd", "dark": null },
    "--color-base-50": { "light": "#ababab", "dark": null },
    "--color-base-60": { "light": "#707070", "dark": null },
    "--color-base-70": { "light": "#5c5c5c", "dark": null },
    "--color-base-100": { "light": "#222222", "dark": null },
    "--color-accent-hsl": {
      "light": "var(--accent-h), var(--accent-s), var(--accent-l)",
      "dark": null
    },
    "--color-accent": {
      "light": "hsl(var(--accent-h), var(--accent-s), var(--accent-l))",
      "dark": null
    },
    "--color-accent-1": {
      "light": "hsl( calc(var(--accent-h) - 1), calc(var(--accent-s) * 1.01), calc(var(--accent-l) * 1.075) )",
      "dark": null
    },
    "--color-accent-2": {
      "light": "hsl( calc(var(--accent-h) - 3), calc(var(--accent-s) * 1.02), calc(var(--accent-l) * 1.15) )",
      "dark": null
    },
    "--background-secondary-alt": {
      "light": "var(--color-base-05)",
      "dark": null
    },
    "--background-modifier-box-shadow": { "light": "#000000", "dark": null },
    "--background-modifier-cover": { "light": "#dcdcdc", "dark": null },
    "--input-shadow": {
      "light": "inset 0 0 0 1px #000000, 0 2px 3px 0 #000000, 0 1px 1.5px 0 #000000, 0 1px 2px 0 #000000, 0 0 0 0 transparent",
      "dark": null
    },
    "--input-shadow-hover": {
      "light": "inset 0 0 0 1px #000000, 0 2px 3px 0 #000000, 0 1px 1.5px 0 #000000, 0 1px 2px 0 #000000, 0 0 0 0 transparent",
      "dark": null
    },
    "--shadow-s": {
      "light": "0px 1px 2px #000000, 0px 3.4px 6.7px #000000, 0px 15px 30px #000000",
      "dark": null
    },
    "--shadow-l": {
      "light": "0px 1.8px 7.3px #000000, 0px 6.3px 24.7px #000000, 0px 30px 90px #000000",
      "dark": null
    },
    "--color-creme-0": { "light": "white", "dark": null },
    "--color-creme-1": { "light": "hsl(34, 33%, 98%)", "dark": null },
    "--color-creme-2": { "light": "hsl(35, 36%, 95%)", "dark": null },
    "--color-creme-3": { "light": "hsl(35, 35%, 90%)", "dark": null },
    "--color-creme-4": { "light": "hsl(34, 37%, 70%)", "dark": null },
    "--color-creme-5": { "light": "hsl(34, 25%, 70%)", "dark": null },
    "--color-creme-6": { "light": "hsl(31, 30%, 50%)", "dark": null },
    "--color-creme-7": { "light": "hsl(31, 35%, 40%)", "dark": null },
    "--color-creme-8": { "light": "hsl(31, 45%, 20%)", "dark": null },
    "--color-creme-gray-1": { "light": "hsl(35, 20%, 94%)", "dark": null },
    "--color-creme-gray-2": { "light": "hsl(35, 20%, 90%)", "dark": null },
    "--color-creme-fade": { "light": "hsla(32, 8%, 67%, 0.5)", "dark": null },
    "--color-shadow": { "light": "hsla(0, 0%, 0%, 0.4)", "dark": null },
    "--color-red": { "light": "#c25757", "dark": null },
    "--color-orange": { "light": "#cf864a", "dark": null },
    "--color-yellow": { "light": "#e1b151", "dark": null },
    "--color-green": { "light": "#8aac6c", "dark": null },
    "--color-cyan": { "light": "#5ba8be", "dark": null },
    "--color-blue": { "light": "#668cb3", "dark": null },
    "--color-purple": { "light": "#877db5", "dark": null },
    "--color-pink": { "light": "#bb77af", "dark": null },
    "--color-red-rgb": { "light": "194, 87, 87", "dark": null },
    "--color-orange-rgb": { "light": "207, 134, 74", "dark": null },
    "--color-yellow-rgb": { "light": "225, 177, 81", "dark": null },
    "--color-green-rgb": { "light": "138, 172, 108", "dark": null },
    "--color-cyan-rgb": { "light": "91, 168, 190", "dark": null },
    "--color-blue-rgb": { "light": "102, 140, 179", "dark": null },
    "--color-purple-rgb": { "light": "135, 125, 181", "dark": null },
    "--color-pink-rgb": { "light": "187, 119, 175", "dark": null },
    "--canvas-color": { "light": null, "dark": "126, 126, 126" }
  },
  ".notice progress[value]::-webkit-progress-bar": {
    "background-color": { "light": "#262626", "dark": null },
    "box-shadow": { "light": "inset 0px 0px 0px 1px #363636", "dark": null }
  },
  ".dropdown": {
    "background-image": {
      "light": null,
      "dark": "url(\"data:image/svg+xml, %3Csvg xmlns=%27http://www.w3.org/2000/svg%27 width=%2724%27 height=%2724%27 viewBox=%270 0 24 24%27 fill=%27none%27 stroke=%27%23FFF%27 opacity=%270.6%27 stroke-width=%272%27 stroke-linecap=%27round%27 stroke-linejoin=%27round%27 %3E%3Cpath d=%27m7 15 5 5 5-5%27/%3E%3Cpath d=%27m7 9 5-5 5 5%27/%3E%3C/svg%3E\")"
    }
  },
  ".pdf-container.mod-themed .popupWrapper > div": {
    "background-color": {
      "light": null,
      "dark": "var(--background-secondary-alt)"
    },
    "filter": {
      "light": null,
      "dark": "drop-shadow(0px 0px 1px var(--color-base-60)) drop-shadow(0px 1px 2px #000000) drop-shadow(0px 4px 6px #000000)"
    }
  },
  ".pdf-container.mod-themed .popupWrapper > div::after": {
    "background-color": {
      "light": null,
      "dark": "var(--background-secondary-alt)"
    }
  },
  ".pdf-container.mod-themed .popupContent:empty + .popupMeta": {
    "background-color": {
      "light": null,
      "dark": "var(--background-secondary-alt)"
    }
  },
  ".mermaid > svg": {
    "filter": {
      "light": null,
      "dark": "invert(100%) hue-rotate(180deg) saturate(1.25)"
    }
  },
  ".canvas-card-menu": {
    "background-color": { "light": null, "dark": "var(--background-secondary)" }
  },
  ".canvas-card-menu .canvas-card-menu-button svg": {
    "fill": { "light": null, "dark": "var(--background-secondary)" }
  },
  ".is-mobile": {
    "--color-base-00": { "light": null, "dark": "#000" },
    "--color-base-10": { "light": null, "dark": "#111" },
    "--color-base-20": { "light": null, "dark": "#1e1e1e" },
    "--tag-background": {
      "light": null,
      "dark": "hsla(var(--interactive-accent-hsl), 0.2)"
    },
    "--search-result-background": {
      "light": null,
      "dark": "var(--background-secondary)"
    },
    "--background-modifier-form-field": {
      "light": null,
      "dark": "var(--background-modifier-border)"
    },
    "--background-modifier-cover": { "light": null, "dark": "#000000" },
    "--background-modifier-hover": {
      "light": null,
      "dark": "rgba(var(--mono-rgb-100), 0.15)"
    },
    "--settings-home-background": {
      "light": null,
      "dark": "var(--background-primary)"
    },
    "--mobile-sidebar-background": {
      "light": null,
      "dark": "var(--background-secondary)"
    },
    "--background-modifier-message": {
      "light": null,
      "dark": "var(--background-secondary)"
    }
  },
  ".is-tablet": {
    "--titlebar-background": {
      "light": null,
      "dark": "var(--background-primary)"
    },
    "--titlebar-background-focused": {
      "light": null,
      "dark": "var(--background-primary)"
    },
    "--interactive-normal": {
      "light": null,
      "dark": "var(--background-modifier-border)"
    },
    "--interactive-hover": {
      "light": null,
      "dark": "var(--background-modifier-border-hover)"
    },
    "--modal-background": {
      "light": null,
      "dark": "var(--background-secondary)"
    }
  },
  ".is-tablet .community-item": {
    "background-color": { "light": null, "dark": "var(--background-secondary)" }
  },
  ".is-tablet .vertical-tab-content": {
    "background-color": { "light": null, "dark": "var(--background-secondary)" }
  },
  ".workspace-drawer-inner": {
    "background-color": { "light": null, "dark": "var(--background-secondary)" }
  },
  ".is-mobile .prompt": {
    "background-color": { "light": null, "dark": "var(--background-secondary)" }
  },
  ".is-mobile .prompt input.prompt-input": {
    "background-color": { "light": null, "dark": "var(--background-secondary)" }
  },
  ".is-tablet .suggestion-container": {
    "background-color": { "light": null, "dark": "var(--background-secondary)" }
  }
}

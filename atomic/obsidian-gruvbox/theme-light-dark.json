{
  "body": {
    "color-scheme": { "light": "light", "dark": null },
    "--highlight-mix-blend-mode": { "light": "darken", "dark": null },
    "--mono-rgb-0": { "light": "255, 255, 255", "dark": null },
    "--mono-rgb-100": { "light": "0, 0, 0", "dark": null },
    "--color-base-00": { "light": "#ffffff", "dark": null },
    "--color-base-05": { "light": "#fcfcfc", "dark": null },
    "--color-base-10": { "light": "#fafafa", "dark": null },
    "--color-base-20": { "light": "#f6f6f6", "dark": null },
    "--color-base-25": { "light": "#e3e3e3", "dark": null },
    "--color-base-30": { "light": "#e0e0e0", "dark": null },
    "--color-base-35": { "light": "#d4d4d4", "dark": null },
    "--color-base-40": { "light": "#bdbdbd", "dark": null },
    "--color-base-50": { "light": "#ababab", "dark": null },
    "--color-base-60": { "light": "#707070", "dark": null },
    "--color-base-70": { "light": "#5c5c5c", "dark": null },
    "--color-base-100": { "light": "#222222", "dark": null },
    "--color-accent-hsl": {
      "light": "var(--accent-h), var(--accent-s), var(--accent-l)",
      "dark": null
    },
    "--color-accent": {
      "light": "hsl(var(--accent-h), var(--accent-s), var(--accent-l))",
      "dark": null
    },
    "--color-accent-1": {
      "light": "hsl(\n    calc(var(--accent-h) - 1),\n    calc(var(--accent-s) * 1.01),\n    calc(var(--accent-l) * 1.075)\n  )",
      "dark": null
    },
    "--color-accent-2": {
      "light": "hsl(\n    calc(var(--accent-h) - 3),\n    calc(var(--accent-s) * 1.02),\n    calc(var(--accent-l) * 1.15)\n  )",
      "dark": null
    },
    "--background-modifier-box-shadow": { "light": "#000000", "dark": null },
    "--background-modifier-cover": { "light": "#dcdcdc", "dark": null },
    "--input-shadow": {
      "light": "inset 0 0 0 1px #000000, 0 2px 3px 0 #000000, 0 1px 1.5px 0 #000000,\n    0 1px 2px 0 #000000, 0 0 0 0 transparent",
      "dark": null
    },
    "--input-shadow-hover": {
      "light": "inset 0 0 0 1px #000000, 0 2px 3px 0 #000000, 0 1px 1.5px 0 #000000,\n    0 1px 2px 0 #000000, 0 0 0 0 transparent",
      "dark": null
    },
    "--shadow-s": {
      "light": "0px 1px 2px #000000, 0px 3.4px 6.7px #000000, 0px 15px 30px #000000",
      "dark": null
    },
    "--shadow-l": {
      "light": "0px 1.8px 7.3px #000000, 0px 6.3px 24.7px #000000, 0px 30px 90px #000000",
      "dark": null
    },
    "--color-red-rgb": { "light": "var(--neutral-red_x)", "dark": null },
    "--color-red": { "light": "var(--neutral-red)", "dark": null },
    "--color-purple-rgb": { "light": "var(--neutral-purple_x)", "dark": null },
    "--color-purple": { "light": "var(--neutral-purple)", "dark": null },
    "--color-green-rgb": { "light": "var(--neutral-green_x)", "dark": null },
    "--color-green": { "light": "var(--neutral-green)", "dark": null },
    "--color-cyan-rgb": { "light": "var(--neutral-blue_x)", "dark": null },
    "--color-cyan": { "light": "var(--neutral-blue)", "dark": null },
    "--color-blue-rgb": { "light": "var(--faded-blue_x)", "dark": null },
    "--color-blue": { "light": "var(--faded-blue)", "dark": null },
    "--color-yellow-rgb": { "light": "var(--neutral-yellow_x)", "dark": null },
    "--color-yellow": { "light": "var(--neutral-yellow)", "dark": null },
    "--color-orange-rgb": { "light": "var(--neutral-orange_x)", "dark": null },
    "--color-orange": { "light": "var(--neutral-orange)", "dark": null },
    "--color-pink-rgb": { "light": "var(--bright-purple_x)", "dark": null },
    "--color-pink": { "light": "var(--bright-purple)", "dark": null },
    "--background-primary": { "light": "var(--light0-hard)", "dark": null },
    "--background-primary-alt": { "light": "var(--light0-hard)", "dark": null },
    "--background-secondary": { "light": "var(--light1)", "dark": null },
    "--background-secondary-alt": { "light": "var(--light1)", "dark": null },
    "--background-modifier-border": { "light": "var(--light2)", "dark": null },
    "--cursor-line-background": {
      "light": "rgba(var(--light1_x), 0.5)",
      "dark": null
    },
    "--text-normal": { "light": "var(--dark0)", "dark": null },
    "--text-faint": { "light": "var(--dark3)", "dark": null },
    "--text-muted": { "light": "var(--dark2)", "dark": null },
    "--link-url": { "light": "var(--neutral-green)", "dark": null },
    "--h1-color": { "light": "var(--neutral-red)", "dark": null },
    "--h2-color": { "light": "var(--neutral-yellow)", "dark": null },
    "--h3-color": { "light": "var(--neutral-green)", "dark": null },
    "--h4-color": { "light": "var(--neutral-aqua)", "dark": null },
    "--h5-color": { "light": "var(--neutral-blue)", "dark": null },
    "--h6-color": { "light": "var(--neutral-purple)", "dark": null },
    "--text-highlight-bg": { "light": "var(--bright-yellow)", "dark": null },
    "--text-highlight-fg": { "light": "var(--dark0)", "dark": null },
    "--text-accent": { "light": "var(--neutral-orange)", "dark": null },
    "--text-accent-hover": { "light": "var(--bright-aqua)", "dark": null },
    "--tag-color": { "light": "var(--neutral-aqua)", "dark": null },
    "--tag-background": { "light": "var(--light1)", "dark": null },
    "--tag-background-hover": {
      "light": "rgba(var(--light1_x), 0.6)",
      "dark": null
    },
    "--titlebar-text-color-focused": {
      "light": "var(--bright-red)",
      "dark": null
    },
    "--inline-title-color": { "light": "var(--bright-yellow)", "dark": null },
    "--bold-color": { "light": "var(--neutral-yellow)", "dark": null },
    "--italic-color": { "light": "var(--neutral-yellow)", "dark": null },
    "--checkbox-color": { "light": "var(--light4)", "dark": null },
    "--checkbox-color-hover": { "light": "var(--light4)", "dark": null },
    "--checkbox-border-color": { "light": "var(--light4)", "dark": null },
    "--checkbox-border-color-hover": { "light": "var(--light4)", "dark": null },
    "--checklist-done-color": {
      "light": "rgba(var(--dark2_x), 0.4)",
      "dark": null
    },
    "--table-header-background": {
      "light": "rgba(var(--light3_x), 0.4)",
      "dark": null
    },
    "--table-header-background-hover": {
      "light": "var(--light2)",
      "dark": null
    },
    "--table-row-even-background": {
      "light": "rgba(var(--light1_x), 0.2)",
      "dark": null
    },
    "--table-row-odd-background": {
      "light": "rgba(var(--light1_x), 0.7)",
      "dark": null
    },
    "--table-row-background-hover": { "light": "var(--light2)", "dark": null },
    "--text-selection": {
      "light": "rgba(var(--neutral-red_x), 0.6)",
      "dark": null
    },
    "--flashing-background": {
      "light": "rgba(var(--neutral-red_x), 0.3)",
      "dark": null
    },
    "--code-normal": { "light": "var(--bright-blue)", "dark": null },
    "--code-background": { "light": "var(--light1)", "dark": null },
    "--mermaid-note": { "light": "var(--bright-blue)", "dark": null },
    "--mermaid-actor": { "light": "var(--light3)", "dark": null },
    "--mermaid-loopline": { "light": "var(--neutral-blue)", "dark": null },
    "--mermaid-exclude": { "light": "var(--light2)", "dark": null },
    "--mermaid-seqnum": { "light": "var(--light0)", "dark": null },
    "--icon-color-hover": { "light": "var(--bright-red)", "dark": null },
    "--icon-color-focused": { "light": "var(--bright-blue)", "dark": null },
    "--nav-item-color-hover": { "light": "var(--bright-red)", "dark": null },
    "--nav-item-color-active": { "light": "var(--neutral-blue)", "dark": null },
    "--nav-file-tag": {
      "light": "rgba(var(--neutral-blue_x), 0.9)",
      "dark": null
    },
    "--graph-line": { "light": "var(--light1)", "dark": null },
    "--graph-node": { "light": "var(--gray)", "dark": null },
    "--graph-node-tag": { "light": "var(--neutral-red)", "dark": null },
    "--graph-node-attachment": { "light": "var(--bright-green)", "dark": null },
    "--calendar-hover": { "light": "var(--bright-red)", "dark": null },
    "--calendar-background-hover": { "light": "var(--light1)", "dark": null },
    "--calendar-week": { "light": "var(--bright-red)", "dark": null },
    "--calendar-today": { "light": "var(--bright-red)", "dark": null },
    "--dataview-key": { "light": "var(--text-faint)", "dark": null },
    "--dataview-key-background": {
      "light": "rgba(var(--faded-red_x), 0.3)",
      "dark": null
    },
    "--dataview-value": { "light": "var(--text-faint)", "dark": null },
    "--dataview-value-background": {
      "light": "rgba(var(--neutral-green_x), 0.2)",
      "dark": null
    },
    "--tab-text-color-focused-active": {
      "light": "var(--neutral-yellow)",
      "dark": null
    },
    "--tab-text-color-focused-active-current": {
      "light": "var(--bright-red)",
      "dark": null
    },
    "--canvas-color": { "light": null, "dark": "126, 126, 126" }
  },
  ".notice progress[value]::-webkit-progress-bar": {
    "background-color": { "light": "#262626", "dark": null },
    "box-shadow": { "light": "inset 0px 0px 0px 1px #363636", "dark": null }
  },
  ".dropdown": {
    "background-image": {
      "light": null,
      "dark": "url(\"data:image/svg+xml,%3Csvg xmlns=%27http://www.w3.org/2000/svg%27 width=%2724%27 height=%2724%27 viewBox=%270 0 24 24%27 fill=%27none%27 stroke=%27%23FFF%27 opacity=%270.6%27 stroke-width=%272%27 stroke-linecap=%27round%27 stroke-linejoin=%27round%27 %3E%3Cpath d=%27m7 15 5 5 5-5%27/%3E%3Cpath d=%27m7 9 5-5 5 5%27/%3E%3C/svg%3E\")"
    }
  },
  ".pdf-container.mod-themed\n  .annotationLayer\n  .textWidgetAnnotation\n  input": {
    "filter": { "light": null, "dark": "invert(100%) hue-rotate(180deg)" }
  },
  ".pdf-container.mod-themed\n  .annotationLayer\n  .textWidgetAnnotation\n  textarea": {
    "filter": { "light": null, "dark": "invert(100%) hue-rotate(180deg)" }
  },
  ".pdf-container.mod-themed .popupWrapper > div": {
    "background-color": {
      "light": null,
      "dark": "var(--background-secondary-alt)"
    },
    "filter": {
      "light": null,
      "dark": "drop-shadow(0px 0px 1px var(--color-base-60))\n    drop-shadow(0px 1px 2px #000000) drop-shadow(0px 4px 6px #000000)"
    }
  },
  ".pdf-container.mod-themed .popupWrapper > div::after": {
    "background-color": {
      "light": null,
      "dark": "var(--background-secondary-alt)"
    }
  },
  ".pdf-container.mod-themed .popupContent:empty + .popupMeta": {
    "background-color": {
      "light": null,
      "dark": "var(--background-secondary-alt)"
    }
  },
  ".mermaid > svg": {
    "filter": {
      "light": null,
      "dark": "invert(100%) hue-rotate(180deg) saturate(1.25)"
    }
  },
  ".canvas-card-menu": {
    "background-color": { "light": null, "dark": "var(--background-secondary)" }
  },
  ".canvas-card-menu .canvas-card-menu-button svg": {
    "fill": { "light": null, "dark": "var(--background-secondary)" }
  },
  ".is-mobile": {
    "--color-base-00": { "light": null, "dark": "#000" },
    "--color-base-10": { "light": null, "dark": "#111" },
    "--color-base-20": { "light": null, "dark": "#1e1e1e" },
    "--tag-background": {
      "light": null,
      "dark": "hsla(var(--interactive-accent-hsl), 0.2)"
    },
    "--search-result-background": {
      "light": null,
      "dark": "var(--background-secondary)"
    },
    "--background-modifier-form-field": {
      "light": null,
      "dark": "var(--background-modifier-border)"
    },
    "--background-modifier-cover": { "light": null, "dark": "#000000" },
    "--background-modifier-hover": {
      "light": null,
      "dark": "rgba(var(--mono-rgb-100), 0.15)"
    },
    "--settings-home-background": {
      "light": null,
      "dark": "var(--background-primary)"
    },
    "--mobile-sidebar-background": {
      "light": null,
      "dark": "var(--background-secondary)"
    },
    "--background-modifier-message": {
      "light": null,
      "dark": "var(--background-secondary)"
    }
  },
  ".is-tablet": {
    "--titlebar-background": {
      "light": null,
      "dark": "var(--background-primary)"
    },
    "--titlebar-background-focused": {
      "light": null,
      "dark": "var(--background-primary)"
    },
    "--interactive-normal": {
      "light": null,
      "dark": "var(--background-modifier-border)"
    },
    "--interactive-hover": {
      "light": null,
      "dark": "var(--background-modifier-border-hover)"
    },
    "--modal-background": {
      "light": null,
      "dark": "var(--background-secondary)"
    }
  },
  ".is-tablet .community-item": {
    "background-color": { "light": null, "dark": "var(--background-secondary)" }
  },
  ".is-tablet .vertical-tab-content": {
    "background-color": { "light": null, "dark": "var(--background-secondary)" }
  },
  ".workspace-drawer-inner": {
    "background-color": { "light": null, "dark": "var(--background-secondary)" }
  },
  ".is-mobile .prompt": {
    "background-color": { "light": null, "dark": "var(--background-secondary)" }
  },
  ".is-mobile .prompt input.prompt-input": {
    "background-color": { "light": null, "dark": "var(--background-secondary)" }
  },
  ".is-tablet .suggestion-container": {
    "background-color": { "light": null, "dark": "var(--background-secondary)" }
  }
}

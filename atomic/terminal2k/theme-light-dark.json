{
  "body": {
    "--background-primary": { "light": "var(--grey-light)", "dark": null },
    "--background-primary-alt": { "light": "var(--grey-mid)", "dark": null },
    "--background-secondary": { "light": "var(--grey-mid)", "dark": null },
    "--background-secondary-alt": { "light": "var(--grey-mid)", "dark": null },
    "--bar-color": { "light": "var(--green-mid-mid)", "dark": null },
    "--blockquote-border-thickness": { "light": "5px", "dark": null },
    "--bold-color": { "light": "var(--green-forest)", "dark": null },
    "--checklist-done-color": { "light": "var(--grey-dark)", "dark": null },
    "--checklist-done-decoration": { "light": "none", "dark": null },
    "--code-builtin-color": { "light": "var(--green-forest)", "dark": null },
    "--code-comment-color": { "light": "var(--grey-dark)", "dark": null },
    "--code-function-color": { "light": "var(--green-forest)", "dark": null },
    "--code-operator-color": { "light": "var(--green-forest)", "dark": null },
    "--code-string-color": { "light": "var(--grey-dark)", "dark": null },
    "--code-variable-color": {
      "light": "var(--black-light-light)",
      "dark": null
    },
    "--codeblock-background": { "light": "var(--grey)", "dark": null },
    "--codeblock-color": { "light": "var(--black-light)", "dark": null },
    "--color-accent": { "light": "var(--green-bright)", "dark": null },
    "--divider-color": { "light": "var(--green-mid-mid)", "dark": null },
    "--file-line-width": { "light": "var(--line-width)", "dark": null },
    "--file-title-color": { "light": "var(--black-light-light)", "dark": null },
    "--folder-filter": {
      "light": "invert(52%) sepia(95%) saturate(856%) hue-rotate(67deg) brightness(118%) contrast(117%)",
      "dark": null
    },
    "--folder-title-color": {
      "light": "var(--black-light-light)",
      "dark": null
    },
    "--h1-color": { "light": "var(--green-forest)", "dark": null },
    "--h2-color": { "light": "var(--green-forest)", "dark": null },
    "--h3-color": { "light": "var(--green-forest)", "dark": null },
    "--h4-color": { "light": "var(--green-forest)", "dark": null },
    "--h5-color": { "light": "var(--green-forest)", "dark": null },
    "--h6-color": { "light": "var(--green-forest)", "dark": null },
    "--hashtag-background": { "light": "var(--grey)", "dark": null },
    "--hashtag-color": { "light": "var(--green-forest)", "dark": null },
    "--highlight-color": { "light": "var(--green-mid)", "dark": null },
    "--hr-thickness": { "light": "var(--hr-height)", "dark": null },
    "--inline-code-background": { "light": "var(--grey)", "dark": null },
    "--inline-code-color": { "light": "var(--green-forest)", "dark": null },
    "--interactive-accent": { "light": "var(--green-bright)", "dark": null },
    "--link-color": { "light": "var(--green-forest)", "dark": null },
    "--links-color": { "light": "var(--green-forest)", "dark": null },
    "--quote-background": { "light": "var(--grey)", "dark": null },
    "--tab-divider-color": { "light": "var(--green-mid-mid)", "dark": null },
    "--tab-outline-color": { "light": "var(--green-mid-mid)", "dark": null },
    "--text-accent": { "light": "var(--green-bright)", "dark": null },
    "--text-normal": { "light": "var(--black-light-light)", "dark": null },
    "--underline-line-color": { "light": "var(--green-bright)", "dark": null },
    "--canvas-color": { "light": null, "dark": "126, 126, 126" }
  },
  ".notice progress[value]::-webkit-progress-bar": {
    "background-color": { "light": "#262626", "dark": null },
    "box-shadow": { "light": "inset 0px 0px 0px 1px #363636", "dark": null }
  },
  ".dropdown": { "background-image": { "light": null, "dark": "url(\"data" } },
  ".pdf-container.mod-themed .annotationLayer .textWidgetAnnotation input": {
    "filter": { "light": null, "dark": "invert(100%) hue-rotate(180deg)" }
  },
  ".pdf-container.mod-themed .annotationLayer .textWidgetAnnotation textarea": {
    "filter": { "light": null, "dark": "invert(100%) hue-rotate(180deg)" }
  },
  ".pdf-container.mod-themed .popupWrapper > div": {
    "background-color": {
      "light": null,
      "dark": "var(--background-secondary-alt)"
    },
    "filter": {
      "light": null,
      "dark": "drop-shadow(0px 0px 1px var(--color-base-60)) drop-shadow(0px 1px 2px rgba(0, 0, 0, 0.4)) drop-shadow(0px 4px 6px rgba(0, 0, 0, 0.4))"
    }
  },
  ".pdf-container.mod-themed .popupWrapper > div::after": {
    "background-color": {
      "light": null,
      "dark": "var(--background-secondary-alt)"
    }
  },
  ".pdf-container.mod-themed .popupContent:empty + .popupMeta": {
    "background-color": {
      "light": null,
      "dark": "var(--background-secondary-alt)"
    }
  },
  ".mermaid > svg": {
    "filter": {
      "light": null,
      "dark": "invert(100%) hue-rotate(180deg) saturate(1.25)"
    }
  },
  ".canvas-card-menu": {
    "background-color": { "light": null, "dark": "var(--background-secondary)" }
  },
  ".canvas-card-menu .canvas-card-menu-button svg": {
    "fill": { "light": null, "dark": "var(--background-secondary)" }
  },
  ".is-mobile": {
    "--background-modifier-cover": {
      "light": null,
      "dark": "rgba(0, 0, 0, 0.5)"
    },
    "--background-modifier-form-field": {
      "light": null,
      "dark": "var(--background-modifier-border)"
    },
    "--background-modifier-hover": {
      "light": null,
      "dark": "rgba(var(--mono-rgb-100), 0.15)"
    },
    "--background-modifier-message": {
      "light": null,
      "dark": "var(--background-secondary)"
    },
    "--color-base-00": { "light": null, "dark": "#000" },
    "--color-base-10": { "light": null, "dark": "#111" },
    "--color-base-20": { "light": null, "dark": "#1e1e1e" },
    "--mobile-sidebar-background": {
      "light": null,
      "dark": "var(--background-secondary)"
    },
    "--search-result-background": {
      "light": null,
      "dark": "var(--background-secondary)"
    },
    "--settings-home-background": {
      "light": null,
      "dark": "var(--background-primary)"
    },
    "--tag-background": {
      "light": null,
      "dark": "hsla(var(--interactive-accent-hsl), 0.2)"
    }
  },
  ".is-tablet": {
    "--interactive-hover": {
      "light": null,
      "dark": "var(--background-modifier-border-hover)"
    },
    "--interactive-normal": {
      "light": null,
      "dark": "var(--background-modifier-border)"
    },
    "--modal-background": {
      "light": null,
      "dark": "var(--background-secondary)"
    },
    "--titlebar-background": {
      "light": null,
      "dark": "var(--background-primary)"
    },
    "--titlebar-background-focused": {
      "light": null,
      "dark": "var(--background-primary)"
    }
  },
  ".is-tablet .community-item": {
    "background-color": { "light": null, "dark": "var(--background-secondary)" }
  },
  ".is-tablet .vertical-tab-content": {
    "background-color": { "light": null, "dark": "var(--background-secondary)" }
  },
  ".workspace-drawer-inner": {
    "background-color": { "light": null, "dark": "var(--background-secondary)" }
  },
  ".is-mobile .prompt": {
    "background-color": { "light": null, "dark": "var(--background-secondary)" }
  },
  ".is-mobile .prompt input.prompt-input": {
    "background-color": { "light": null, "dark": "var(--background-secondary)" }
  },
  ".is-tablet .suggestion-container": {
    "background-color": { "light": null, "dark": "var(--background-secondary)" }
  }
}

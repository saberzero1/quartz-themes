{
  "body": {
    "--accent-l-closer": { "light": "max(var(--accent-l), 60%)", "dark": null },
    "--accent-l-readable": {
      "light": "min(var(--accent-l), 35%)",
      "dark": null
    },
    "--accent-minus-120": {
      "light": "calc(var(--accent-h) - 120)",
      "dark": null
    },
    "--accent-minus-60": {
      "light": "calc(var(--accent-h) - 60)",
      "dark": null
    },
    "--accent-plus-120": {
      "light": "calc(var(--accent-h) + 120)",
      "dark": null
    },
    "--accent-plus-60": { "light": "calc(var(--accent-h) + 60)", "dark": null },
    "--accent-s-pastel": { "light": "min(var(--accent-s), 50%)", "dark": null },
    "--background-modifier-border": {
      "light": "var(--hr-color)",
      "dark": null
    },
    "--background-modifier-border-hover": {
      "light": "hsla( var(--accent-h), calc(var(--bg-s-modifier) + 25%), calc(var(--bg-l-modifier) + 85%) )",
      "dark": null
    },
    "--background-modifier-form-field": {
      "light": "var(--interactive-normal)",
      "dark": null
    },
    "--background-modifier-hover": {
      "light": "hsla( var(--accent-h), calc(var(--bg-s-modifier) + 35%), calc(var(--bg-l-modifier) + 50%), 0.5 )",
      "dark": null
    },
    "--background-primary": {
      "light": "hsl( var(--accent-h), calc(var(--bg-s-modifier) + 35%), calc(var(--bg-l-modifier) + 75%) )",
      "dark": null
    },
    "--background-primary-alt": {
      "light": "hsl( var(--accent-h), calc(var(--bg-s-modifier) + 35%), calc(var(--bg-l-modifier) + 70%) )",
      "dark": null
    },
    "--background-secondary": {
      "light": "hsl( var(--accent-h), calc(var(--bg-s-modifier) + 35%), calc(var(--bg-l-modifier) + 65%) )",
      "dark": null
    },
    "--background-secondary-alt": {
      "light": "hsl( var(--accent-h), calc(var(--bg-s-modifier) + 35%), calc(var(--bg-l-modifier) + 60%) )",
      "dark": null
    },
    "--bg-l-modifier": { "light": "0%", "dark": null },
    "--bg-l-modifier-inverted": {
      "light": "calc(var(--bg-l-modifier) * -1)",
      "dark": null
    },
    "--bg-s-modifier": {
      "light": "calc(var(--bg-l-modifier) * 0.5)",
      "dark": null
    },
    "--blockquote-border-color": {
      "light": "var(--interactive-accent)",
      "dark": null
    },
    "--checkbox-color": { "light": "var(--interactive-accent)", "dark": null },
    "--checkbox-color-hover": {
      "light": "var(--interactive-accent-hover)",
      "dark": null
    },
    "--divider-color": { "light": "var(--hr-color)", "dark": null },
    "--divider-color-hover": {
      "light": "hsl( var(--accent-minus-60), var(--accent-s), var(--accent-l-readable) )",
      "dark": null
    },
    "--hr-color": {
      "light": "hsl( var(--accent-h), calc(var(--bg-s-modifier) + 25%), calc(var(--bg-l-modifier) + 90%) )",
      "dark": null
    },
    "--input-shadow": { "light": "none", "dark": null },
    "--input-shadow-hover": { "light": "none", "dark": null },
    "--interactive-accent": {
      "light": "hsl( var(--accent-plus-60), var(--accent-s-pastel), var(--accent-l-closer) )",
      "dark": null
    },
    "--interactive-accent-hover": {
      "light": "hsl( calc(var(--accent-plus-60) - 3), calc(var(--accent-s-pastel) * 1.02), calc(var(--accent-l-closer) * 1.15) )",
      "dark": null
    },
    "--interactive-hover": {
      "light": "hsl( var(--accent-h), calc(var(--bg-s-modifier) + 40%), calc(var(--bg-l-modifier) + 90%) )",
      "dark": null
    },
    "--interactive-normal": {
      "light": "hsl( var(--accent-h), calc(var(--bg-s-modifier) + 40%), calc(var(--bg-l-modifier) + 87%) )",
      "dark": null
    },
    "--modal-border-color": { "light": "var(--divider-color)", "dark": null },
    "--nav-item-background-selected": {
      "light": "hsla( var(--accent-minus-60), var(--accent-s), var(--accent-l-closer), 0.17 )",
      "dark": null
    },
    "--nav-item-border-color": {
      "light": "var(--divider-color)",
      "dark": null
    },
    "--pdf-shadow": { "light": "none", "dark": null },
    "--pdf-thumbnail-shadow": { "light": "none", "dark": null },
    "--sidebar-left-border-color": {
      "light": "var(--divider-color)",
      "dark": null
    },
    "--status-bar-border-color": {
      "light": "var(--divider-color)",
      "dark": null
    },
    "--swatch-shadow": { "light": "none", "dark": null },
    "--tab-stacked-shadow": { "light": "none", "dark": null },
    "--table-add-button-border-color": {
      "light": "var(--text-muted)",
      "dark": null
    },
    "--table-border-color": { "light": "var(--text-muted)", "dark": null },
    "--table-header-border-color": {
      "light": "var(--text-muted)",
      "dark": null
    },
    "--table-selection": {
      "light": "hsla( var(--accent-minus-60), var(--accent-s), var(--accent-l-closer), 0.2 )",
      "dark": null
    },
    "--table-selection-border-color": {
      "light": "hsl( var(--accent-minus-60), var(--accent-s), var(--accent-l-readable) )",
      "dark": null
    },
    "--tag-background": {
      "light": "hsla( var(--accent-minus-60), var(--accent-s), var(--accent-l-closer), 0.25 )",
      "dark": null
    },
    "--tag-background-hover": {
      "light": "hsla( var(--accent-minus-60), var(--accent-s), var(--accent-l-closer), 0.35 )",
      "dark": null
    },
    "--text-accent": {
      "light": "hsl( var(--accent-minus-60), var(--accent-s), var(--accent-l-readable) )",
      "dark": null
    },
    "--text-accent-hover": {
      "light": "hsl( calc(var(--accent-minus-60) - 3), calc(var(--accent-s) * 1.02), calc(var(--accent-l-readable) * 1.15) )",
      "dark": null
    },
    "--text-faint": { "light": "hsl(var(--accent-h), 20%, 47%)", "dark": null },
    "--text-highlight-bg": {
      "light": "hsla( var(--accent-plus-60), var(--accent-s-pastel), var(--accent-l-closer), 0.6 )",
      "dark": null
    },
    "--text-muted": { "light": "hsl(var(--accent-h), 15%, 33%)", "dark": null },
    "--text-normal": { "light": "hsl(0, 0%, 13%)", "dark": null },
    "--text-selection": {
      "light": "hsla( var(--accent-minus-60), var(--accent-s), var(--accent-l-closer), 0.25 )",
      "dark": null
    },
    "--titlebar-border-color": {
      "light": "var(--divider-color)",
      "dark": null
    },
    "--canvas-color": { "light": null, "dark": "126, 126, 126" }
  },
  ".notice progress[value]::-webkit-progress-bar": {
    "background-color": { "light": "#262626", "dark": null },
    "box-shadow": { "light": "inset 0px 0px 0px 1px #363636", "dark": null }
  },
  ".dropdown": { "background-image": { "light": null, "dark": "url(\"data" } },
  ".pdf-container.mod-themed .annotationLayer .textWidgetAnnotation input": {
    "filter": { "light": null, "dark": "invert(100%) hue-rotate(180deg)" }
  },
  ".pdf-container.mod-themed .annotationLayer .textWidgetAnnotation textarea": {
    "filter": { "light": null, "dark": "invert(100%) hue-rotate(180deg)" }
  },
  ".pdf-container.mod-themed .popupWrapper > div": {
    "background-color": {
      "light": null,
      "dark": "var(--background-secondary-alt)"
    },
    "filter": {
      "light": null,
      "dark": "drop-shadow(0px 0px 1px var(--color-base-60)) drop-shadow(0px 1px 2px rgba(0, 0, 0, 0.4)) drop-shadow(0px 4px 6px rgba(0, 0, 0, 0.4))"
    }
  },
  ".pdf-container.mod-themed .popupWrapper > div::after": {
    "background-color": {
      "light": null,
      "dark": "var(--background-secondary-alt)"
    }
  },
  ".pdf-container.mod-themed .popupContent:empty + .popupMeta": {
    "background-color": {
      "light": null,
      "dark": "var(--background-secondary-alt)"
    }
  },
  ".mermaid > svg": {
    "filter": {
      "light": null,
      "dark": "invert(100%) hue-rotate(180deg) saturate(1.25)"
    }
  },
  ".canvas-card-menu": {
    "background-color": { "light": null, "dark": "var(--background-secondary)" }
  },
  ".canvas-card-menu .canvas-card-menu-button svg": {
    "fill": { "light": null, "dark": "var(--background-secondary)" }
  },
  ".is-mobile": {
    "--background-modifier-cover": {
      "light": null,
      "dark": "rgba(0, 0, 0, 0.5)"
    },
    "--background-modifier-form-field": {
      "light": null,
      "dark": "var(--background-modifier-border)"
    },
    "--background-modifier-hover": {
      "light": null,
      "dark": "rgba(var(--mono-rgb-100), 0.15)"
    },
    "--background-modifier-message": {
      "light": null,
      "dark": "var(--background-secondary)"
    },
    "--color-base-00": { "light": null, "dark": "#000" },
    "--color-base-10": { "light": null, "dark": "#111" },
    "--color-base-20": { "light": null, "dark": "#1e1e1e" },
    "--mobile-sidebar-background": {
      "light": null,
      "dark": "var(--background-secondary)"
    },
    "--search-result-background": {
      "light": null,
      "dark": "var(--background-secondary)"
    },
    "--settings-home-background": {
      "light": null,
      "dark": "var(--background-primary)"
    },
    "--tag-background": {
      "light": null,
      "dark": "hsla(var(--interactive-accent-hsl), 0.2)"
    }
  },
  ".is-tablet": {
    "--interactive-hover": {
      "light": null,
      "dark": "var(--background-modifier-border-hover)"
    },
    "--interactive-normal": {
      "light": null,
      "dark": "var(--background-modifier-border)"
    },
    "--modal-background": {
      "light": null,
      "dark": "var(--background-secondary)"
    },
    "--titlebar-background": {
      "light": null,
      "dark": "var(--background-primary)"
    },
    "--titlebar-background-focused": {
      "light": null,
      "dark": "var(--background-primary)"
    }
  },
  ".is-tablet .community-item": {
    "background-color": { "light": null, "dark": "var(--background-secondary)" }
  },
  ".is-tablet .vertical-tab-content": {
    "background-color": { "light": null, "dark": "var(--background-secondary)" }
  },
  ".workspace-drawer-inner": {
    "background-color": { "light": null, "dark": "var(--background-secondary)" }
  },
  ".is-mobile .prompt": {
    "background-color": { "light": null, "dark": "var(--background-secondary)" }
  },
  ".is-mobile .prompt input.prompt-input": {
    "background-color": { "light": null, "dark": "var(--background-secondary)" }
  },
  ".is-tablet .suggestion-container": {
    "background-color": { "light": null, "dark": "var(--background-secondary)" }
  }
}

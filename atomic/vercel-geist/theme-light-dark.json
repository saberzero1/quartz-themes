{
  "body": {
    "--background-1": { "light": "var(--vercel-gray-100)", "dark": null },
    "--background-2": { "light": "var(--vercel-gray-200)", "dark": null },
    "--background-3": { "light": "var(--vercel-gray-300)", "dark": null },
    "--background-accent": { "light": "var(--accent-blue)", "dark": null },
    "--background-modifier-border": {
      "light": "var(--border-1)",
      "dark": null
    },
    "--background-modifier-box-shadow": {
      "light": "rgba(0, 0, 0, 0.1)",
      "dark": null
    },
    "--background-modifier-cover": {
      "light": "rgba(0, 0, 0, 0.4)",
      "dark": null
    },
    "--background-modifier-error": { "light": "#ff0000", "dark": null },
    "--background-modifier-error-hover": { "light": "#ff3333", "dark": null },
    "--background-modifier-form-field": {
      "light": "rgba(0, 0, 0, 0.1)",
      "dark": null
    },
    "--background-modifier-form-field-highlighted": {
      "light": "rgba(0, 0, 0, 0.2)",
      "dark": null
    },
    "--background-modifier-success": {
      "light": "var(--vercel-blue-600)",
      "dark": null
    },
    "--background-primary": { "light": "var(--background-1)", "dark": null },
    "--background-primary-alt": {
      "light": "var(--background-2)",
      "dark": null
    },
    "--background-secondary": { "light": "var(--background-2)", "dark": null },
    "--background-secondary-alt": {
      "light": "var(--background-3)",
      "dark": null
    },
    "--blockquote-border": { "light": "var(--vercel-blue-600)", "dark": null },
    "--border-1": { "light": "var(--vercel-gray-200)", "dark": null },
    "--border-2": { "light": "var(--vercel-gray-300)", "dark": null },
    "--border-3": { "light": "var(--vercel-blue-600)", "dark": null },
    "--border-color": { "light": "var(--border-1)", "dark": null },
    "--code-block": { "light": "var(--text-1)", "dark": null },
    "--heading-color-h1": { "light": "var(--vercel-blue-500)", "dark": null },
    "--heading-color-h2": { "light": "var(--vercel-blue-600)", "dark": null },
    "--heading-color-h3": { "light": "var(--text-1)", "dark": null },
    "--heading-color-h4": { "light": "var(--text-1)", "dark": null },
    "--heading-color-h5": { "light": "var(--text-1)", "dark": null },
    "--heading-color-h6": { "light": "var(--text-2)", "dark": null },
    "--highlight-high": { "light": "var(--vercel-blue-300)", "dark": null },
    "--highlight-low": { "light": "var(--vercel-gray-200)", "dark": null },
    "--highlight-med": { "light": "var(--vercel-gray-300)", "dark": null },
    "--inline-code": { "light": "var(--vercel-blue-600)", "dark": null },
    "--inline-code-bg": { "light": "var(--vercel-gray-200)", "dark": null },
    "--interactive-accent": { "light": "var(--accent-blue)", "dark": null },
    "--interactive-accent-active": {
      "light": "var(--vercel-blue-600)",
      "dark": null
    },
    "--interactive-accent-hover": {
      "light": "var(--vercel-blue-500)",
      "dark": null
    },
    "--interactive-hover": { "light": "var(--highlight-low)", "dark": null },
    "--interactive-normal": { "light": "var(--background-2)", "dark": null },
    "--link-color": { "light": "var(--vercel-blue-600)", "dark": null },
    "--link-color-hover": { "light": "var(--vercel-blue-700)", "dark": null },
    "--pre-code": { "light": "var(--background-2)", "dark": null },
    "--pre-code-bg": { "light": "var(--vercel-gray-200)", "dark": null },
    "--scrollbar-active-thumb-bg": { "light": "var(--border-2)", "dark": null },
    "--scrollbar-bg": { "light": "transparent", "dark": null },
    "--scrollbar-thumb-bg": { "light": "var(--border-1)", "dark": null },
    "--text-1": { "light": "var(--vercel-gray-900)", "dark": null },
    "--text-2": { "light": "var(--vercel-gray-600)", "dark": null },
    "--text-accent": { "light": "var(--text-accent)", "dark": null },
    "--text-accent-hover": { "light": "var(--link-color-hover)", "dark": null },
    "--text-error": { "light": "#ff0000", "dark": null },
    "--text-error-hover": { "light": "#ff3333", "dark": null },
    "--text-faint": { "light": "var(--text-2)", "dark": null },
    "--text-highlight-bg": { "light": "var(--vercel-blue-300)", "dark": null },
    "--text-muted": { "light": "var(--text-2)", "dark": null },
    "--text-normal": { "light": "var(--text-1)", "dark": null },
    "--text-on-accent": { "light": "#ffffff", "dark": null },
    "--text-selection": { "light": "rgba(77, 139, 249, 0.3)", "dark": null },
    "--titlebar-background": { "light": "var(--background-1)", "dark": null },
    "--titlebar-background-focused": {
      "light": "var(--background-2)",
      "dark": null
    },
    "--vim-cursor": { "light": "var(--vercel-blue-600)", "dark": null },
    "--canvas-color": { "light": null, "dark": "126, 126, 126" }
  },
  ".notice progress[value]::-webkit-progress-bar": {
    "background-color": { "light": "#262626", "dark": null },
    "box-shadow": { "light": "inset 0px 0px 0px 1px #363636", "dark": null }
  },
  "code[class*=\"language-\"]": {
    "background-color": "var(--pre-code)",
    "color": "var(--text-faint)",
    "text-shadow": "none"
  },
  "pre[class*=\"language-\"]": {
    "background-color": "var(--pre-code)",
    "color": "var(--text-faint)",
    "text-shadow": "none"
  },
  ".dropdown": { "background-image": { "light": null, "dark": "url(\"data" } },
  ".pdf-container.mod-themed .annotationLayer .textWidgetAnnotation input": {
    "filter": { "light": null, "dark": "invert(100%) hue-rotate(180deg)" }
  },
  ".pdf-container.mod-themed .annotationLayer .textWidgetAnnotation textarea": {
    "filter": { "light": null, "dark": "invert(100%) hue-rotate(180deg)" }
  },
  ".pdf-container.mod-themed .popupWrapper > div": {
    "background-color": {
      "light": null,
      "dark": "var(--background-secondary-alt)"
    },
    "filter": {
      "light": null,
      "dark": "drop-shadow(0px 0px 1px var(--color-base-60)) drop-shadow(0px 1px 2px rgba(0, 0, 0, 0.4)) drop-shadow(0px 4px 6px rgba(0, 0, 0, 0.4))"
    }
  },
  ".pdf-container.mod-themed .popupWrapper > div::after": {
    "background-color": {
      "light": null,
      "dark": "var(--background-secondary-alt)"
    }
  },
  ".pdf-container.mod-themed .popupContent:empty + .popupMeta": {
    "background-color": {
      "light": null,
      "dark": "var(--background-secondary-alt)"
    }
  },
  ".mermaid > svg": {
    "filter": {
      "light": null,
      "dark": "invert(100%) hue-rotate(180deg) saturate(1.25)"
    }
  },
  ".canvas-card-menu": {
    "background-color": { "light": null, "dark": "var(--background-secondary)" }
  },
  ".canvas-card-menu .canvas-card-menu-button svg": {
    "fill": { "light": null, "dark": "var(--background-secondary)" }
  },
  ".is-mobile": {
    "--background-modifier-cover": {
      "light": null,
      "dark": "rgba(0, 0, 0, 0.5)"
    },
    "--background-modifier-form-field": {
      "light": null,
      "dark": "var(--background-modifier-border)"
    },
    "--background-modifier-hover": {
      "light": null,
      "dark": "rgba(var(--mono-rgb-100), 0.15)"
    },
    "--background-modifier-message": {
      "light": null,
      "dark": "var(--background-secondary)"
    },
    "--color-base-00": { "light": null, "dark": "#000" },
    "--color-base-10": { "light": null, "dark": "#111" },
    "--color-base-20": { "light": null, "dark": "#1e1e1e" },
    "--mobile-sidebar-background": {
      "light": null,
      "dark": "var(--background-secondary)"
    },
    "--search-result-background": {
      "light": null,
      "dark": "var(--background-secondary)"
    },
    "--settings-home-background": {
      "light": null,
      "dark": "var(--background-primary)"
    },
    "--tag-background": {
      "light": null,
      "dark": "hsla(var(--interactive-accent-hsl), 0.2)"
    }
  },
  ".is-tablet": {
    "--interactive-hover": {
      "light": null,
      "dark": "var(--background-modifier-border-hover)"
    },
    "--interactive-normal": {
      "light": null,
      "dark": "var(--background-modifier-border)"
    },
    "--modal-background": {
      "light": null,
      "dark": "var(--background-secondary)"
    },
    "--titlebar-background": {
      "light": null,
      "dark": "var(--background-primary)"
    },
    "--titlebar-background-focused": {
      "light": null,
      "dark": "var(--background-primary)"
    }
  },
  ".is-tablet .community-item": {
    "background-color": { "light": null, "dark": "var(--background-secondary)" }
  },
  ".is-tablet .vertical-tab-content": {
    "background-color": { "light": null, "dark": "var(--background-secondary)" }
  },
  ".workspace-drawer-inner": {
    "background-color": { "light": null, "dark": "var(--background-secondary)" }
  },
  ".is-mobile .prompt": {
    "background-color": { "light": null, "dark": "var(--background-secondary)" }
  },
  ".is-mobile .prompt input.prompt-input": {
    "background-color": { "light": null, "dark": "var(--background-secondary)" }
  },
  ".is-tablet .suggestion-container": {
    "background-color": { "light": null, "dark": "var(--background-secondary)" }
  }
}

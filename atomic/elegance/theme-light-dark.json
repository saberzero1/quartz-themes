{
  "body": {
    "--red-rgb": "255, 85, 85",
    "--orange-rgb": "255, 184, 108",
    "--yellow-rgb": "241, 250, 140",
    "--green-rgb": "80, 250, 123",
    "--cyan-rgb": "139, 233, 253",
    "--blue-rgb": "98, 114, 164",
    "--purple-rgb": "189, 147, 249",
    "--pink-rgb": "255, 121, 198",
    "--red": "#ff5555",
    "--orange": "#ffb86c",
    "--yellow": "#f1fa8c",
    "--green": "#50fa7b",
    "--cyan": "#8be9fd",
    "--blue": "#6495ed",
    "--purple": "#bd93f9",
    "--pink": "#ff79c6"
  },
  ".empty-state": {
    "font-size": "20px",
    "animation-name": { "light": "title_light", "dark": "title_dark" },
    "animation-duration": "3.5s",
    "animation-iteration-count": "infinite"
  },
  ".view-header": {
    "border-radius": "6px",
    "background": {
      "light": null,
      "dark": "linear-gradient(to left, #7c7c7c30, #808080)"
    },
    "border-top": {
      "light": null,
      "dark": "1px solid var(--border-color) !important"
    },
    "border-bottom": { "light": null, "dark": "2px solid #737373 !important" }
  },
  ".setting-hotkey.has-conflict": {
    "background-color": "var(--background-modifier-error)",
    "color": { "light": "black", "dark": "white" }
  },
  ".tooltip": {
    "border": "1px solid gray",
    "background-color": { "light": "#ffffff", "dark": "#000000" },
    "color": "var(--text-normal)"
  },
  ".menu": { "background-color": "#000000" },
  ".menu-item.selected:not(.is-disabled):not(.is-label)": {
    "background-color": "#000000",
    "border": "2px solid var(--text-accent)",
    "transition": "transform 0.29s ease-out 0s"
  },
  ".cm-s-obsidian span.cm-url": {
    "color": { "light": "#ea9400", "dark": "#ffdbb4" },
    "font-style": "italic",
    "cursor": "pointer"
  },
  ".progress-bar-message::before": {
    "content": "\"\"",
    "display": "block",
    "background": {
      "light": "center / contain no-repeat url(\"https://github.com/Emrie-Candera/Bubble-Space-Theme/blob/main/theme%20image/Astronaut.gif?raw=true\")",
      "dark": "center / contain no-repeat url(\"https://www.tumblr.com/guitar-slut/154962043348?raw=true\")"
    },
    "width": "300px",
    "height": "200px",
    "margin-left": "0",
    "margin-right": "0",
    "margin-top": "-50px",
    "margin-bottom": "0px",
    "border-radius": "none"
  },
  ".progress-bar-message": {
    "font-family": "arial",
    "font-size": "21px",
    "font-weight": { "light": "bolder", "dark": "bold" },
    "color": { "light": "white", "dark": null }
  },
  ".progress-bar-line": {
    "background-color": { "light": "#ffffff", "dark": null }
  },
  ".proress-bar-subline": {
    "background-color": { "light": "#ffffff", "dark": null }
  },
  ".progress-bar": {
    "background-color": { "light": "#4ba8e7", "dark": "#262639" }
  },
  ".cards table.dataview thead:before": {
    "background-image": {
      "light": "url(\"data:image/svg+xml, utf8, <svg xmlns='http://www.w3.org/2000/svg' fill='none' viewBox='0 0 100 100'><path fill='black' d='M49.792 33.125l-5.892 5.892L33.333 28.45V83.333H25V28.45L14.438 39.017L8.542 33.125L29.167 12.5l20.625 20.625zm41.667 33.75L70.833 87.5l-20.625 -20.625l5.892 -5.892l10.571 10.567L66.667 16.667h8.333v54.883l10.567 -10.567l5.892 5.892z'></path></svg>\")",
      "dark": null
    }
  },
  "img:hover": {
    "transform": "scale(1.02)",
    "transition": "transform 0.45s ease-in-out 0s"
  },
  ".cm-active.cm-line": {
    "border-radius": "3px",
    "animation-name": {
      "light": "active_line_light",
      "dark": "active_line_dark"
    },
    "animation-duration": { "light": "2.3s", "dark": "2.5s" },
    "animation-iteration-count": "infinite"
  },
  "#calendar-container .week-num": {
    "background-color": "#ffb86c",
    "font-weight": "900"
  },
  "#calendar-container .week-num:hover": { "background-color": "#ffb86c" },
  ".mermaid > svg": {
    "filter": {
      "light": null,
      "dark": "invert(100%) hue-rotate(180deg) saturate(1.25)"
    },
    "} .markdown-source-view.mod-cm6 .cm-hmd-codeblock { --indentation-guide-editing-indent": {
      "light": null,
      "dark": "0"
    },
    "} .markdown-source-view.mod-cm6 .cm-preview-code-block pre { margin": {
      "light": null,
      "dark": "0"
    },
    "} .markdown-source-view.mod-cm6 .code-block-flair { position": {
      "light": null,
      "dark": "absolute"
    },
    "right": { "light": null, "dark": "6px" },
    "top": { "light": null, "dark": "6px" },
    "z-index": { "light": null, "dark": "1" },
    "display": { "light": null, "dark": "inline-block" },
    "padding": { "light": null, "dark": "var(--size-4-1) var(--size-4-2)" },
    "border-radius": { "light": null, "dark": "var(--code-radius)" },
    "font-family": { "light": null, "dark": "var(--font-interface)" },
    "font-size": { "light": null, "dark": "var(--font-ui-smaller)" },
    "color": { "light": null, "dark": "var(--text-muted)" },
    "cursor": { "light": null, "dark": "var(--cursor)" }
  },
  ".graph-view.color-fill-attachment": {
    "color": { "light": null, "dark": "#ff5555" }
  },
  ".graph-view.color-fill-unresolved": {
    "color": { "light": null, "dark": "#6272a4" }
  },
  "code[class*=\"language-\"]": {
    ".theme-dark pre[class*=\"language-\"], .theme-light code[class*=\"language-\"], .theme-light pre[class*=\"language-\"] { text-shadow": {
      "light": null,
      "dark": "none !important"
    },
    "color": { "light": null, "dark": "var(--text-faint) !important" },
    "background-color": { "light": null, "dark": "var(--pre-code)" }
  },
  "input[data-task=\"i\"]:checked": {
    ".theme-dark li[data-task=\"i\"] > input": {
      "light": null,
      "dark": "checked, .theme-dark li[data-task=\"i\"] > p > input:checked { background-image:url(\"data:image/svg+xml, %3Csvg xmlns='http%3A%2F%2Fwww.w3.org%2F2000%2Fsvg' width='20' height='20' preserveAspectRatio='xMidYMid meet' viewBox='0 0 512 512'%3E%3Cpath fill='none' stroke='black' stroke-opacity='0.8' stroke-linecap='round' stroke-linejoin='round' stroke-width='40' d='M196 220h64v172'%2F%3E%3Cpath fill='none' stroke='black' stroke-opacity='0.8' stroke-linecap='round' stroke-miterlimit='10' stroke-width='40' d='M187 396h138'%2F%3E%3Cpath fill='black' fill-opacity='0.8' d='M256 160a32 32 0 1 1 32-32a32 32 0 0 1-32 32Z'%2F%3E%3C%2Fsvg%3E\")"
    }
  },
  "input[data-task=\"S\"]:checked": {
    ".theme-dark li[data-task=\"S\"] > input": {
      "light": null,
      "dark": "checked, .theme-dark li[data-task=\"S\"] > p > input:checked { background-image:url(\"data:image/svg+xml, %3Csvg xmlns='http%3A%2F%2Fwww.w3.org%2F2000%2Fsvg' width='20' height='20' preserveAspectRatio='xMidYMid meet' viewBox='0 0 48 48'%3E%3Cpath fill-opacity='0.8' fill='black' fill-rule='evenodd' d='M26 8a2 2 0 1 0-4 0v2a8 8 0 1 0 0 16v8a4.002 4.002 0 0 1-3.773-2.666a2 2 0 0 0-3.771 1.332A8.003 8.003 0 0 0 22 38v2a2 2 0 1 0 4 0v-2a8 8 0 1 0 0-16v-8a4.002 4.002 0 0 1 3.773 2.666a2 2 0 0 0 3.771-1.332A8.003 8.003 0 0 0 26 10V8Zm-4 6a4 4 0 0 0 0 8v-8Zm4 12v8a4 4 0 0 0 0-8Z' clip-rule='evenodd'%2F%3E%3C%2Fsvg%3E\")"
    }
  }
}

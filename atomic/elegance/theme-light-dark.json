    "--red-rgb": { "light": "255, 85, 85", "dark": null },
    "--orange-rgb": { "light": "255, 184, 108", "dark": null },
    "--yellow-rgb": { "light": "241, 250, 140", "dark": null },
    "--green-rgb": { "light": "80, 250, 123", "dark": null },
    "--cyan-rgb": { "light": "139, 233, 253", "dark": null },
    "--blue-rgb": { "light": "98, 114, 164", "dark": null },

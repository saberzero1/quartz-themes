    "--pink": { "light": "#ff79c6", "dark": null },
    "--canvas-color": { "light": null, "dark": "126, 126, 126" }
  },
  ".notice progress[value]::-webkit-progress-bar": {
    "background-color": { "light": "#262626", "dark": null },
    "box-shadow": { "light": "inset 0px 0px 0px 1px #363636", "dark": null }
  },
  ".empty-state": {
    "font-size": "20px",
    "animation-name": { "light": "title_light", "dark": "title_dark" },
    "animation-duration": "3.5s",
    "animation-iteration-count": "infinite"
  },
  ".view-header": {
    "border-radius": "6px",
    "background": {
      "light": null,
      "dark": "linear-gradient(to left, #7c7c7c30, #808080)"
    },
    "border-top": {
      "light": null,
      "dark": "1px solid var(--border-color) !important"
    },
    "border-bottom": { "light": null, "dark": "2px solid #737373 !important" }
  },
  ".setting-hotkey.has-conflict": {
    "background-color": "var(--background-modifier-error)",
    "color": { "light": "black", "dark": "white" }
  },
  ".tooltip": {
    "border": "1px solid gray",
    "background-color": { "light": "#ffffff", "dark": "#000000" },
    "color": "var(--text-normal)"
  },
  ".menu": { "background-color": "#000000" },
  ".menu-item.selected:not(.is-disabled):not(.is-label)": {
    "background-color": "#000000",
    "border": "2px solid var(--text-accent)",
    "transition": "transform 0.29s ease-out 0s"
  },
  ".cm-s-obsidian span.cm-url": {
    "color": { "light": "#ea9400", "dark": "#ffdbb4" },
    "font-style": "italic",
    "cursor": "pointer"
  },
  "code[class*=\"language-\"]": {
    "background-color": "var(--pre-code)",
    "color": "var(--text-faint) !important",
    "text-shadow": "none !important"
  },
  "pre[class*=\"language-\"]": {
    "background-color": "var(--pre-code)",
    "color": "var(--text-faint) !important",
    "text-shadow": "none !important"
  },
  ".progress-bar-message::before": {
    "content": "\"\"",
    "display": "block",
    "background": {
      "light": "center / contain no-repeat url(\"https://github.com/Emrie-Candera/Bubble-Space-Theme/blob/main/theme%20image/Astronaut.gif?raw=true\")",
      "dark": "center / contain no-repeat url(\"https://www.tumblr.com/guitar-slut/154962043348?raw=true\")"
    },
    "width": "300px",
    "height": "200px",
    "margin-left": "0",
    "margin-right": "0",
    "margin-top": "-50px",
    "margin-bottom": "0px",
    "border-radius": "none"
  },
  ".progress-bar-message": {
    "font-family": "arial",
    "font-size": "21px",
    "font-weight": { "light": "bolder", "dark": "bold" },
    "color": { "light": "white", "dark": null }
  },
  ".progress-bar-line": {
    "background-color": { "light": "#ffffff", "dark": null }
  },
  ".proress-bar-subline": {
    "background-color": { "light": "#ffffff", "dark": null }
  },
  ".progress-bar": {
    "background-color": { "light": "#4ba8e7", "dark": "#262639" }

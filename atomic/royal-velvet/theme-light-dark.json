{
  "body": {
    "color-scheme": { "light": "light", "dark": null },
    "--highlight-mix-blend-mode": { "light": "darken", "dark": null },
    "--mono-rgb-0": { "light": "255, 255, 255", "dark": null },
    "--mono-rgb-100": { "light": "0, 0, 0", "dark": null },
    "--color-red-rgb": { "light": "233, 49, 71", "dark": null },
    "--color-red": { "light": "#e93147", "dark": null },
    "--color-orange-rgb": { "light": "236, 117, 0", "dark": null },
    "--color-orange": { "light": "#ec7500", "dark": null },
    "--color-yellow-rgb": { "light": "224, 172, 0", "dark": null },
    "--color-yellow": { "light": "#e0ac00", "dark": null },
    "--color-green-rgb": { "light": "8, 185, 78", "dark": null },
    "--color-green": { "light": "#08b94e", "dark": null },
    "--color-cyan-rgb": { "light": "0, 191, 188", "dark": null },
    "--color-cyan": { "light": "#00bfbc", "dark": null },
    "--color-blue-rgb": { "light": "8, 109, 221", "dark": null },
    "--color-blue": { "light": "#086ddd", "dark": null },
    "--color-purple-rgb": { "light": "120, 82, 238", "dark": null },
    "--color-purple": { "light": "#7852ee", "dark": null },
    "--color-pink-rgb": { "light": "213, 57, 132", "dark": null },
    "--color-pink": { "light": "#d53984", "dark": null },
    "--color-base-00": { "light": "#ffffff", "dark": null },
    "--color-base-05": { "light": "#fcfcfc", "dark": null },
    "--color-base-10": { "light": "#fafafa", "dark": null },
    "--color-base-20": { "light": "#f6f6f6", "dark": null },
    "--color-base-25": { "light": "#e3e3e3", "dark": null },
    "--color-base-30": { "light": "#e0e0e0", "dark": null },
    "--color-base-35": { "light": "#d4d4d4", "dark": null },
    "--color-base-40": { "light": "#bdbdbd", "dark": null },
    "--color-base-50": { "light": "#ababab", "dark": null },
    "--color-base-60": { "light": "#707070", "dark": null },
    "--color-base-70": { "light": "#5c5c5c", "dark": null },
    "--color-base-100": { "light": "#222222", "dark": null },
    "--color-accent-hsl": {
      "light": "var(--accent-h), var(--accent-s), var(--accent-l)",
      "dark": null
    },
    "--color-accent": {
      "light": "hsl(var(--accent-h), var(--accent-s), var(--accent-l))",
      "dark": null
    },
    "--color-accent-1": {
      "light": "hsl( calc(var(--accent-h) - 1), calc(var(--accent-s) * 1.01), calc(var(--accent-l) * 1.075) )",
      "dark": null
    },
    "--color-accent-2": {
      "light": "hsl( calc(var(--accent-h) - 3), calc(var(--accent-s) * 1.02), calc(var(--accent-l) * 1.15) )",
      "dark": null
    },
    "--input-shadow-hover": {
      "light": "inset 0 0 0 1px #000000, 0 2px 3px 0 #000000, 0 1px 1.5px 0 #000000, 0 1px 2px 0 #000000, 0 0 0 0 transparent",
      "dark": null
    },
    "--shadow-s": {
      "light": "0px 1px 2px #000000, 0px 3.4px 6.7px #000000, 0px 15px 30px #000000",
      "dark": null
    },
    "--shadow-l": {
      "light": "0px 1.8px 7.3px #000000, 0px 6.3px 24.7px #000000, 0px 30px 90px #000000",
      "dark": null
    },
    "--layer-0": { "light": "hsl(var(--layer-hue-sat), 89%)", "dark": null },
    "--layer-1": { "light": "hsl(var(--layer-hue-sat), 99%)", "dark": null },
    "--layer-2": { "light": "hsl(var(--layer-hue-sat), 96%)", "dark": null },
    "--layer-3": { "light": "hsl(var(--layer-hue-sat), 83%)", "dark": null },
    "--layer-4": { "light": "hsl(var(--layer-hue-sat), 80%)", "dark": null },
    "--on-layer-tuple": { "light": "234, 14%, 15%", "dark": null },
    "--red-hue": { "light": "0", "dark": null },
    "--orange-hue": { "light": "30", "dark": null },
    "--yellow-hue": { "light": "40", "dark": null },
    "--green-hue": { "light": "105", "dark": null },
    "--blue-hue": { "light": "200", "dark": null },
    "--purple-hue": { "light": "300", "dark": null },
    "--purple-pink-hue": { "light": "310", "dark": null },
    "--pink-hue": { "light": "330", "dark": null },
    "--primary-sat": { "light": "100%", "dark": null },
    "--secondary-sat": { "light": "80%", "dark": null },
    "--primary-lit": { "light": "43%", "dark": null },
    "--secondary-lit": { "light": "50%", "dark": null },
    "--background-modifier-border": { "light": "transparent", "dark": null },
    "--background-modifier-border-focus": {
      "light": "transparent",
      "dark": null
    },
    "--background-modifier-border-hover": {
      "light": "transparent",
      "dark": null
    },
    "--background-modifier-box-shadow": {
      "light": "var(--layer-3)",
      "dark": null
    },
    "--background-modifier-cover": { "light": "var(--layer-0)", "dark": null },
    "--background-modifier-error": { "light": "var(--red-900)", "dark": null },
    "--background-modifier-error-hover": {
      "light": "var(--red)",
      "dark": null
    },
    "--background-modifier-error-rgb": { "light": "61, 0, 0", "dark": null },
    "--background-modifier-form-field": {
      "light": "var(--pink-100)",
      "dark": null
    },
    "--background-modifier-form-field-highlighted": {
      "light": "var(--pink-300)",
      "dark": null
    },
    "--background-modifier-success": {
      "light": "var(--green-200)",
      "dark": null
    },
    "--background-primary": { "light": "var(--layer-2)", "dark": null },
    "--background-primary-alt": { "light": "var(--layer-1)", "dark": null },
    "--background-secondary": { "light": "var(--layer-1)", "dark": null },
    "--background-secondary-alt": { "light": "var(--layer-0)", "dark": null },
    "--blockquote-border": { "light": "var(--red)", "dark": null },
    "--blockquote-border-color": { "light": "none", "dark": null },
    "--blue": { "light": "hsl(var(--blue-tuple))", "dark": null },
    "--blue-600": { "light": "hsla(var(--blue-tuple), 0.6)", "dark": null },
    "--blue-secondary-tuple": { "light": "", "dark": null },
    "--canvas-color": { "light": null, "dark": "126, 126, 126" }
  },
  ".notice progress[value]::-webkit-progress-bar": {
    "background-color": { "light": "#262626", "dark": null },
    "box-shadow": { "light": "inset 0px 0px 0px 1px #363636", "dark": null }
  },
  ".dropdown": {
    "background-image": {
      "light": "url( data:image/svg + xml, charset=US-ASCII, %3Csvg%20xmlns=%22http://www.w3.org/2000/svg%22%20width=%22292.4%22%20height=%22292.4%22%3E%3Cpath%20d=%22M287%2069.4a17.6%2017.6%200%200%200-13-5.4H18.4c-5%200-9.3%201.8-12.9%205.4A17.6%2017.6%200%200%200%200%2082.2c0%205%201.8%209.3%205.4%2012.9l128%20127.9c3.6%203.6%207.8%205.4%2012.8%205.4s9.2-1.8%2012.8-5.4L287%2095c3.5-3.5%205.4-7.8%205.4-12.8%200-5-1.9-9.2-5.5-12.8z%22%20fill=%22hsl( 234, %2014%25, %2015%25 )%22/%3E%3C/svg%3E )",
      "dark": "url( data:image/svg + xml, charset=US-ASCII, %3Csvg%20xmlns=%22http://www.w3.org/2000/svg%22%20width=%22292.4%22%20height=%22292.4%22%3E%3Cpath%20d=%22M287%2069.4a17.6%2017.6%200%200%200-13-5.4H18.4c-5%200-9.3%201.8-12.9%205.4A17.6%2017.6%200%200%200%200%2082.2c0%205%201.8%209.3%205.4%2012.9l128%20127.9c3.6%203.6%207.8%205.4%2012.8%205.4s9.2-1.8%2012.8-5.4L287%2095c3.5-3.5%205.4-7.8%205.4-12.8%200-5-1.9-9.2-5.5-12.8z%22%20fill=%22hsl( 60, %2030%25, %2096.1%25 )%22/%3E%3C/svg%3E )"
    }
  },
  ".pdf-container.mod-themed .popupWrapper > div": {
    "background-color": {
      "light": null,
      "dark": "var(--background-secondary-alt)"
    },
    "filter": {
      "light": null,
      "dark": "drop-shadow(0px 0px 1px var(--color-base-60)) drop-shadow(0px 1px 2px #000000) drop-shadow(0px 4px 6px #000000)"
    }
  },
  ".pdf-container.mod-themed .popupWrapper > div::after": {
    "background-color": {
      "light": null,
      "dark": "var(--background-secondary-alt)"
    }
  },
  ".pdf-container.mod-themed .popupContent:empty + .popupMeta": {
    "background-color": {
      "light": null,
      "dark": "var(--background-secondary-alt)"
    }
  },
  ".mermaid > svg": {
    "filter": {
      "light": null,
      "dark": "invert(100%) hue-rotate(180deg) saturate(1.25)"
    }
  },
  ".canvas-card-menu": {
    "background-color": { "light": null, "dark": "var(--background-secondary)" }
  },
  ".canvas-card-menu .canvas-card-menu-button svg": {
    "fill": { "light": null, "dark": "var(--background-secondary)" }
  },
  ".is-mobile": {
    "--color-base-00": { "light": null, "dark": "#000" },
    "--color-base-10": { "light": null, "dark": "#111" },
    "--color-base-20": { "light": null, "dark": "#1e1e1e" },
    "--tag-background": {
      "light": null,
      "dark": "hsla(var(--interactive-accent-hsl), 0.2)"
    },
    "--search-result-background": {
      "light": null,
      "dark": "var(--background-secondary)"
    },
    "--background-modifier-form-field": {
      "light": null,
      "dark": "var(--background-modifier-border)"
    },
    "--background-modifier-cover": { "light": null, "dark": "#000000" },
    "--background-modifier-hover": {
      "light": null,
      "dark": "rgba(var(--mono-rgb-100), 0.15)"
    },
    "--settings-home-background": {
      "light": null,
      "dark": "var(--background-primary)"
    },
    "--mobile-sidebar-background": {
      "light": null,
      "dark": "var(--background-secondary)"
    },
    "--background-modifier-message": {
      "light": null,
      "dark": "var(--background-secondary)"
    }
  },
  ".is-tablet": {
    "--titlebar-background": {
      "light": null,
      "dark": "var(--background-primary)"
    },
    "--titlebar-background-focused": {
      "light": null,
      "dark": "var(--background-primary)"
    },
    "--interactive-normal": {
      "light": null,
      "dark": "var(--background-modifier-border)"
    },
    "--interactive-hover": {
      "light": null,
      "dark": "var(--background-modifier-border-hover)"
    },
    "--modal-background": {
      "light": null,
      "dark": "var(--background-secondary)"
    }
  },
  ".is-tablet .community-item": {
    "background-color": { "light": null, "dark": "var(--background-secondary)" }
  },
  ".is-tablet .vertical-tab-content": {
    "background-color": { "light": null, "dark": "var(--background-secondary)" }
  },
  ".workspace-drawer-inner": {
    "background-color": { "light": null, "dark": "var(--background-secondary)" }
  },
  ".is-mobile .prompt": {
    "background-color": { "light": null, "dark": "var(--background-secondary)" }
  },
  ".is-mobile .prompt input.prompt-input": {
    "background-color": { "light": null, "dark": "var(--background-secondary)" }
  },
  ".is-tablet .suggestion-container": {
    "background-color": { "light": null, "dark": "var(--background-secondary)" }
  }
}

{
  ".workspace-tabs .workspace-leaf .view-content": {
    "color": "var(--text-color)"
  },
  ".markdown-preview-view": { "color": "var(--text-color)" },
  ".is-mobile .mobile-navbar": {
    "border-top": {
      "light": "#000000 solid 0.8px",
      "dark": "#ffffff solid 0.5px"
    },
    "background-color": "transparent"
  },
  ".is-mobile .mobile-toolbar-options-container": {
    "border-top": {
      "light": "#000000 solid 0.8px",
      "dark": "#ffffff solid 0.5px"
    },
    "background-color": { "light": "#e5e5eb", "dark": "#313131" }
  },
  ".tag-pane-tag-text": { "background-color": "#000000" },
  ".markdown-source-view.mod-cm6 .edit-block-button": {
    "color": { "light": "white", "dark": null }
  },
  ".markdown-source-view.mod-cm6 .edit-block-button:hover": {
    "color": { "light": "white", "dark": null }
  },
  "::-webkit-scrollbar-thumb": {
    "background-color": {
      "light": "#000000 !important",
      "dark": "#ffffff !important"
    }
  },
  ".is-translucent td.svelte-egt0yd": {
    "border-right": { "light": "1px solid #696969", "dark": null }
  },
  ".is-translucent .status-bar": {
    "background-color": { "light": "#f5f5f5", "dark": "#1c1c1c" },
    "-webkit-backdrop-filter": { "light": "blur(100px)", "dark": "blur(10px)" },
    "backdrop-filter": { "light": "blur(100px)", "dark": "blur(10px)" }
  },
  ".is-translucent .search-result-file-matches": {
    "background-color": { "light": "transparent !important", "dark": null }
  },
  ".is-translucent .search-result-file-match-destination-file": {
    "background-color": { "light": "#ffffff7b", "dark": "#ffffff" }
  },
  ".is-translucent .menu": {
    "background": { "light": "#f0f0f0", "dark": "#1e1e1e" },
    "backdrop-filter": "blur(30px)"
  },
  ".mermaid > svg": {
    "filter": {
      "light": null,
      "dark": "invert(100%) hue-rotate(180deg) saturate(1.25)"
    },
    "} .markdown-source-view.mod-cm6 .cm-hmd-codeblock { --indentation-guide-editing-indent": {
      "light": null,
      "dark": "0"
    },
    "} .markdown-source-view.mod-cm6 .cm-preview-code-block pre { margin": {
      "light": null,
      "dark": "0"
    },
    "} .markdown-source-view.mod-cm6 .code-block-flair { position": {
      "light": null,
      "dark": "absolute"
    },
    "right": { "light": null, "dark": "6px" },
    "top": { "light": null, "dark": "6px" },
    "z-index": { "light": null, "dark": "1" },
    "display": { "light": null, "dark": "inline-block" },
    "padding": { "light": null, "dark": "var(--size-4-1) var(--size-4-2)" },
    "border-radius": { "light": null, "dark": "var(--code-radius)" },
    "font-family": { "light": null, "dark": "var(--font-interface)" },
    "font-size": { "light": null, "dark": "var(--font-ui-smaller)" },
    "color": { "light": null, "dark": "var(--text-muted)" },
    "cursor": { "light": null, "dark": "var(--cursor)" }
  },
  ".is-mobile .view-header": {
    "border-bottom": { "light": null, "dark": "#ffffff solid 0.5px" }
  },
  ".search-result-file-matches": {
    "background-color": { "light": null, "dark": "transparent !important" },
    "box-shadow": { "light": null, "dark": "0 0 0 1px #c1c1c114 !important" }
  },
  ".search-result-file-match": {
    "border-bottom": { "light": null, "dark": "1px solid #c1c1c114 !important" }
  },
  ".is-translucent .markdown-source-view.mod-cm6 .cm-editor": {
    "background-color": { "light": null, "dark": "transparent" }
  },
  ".is-translucent .markdown-preview-view": {
    "background-color": { "light": null, "dark": "transparent !important" }
  },
  "#editingToolbarModalBar button.editingToolbar-Divider-Line": {
    ".theme-dark #editingToolbarPopoverBar button.editingToolbar-Divider-Line { background-color": {
      "light": null,
      "dark": "transparent !important"
    }
  },
  ":not(.markmap-fold).markmap-node > circle": {
    "fill": { "light": null, "dark": "black" }
  }
}

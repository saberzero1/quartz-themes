{
  "body": {
    "--background-modifier-border": { "light": "#af303d", "dark": null },
    "--background-primary": { "light": "#e7e7e7", "dark": null },
    "--background-primary-alt": { "light": "#707070", "dark": null },
    "--background-secondary": { "light": "#991e2a", "dark": null },
    "--background-secondary-alt": { "light": "#991e2a", "dark": null },
    "--code-background": { "light": "#ebd9d9", "dark": null },
    "--code-size": { "light": "var(--font-medium)", "dark": null },
    "--font-headers": { "light": "var(--font-text-headers)", "dark": null },
    "--font-interface": { "light": "var(--font-text-normal)", "dark": null },
    "--font-monospace": { "light": "var(--font-code-blocks)", "dark": null },
    "--font-text": { "light": "var(--font-text-normal)", "dark": null },
    "--icon-color": { "light": "#bebebe", "dark": null },
    "--icon-color-focused": { "light": "#d8d8d8", "dark": null },
    "--icon-color-hover": { "light": "#ffffff", "dark": null },
    "--inline-code": { "light": "#458588", "dark": null },
    "--inline-title-font": {
      "light": "var(--font-text-headers)",
      "dark": null
    },
    "--interactive-accent": { "light": "#ff3e3e", "dark": null },
    "--interactive-accent-rgb": { "light": "#ff3e3e", "dark": null },
    "--interactive-before": { "light": "#a89984", "dark": null },
    "--nav-item-color-active": { "light": "#ffffff", "dark": null },
    "--nav-item-color-hover": { "light": "#ffeeee", "dark": null },
    "--nav-item-color-selected": { "light": "#eba2a2", "dark": null },
    "--nav-item-weight": { "light": "bold", "dark": null },
    "--pre-code": { "light": "#bebebe", "dark": null },
    "--status-bar-text-color": { "light": "#ffffff", "dark": null },
    "--tab-font-weight": { "light": "bold", "dark": null },
    "--text-a": { "light": "#ff3e3e", "dark": null },
    "--text-a-hover": { "light": "#8a8e96", "dark": null },
    "--text-accent": { "light": "#ff3e3e", "dark": null },
    "--text-accent-hover": { "light": "#8e929b", "dark": null },
    "--text-faint": { "light": "#ff8585", "dark": null },
    "--text-h1-background": { "light": "#c21f25", "dark": null },
    "--text-h2-background": { "light": "#e93538", "dark": null },
    "--text-h3-background": { "light": "#221e1f", "dark": null },
    "--text-h4-background": { "light": "#87007f", "dark": null },
    "--text-h5-background": { "light": "#67bc45", "dark": null },
    "--text-highlight-bg": { "light": "#5ea9ff", "dark": null },
    "--text-link": { "light": "#424448", "dark": null },
    "--text-normal": { "light": "#000000", "dark": null },
    "--text-selection": { "light": "#8fc3ff", "dark": null },
    "--text-title-h1": { "light": "#e7e7e7", "dark": null },
    "--text-title-h2": { "light": "#e7e7e7", "dark": null },
    "--text-title-h3": { "light": "#e7e7e7", "dark": null },
    "--text-title-h4": { "light": "#e7e7e7", "dark": null },
    "--text-title-h5": { "light": "#e7e7e7", "dark": null },
    "--text-title-h6": { "light": "#640a0d", "dark": null },
    "--text-white": { "light": "#ffffff", "dark": null },
    "--titlebar-text-color": { "light": "#ffffff", "dark": null },
    "--titlebar-text-color-focused": { "light": "#ffffff", "dark": null },
    "--vim-cursor": { "light": "#ff3e3e", "dark": null },
    "--canvas-color": { "light": null, "dark": "126, 126, 126" }
  },
  ".notice progress[value]::-webkit-progress-bar": {
    "background-color": { "light": "#262626", "dark": null },
    "box-shadow": { "light": "inset 0px 0px 0px 1px #363636", "dark": null }
  },
  ".workspace-leaf.mod-active .clickable-icon": {
    "color": { "light": "#8a8e96", "dark": "#d38dff" },
    "opacity": "1"
  },
  ".tree-item-self": { "--text-normal": "rgb(255, 255, 255)" },
  ".workspace-tab-header-inner-icon": {
    "--tab-text-color-focused-active-current": "#ffffff"
  },
  ".workspace-tab-header": {
    "color": { "light": "#e77474", "dark": "#81e774" },
    "font-family": "var(--font-code-blocks)"
  },
  ".menu-item-title": { "color": { "light": "#e6e6e6", "dark": "#ffffff" } },
  ".menu-item-icon": {
    "color": { "light": "#e6e6e6", "dark": "var(--icon-color)" }
  },
  ".plugin-tabs .mod-root.workspace-split.mod-vertical div.workspace-leaf": {
    "border-bottom-width": { "light": "0px", "dark": null },
    "border-color": {
      "light": "var(--background-secondary-alt)",
      "dark": null
    },
    "border-left-width": { "light": "3px", "dark": null },
    "border-right-width": { "light": "0px", "dark": null }
  },
  ".dropdown": { "background-image": { "light": null, "dark": "url(\"data" } },
  ".pdf-container.mod-themed .annotationLayer .textWidgetAnnotation input": {
    "filter": { "light": null, "dark": "invert(100%) hue-rotate(180deg)" }
  },
  ".pdf-container.mod-themed .annotationLayer .textWidgetAnnotation textarea": {
    "filter": { "light": null, "dark": "invert(100%) hue-rotate(180deg)" }
  },
  ".pdf-container.mod-themed .popupWrapper > div": {
    "background-color": {
      "light": null,
      "dark": "var(--background-secondary-alt)"
    },
    "filter": {
      "light": null,
      "dark": "drop-shadow(0px 0px 1px var(--color-base-60)) drop-shadow(0px 1px 2px rgba(0, 0, 0, 0.4)) drop-shadow(0px 4px 6px rgba(0, 0, 0, 0.4))"
    }
  },
  ".pdf-container.mod-themed .popupWrapper > div::after": {
    "background-color": {
      "light": null,
      "dark": "var(--background-secondary-alt)"
    }
  },
  ".pdf-container.mod-themed .popupContent:empty + .popupMeta": {
    "background-color": {
      "light": null,
      "dark": "var(--background-secondary-alt)"
    }
  },
  ".mermaid > svg": {
    "filter": {
      "light": null,
      "dark": "invert(100%) hue-rotate(180deg) saturate(1.25)"
    }
  },
  ".canvas-card-menu": {
    "background-color": { "light": null, "dark": "var(--background-secondary)" }
  },
  ".canvas-card-menu .canvas-card-menu-button svg": {
    "fill": { "light": null, "dark": "var(--background-secondary)" }
  },
  ".is-mobile": {
    "--background-modifier-cover": {
      "light": null,
      "dark": "rgba(0, 0, 0, 0.5)"
    },
    "--background-modifier-form-field": {
      "light": null,
      "dark": "var(--background-modifier-border)"
    },
    "--background-modifier-hover": {
      "light": null,
      "dark": "rgba(var(--mono-rgb-100), 0.15)"
    },
    "--background-modifier-message": {
      "light": null,
      "dark": "var(--background-secondary)"
    },
    "--color-base-00": { "light": null, "dark": "#000" },
    "--color-base-10": { "light": null, "dark": "#111" },
    "--color-base-20": { "light": null, "dark": "#1e1e1e" },
    "--mobile-sidebar-background": {
      "light": null,
      "dark": "var(--background-secondary)"
    },
    "--search-result-background": {
      "light": null,
      "dark": "var(--background-secondary)"
    },
    "--settings-home-background": {
      "light": null,
      "dark": "var(--background-primary)"
    },
    "--tag-background": {
      "light": null,
      "dark": "hsla(var(--interactive-accent-hsl), 0.2)"
    }
  },
  ".is-tablet": {
    "--interactive-hover": {
      "light": null,
      "dark": "var(--background-modifier-border-hover)"
    },
    "--interactive-normal": {
      "light": null,
      "dark": "var(--background-modifier-border)"
    },
    "--modal-background": {
      "light": null,
      "dark": "var(--background-secondary)"
    },
    "--titlebar-background": {
      "light": null,
      "dark": "var(--background-primary)"
    },
    "--titlebar-background-focused": {
      "light": null,
      "dark": "var(--background-primary)"
    }
  },
  ".is-tablet .community-item": {
    "background-color": { "light": null, "dark": "var(--background-secondary)" }
  },
  ".is-tablet .vertical-tab-content": {
    "background-color": { "light": null, "dark": "var(--background-secondary)" }
  },
  ".workspace-drawer-inner": {
    "background-color": { "light": null, "dark": "var(--background-secondary)" }
  },
  ".is-mobile .prompt": {
    "background-color": { "light": null, "dark": "var(--background-secondary)" }
  },
  ".is-mobile .prompt input.prompt-input": {
    "background-color": { "light": null, "dark": "var(--background-secondary)" }
  },
  ".is-tablet .suggestion-container": {
    "background-color": { "light": null, "dark": "var(--background-secondary)" }
  },
  ".cm-s-obsidian span.obsidian-search-match-highlight": {
    "background-color": { "light": null, "dark": "#3b2c1f" },
    "color": { "light": null, "dark": "var(--text-normal)" }
  },
  ".plugin-tabs  .mod-root.workspace-split.mod-vertical .workspace-split.mod-vertical > div.workspace-leaf.mod-active": {
    "border": { "light": null, "dark": "0" },
    "border-radius": { "light": null, "dark": "0px 0px 0px 0px" }
  },
  ".plugin-tabs  .mod-root.workspace-split.mod-vertical > div.workspace-leaf.mod-active": {
    "border": { "light": null, "dark": "0" },
    "border-radius": { "light": null, "dark": "0px 0px 0px 0px" }
  }
}
